{
  Map<AbstractConceptDeclaration,Set<SNode>> map;
  if (noInheritance) {
    map=myNodesNoInheritance;
  }
 else {
    map=myNodesAll;
  }
  Set<SNode> set=map.get(acd);
  if (set != null) {
    set.remove(node);
    if (set.isEmpty()) {
      map.remove(acd);
    }
  }
 else {
    LOG.errorWithTrace("Can't find cache for " + acd.getName() + " initialized = "+ myInitialized);
  }
}
