{
  String conceptFqName=NameUtil.nodeFQName(acd);
  Map<String,Set<SNode>> map;
  if (noInheritance) {
    map=myNodesNoInheritance;
  }
 else {
    map=myNodesAll;
  }
  Set<SNode> set=map.get(conceptFqName);
  if (set == null) {
    set=new HashSet<SNode>(1);
    map.put(conceptFqName,set);
  }
  set.add(node);
}
