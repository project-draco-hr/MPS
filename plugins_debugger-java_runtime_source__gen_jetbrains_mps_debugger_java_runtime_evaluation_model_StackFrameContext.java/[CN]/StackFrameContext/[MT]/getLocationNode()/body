{
  JavaStackFrame javaStackFrame=myUiState.getStackFrame();
  SNode locationNode=null;
  if (javaStackFrame != null) {
    StackFrame stackFrame=javaStackFrame.getStackFrame();
    if (stackFrame != null) {
      try {
        Location location=stackFrame.location();
        locationNode=TraceInfoUtilComponent.getInstance().getNode(location.declaringType().name(),location.sourceName(),location.lineNumber());
      }
 catch (      Throwable t) {
        if (log.isErrorEnabled()) {
          log.error("",t);
        }
      }
    }
  }
  return locationNode;
}
