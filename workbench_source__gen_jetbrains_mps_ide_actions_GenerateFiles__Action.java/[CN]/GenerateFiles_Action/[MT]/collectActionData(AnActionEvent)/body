{
  if (!(super.collectActionData(event))) {
    return false;
  }
  this.project=event.getData(MPSDataKeys.MPS_PROJECT);
  if (this.project == null) {
    return false;
  }
  this.ppNode=event.getData(MPSDataKeys.LOGICAL_VIEW_NODE);
  if (this.ppNode == null) {
    return false;
  }
  return true;
}
