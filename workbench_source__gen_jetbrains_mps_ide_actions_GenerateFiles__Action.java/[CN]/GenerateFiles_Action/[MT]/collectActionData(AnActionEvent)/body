{
  if (!(super.collectActionData(event))) {
    return false;
  }
  this.project=event.getData(MPSDataKeys.MPS_PROJECT);
  if (this.project == null) {
    return false;
  }
  this.ppNodes=event.getData(MPSDataKeys.LOGICAL_VIEW_NODES);
  if (this.ppNodes == null) {
    return false;
  }
  return true;
}
