{
  EditorCell contextCell=editorContext.getContextCell();
  if ((contextCell == null)) {
    return false;
  }
  SNode contextNode=contextCell.getSNode();
  if (contextNode == null) {
    return false;
  }
  if (!(SNodeUtil.isInstanceOf(contextNode,SConceptRepository.getInstance().getConcept("jetbrains.mpslite.structure.LinePart")))) {
    return false;
  }
  return true;
}
