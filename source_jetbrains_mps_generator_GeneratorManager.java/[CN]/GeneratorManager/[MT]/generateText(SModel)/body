{
  OutputView view=myProject.getComponent(OutputView.class);
  view.clear();
  Iterator<SemanticNode> roots=targetModel.roots();
  while (roots.hasNext()) {
    SemanticNode node=roots.next();
    String nodeText=generateText(node);
    view.append(nodeText);
    view.append("\n");
    view.append("\r\n-------------------------------------------------------------------------------");
  }
  view.activate();
}
