{
  super(whatToDo,logger);
  myBuildServerMessageFormat=getBuildServerMessageFormat();
  File tmpDir;
  try {
    tmpDir=File.createTempFile("gentest_","tmp");
    tmpDir.delete();
    tmpDir.mkdir();
  }
 catch (  IOException ex) {
    throw new RuntimeException(ex);
  }
  this.tmpPath=tmpDir.getAbsolutePath();
}
