{
  String result=NameUtil.shortNameFromLongName(myCell.getClass().getName());
  if (myCell instanceof EditorCell_Label) {
    String text=((EditorCell_Label)myCell).getRenderedText();
    result+=" text = \"" + text + "\"";
  }
  if (myCell.isErrorState()) {
    result+=" (error state)";
  }
  return result;
}
