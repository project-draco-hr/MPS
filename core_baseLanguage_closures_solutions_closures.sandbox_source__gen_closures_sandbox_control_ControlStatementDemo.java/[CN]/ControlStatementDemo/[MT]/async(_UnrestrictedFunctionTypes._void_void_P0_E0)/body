{
  new Thread(){
    public void run(){
      for (Result<?,?> __result__=block.invokeUnrestricted(); ; ) {
switch (__result__.getOutcome()) {
case RETURN_VALUE:
case RETURN_VOID:
case BREAK:
          return;
case TERMINATE_VOID:
        break;
default :
      break;
  }
  break;
}
}
}
.start();
}
