{
  ModelAccess.assertLegalWrite();
  if (oldModel != null) {
    oldModel.setModelDescriptor(null);
  }
  if (newModel != null) {
    newModel.setModelDescriptor(this);
  }
  if (oldModel != null) {
    notifyModelReplaced(oldModel);
  }
  fireModelReplaced();
  MPSModuleRepository.getInstance().invalidateCaches();
}
