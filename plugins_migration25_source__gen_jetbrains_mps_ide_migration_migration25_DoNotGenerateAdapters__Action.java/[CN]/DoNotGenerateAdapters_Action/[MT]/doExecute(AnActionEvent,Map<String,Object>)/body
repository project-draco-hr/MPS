{
  try {
    for (    Language lang : ListSequence.fromList(((MPSProject)MapSequence.fromMap(_params).get("project")).getProjectModules(Language.class))) {
      if (lang.getModuleDescriptor().isDoNotGenerateAdapters()) {
        continue;
      }
      lang.getModuleDescriptor().setDoNotGenerateAdapters(true);
      lang.save();
    }
  }
 catch (  Throwable t) {
    if (log.isErrorEnabled()) {
      log.error("User's action execute method failed. Action:" + "DoNotGenerateAdapters",t);
    }
  }
}
