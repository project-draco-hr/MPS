{
  super(role,sourceNode);
  if (!sourceNode.getModel().getSModelReference().equals(targetModelReference)) {
    myExternal=true;
    myTargetModelReference=targetModelReference;
  }
  myMature=mature;
  if (!mature) {
synchronized (ourImmatureReferences) {
      ourImmatureReferences.add(this);
    }
  }
}
