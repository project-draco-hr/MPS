{
  if (myImmatureTargetNode != null) {
    if (jetbrains.mps.util.SNodeOperations.isRegistered(getSourceNode()) && jetbrains.mps.util.SNodeOperations.isRegistered(myImmatureTargetNode) && !(jetbrains.mps.util.SNodeOperations.isDisposed(getSourceNode()) || jetbrains.mps.util.SNodeOperations.isDisposed(myImmatureTargetNode))) {
      makeMature();
    }
    if (force && myImmatureTargetNode != null) {
      if (jetbrains.mps.util.SNodeOperations.isRegistered(getSourceNode()) && !jetbrains.mps.util.SNodeOperations.isDisposed(getSourceNode())) {
        error("Impossible to resolve immature reference",new ProblemDescription(new SNodePointer(myImmatureTargetNode),"ImmatureTargetNode(modelID: " + (myImmatureTargetNode.getModel() == null ? "null" : myImmatureTargetNode.getModel().toString()) + ", nodeID: "+ myImmatureTargetNode.getSNodeId().toString()+ "): isRegistered = "+ jetbrains.mps.util.SNodeOperations.isRegistered(myImmatureTargetNode)+ ", isDisposed = "+ jetbrains.mps.util.SNodeOperations.isDisposed(myImmatureTargetNode)+ dumpUnregisteredTrace()));
        myImmatureTargetNode=null;
      }
    }
  }
  return myImmatureTargetNode == null;
}
