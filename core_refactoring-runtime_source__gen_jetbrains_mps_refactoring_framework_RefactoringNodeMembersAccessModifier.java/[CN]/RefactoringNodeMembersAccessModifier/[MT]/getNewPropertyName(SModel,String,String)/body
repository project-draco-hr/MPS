{
  if (isModificationMode(model)) {
    String newRole=getNewFeatureRole_internal(conceptFQName,propertyName,StructureModificationData.ConceptFeatureKind.PROPERTY);
    if (newRole == null) {
      return propertyName;
    }
 else {
      return newRole;
    }
  }
 else {
    return propertyName;
  }
}
