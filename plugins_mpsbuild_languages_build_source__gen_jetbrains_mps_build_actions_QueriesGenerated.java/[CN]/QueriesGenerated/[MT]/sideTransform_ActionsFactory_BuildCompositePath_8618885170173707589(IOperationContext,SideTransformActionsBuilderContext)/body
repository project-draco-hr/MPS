        SNodeFactoryOperations.setNewChild(_context.getSourceNode(),"tail","jetbrains.mps.build.structure.BuildCompositePath");
        return _context.getSourceNode();
      }
      public String getMatchingText(      String pattern){
        return "/";
      }
