{
  editorCell.setSubstituteInfo(new OldNewCompositeSubstituteInfo(myEditorContext,new SChildSubstituteInfo(editorCell,myOwnerNode,MetaAdapterFactory.getContainmentLink(0x1a8554c4eb8443baL,0x8c346f0d90c6e75aL,0x3ac2ae2c0bcf368bL,0x3ac2ae2c0bcf36b7L,"scope"),child),new DefaultChildSubstituteInfo(myOwnerNode,myContainmentLink.getDeclarationNode(),myEditorContext)));
  if (editorCell.getRole() == null) {
    editorCell.setRole("scope");
  }
}
