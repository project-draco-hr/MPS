{
  return CommandProcessor.instance().executeLightweightCommand(new Calculable<String>(){
    public String calculate(){
      if (node == node.getContainingRoot())       return "";
      return "role: " + "<i>" + node.getRole_() + "</i>"+ "; "+ "in: "+ "<i>"+ node.getParent()+ "</i>";
    }
  }
);
}
