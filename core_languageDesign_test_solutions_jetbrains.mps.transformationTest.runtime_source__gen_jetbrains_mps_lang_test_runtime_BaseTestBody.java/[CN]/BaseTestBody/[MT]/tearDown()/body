{
  final BaseTestBody test=this;
  SwingUtilities.invokeAndWait(new Runnable(){
    public void run(){
      ModelAccess.instance().runWriteActionInCommand(new Runnable(){
        public void run(){
          for (          SNode copy : test.myCopyes) {
            test.myModel.getSModel().removeRoot(copy);
          }
        }
      }
);
    }
  }
);
}
