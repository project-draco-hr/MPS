{
  if (myDisposed) {
    return;
  }
  if (myStateToRestore == null) {
    myModelsDialog.rebuildLater();
  }
 else {
    resetState();
  }
  myModelsDialog.rootsDialogClosed();
  myMineEditor.dispose();
  myMineEditor=null;
  myResultEditor.dispose();
  myResultEditor=null;
  myRepositoryEditor.dispose();
  myRepositoryEditor=null;
  myDisposed=true;
  super.dispose();
}
