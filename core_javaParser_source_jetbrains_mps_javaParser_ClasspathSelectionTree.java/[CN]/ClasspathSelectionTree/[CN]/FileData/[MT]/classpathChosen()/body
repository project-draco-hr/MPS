{
  if (myFile.isDirectory()) {
    return new FileClassPathItem(myFile.getAbsolutePath());
  }
 else   if (myFile.getName().endsWith(".jar")) {
    try {
      return new JarFileClassPathItem(new FileSystemFile(myFile));
    }
 catch (    IOException ex) {
      LOG.error(ex);
      return null;
    }
  }
  return null;
}
