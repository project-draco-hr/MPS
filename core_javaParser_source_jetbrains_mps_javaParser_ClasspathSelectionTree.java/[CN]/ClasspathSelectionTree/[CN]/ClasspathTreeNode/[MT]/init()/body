{
  if (myIsInitialized) {
    return;
  }
  try {
    FileData fileData=getFileData();
    File file=fileData.getFile();
    if (file != null && file.isDirectory()) {
      for (      File childFile : file.listFiles()) {
        add(new ClasspathTreeNode(childFile));
      }
    }
  }
  finally {
    myIsInitialized=true;
  }
}
