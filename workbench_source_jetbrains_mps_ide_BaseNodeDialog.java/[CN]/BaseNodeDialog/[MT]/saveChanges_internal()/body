{
  if (!validateNode())   return true;
  CommandProcessor.instance().executeCommand(new Runnable(){
    public void run(){
      myEditorComponent.dispose();
      try {
        saveChanges();
      }
 catch (      Throwable t) {
        JOptionPane.showInputDialog(BaseNodeDialog.this,"Exception during save");
        LOG.error(t);
      }
    }
  }
);
  return false;
}
