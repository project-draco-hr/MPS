{
  myEditorComponent.editNode(getNode());
  return new IPreferencesPage(){
    public String getName(){
      return getTitle();
    }
    public Icon getIcon(){
      return BaseNodeDialog.this.getIcon();
    }
    public JComponent getComponent(){
      return getMainComponent();
    }
    public boolean validate(){
      return validateNode();
    }
    public void commit(){
      buttonOK();
    }
  }
;
}
