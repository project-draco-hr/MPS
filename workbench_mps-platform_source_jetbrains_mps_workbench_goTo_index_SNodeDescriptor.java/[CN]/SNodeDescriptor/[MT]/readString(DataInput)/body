{
  int c=in.readByte();
  if (c == 0x70) {
    return null;
  }
  StringBuilder sb=null;
  while (c == 42) {
    String prefix=in.readUTF();
    if (sb == null) {
      sb=new StringBuilder(prefix);
    }
 else {
      sb.append(prefix);
    }
    c=in.readByte();
  }
  return sb == null ? in.readUTF() : sb.append(in.readUTF()).toString();
}
