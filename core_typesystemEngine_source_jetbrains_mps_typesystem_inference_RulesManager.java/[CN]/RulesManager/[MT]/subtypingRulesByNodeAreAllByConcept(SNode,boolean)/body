{
synchronized (RULES_LOCK) {
    loadLanguage(node.getLanguageNamespace());
    for (    SubtypingRule_Runtime rule : mySubtypingRules.getRules(node)) {
      if (!isWeak && rule.isWeak()) {
        continue;
      }
      if (!rule.isApplicable(node)) {
        return false;
      }
    }
    return true;
  }
}
