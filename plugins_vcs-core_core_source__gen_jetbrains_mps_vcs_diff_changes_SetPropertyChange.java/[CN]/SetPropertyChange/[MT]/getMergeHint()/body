    if ((hint == null)) {
      hint=AttributeOperations.getAttribute(c,new IAttributeDescriptor.NodeAttribute("jetbrains.mps.vcs.mergehints.structure.MergeHint"));
    }
    if ((hint != null)) {
      myMergeHint=new SNodePointer(hint);
    }
  }
  return myMergeHint;
