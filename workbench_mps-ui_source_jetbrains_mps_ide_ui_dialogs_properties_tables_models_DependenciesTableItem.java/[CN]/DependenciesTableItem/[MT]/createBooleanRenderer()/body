{
  return new BooleanTableCellRenderer(){
    @Override public Component getTableCellRendererComponent(    JTable table,    Object value,    boolean isSel,    boolean hasFocus,    int row,    int column){
      if (value instanceof Boolean)       return super.getTableCellRendererComponent(table,value,isSel,hasFocus,row,column);
 else       return new Component(){
      }
;
    }
  }
;
}
