{
  if (myTargetNode != null) {
    return myTargetNode;
  }
 else   if (myFailed) {
    return null;
  }
  myTargetNode=myReferenceInfo.doResolve(myGenerator);
  if (myTargetNode == null) {
    myFailed=true;
    if (myReferenceInfo.isRequired()) {
      myReferenceInfo.showErrorMessage(myGenerator);
    }
  }
  myReferenceInfo=null;
  myGenerator=null;
  return myTargetNode;
}
