{
switch (fdk.getSeverity()) {
case ERROR:
    LOG.error("[" + fdk.getPriority() + "] -- "+ fdk.getMessage());
  break;
case WARNING:
LOG.warning("[" + fdk.getPriority() + "] -- "+ fdk.getMessage());
break;
case INFO:
LOG.info("[" + fdk.getPriority() + "] -- "+ fdk.getMessage());
break;
default :
break;
}
}
