{
  int wordStart=-1;
  for (int i=0; i < myData.length; i++) {
    char c=myData[i];
    if (c >= 'a' && c <= 'z' || c >= 'A' && c <= 'Z') {
      if (wordStart == -1) {
        wordStart=i;
      }
    }
 else     if (wordStart >= 0) {
      processWord(wordStart,i - wordStart);
      myPrevWord=new Pair<Integer,Integer>(wordStart,i - wordStart);
      wordStart=-1;
    }
  }
}
