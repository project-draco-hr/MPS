{
  if (ListSequence.fromList(myWhatToTest).isEmpty()) {
    throw new ExecutionException("Nothing to test.");
  }
  String workingDir=myConfigurationRunParameters.getWorkingDirectory();
  myCurrentProcess=OutputRedirector.redirect(new Junit_Command().setJrePath(myConfigurationRunParameters.getAlternativeJRE()).setVirtualMachineParameter(myConfigurationRunParameters.getVMParameters()).setWorkingDirectory(new File((StringUtils.isEmpty(workingDir) ? System.getProperty("user.home") : workingDir))).setTests(myWhatToTest).createProcess(),new UnitTestProcessListener(myDispatcher));
  if (myCurrentProcess == null) {
    myState.terminate();
    return null;
  }
  return myCurrentProcess;
}
