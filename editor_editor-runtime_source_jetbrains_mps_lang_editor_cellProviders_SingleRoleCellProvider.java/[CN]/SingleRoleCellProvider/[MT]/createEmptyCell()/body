{
  EditorCell_Label emptyCell=myContainmentLink.isOptional() ? new EditorCell_Constant(myEditorContext,myOwnerNode,"") : new EditorCell_Error(myEditorContext,myOwnerNode,getNoTargetText());
  emptyCell.setDefaultText(getNoTargetText());
  emptyCell.setEditable(true);
  emptyCell.setRole(getRole());
  return emptyCell;
}
