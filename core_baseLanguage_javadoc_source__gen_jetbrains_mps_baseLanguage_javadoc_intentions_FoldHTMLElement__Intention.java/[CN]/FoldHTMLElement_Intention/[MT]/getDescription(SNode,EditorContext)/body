{
  String name=(StringUtils.isEmpty(SPropertyOperations.getString(node,"name")) ? "..." : SPropertyOperations.getString(node,"name"));
  return (ListSequence.<SNode>fromList(SLinkOperations.getTargets(node,"line",true)).isEmpty() ? String.format(Locale.getDefault(),"Convert to <%s></%s>",name,name) : String.format(Locale.getDefault(),"Convert to <%s />",name));
}
