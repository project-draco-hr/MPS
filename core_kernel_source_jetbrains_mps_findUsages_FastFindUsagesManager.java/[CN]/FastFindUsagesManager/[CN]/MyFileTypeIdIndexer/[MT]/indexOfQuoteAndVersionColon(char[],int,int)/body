{
  int[] result={-1,-1};
  for (int i=start; i < charsLength; i++) {
    if (chars[i] == '"') {
      if (result[0] == -1)       result[0]=i;
      result[1]=i;
      return result;
    }
    if (chars[i] == ':' && (i + 1 < charsLength) && chars[i + 1] >= '0' && chars[i + 1] <= '9') {
      result[0]=i;
    }
    if (chars[i] == '\n') {
      return new int[]{-1,-1};
    }
  }
  return new int[]{-1,-1};
}
