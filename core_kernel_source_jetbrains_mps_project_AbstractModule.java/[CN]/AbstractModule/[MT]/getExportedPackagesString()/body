{
  StringBuilder result=new StringBuilder();
  List<String> packs=getExportedPackages();
  for (int i=0; i < packs.size(); i++) {
    String s=getExportedPackages().get(i);
    result.append("  ").append(s);
    if (i != packs.size() - 1) {
      result.append(",");
    }
    result.append("\n");
  }
  return result.toString();
}
