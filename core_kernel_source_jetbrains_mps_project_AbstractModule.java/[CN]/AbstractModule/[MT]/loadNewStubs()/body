{
  loadJavaStubModelRoots();
  myManager.dispose();
  if (!areJavaStubsEnabled()) {
    return;
  }
  myManager=new MyClassPathModelRootManager();
  SModel sm=new SModel();
  sm.setLoading(true);
  ModelRoot mr=ModelRoot.newInstance(sm);
  mr.setPrefix("");
  myManager.read(new SModelRoot(this,mr),this);
}
