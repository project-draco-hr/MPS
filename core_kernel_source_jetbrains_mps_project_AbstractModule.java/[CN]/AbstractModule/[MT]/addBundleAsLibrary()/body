{
  if (getModuleDescriptor() != null && !getModuleDescriptor().getCompileInMPS()) {
    for (    ModelRoot sme : getModuleDescriptor().getStubModelEntries()) {
      if (sme.getManager().getClassName().startsWith("JavaStubs") && (MacrosFactory.SOLUTION_DESCRIPTOR + "/classes").equals(sme.getPath())) {
        return true;
      }
    }
  }
  return false;
}
