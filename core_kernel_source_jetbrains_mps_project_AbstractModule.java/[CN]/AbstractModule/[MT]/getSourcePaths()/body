{
  List<String> result=new ArrayList<String>();
  ModuleDescriptor descriptor=getModuleDescriptor();
  if (descriptor != null) {
    for (    String p : descriptor.getSourcePaths()) {
      result.add(p);
    }
  }
  result.add(getGeneratorOutputPath());
  result.add(getTestsGeneratorOutputPath());
  return result;
}
