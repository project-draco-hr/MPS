{
  CompositeClassPathItem result=new CompositeClassPathItem();
  for (  String s : getClassPath()) {
    try {
      IFile file=FileSystem.getFile(s);
      if (!file.exists()) {
        LOG.error("Can't load class path item " + s + " in "+ this);
      }
 else {
        IClassPathItem currentItem;
        if (file.isDirectory()) {
          currentItem=new FileClassPathItem(s);
        }
 else {
          currentItem=new JarFileClassPathItem(s);
        }
        result.add(currentItem);
      }
    }
 catch (    IOException e) {
      LOG.error(e);
    }
  }
  if (isPackaged()) {
    File parent=getBundleHome().getParentFile();
    String name=getModuleUID() + ".lib.jar";
    File file=new File(parent,name);
    if (file.exists()) {
      try {
        result.add(new JarFileClassPathItem(file.getPath()));
      }
 catch (      IOException e) {
        LOG.error(e);
      }
    }
  }
  myClassPath=result;
}
