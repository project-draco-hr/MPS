{
  boolean save=false;
  if (isPackaged()) {
    updateDescriptorClasspath();
  }
 else {
    Set<String> visited=new HashSet<String>();
    for (    ClassPathEntry e : getModuleDescriptor().getClassPathEntries()) {
      if (visited.contains(e.getPath())) {
        e.delete();
        save=true;
      }
      visited.add(e.getPath());
    }
  }
  if (save && !isPackaged()) {
    save();
  }
}
