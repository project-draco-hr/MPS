{
  List<DevKit> result=new ArrayList<DevKit>();
  for (  String namespace : getUsedDevKitNamespaces()) {
    DevKit dk=MPSModuleRepository.getInstance().getDevKit(namespace);
    if (dk != null) {
      result.add(dk);
    }
 else {
      LOG.error("Can't load devkit " + dk + " from "+ this);
    }
  }
  return result;
}
