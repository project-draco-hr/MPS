{
  EditorCell editorCell=editorContext.getCellFactory().createEditorComponentCell(node,"jetbrains.mps.baseLanguage.editor.SuperMethodCallComponent");
  Style style=new StyleImpl();
  style.set(StyleAttributes.PUNCTUATION_LEFT,0,true);
  editorCell.getStyle().putAll(style);
  QualifiedSuperMethodCall.setCellActions(editorCell,node,editorContext);
  return editorCell;
}
