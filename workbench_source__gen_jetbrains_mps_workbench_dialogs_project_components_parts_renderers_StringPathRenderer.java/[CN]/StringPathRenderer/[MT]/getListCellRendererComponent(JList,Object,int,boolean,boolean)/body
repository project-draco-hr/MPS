{
  String path=(String)value;
  Component result=super.getListCellRendererComponent(list,path,index,isSelected,cellHasFocus);
  if (path == null) {
    return result;
  }
  if (isSelected) {
    return result;
  }
  if (!(StateUtil.isAvailable(path))) {
    this.setForeground(Color.RED);
  }
  return result;
}
