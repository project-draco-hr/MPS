{
  ArrayList<TextUnit> rv=new ArrayList<TextUnit>();
  for (  SNode root : model.getRootNodes()) {
    if (root.isInstanceOfConcept(MetaAdapterFactory.getConcept(0x772f6dcd8c0d48f7L,0x869c908e036f7c8eL,0x11d45f8a8b6L,"jetbrains.mps.sampleXML.structure.Document"))) {
      String fname=getFileName_Document(root);
      String ext=getFileExtension_Document(root);
      rv.add(new RegularTextUnit(root,(ext == null ? fname : (fname + '.' + ext))));
      continue;
    }
  }
  return rv;
}
