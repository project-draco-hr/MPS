{
  DataSource source=md.getSource();
  if (!(source instanceof FileDataSource) && !source.getClass().getCanonicalName().contains("FilePerRootDataSource"))   return;
  String file=source instanceof FileDataSource ? ((FileDataSource)source).getFile().getPath() : (source.getLocation().replace("IdeaFile{file://","").replace("}","") + File.separator + MPSExtentions.DOT_MODEL_HEADER);
  myPathsToModelDescriptorMap.put(file,md);
}
