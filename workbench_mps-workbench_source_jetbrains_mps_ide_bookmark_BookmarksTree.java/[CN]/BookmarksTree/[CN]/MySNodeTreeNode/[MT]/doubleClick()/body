{
  myProject.getModelAccess().runWriteInEDT(new Runnable(){
    @Override public void run(){
      SNode openNode=getSNode();
      if (openNode == null)       return;
      NavigationSupport.getInstance().openNode(myProject,openNode,true,!(openNode.getModel() != null && openNode.getParent() == null));
    }
  }
);
}
