{
  if (!isRelative() && other.isRelative()) {
    return false;
  }
  if (getSeparator() != other.getSeparator()) {
    return false;
  }
  if (getNameCount() < other.getNameCount()) {
    return false;
  }
  for (int i=0; i < other.getNameCount(); ++i) {
    if (!getName(i).equals(other.getName(i))) {
      return false;
    }
  }
  return true;
}
