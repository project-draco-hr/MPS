{
  int c=readByte();
  if (c == 0x70) {
    return null;
  }
 else   if (c == 0x48) {
    UUID uuid=new UUID(readLong(),readLong());
    return ModuleId.regular(uuid);
  }
 else   if (c == 0x47) {
    return ModuleId.foreign(readString());
  }
 else {
    throw new IOException("unknown id");
  }
}
