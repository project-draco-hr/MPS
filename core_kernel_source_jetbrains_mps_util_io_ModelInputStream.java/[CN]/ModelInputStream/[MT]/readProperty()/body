{
  byte b=readByte();
  if (b == NULL) {
    return null;
  }
  if (b == PROPERTY_INDEX) {
    return myProperties.get(readShort());
  }
  if (b != PROPERTY) {
    throw new IOException();
  }
  final SConcept c=readConcept();
  SProperty p=MetaAdapterFactory.getProperty(new SPropertyId(IdHelper.getConceptId(c),readLong()),readString());
  myProperties.add(p);
  return p;
}
