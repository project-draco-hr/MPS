{
  List<SNode> rules=getRules(concept,false);
  if (rules.size() == 1) {
    context.getComponent(MPSEditorOpener.class).openNode(rules.get(0));
    return;
  }
  MyMenu m=new MyMenu(rules,context);
  int x=0;
  int y=0;
  if (cell != null) {
    x=cell.getX();
    y=cell.getY();
  }
  m.show(frame,x,y);
}
