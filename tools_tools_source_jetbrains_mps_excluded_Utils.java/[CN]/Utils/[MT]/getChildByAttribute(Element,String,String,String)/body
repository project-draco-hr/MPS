{
  Element result=null;
  for (  Object component : element.getChildren(tagName)) {
    Element componentXml=(Element)component;
    if (componentXml.getAttributeValue(attributeName).equals(attributeValue)) {
      if (result == null) {
        result=componentXml;
      }
 else {
        return null;
      }
    }
  }
  return result;
}
