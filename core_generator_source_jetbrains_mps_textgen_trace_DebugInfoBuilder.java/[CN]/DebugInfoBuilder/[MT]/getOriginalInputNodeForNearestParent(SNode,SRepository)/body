{
  while (output != null) {
    SNode input=output;
    while (input != null && (input.getModel().getModule() instanceof TransientSModule)) {
      SNodeReference inputRef=TracingUtil.getInput(input);
      input=inputRef == null ? null : inputRef.resolve(repository);
    }
    if (input != null) {
      return input;
    }
    output=output.getParent();
  }
  return null;
}
