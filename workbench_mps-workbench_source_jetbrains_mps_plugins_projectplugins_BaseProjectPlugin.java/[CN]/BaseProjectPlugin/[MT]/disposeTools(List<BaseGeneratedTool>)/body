{
  for (  BaseGeneratedTool tool : toolsToDispose) {
    if (myInitializedTools.get().contains(tool)) {
      try {
        tool.unregister();
        tool.dispose();
      }
 catch (      Throwable t) {
        LOG.error("Exception on a tool dispose: " + tool,t);
      }
 finally {
        myInitializedTools.get().remove(tool);
      }
    }
  }
}
