{
  if (newChild != null && !newChild.getConcept().isSubConceptOf(myLink.getTargetConcept())) {
    LOG.error("couldn't set instance of " + newChild.getConcept().getName() + " as child '"+ myLink.getRoleName()+ "' to "+ SNodeOperations.getDebugText(parentNode));
    return newChild;
  }
  if (oldChild == null) {
    if (newChild != null) {
      parentNode.addChild(myLink,newChild);
    }
  }
 else {
    SNodeUtil.replaceWithAnother(oldChild,newChild);
    oldChild.delete();
  }
  return newChild;
}
