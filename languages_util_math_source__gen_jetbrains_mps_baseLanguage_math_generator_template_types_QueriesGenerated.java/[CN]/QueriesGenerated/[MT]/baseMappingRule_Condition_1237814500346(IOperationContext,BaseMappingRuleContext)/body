{
  if (SNodeOperations.isInstanceOf(SNodeOperations.getParent(_context.getNode()),"jetbrains.mps.baseLanguage.structure.BinaryOperation")) {
    boolean b=MathTypeUtil.binaryOperationRequiresWidening(SNodeOperations.cast(SNodeOperations.getParent(_context.getNode()),"jetbrains.mps.baseLanguage.structure.BinaryOperation"),_context.getNode()) != 0;
    return b;
  }
  return false;
}
