{
  SubtypingCache cache=myTypeChecker.getSubtypingCache();
  if (cache != null) {
    Pair<Boolean,SNode> coerced=cache.getCoerced(subtype,pattern,isWeak);
    if (coerced != null) {
      return coerced;
    }
  }
  cache=myTypeChecker.getGlobalSubtypingCache();
  if (cache != null) {
    Pair<Boolean,SNode> coerced=cache.getCoerced(subtype,pattern,isWeak);
    if (coerced != null)     return coerced;
  }
  return null;
}
