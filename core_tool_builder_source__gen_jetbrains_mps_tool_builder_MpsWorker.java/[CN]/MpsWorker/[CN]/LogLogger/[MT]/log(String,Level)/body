{
switch (level.toInt()) {
case Level.ERROR_INT:
    MpsWorker.LOG.error(text);
  break;
case Level.WARN_INT:
MpsWorker.LOG.warn(text);
break;
case Level.INFO_INT:
MpsWorker.LOG.info(text);
break;
case Level.DEBUG_INT:
MpsWorker.LOG.debug(text);
break;
default :
MpsWorker.LOG.fatal("[unknown level " + level + "] "+ text);
break;
}
}
