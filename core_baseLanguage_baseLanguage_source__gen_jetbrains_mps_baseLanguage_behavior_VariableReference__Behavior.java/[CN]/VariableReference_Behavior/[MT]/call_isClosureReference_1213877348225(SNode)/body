{
  SNode closureoid=SNodeOperations.getAncestor(thisNode,"jetbrains.mps.baseLanguage.structure.Closureoid",false,false);
  if ((closureoid == null)) {
    return false;
  }
  SNode declaration=SLinkOperations.getTarget(thisNode,"variableDeclaration",false);
  if (ListSequence.fromList(SNodeOperations.getAncestors(declaration,null,false)).contains(closureoid)) {
    return false;
  }
  return true;
}
