{
  int index;
  if (ourFreeIndices.isEmpty()) {
    index=ourAttributes.size();
    ourAttributes.add(a);
  }
 else {
    index=ourFreeIndices.iterator().next();
    ourFreeIndices.remove(index);
    ourAttributes.set(index,a);
  }
  if (!isSimple(a)) {
    ourNotSimpleAttributes.add(a);
  }
  return index;
}
