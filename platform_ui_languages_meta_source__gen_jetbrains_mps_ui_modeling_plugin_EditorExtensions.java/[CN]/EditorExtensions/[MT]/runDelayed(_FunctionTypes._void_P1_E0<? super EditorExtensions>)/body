{
synchronized (EditorExtensions.class) {
    if (INSTANCE == null && DISPOSED) {
      LOG.warn("EditorExtensions.runDelayed called after dispose");
    }
 else {
      ListSequence.fromList(InitDelayedBlocks).addElement(block);
    }
    if (INSTANCE != null) {
      INSTANCE.runAllDelayed();
    }
  }
}
