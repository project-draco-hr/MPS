{
  super(project);
  try {
    myOptions=project.getComponent(ProjectPluginManager.class).getPrefsComponent(PersistentOptions_PreferencesComponent.class);
  }
 catch (  NullPointerException e) {
  }
  myCheckBox=new JCheckBox("Add @Override annotation");
