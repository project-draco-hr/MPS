{
  int x=mEvent.getX();
  int y=mEvent.getY();
  if (x >= this.myX && x <= this.myX + this.myWidth && y > this.myY && y <= this.myY + this.myHeight) {
    for (    IBlockListener<T> listener : this.myBlockListeners) {
      listener.mousePressed(mEvent,AbstractBlock.this.getSourceObject());
    }
    return true;
  }
 else {
    return false;
  }
}
