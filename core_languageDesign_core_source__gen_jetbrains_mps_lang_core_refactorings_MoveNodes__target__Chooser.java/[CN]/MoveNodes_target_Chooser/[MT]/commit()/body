{
  Object selectedObject=MoveNodes_target_Chooser.this.myMainComponent.getSelectedObject();
  if (!(selectedObject instanceof SNode) && !(selectedObject instanceof SModelDescriptor)) {
    throw new InvalidInputValueException("select node or model");
  }
  MoveNodes_target_Chooser.this.getRefactoringContext().setParameter("target",selectedObject);
}
