{
  if (!(super.collectActionData(event))) {
    return false;
  }
  this.scope=event.getData(MPSDataKeys.SCOPE);
  if (this.scope == null) {
    return false;
  }
  this.context=event.getData(MPSDataKeys.OPERATION_CONTEXT);
  if (this.context == null) {
    return false;
  }
  this.model=event.getData(MPSDataKeys.MODEL);
  if (this.model == null) {
    return false;
  }
  return true;
}
