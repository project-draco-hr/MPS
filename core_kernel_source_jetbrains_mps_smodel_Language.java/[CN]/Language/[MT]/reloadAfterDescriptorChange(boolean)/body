{
  MPSModuleRepository.getInstance().unRegisterModules(this);
  for (  Generator generator : getGenerators()) {
    generator.dispose();
  }
  rereadModels();
  updatePackagedDescriptorClasspath();
  updateLanguageRuntimeClassPathItem();
  updateClassPath();
  if (reloadClasses) {
    updateStubs();
  }
  revalidateGenerators();
}
