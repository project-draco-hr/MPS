{
  boolean changed=false;
  for (  LanguageReference ref : myLanguageDescriptor.getExtendedLanguages()) {
    if (getNamespace().equals(ref.getName())) {
      myLanguageDescriptor.removeChild(ref);
      changed=true;
    }
  }
  if (changed && !getDescriptorFile().isReadOnly()) {
    save();
  }
}
