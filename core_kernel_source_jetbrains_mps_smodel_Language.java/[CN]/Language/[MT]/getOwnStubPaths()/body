{
  if (isPackaged()) {
    return Collections.singletonList(FileSystem.getInstance().getBundleHome(getDescriptorFile()).getPath());
  }
  IFile classesGen=ProjectPathUtil.getClassesGenFolder(getDescriptorFile());
  if (classesGen != null) {
    return Collections.singletonList(classesGen.getPath());
  }
  return Collections.emptyList();
}
