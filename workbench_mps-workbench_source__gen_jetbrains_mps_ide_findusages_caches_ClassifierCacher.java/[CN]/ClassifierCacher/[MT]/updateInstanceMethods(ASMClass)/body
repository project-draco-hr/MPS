{
  for (  ASMMethod m : ac.getDeclaredMethods()) {
    if (m.isStatic()) {
      continue;
    }
    if (m.isBridge()) {
      continue;
    }
    if (m.isCompilerGenerated()) {
      continue;
    }
    instance("jetbrains.mps.baseLanguage.structure.InstanceMethodDeclaration");
    createVisibility(m);
    updateBaseMethod(m);
  }
}
