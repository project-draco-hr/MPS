{
  if (editorCell.getSubstituteInfo() == null || editorCell.getSubstituteInfo() instanceof DefaultSubstituteInfo) {
    editorCell.setSubstituteInfo(new CompositeSubstituteInfo(myEditorContext,new AggregationCellContext(myOwnerNode,child,myContainmentLink.getDeclarationNode()),new SubstituteInfoPartExt[]{new NodeErrorCheckOperation_Editor.NodeErrorCheckOperation_generic_cellMenu_kzyi6r_a0b0(),new SChildSubstituteInfoPartEx(editorCell)}));
  }
  if (editorCell.getRole() == null) {
    editorCell.setRole("errorRef");
  }
}
