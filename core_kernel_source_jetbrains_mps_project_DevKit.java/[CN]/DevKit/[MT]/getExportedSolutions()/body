{
  List<Solution> result=new ArrayList<Solution>();
  for (  ModuleReference ref : myDescriptor.getExportedSolutions()) {
    String uid=ref.getModuleFqName();
    Solution solution=MPSModuleRepository.getInstance().getSolution(uid);
    if (solution != null) {
      result.add(solution);
    }
  }
  return result;
}
