{
  List<DevKit> result=new ArrayList<DevKit>();
  for (  ModuleReference ref : myDescriptor.getExtendedDevkits()) {
    String uid=ref.getModuleFqName();
    DevKit devKit=ModuleRepositoryFacade.getInstance().getModule(uid,DevKit.class);
    if (devKit != null) {
      result.add(devKit);
    }
  }
  return result;
}
