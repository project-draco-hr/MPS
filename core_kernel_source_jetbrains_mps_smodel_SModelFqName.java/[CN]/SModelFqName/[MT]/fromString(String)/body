{
  if (s == null)   s="";
  int indexSlash=s.indexOf("/");
  String module="";
  if (indexSlash >= 0) {
    module=s.substring(0,indexSlash);
    s=s.substring(indexSlash + 1,s.length());
  }
  int index=s.indexOf("@");
  String stereotype="";
  if (index >= 0) {
    stereotype=s.substring(index + 1);
  }
  String longName=s;
  if (index >= 0) {
    longName=s.substring(0,index);
  }
  return new SModelFqName(module,longName,stereotype);
}
