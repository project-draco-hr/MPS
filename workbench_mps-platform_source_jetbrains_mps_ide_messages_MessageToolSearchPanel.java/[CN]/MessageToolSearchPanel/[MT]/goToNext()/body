{
  if (myResults.isEmpty())   return;
  addToHistory();
  int selected=myList.getSelectedIndex();
  if (selected != -1) {
    for (    Integer index : myResults) {
      if (selected < index) {
        myList.setSelectedIndex(index);
        return;
      }
    }
  }
  myList.setSelectedIndex(myResults.get(0));
}
