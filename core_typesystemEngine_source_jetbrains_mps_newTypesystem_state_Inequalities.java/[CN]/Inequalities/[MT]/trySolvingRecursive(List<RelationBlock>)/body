{
  for (  RelationBlock inequality : inequalities) {
    if (!(TypesUtil.isVariable(inequality.getLeftNode()) && TypesUtil.hasVariablesInside(inequality.getLeftNode()) && TypesUtil.isVariable(inequality.getRightNode())) && TypesUtil.getVariables(inequality.getLeftNode(),myState).contains(myState.getRepresentative(inequality.getRightNode())) && myState.getBlocks().contains(inequality)) {
      myState.executeOperation(new RemoveBlockOperation(inequality));
      return true;
    }
  }
  return false;
}
