{
  try {
    DiagramEditorTool_Tool diagramPalette=((Project)MapSequence.fromMap(_params).get("ideaProject")).getComponent(ProjectPluginManager.class).getTool(DiagramEditorTool_Tool.class);
    if (diagramPalette != null) {
      diagramPalette.openToolLater(true);
    }
  }
 catch (  Throwable t) {
    if (LOG.isEnabledFor(Priority.ERROR)) {
      LOG.error("User's action execute method failed. Action:" + "ShowDiagramEditor",t);
    }
  }
}
