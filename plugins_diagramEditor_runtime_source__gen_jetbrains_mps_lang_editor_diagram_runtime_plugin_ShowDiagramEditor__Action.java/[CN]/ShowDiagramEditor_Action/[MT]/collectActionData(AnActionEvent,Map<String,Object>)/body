{
  if (!(super.collectActionData(event,_params))) {
    return false;
  }
  MapSequence.fromMap(_params).put("ideaProject",event.getData(PlatformDataKeys.PROJECT_CONTEXT));
  if (MapSequence.fromMap(_params).get("ideaProject") == null) {
    return false;
  }
  MapSequence.fromMap(_params).put("context",event.getData(MPSCommonDataKeys.OPERATION_CONTEXT));
  if (MapSequence.fromMap(_params).get("context") == null) {
    return false;
  }
  MapSequence.fromMap(_params).put("cell",event.getData(MPSEditorDataKeys.EDITOR_CELL));
  if (MapSequence.fromMap(_params).get("cell") == null) {
    return false;
  }
  return true;
}
