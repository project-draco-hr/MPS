{
  if (node == null) {
    return EMPTY_LIST;
  }
  List<SNode> result=new ArrayList<SNode>();
  if (!(inclusion)) {
    node=node.getParent();
  }
  while (node != null) {
    if (ancestorConcept == null || SNodeOperations.isInstanceOf(node,ancestorConcept)) {
      result.add(node);
    }
    node=node.getParent();
  }
  return result;
}
