{
  List<SNode> result=new ArrayList<SNode>();
  if (node == null) {
    return result;
  }
  if (!(inclusion)) {
    node=node.getParent();
  }
  while (node != null) {
    if (ancestorConceptFqName == null || SNodeUtil.isInstanceOf(node,jetbrains.mps.util.SNodeOperations.getConcept(ancestorConceptFqName))) {
      result.add(node);
    }
    node=node.getParent();
  }
  return result;
}
