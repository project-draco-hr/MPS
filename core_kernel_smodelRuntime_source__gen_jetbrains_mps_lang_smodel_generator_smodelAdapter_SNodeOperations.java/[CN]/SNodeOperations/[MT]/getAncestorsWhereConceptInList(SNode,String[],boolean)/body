{
  if (node == null) {
    return EMPTY_LIST;
  }
  List<SNode> result=new ArrayList<SNode>();
  if (!(inclusion) && node != null) {
    node=node.getParent();
  }
  while (node != null) {
    if (SNodeOperations._isInstanceOf(node,ancestorConceptFqNames)) {
      result.add(node);
    }
    node=node.getParent();
  }
  return result;
}
