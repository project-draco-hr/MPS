{
  try {
    event.getPresentation().setText(((NewAccessoryModel_Action.this.treeNode instanceof ProjectModuleTreeNode ? "" : "New ")) + "Accesory Model");
  }
 catch (  Throwable t) {
    if (log.isErrorEnabled()) {
      log.error("User's action doUpdate method failed. Action:" + "NewAccessoryModel",t);
    }
    this.disable(event.getPresentation());
  }
}
