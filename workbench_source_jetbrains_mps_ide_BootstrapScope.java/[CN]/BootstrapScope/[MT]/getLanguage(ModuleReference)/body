{
  for (  Language l : getModules(Language.class)) {
    if (moduleReference.getModuleId() != null) {
      if (l.getModuleId().equals(moduleReference.getModuleId())) {
        return l;
      }
    }
 else {
      if (l.getModuleFqName().equals(moduleReference.getModuleFqName())) {
        return l;
      }
    }
  }
  return null;
}
