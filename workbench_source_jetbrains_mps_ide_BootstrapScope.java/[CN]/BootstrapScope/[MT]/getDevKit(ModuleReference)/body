{
  for (  DevKit d : getModules(DevKit.class)) {
    if (moduleReference.getModuleId() != null) {
      if (d.getModuleId().equals(moduleReference.getModuleId())) {
        return d;
      }
    }
 else {
      if (d.getModuleFqName().equals(moduleReference.getModuleFqName())) {
        return d;
      }
    }
  }
  return null;
}
