{
  if (getLastNode() == null) {
    return false;
  }
  boolean current=false;
  final SNode lastNode=getLastNode().resolve(MPSModuleRepository.getInstance());
  for (  SNode aspect : tab.getNodes()) {
    if (aspect.getContainingRoot().equals(lastNode)) {
      current=true;
      break;
    }
  }
  return current;
}
