{
  MPSProject project=launchProject();
  ChangeModelProcessor changeModelProcessor=new ChangeModelProcessor(project,modelName,changer);
  IGenerationHandler generationHandler=getFilesGenType();
  boolean baseIsOk=changeModelProcessor.changeStep(generationHandler);
  boolean newIsOk=changeModelProcessor.revertStep(generationHandler);
  changeModelProcessor.getModelToChange().setLoading(false);
  changeModelProcessor.generateModelToChange(generationHandler);
  disposeProject(project);
  assertNoProblems(baseIsOk);
  assertDependenciesChecked(newIsOk);
}
