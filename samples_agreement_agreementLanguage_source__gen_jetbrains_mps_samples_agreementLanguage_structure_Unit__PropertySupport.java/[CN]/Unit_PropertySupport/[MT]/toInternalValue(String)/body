{
  if (value == null) {
    return null;
  }
  Iterator<Unit> constants=ListSequence.<Unit>fromList(Unit.getConstants()).iterator();
  while (constants.hasNext()) {
    Unit constant=constants.next();
    if (value.equals(constant.getName())) {
      return constant.getValueAsString();
    }
  }
  return null;
}
