{
  for (  VirtualFile f : data.getFiles()) {
    try {
      if (!FileTypeManager.getInstance().isFileIgnored(f.getName())) {
        if (!data.isCut()) {
          f.copy(this,basedir,f.getName());
        }
      }
    }
 catch (    IOException e) {
      LOG.error("Error while pasting " + f + "\n",e);
    }
  }
}
