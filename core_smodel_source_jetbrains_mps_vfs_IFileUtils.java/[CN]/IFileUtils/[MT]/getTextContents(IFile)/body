{
  BufferedReader br=null;
  try {
    br=new BufferedReader(new InputStreamReader(file.openInputStream()));
    StringBuilder sb=new StringBuilder();
    while (br.ready()) {
      sb.append(br.readLine());
    }
    return sb.toString();
  }
  finally {
    if (br != null) {
      br.close();
    }
  }
}
