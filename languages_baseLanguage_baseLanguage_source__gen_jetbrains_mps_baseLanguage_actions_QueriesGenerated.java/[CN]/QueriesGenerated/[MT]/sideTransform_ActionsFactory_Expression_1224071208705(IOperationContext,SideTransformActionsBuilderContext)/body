        SLinkOperations.setTarget(result,"classifierType",SNodeOperations.cast(type,"jetbrains.mps.baseLanguage.structure.ClassifierType"),true);
      }
      SNodeOperations.replaceWithAnother(_context.getSourceNode(),result);
