{
  ServiceAgreement result=new ServiceAgreement();
  result.registerValue("BASE_RATE");
  result.setValue("BASE_RATE",10.0,MfDate.PAST);
  result.setValue("BASE_RATE",12.0,new MfDate(1999,12,1));
  result.addPostingRule(EventType.USAGE,new MultiplyByRatePR(AccountType.BASE_USAGE,true),new MfDate(1999,10,1));
  result.addPostingRule(EventType.SERVICE_CALL,new AmountFormulaPR(0.5,Money.dollars(10),AccountType.SERVICE,true),new MfDate(1999,10,1));
  result.addPostingRule(EventType.SERVICE_CALL,new AmountFormulaPR(0.5,Money.dollars(15),AccountType.SERVICE,true),new MfDate(1999,12,1));
  result.addPostingRule(EventType.TAX,new AmountFormulaPR(0.055,Money.dollars(0),AccountType.TAX,false),new MfDate(1999,10,1));
  return result;
}
