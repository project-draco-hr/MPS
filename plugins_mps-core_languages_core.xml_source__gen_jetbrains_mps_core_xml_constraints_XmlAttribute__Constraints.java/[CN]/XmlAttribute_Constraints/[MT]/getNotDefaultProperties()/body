{
  Map<String,PropertyConstraintsDescriptor> properties=new HashMap();
  properties.put("attrName",new BasePropertyConstraintsDescriptor("attrName",this){
    @Override public boolean hasOwnValidator(){
      return true;
    }
    @Override public boolean validateValue(    SNode node,    String propertyValue,    IScope scope){
      String propertyName="attrName";
      return XmlNameUtil.isName((SPropertyOperations.getString(propertyValue)));
    }
  }
);
  return properties;
}
