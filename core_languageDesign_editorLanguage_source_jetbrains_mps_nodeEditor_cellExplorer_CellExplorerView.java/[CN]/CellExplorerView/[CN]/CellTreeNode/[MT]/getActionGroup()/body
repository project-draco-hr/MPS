{
  DefaultActionGroup group=new DefaultActionGroup();
  group.add(new BaseAction("Select In Editor"){
    protected void doExecute(    AnActionEvent e){
      showCell();
    }
  }
);
  group.add(new BaseAction("Properties"){
    protected void doExecute(    AnActionEvent e){
      new CellPropertiesWindow(myCell,null);
    }
  }
);
  return group;
}
