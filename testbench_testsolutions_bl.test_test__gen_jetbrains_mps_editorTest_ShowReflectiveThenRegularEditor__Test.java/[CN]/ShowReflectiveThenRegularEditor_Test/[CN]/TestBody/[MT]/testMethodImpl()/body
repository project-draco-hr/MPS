{
  initEditor("2746054822154165514","2746054822154165524");
  this.invokeAction("jetbrains.mps.ide.editor.actions.ShowReflectiveEditor_Action");
  Assert.assertTrue(Sequence.fromIterable(Sequence.fromArray(this.getEditorComponent().getUpdater().getExplicitEditorHintsForNode(this.getEditorComponent().getSelectedNode().getReference()))).contains("jetbrains.mps.lang.core.editor.BaseEditorContextHints.reflectiveEditor"));
  this.invokeAction("jetbrains.mps.ide.editor.actions.ShowRegularEditor_Action");
}
