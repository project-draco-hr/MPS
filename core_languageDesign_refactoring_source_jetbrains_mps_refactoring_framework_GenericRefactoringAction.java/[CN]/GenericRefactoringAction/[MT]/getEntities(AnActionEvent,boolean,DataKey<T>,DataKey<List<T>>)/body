{
  T single=e.getData(singleKey);
  List<T> list=e.getData(listKey);
  List<T> res=new ArrayList<T>(list == null ? 1 : list.size() + 1);
  if (list != null) {
    res.addAll(list);
  }
  if (single != null && !res.contains(single)) {
    res.add(single);
  }
  if (oneEntity && res.size() > 1) {
    res.clear();
  }
  return res;
}
