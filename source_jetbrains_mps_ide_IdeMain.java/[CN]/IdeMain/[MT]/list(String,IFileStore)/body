{
  if (fs.fetchInfo().isDirectory()) {
    System.out.println(indent + fs.getName() + "/");
    try {
      for (      String s : fs.childNames(EFS.NONE,null)) {
        list(indent + "  ",fs.getChild(s));
      }
    }
 catch (    CoreException e) {
      throw new RuntimeException(e);
    }
  }
 else {
    System.out.println(indent + fs.getName());
  }
}
