{
  int hash=getTypechecking().getNodesWithErrors().hashCode();
  if (hash != myOldHash) {
    myRequesting.clear();
    myOldHash=hash;
  }
  Integer oldHash=myRequesting.get(requesting);
  if (oldHash == null || oldHash != hash) {
    myRequesting.put(requesting,hash);
    return true;
  }
  return false;
}
