{
  setRootVisible(false);
  MPSTreeNode result;
  Collection<MPSTreeNode> nodes=create(myOperations,false);
  if (nodes == null) {
    result=new TextTreeNode("Empty type system trace");
    setRootVisible(true);
  }
 else {
    result=new TextTreeNode("Details");
    for (    MPSTreeNode node : nodes) {
      result.add(node);
    }
  }
  return result;
}
