{
  if (!(super.collectActionData(event))) {
    return false;
  }
  this.project=event.getData(MPSDataKeys.PROJECT);
  if (this.project == null) {
    return false;
  }
  this.scope=event.getData(MPSDataKeys.SCOPE);
  if (this.scope == null) {
    return false;
  }
  this.model=event.getData(MPSDataKeys.MODEL);
  if (this.model == null) {
    return false;
  }
  return true;
}
