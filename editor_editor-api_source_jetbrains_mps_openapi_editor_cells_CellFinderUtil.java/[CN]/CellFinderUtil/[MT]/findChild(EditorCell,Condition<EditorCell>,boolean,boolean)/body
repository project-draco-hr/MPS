{
  if (includeThis && condition.met(cell)) {
    return cell;
  }
  DfsTraverser traverser=new DfsTraverser(cell,forward,true);
  while (traverser.getCurrent() != null) {
    if (condition.met(traverser.getCurrent())) {
      return traverser.getCurrent();
    }
    traverser.next();
  }
  return null;
}
