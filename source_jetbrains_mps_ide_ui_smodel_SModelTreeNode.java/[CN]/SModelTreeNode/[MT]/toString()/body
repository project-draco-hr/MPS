{
  SModelUID uid;
  if (getSModelDescriptor() != null) {
    uid=getSModelDescriptor().getModelUID();
  }
 else {
    uid=getSModel().getUID();
  }
  String name=myShowLongName ? uid.toString() : uid.getShortName();
  String result;
  if (myLabel != null) {
    result=myLabel + " : " + name;
  }
 else {
    result=name;
  }
  if (generationRequired()) {
    result+=" (generation required)";
  }
  return result;
}
