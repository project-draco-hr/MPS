{
  ModelCheckResult r=getModelCheckResult();
  if (r != null && r.hasErrors()) {
    return Color.RED;
  }
  SModelDescriptor sm=getSModelDescriptor();
  if (sm != null && sm.isInitialized() && SModelRepository.getInstance().isChanged(sm)) {
    return new Color(0x00,0x00,0x90);
  }
  return Color.BLACK;
}
