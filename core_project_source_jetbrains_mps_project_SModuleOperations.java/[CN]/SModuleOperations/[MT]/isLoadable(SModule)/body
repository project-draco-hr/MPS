{
  if (module instanceof Language)   return true;
  if (module instanceof Generator)   return true;
  if (module instanceof Solution) {
    Solution solution=(Solution)module;
    if (solution.getKind() != SolutionKind.NONE)     return true;
    if (solution.getFacet(CustomClassLoadingFacet.class) != null)     return true;
  }
  return false;
}
