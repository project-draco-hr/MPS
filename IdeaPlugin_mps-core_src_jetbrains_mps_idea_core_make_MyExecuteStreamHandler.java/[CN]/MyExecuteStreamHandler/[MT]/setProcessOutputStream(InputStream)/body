{
  myOutputReadingThread=new Thread(new Runnable(){
    public void run(){
      Scanner s=new Scanner(is);
      while (s.hasNextLine()) {
        String line=s.nextLine();
        if (line.indexOf(WRITTEN) == 0) {
          reportWrittenFile(line.substring(WRITTEN.length()));
        }
 else {
          logOutput(line);
        }
      }
    }
  }
);
}
