{
  if (getNode() == null)   return null;
  if (editor instanceof InspectorEditorComponent) {
    return null;
  }
  SNode parent=getNode().getParent();
  EditorCell result=null;
  while (parent != null) {
    result=editor.getBigValidCellForNode(parent);
    if (result != null) {
      return result;
    }
    parent=parent.getParent();
  }
  return result;
}
