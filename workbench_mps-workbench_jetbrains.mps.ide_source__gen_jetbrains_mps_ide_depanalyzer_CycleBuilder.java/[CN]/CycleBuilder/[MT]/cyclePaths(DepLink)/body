{
  if (LOG.isDebugEnabled()) {
    LOG.debug(String.format("\nStart path cycle calculation from %s",depLink));
  }
  myTarget=depLink;
  myCycles.clear();
  myReusedChecked.clear();
  myCurrent=new DepPath();
  myCurrent.push(depLink);
  myNestLevelDebug=0;
  nextPathLevel(depLink);
  myCurrent=null;
  myTarget=null;
  return myCycles;
}
