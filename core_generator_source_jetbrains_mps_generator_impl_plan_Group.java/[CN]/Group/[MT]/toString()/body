{
  StringBuilder sb=new StringBuilder();
  if (isTopPriority()) {
    sb.append("Top");
  }
  if (isEmpty()) {
    sb.append("Empty");
  }
  sb.append("Group[");
  for (  TemplateMappingConfiguration c : myMappings) {
    sb.append(c.getName());
    sb.append(',');
  }
  sb.append(']');
  return sb.toString();
}
