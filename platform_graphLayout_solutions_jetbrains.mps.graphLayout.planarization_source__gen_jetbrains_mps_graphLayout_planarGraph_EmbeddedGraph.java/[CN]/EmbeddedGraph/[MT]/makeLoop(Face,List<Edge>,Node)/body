{
  face.makeEndsWith(node);
  for (  Edge edge : ListSequence.fromList(loop)) {
    addLastDart(face,new Dart(edge,edge.getSource()));
  }
  Face newFace=new Face(getGraph());
  for (  Edge edge : ListSequence.fromList(loop)) {
    newFace.addFirst(new Dart(edge,edge.getTarget()));
  }
  addFace(newFace);
  return newFace;
}
