{
  SModel model=node.getModel();
  DebugInfo debugInfo=TraceInfoCache.getInstance().get(model.getModelDescriptor());
  if (debugInfo == null) {
    return null;
  }
  UnitPositionInfo unitForNode=debugInfo.getUnitForNode(node.getId());
  if (unitForNode != null) {
    return unitForNode.getUnitName();
  }
  return null;
}
