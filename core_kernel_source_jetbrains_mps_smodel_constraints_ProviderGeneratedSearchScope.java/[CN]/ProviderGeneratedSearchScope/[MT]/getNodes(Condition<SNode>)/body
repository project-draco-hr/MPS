{
  Iterable<SNode> seq=(Iterable<SNode>)myProvider.createSearchScopeOrListOfNodes(myOperationContext,myContext);
  List<SNode> result=new ArrayList<SNode>();
  if (seq != null) {
    for (    SNode node : seq) {
      if (condition.met(node)) {
        result.add(node);
      }
    }
  }
  return result;
}
