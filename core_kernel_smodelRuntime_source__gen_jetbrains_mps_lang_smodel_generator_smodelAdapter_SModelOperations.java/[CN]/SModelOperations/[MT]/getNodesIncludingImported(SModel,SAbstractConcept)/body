{
  if (model == null) {
    return Collections.emptyList();
  }
  if (concept == null) {
    return allNodesIncludingImported(model,false,null);
  }
  final SNode conceptNode=(SNode)concept.getDeclarationNode();
  if (conceptNode == null) {
    return Collections.emptyList();
  }
  return allNodesIncludingImported(model,false,conceptNode);
}
