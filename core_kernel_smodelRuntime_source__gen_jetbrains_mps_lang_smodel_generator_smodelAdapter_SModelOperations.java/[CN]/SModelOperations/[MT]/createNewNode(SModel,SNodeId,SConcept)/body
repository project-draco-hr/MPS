{
  if (concept == null) {
    return null;
  }
  SNode result=SModelUtil_new.instantiateConceptDeclaration(concept,model,id,false);
  if (result == null) {
    return null;
  }
  if (id != null) {
    ((jetbrains.mps.smodel.SNode)result).setId(id);
  }
  BehaviorReflection.initNode(result);
  return ((SNode)result);
}
