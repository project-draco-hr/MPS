{
  if (model == null) {
    return Collections.emptyList();
  }
  if (concept == null) {
    return allNodesIncludingImported(model,true,null);
  }
  SNode conceptNode=(SNode)concept.getDeclarationNode();
  if (conceptNode == null) {
    return Collections.emptyList();
  }
  return allNodesIncludingImported(model,true,conceptNode);
}
