{
  try {
    MPSEditorOpener opener=this.context.getComponent(MPSEditorOpener.class);
    for (    SNode node : this.nodes) {
      if (node != null) {
        opener.openNode(node,this.context);
      }
    }
  }
 catch (  Throwable t) {
    LOG.error("User's action execute method failed. Action:" + "EditNode",t);
  }
}
