{
  if (node == null) {
    throw new IllegalArgumentException("initNode on null node");
  }
  for (int i=constructors.size() - 1; i >= 0; i--) {
    try {
      constructors.get(i).invoke(null,node);
    }
 catch (    IllegalAccessException e) {
      LOG.error(e);
    }
catch (    InvocationTargetException e) {
      e.printStackTrace();
    }
  }
}
