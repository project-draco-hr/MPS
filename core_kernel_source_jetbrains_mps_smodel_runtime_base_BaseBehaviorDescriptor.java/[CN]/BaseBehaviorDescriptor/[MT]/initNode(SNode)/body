{
  if (node == null) {
    throw new IllegalArgumentException("initNode on null node");
  }
  for (int i=myConstructors.size() - 1; i >= 0; i--) {
    try {
      myConstructors.get(i).invoke(null,node);
    }
 catch (    IllegalAccessException e) {
      LOG.error(null,e);
    }
catch (    InvocationTargetException e) {
      e.printStackTrace();
    }
  }
}
