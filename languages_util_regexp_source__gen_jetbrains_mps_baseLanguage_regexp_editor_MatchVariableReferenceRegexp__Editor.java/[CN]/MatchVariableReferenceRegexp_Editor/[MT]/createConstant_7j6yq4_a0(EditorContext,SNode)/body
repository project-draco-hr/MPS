{
  EditorCell_Constant editorCell=new EditorCell_Constant(editorContext,node,"\\");
  editorCell.setCellId("Constant_7j6yq4_a0");
  Style style=new StyleImpl();
  RegexpStylesheet_StyleSheet.apply_RegexpBrace(style,editorCell);
