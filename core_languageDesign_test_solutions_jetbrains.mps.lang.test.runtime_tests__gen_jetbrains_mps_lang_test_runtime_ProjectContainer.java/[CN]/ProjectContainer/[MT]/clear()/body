{
  try {
    SwingUtilities.invokeAndWait(new Runnable(){
      public void run(){
        if (ProjectContainer.this.lastProject != null) {
          ProjectContainer.this.lastProject.dispose();
          ProjectContainer.this.lastProject=null;
          ProjectContainer.this.projectName=null;
        }
      }
    }
);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
