{
  if (isMale(person)) {
    if (isChild(person)) {
      return "boy";
    }
    if (isAdult(person)) {
      return "man";
    }
  }
  if (isFemale(person)) {
    if (isChild(person)) {
      return "girl";
    }
    if (isAdult(person)) {
      return "woman";
    }
  }
  return "Nothing to show here";
}
