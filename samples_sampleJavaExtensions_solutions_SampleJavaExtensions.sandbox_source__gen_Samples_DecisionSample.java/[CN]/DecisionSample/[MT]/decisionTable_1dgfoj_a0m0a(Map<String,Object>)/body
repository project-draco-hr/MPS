{
  if (isMale(person)) {
    if (isBaby(person)) {
      return 100;
    }
    if (isChild(person)) {
      return 50;
    }
    if (isAdult(person)) {
      return 5 + 2 * this.seasonalBonus();
    }
    if (isRetired(person)) {
      return 20;
    }
  }
  if (isFemale(person)) {
    if (isBaby(person)) {
      return 100;
    }
    if (isChild(person)) {
      return 50;
    }
    if (isAdult(person)) {
      return 10 + this.seasonalBonus();
    }
    if (isRetired(person)) {
      return 10 + ((MapSequence.fromMap(person).get("name") == "Susan") ? this.seasonalBonus() : 40);
    }
  }
  return 0;
}
