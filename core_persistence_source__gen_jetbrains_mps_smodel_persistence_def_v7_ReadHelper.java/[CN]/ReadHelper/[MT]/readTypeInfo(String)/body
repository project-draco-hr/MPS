{
  ConceptKind kind;
  StaticScope scope;
  if (s.length() != 2) {
    return null;
  }
switch (s.charAt(0)) {
case 'n':
    kind=ConceptKind.NORMAL;
  break;
case 'i':
kind=ConceptKind.INTERFACE;
break;
case 'l':
kind=ConceptKind.IMPLEMENTATION;
break;
default :
return null;
}
switch (s.charAt(1)) {
case 'g':
scope=StaticScope.GLOBAL;
break;
case 'r':
scope=StaticScope.ROOT;
break;
case 'n':
scope=StaticScope.NONE;
break;
default :
return null;
}
return new Pair(kind,scope);
}
