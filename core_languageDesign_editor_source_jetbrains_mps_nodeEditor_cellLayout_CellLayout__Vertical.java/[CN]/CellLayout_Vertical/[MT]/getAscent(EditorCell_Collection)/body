{
  for (  EditorCell cell : editorCells.getCells()) {
    if (cell.getStyle().get(StyleAttributes.BASE_LINE_CELL)) {
      return cell.getY() - editorCells.getY() + cell.getAscent();
    }
  }
  for (  EditorCell cell : editorCells.getCells()) {
    int result=cell.getAscent();
    if (result > 0) {
      return result;
    }
  }
  return 0;
}
