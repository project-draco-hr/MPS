{
  if (countDown > 0) {
    skipping:     do {
      --countDown;
switch (page) {
case TAKE:
case CUT:
        takeNext();
      return;
case SKIP:
case TAIL:
    if (!skipNext()) {
      return;
    }
  break;
default :
break;
}
}
 while (countDown > 0);
}
switch (page) {
case TAKE:
case CUT:
stop();
break;
case SKIP:
case TAIL:
takeNext();
break;
default :
break;
}
}
