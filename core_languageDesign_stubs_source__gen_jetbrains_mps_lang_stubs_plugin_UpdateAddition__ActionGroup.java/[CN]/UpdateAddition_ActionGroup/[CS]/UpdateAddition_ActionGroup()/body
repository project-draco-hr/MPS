{
  super("Update Stub Refs",ID);
  this.setIsInternal(false);
  this.setPopup(true);
  try {
    UpdateAddition_ActionGroup.this.addAction("jetbrains.mps.lang.stubs.plugin.UpdateLanguageAccessories_Action");
