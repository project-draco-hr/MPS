{
  for (  ProjectJdk jdk : ProjectJdkTable.getInstance().getAllJdks()) {
    if (("" + jdk.getVersionString()).startsWith("java version \"1.5") || ("" + jdk.getVersionString()).startsWith("java version \"1.6") || ("" + jdk.getVersionString()).startsWith("java version \"1.7"))     return jdk;
  }
  return null;
}
