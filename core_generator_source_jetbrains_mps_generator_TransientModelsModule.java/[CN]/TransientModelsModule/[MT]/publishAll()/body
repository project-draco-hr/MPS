{
  for (  TransientSModelDescriptor model : myModelVault.modelsToPublish()) {
    if (myPublished.add(model)) {
      registerModel(model);
    }
  }
  for (  TransientSModelDescriptor model : myModelVault.modelsNotToPublish()) {
    if (myPublished.contains(model)) {
      removeModel(model);
    }
  }
}
