{
  while (!isValidName(longName,stereotype)) {
    stereotype+="_";
  }
  String modelName=stereotype == null ? longName : longName + "@" + stereotype;
  SModel result=new TransientSModelDescriptor(modelName);
  result.load();
  myModels.put(result.getModelName(),result);
  invalidateCaches();
  return result;
}
