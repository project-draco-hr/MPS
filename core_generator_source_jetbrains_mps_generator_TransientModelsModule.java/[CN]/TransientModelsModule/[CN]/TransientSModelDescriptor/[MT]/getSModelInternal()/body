{
  if (mySModel != null) {
    return mySModel;
  }
synchronized (this) {
    if (mySModel == null) {
      mySModel=createModel();
      mySModel.setModelDescriptor(this);
      if (wasUnloaded) {
        SModelOperations.validateLanguagesAndImports(this,false,false);
      }
      fireModelStateChanged(ModelLoadingState.FULLY_LOADED);
    }
  }
  return mySModel;
}
