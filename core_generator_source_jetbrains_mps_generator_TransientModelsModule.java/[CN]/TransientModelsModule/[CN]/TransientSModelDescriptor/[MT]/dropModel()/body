{
  if (mySModel != null) {
    LOG.debug("Dropped " + getSModelReference());
    final SModel oldModel=mySModel;
    mySModel=null;
    if (ModelAccess.instance().canWrite()) {
      notifyModelReplaced(oldModel);
    }
 else {
      ModelAccess.instance().runWriteInEDT(new Runnable(){
        @Override public void run(){
          notifyModelReplaced(oldModel);
        }
      }
);
    }
  }
}
