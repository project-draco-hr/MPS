{
  if (!(RulesUtil.withinInferenceItem(typeOfExpression))) {
    TypeChecker.getInstance().reportTypeError(typeOfExpression,"TYPEOF should be used only within inference rules","jetbrains.mps.bootstrap.helgins.helgins","1195217231011");
  }
  if (!(!(SNodeOperations.isInstanceOf(SLinkOperations.getTarget(typeOfExpression,"term",true),"jetbrains.mps.bootstrap.helgins.structure.TypeVarReference")))) {
    TypeChecker.getInstance().reportTypeError(typeOfExpression,"type of a type has little sense","jetbrains.mps.bootstrap.helgins.helgins","1204815653385");
  }
}
