{
  return new Java_Command().setClassPath(Ant_Command.getAntClassPath(myAntLocation)).setProgramParameter("-Djava.home=" + Java_Command.getJdkHome() + " -Dant.home="+ myAntLocation+ " "+ IterableUtils.join(Sequence.fromIterable(Ant_Command.getMacroValues())," ")+ " "+ ((StringUtils.isNotEmpty(myOptions) ? myOptions + " " : ""))+ "-f "+ antFilePath).createProcess("org.apache.tools.ant.launch.Launcher");
}
