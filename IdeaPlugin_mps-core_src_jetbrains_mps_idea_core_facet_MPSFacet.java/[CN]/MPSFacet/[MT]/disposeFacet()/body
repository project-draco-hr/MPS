{
  if (!wasInitialized()) {
    return;
  }
  ModelAccess.instance().runWriteAction(new Runnable(){
    @Override public void run(){
      try {
        LOG.info(MPSBundle.message("facet.module.loaded",Solution.class.getDeclaredMethod(ApplicationInfo.getInstance().getMajorVersion().equals("12") ? "getModuleFqName" : "getModuleName").invoke(mySolution)));
      }
 catch (      IllegalAccessException e) {
      }
catch (      InvocationTargetException e) {
      }
catch (      NoSuchMethodException e) {
      }
      MPSModuleRepository.getInstance().unregisterModule(mySolution,myMpsProject);
      mySolution=null;
    }
  }
);
}
