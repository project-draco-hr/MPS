{
  String errorString=getErrorString();
  if (errorString != null) {
    setErrorText(errorString);
    String optionsText=errorString + "\n\n" + "Apply changes anyway?";
    int option=JOptionPane.showConfirmDialog(BaseNodeDialog.this,optionsText,"Errors in " + BaseNodeDialog.this.getTitle(),JOptionPane.OK_CANCEL_OPTION,JOptionPane.WARNING_MESSAGE);
    if (option != JOptionPane.OK_OPTION) {
      return false;
    }
  }
  return true;
}
