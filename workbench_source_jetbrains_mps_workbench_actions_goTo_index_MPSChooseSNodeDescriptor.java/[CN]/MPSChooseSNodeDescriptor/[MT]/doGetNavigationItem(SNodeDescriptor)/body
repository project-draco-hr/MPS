{
  return new RootNodeElement(object){
    private MPSProject myProject=getProject();
    public void navigate(    boolean requestFocus){
      ModelAccess.instance().runReadAction(new Runnable(){
        public void run(){
          myProject.getComponentSafe(MPSEditorOpener.class).openNode(SModelUtil_new.findNodeByFQName(object.getNodeName(),BaseAdapter.class,GlobalScope.getInstance()).getNode());
        }
      }
);
    }
  }
;
}
