{
  ConstructorInvocationStatement result=x.isSuperAccess() ? SuperConstructorInvocation.newInstance(myCurrentModel) : ThisConstructorInvocation.newInstance(myCurrentModel);
  addCallArgs(x.arguments,result);
  jetbrains.mps.baseLanguage.structure.ConstructorDeclaration cdecl=(jetbrains.mps.baseLanguage.structure.ConstructorDeclaration)myBindingMap.get(x.binding);
  result.setConstructorDeclaration(cdecl);
  return result;
}
