{
  if (value == null) {
    return true;
  }
  Iterator<RuleType> constants=ListSequence.<RuleType>fromList(RuleType.getConstants()).iterator();
  while (constants.hasNext()) {
    RuleType constant=constants.next();
    if (value.equals(constant.getName())) {
      return true;
    }
  }
  return false;
}
