{
  boolean result;
switch (kind) {
case EQ:
    result=compareValue == 0;
  break;
case NE:
result=compareValue != 0;
break;
case LT:
result=compareValue < 0;
break;
case GT:
result=compareValue > 0;
break;
case LE:
result=compareValue <= 0;
break;
case GE:
result=compareValue >= 0;
break;
default :
throw new UnsupportedOperationException("Unsupported compare type: " + kind);
}
return result;
}
