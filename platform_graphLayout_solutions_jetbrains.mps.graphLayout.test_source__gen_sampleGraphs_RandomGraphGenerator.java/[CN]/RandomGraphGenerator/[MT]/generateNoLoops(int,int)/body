{
  Graph g=new Graph();
  for (int i=0; i < numNodes; i++) {
    g.addNode();
  }
  for (int i=0; i < numEdges; i++) {
    int s=0;
    int t=0;
    while (s == t) {
      s=RandomGraphGenerator.rand.nextInt(numNodes);
      t=RandomGraphGenerator.rand.nextInt(numNodes);
    }
    g.getNode(s).addEdgeTo(g.getNode(t));
  }
  return g;
}
