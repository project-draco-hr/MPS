{
  for (  GenerationRootDependencies rd : dependencies.getRootDependencies()) {
    String id=rd.getRootId();
    if (id == null) {
      id=CONDITIONALS_ID;
    }
    if (rd.isDependsOnConditionals()) {
      graph.get(CONDITIONALS_ID).add(id);
    }
    for (    String s : rd.getLocal()) {
      Set<String> r=graph.get(s);
      if (r != null) {
        r.add(id);
      }
    }
  }
}
