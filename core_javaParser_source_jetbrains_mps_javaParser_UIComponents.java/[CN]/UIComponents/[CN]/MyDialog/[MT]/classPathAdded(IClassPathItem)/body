{
  for (  String unresolvedFQName : new ArrayList<String>(myUnresolvedFQNames)) {
    if (classPath.getClass(unresolvedFQName) != null || !classPath.getAvailableClasses(unresolvedFQName).isEmpty() || !classPath.getSubpackages(unresolvedFQName).isEmpty()) {
      addClassInClassPath(classPath,unresolvedFQName);
      break;
    }
  }
}
