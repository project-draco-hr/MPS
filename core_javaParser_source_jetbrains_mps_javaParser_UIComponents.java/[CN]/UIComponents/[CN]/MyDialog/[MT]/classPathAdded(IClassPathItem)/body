{
  if (classPath == null)   return;
  myAdditionalClasspaths.add(classPath);
  for (  String unresolvedFQName : new ArrayList<String>(myUnresolvedFQNames)) {
    if (classPath.getClass(unresolvedFQName) != null || classPath.getAvailableRootClasses(unresolvedFQName).iterator().hasNext() || classPath.getSubpackages(unresolvedFQName).iterator().hasNext()) {
      addClassInClassPath(classPath,unresolvedFQName);
    }
  }
}
