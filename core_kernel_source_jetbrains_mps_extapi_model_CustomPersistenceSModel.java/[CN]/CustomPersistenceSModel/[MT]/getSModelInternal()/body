{
  if (myModel == null) {
    final ModelLoadingState oldState;
synchronized (this) {
      oldState=getLoadingState();
      if (myModel == null) {
        myModel=loadSModel();
        myModel.setModelDescriptor(this);
        setLoadingState(ModelLoadingState.FULLY_LOADED);
      }
    }
    fireModelStateChanged(oldState,getLoadingState());
  }
  return myModel;
}
