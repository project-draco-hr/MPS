{
  DebugInfo debugInfo=TraceInfoCache.getInstance().get(foundCell.getEditor().getEditedNode().getModel().getModelDescriptor());
  if (debugInfo == null) {
    return null;
  }
  EditorCell cell=foundCell;
  while (cell != null) {
    SNode node=cell.getSNode();
    if (node != null) {
      if (!(debugInfo.getPositions(node).isEmpty())) {
        break;
      }
    }
    cell=cell.getParent();
  }
  return cell;
}
