{
  super("Build",ID);
  this.setIsAlwaysVisible(true);
  this.setIsInternal(false);
  this.setMnemonic("B".charAt(0));
  this.setPopup(false);
  try {
    this.add(new MakeAllAction());
    this.add(new CleanAllAction());
    this.add(new RebuildAllAction());
  }
 catch (  Throwable t) {
    LOG.error("User group error",t);
  }
}
