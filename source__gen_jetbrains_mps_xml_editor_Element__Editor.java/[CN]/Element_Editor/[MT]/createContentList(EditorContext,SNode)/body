{
  if (this.myContentListHandler == null) {
    this.myContentListHandler=new Element_Editor_ContentListHandler(node,"content",context);
  }
  EditorCell_Collection editorCell=this.myContentListHandler.createCells(context,new CellLayout_Vertical(),false);
  editorCell.setSelectable(false);
  editorCell.setDrawBorder(false);
  editorCell.setGridLayout(false);
  editorCell.setDrawBrackets(false);
  editorCell.setBracketsColor(Color.black);
  editorCell.setUsesBraces(false);
  editorCell.putUserObject(EditorCell.ROLE,this.myContentListHandler.getElementRole());
  editorCell.setLayoutConstraint("");
  ((EditorCell_Basic)editorCell).setParseable(false);
  return editorCell;
}
