{
  tokenOffset=currOffset;
  name=null;
  if (chr == '<') {
    shift();
    skipSpaces();
    if (Character.isJavaIdentifierStart(chr)) {
      skipName();
      while (chr != 0 && chr != '>') {
        shift();
      }
      if (chr == '>') {
        int slashIndex=currOffset - 1;
        shift();
        while (slashIndex > tokenOffset && Character.isWhitespace(data[slashIndex])) {
          slashIndex--;
        }
        return data[slashIndex] == '/' ? SIMPLE_TAG : OPEN_TAG;
      }
      return OTHER;
    }
 else     if (chr == '/') {
      shift();
      skipSpaces();
      if (Character.isJavaIdentifierStart(chr)) {
        skipName();
        while (chr != 0 && chr != '>') {
          shift();
        }
        if (chr == '>') {
          shift();
          return CLOSE_TAG;
        }
      }
      return OTHER;
    }
  }
  if (chr != 0) {
    while (chr != 0 && chr != '<') {
      shift();
    }
    return OTHER;
  }
  return EOI;
}
