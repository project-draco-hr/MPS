{
  super("JAVA","application/x-java","text/x-java");
  SyntaxHighlighterFactory.LANGUAGE_FACTORY.addExplicitExtension(this,new SyntaxHighlighterFactory(){
    @NotNull public SyntaxHighlighter getSyntaxHighlighter(    Project project,    VirtualFile virtualFile){
      return new JavaFileHighlighter();
    }
  }
);
}
