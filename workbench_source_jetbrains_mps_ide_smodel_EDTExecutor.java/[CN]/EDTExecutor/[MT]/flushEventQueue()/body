{
  LOG.assertLog(!ThreadUtils.isEventDispatchThread(),"possible deadlock");
  while (true) {
    try {
synchronized (myLock) {
        if (myToExecuteRead.isEmpty() && myToExecuteCommand.isEmpty() && myToExecuteWrite.isEmpty()) {
          return;
        }
        myLock.wait(100);
      }
    }
 catch (    InterruptedException e) {
      LOG.error(e);
    }
  }
}
