{
synchronized (myLock) {
    if (hasCommandWorker) {
      return;
    }
    hasCommandWorker=true;
  }
  try {
    final Project project=peekToExecuteProject();
    if (project != null) {
      myModelAccess.tryWriteInCommand(new Runnable(){
        public void run(){
          Runnable command;
          while ((command=getToExecuteCommand(project)) != null) {
            command.run();
          }
        }
      }
,project);
    }
  }
  finally {
synchronized (myLock) {
      hasCommandWorker=false;
      myLock.notifyAll();
    }
  }
}
