{
synchronized (myLock) {
    if (myToExecuteCommand.isEmpty())     return null;
    Pair<Runnable,Project> pair=myToExecuteCommand.peek();
    if (pair.getSecond() == p) {
      myToExecuteCommand.remove(pair);
      if (myToExecuteCommand.isEmpty())       myLock.notifyAll();
      return pair.getFirst();
    }
    return null;
  }
}
