{
synchronized (myLock) {
    if (hasWriteWorker) {
      return;
    }
    hasWriteWorker=true;
  }
  try {
    myModelAccess.tryWrite(new Runnable(){
      public void run(){
        Runnable command;
        while ((command=getToExecuteWrite()) != null) {
          command.run();
        }
      }
    }
);
  }
  finally {
synchronized (myLock) {
      hasWriteWorker=false;
      myLock.notifyAll();
    }
  }
}
