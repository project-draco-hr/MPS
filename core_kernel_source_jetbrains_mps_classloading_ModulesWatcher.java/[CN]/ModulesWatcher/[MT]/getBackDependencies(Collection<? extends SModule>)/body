{
synchronized (LOCK) {
    if (myChanged)     recountStatus();
    final Collection<SModule> result=new ArrayList<SModule>();
    myBackDepGraph.dfs(modules,new VertexVisitor<SModule>(){
      @Override public void visit(      SModule module){
        result.add(module);
      }
    }
);
    return result;
  }
}
