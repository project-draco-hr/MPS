{
  updateIfChanged();
  if (!getAllModules().contains(mRef)) {
    return INVALID;
  }
 else {
synchronized (myStatusMapLock) {
      if (!myStatusMap.containsKey(mRef)) {
        LOG.warn("No status for the module " + mRef);
        return INVALID;
      }
 else {
        return myStatusMap.get(mRef);
      }
    }
  }
}
