{
synchronized (LOCK) {
    for (    SModule module : modules) {
      if (!myLoadableCondition.met(module)) {
        LOG.error("Non-loadable module was passed to ModulesWatcher",new Throwable());
        continue;
      }
      myDepGraph.addVertex(module);
      myBackDepGraph.addVertex(module);
    }
    myChanged=true;
  }
}
