{
  int res;
  if (TemporaryModels.isTemporary(myModel)) {
    res=JOptionPane.YES_OPTION;
  }
 else {
    res=JOptionPane.showConfirmDialog(myFrame,"<html>Model <b>" + modelToImport.getName() + "</b> is owned by module <b>"+ mRefToImport.getModuleName()+ "</b> which is not imported.</html>\n\n"+ "Importing the module will take some time.\n"+ "Do you want to automatically import the module?","Module import",JOptionPane.YES_NO_OPTION);
  }
  return res == JOptionPane.YES_OPTION;
}
