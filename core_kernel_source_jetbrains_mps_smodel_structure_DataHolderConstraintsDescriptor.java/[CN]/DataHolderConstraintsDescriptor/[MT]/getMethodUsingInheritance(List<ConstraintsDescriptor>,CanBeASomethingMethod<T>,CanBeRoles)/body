{
  if (direct != null) {
    return direct;
  }
 else {
    for (    ConstraintsDescriptor parentDescriptor : parentDescriptors) {
      CanBeASomethingMethod<?> superMethod=parentDescriptor.getCanBeASomethingMethod(role);
      if (superMethod != null) {
        return (CanBeASomethingMethod<T>)superMethod;
      }
    }
  }
  return null;
}
