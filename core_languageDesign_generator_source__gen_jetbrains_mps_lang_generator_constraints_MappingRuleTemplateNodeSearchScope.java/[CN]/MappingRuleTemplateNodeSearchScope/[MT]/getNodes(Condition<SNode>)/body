{
  if (condition == Condition.FALSE_CONDITION) {
    return new ArrayList<SNode>();
  }
  ensureInitialized();
  if (condition == Condition.TRUE_CONDITION) {
    return myOwnNodes;
  }
  List<SNode> resut=new ArrayList<SNode>();
  for (  SNode node : myOwnNodes) {
    if (condition.met(node)) {
      resut.add(node);
    }
  }
  return resut;
}
