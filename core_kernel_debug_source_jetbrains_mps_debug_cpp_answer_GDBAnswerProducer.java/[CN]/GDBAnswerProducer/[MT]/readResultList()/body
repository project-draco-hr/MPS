{
  RecordValue recordValue=new RecordValue();
  while (true) {
    Pair<String,GDBValue> pair=readResult();
    recordValue.putProperty(pair.o1,pair.o2);
    if (myOffset >= myInput.length() || currentChar() == '\n' || currentChar() == '\r') {
      return recordValue;
    }
 else     if (currentChar() == ',') {
      myOffset++;
    }
 else {
      error("");
      return null;
    }
  }
}
