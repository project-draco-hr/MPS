{
  if (value == null) {
    return null;
  }
  Iterator<Option> constants=ListSequence.<Option>fromList(Option.getConstants()).iterator();
  while (constants.hasNext()) {
    Option constant=constants.next();
    if (value.equals(constant.getName())) {
      return constant.getValueAsString();
    }
  }
  return null;
}
