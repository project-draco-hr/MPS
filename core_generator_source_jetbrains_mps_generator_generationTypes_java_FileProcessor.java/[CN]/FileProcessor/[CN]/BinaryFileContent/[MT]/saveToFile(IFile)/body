{
  if (file.exists() && isUnchanged(file)) {
    return;
  }
  OutputStream stream=null;
  try {
    stream=file.openOutputStream();
    stream.write(myContent);
  }
 catch (  IOException e) {
    LOG.error(null,e);
  }
 finally {
    if (stream != null) {
      try {
        stream.close();
      }
 catch (      IOException ignored) {
      }
    }
  }
}
