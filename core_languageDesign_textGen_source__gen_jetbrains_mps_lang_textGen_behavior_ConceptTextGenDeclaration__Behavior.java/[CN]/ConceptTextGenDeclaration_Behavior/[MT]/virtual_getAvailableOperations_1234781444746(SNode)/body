{
  List<SNode> result=ListSequence.fromList(new ArrayList<SNode>());
  List<SNode> roots=SModelOperations.getRoots(SNodeOperations.getModel(thisNode),"jetbrains.mps.lang.textGen.structure.LanguageTextGenDeclaration");
