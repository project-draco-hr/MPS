{
  Set<String> result=new HashSet<String>();
  File dir=getModelDir(namespace);
  if (dir.exists()) {
    for (    File file : dir.listFiles()) {
      if (file.isDirectory()) {
        if (namespace.length() > 0) {
          result.add(namespace + "." + file.getName());
        }
 else {
          result.add(namespace + file.getName());
        }
      }
    }
  }
  return result;
}
