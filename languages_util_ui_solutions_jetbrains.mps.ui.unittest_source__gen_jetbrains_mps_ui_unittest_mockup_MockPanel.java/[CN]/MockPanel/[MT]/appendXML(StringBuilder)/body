{
  sb.append("<panel type=\"" + this.type + "\">");
  for (  MockPanel.WidgetComp w : ListSequence.<MockPanel.WidgetComp>fromList(this.widgets)) {
    sb.append("<child comp=\"" + w.compartment() + "\">");
    w.widget().appendXML(sb);
    sb.append("</child>");
  }
  sb.append("</panel>");
  return sb;
}
