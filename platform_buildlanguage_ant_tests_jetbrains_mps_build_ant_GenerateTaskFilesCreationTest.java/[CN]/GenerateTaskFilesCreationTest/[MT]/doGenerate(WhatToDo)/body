{
  MpsWorker mpsWorker=new GeneratorWorker(whatToDo,new ProjectComponent(){
    public void log(    String msg){
      System.out.println(msg);
    }
    @Override public void log(    String msg,    int msgLevel){
      String levelText="";
switch (msgLevel) {
case Project.MSG_ERR:
        levelText="ERROR";
      break;
case Project.MSG_WARN:
    levelText="WARN";
  break;
case Project.MSG_INFO:
levelText="INFO";
break;
case Project.MSG_VERBOSE:
levelText="VERBOSE";
break;
case Project.MSG_DEBUG:
levelText="DEBUG";
break;
default :
fail("Unknown log message levelText " + msgLevel);
}
System.out.println(levelText + ": " + msg);
}
}
);
mpsWorker.loadObjects();
}
