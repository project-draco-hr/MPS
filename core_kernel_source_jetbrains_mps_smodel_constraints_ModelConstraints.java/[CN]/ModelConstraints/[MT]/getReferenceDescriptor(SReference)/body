{
  ModelAccess.assertLegalRead();
  SNode node=reference.getSourceNode();
  SNode concept=node.getConceptDeclarationNode();
  return new OkReferenceDescriptor(reference,concept,node.getParent(),node,reference.getRole(),reference.getRole(),0,SModelUtil.getLinkDeclarationTarget(SModelSearchUtil.findLinkDeclaration(concept,node.getRole())),node.getRoleLink());
}
