{
  if (!concept.isValid()) {
    return MetaAdapterByDeclaration.asInstanceConcept(concept);
  }
  ConceptDescriptor descriptor=ConceptRegistry.getInstance().getConceptDescriptor(ConceptRegistryUtil.getConstraintsDescriptor(concept).getDefaultConcreteConcept());
  return MetaAdapterByDeclaration.asInstanceConcept(MetaAdapterFactory.getAbstractConcept(descriptor));
}
