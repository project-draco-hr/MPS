{
  ModelAccess.instance().tryRead(new Runnable(){
    public void run(){
      long start=System.currentTimeMillis();
      while (true) {
        if (System.currentTimeMillis() - start > MAX_TIME)         break;
        Runnable command=getToExecuteRead();
        if (command == null)         break;
        command.run();
      }
    }
  }
);
}
