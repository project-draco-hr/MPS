{
  LOG.assertLog(!ThreadUtils.isEventDispatchThread(),"possible deadlock");
  while (true) {
    try {
synchronized (myLock) {
        if (myToExecuteRead.isEmpty() && myToExecuteCommand.isEmpty()) {
          return;
        }
      }
      Thread.sleep(10);
    }
 catch (    InterruptedException e) {
      LOG.error(e);
    }
  }
}
