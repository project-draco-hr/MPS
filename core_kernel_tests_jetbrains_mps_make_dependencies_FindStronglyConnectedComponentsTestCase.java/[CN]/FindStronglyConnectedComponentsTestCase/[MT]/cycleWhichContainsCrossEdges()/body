{
  IntGraph g=new IntGraph(3);
  g.addEdges(0,1);
  g.addEdges(0,2);
  g.addEdges(2,1);
  g.addEdges(1,0);
  List<Set<IVertex>> condensation=Graphs.getInstance().findStronglyConnectedComponents(g.getGraph());
  Assert.assertEquals(CollectionUtil.list(CollectionUtil.stableSet(new IntVertex(0),new IntVertex(1),new IntVertex(2))),condensation);
}
