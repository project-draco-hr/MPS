{
  IntGraph g=new IntGraph(3);
  g.addEdges(1,0);
  g.addEdges(2,0);
  List<Set<IVertex>> condensation=Graphs.getInstance().findStronglyConnectedComponents(g.getGraph());
  Assert.assertEquals(CollectionUtil.list(CollectionUtil.set(new IntVertex(2)),CollectionUtil.set(new IntVertex(1)),CollectionUtil.set(new IntVertex(0))),condensation);
}
