{
  for (  IBlock block : this.myBlocks) {
    block.paint(g);
  }
  List<Line> lines=new ArrayList<Line>(this.myLines);
  Collections.sort(lines);
  for (  Line line : lines) {
    line.paint(g,this.myComponent.getBackground());
  }
  for (  ArrowHead arrowHead : this.myArrowHeads) {
    arrowHead.paint(g);
  }
}
