{
  String cname;
  String linkRole;
  if (conceptName == null) {
    cname=MPSModuleRepository.getInstance().getDebugRegistry().getConceptName(roleId.getConceptId());
    linkRole=getRole();
  }
 else {
    cname=conceptName;
    linkRole=this.role;
  }
  SNode concept=SModelUtil.findConceptDeclaration(cname);
  if ((concept == null)) {
    return null;
  }
  return (SNode)new ConceptAndSuperConceptsScope(concept).getLinkDeclarationByRole(linkRole);
}
