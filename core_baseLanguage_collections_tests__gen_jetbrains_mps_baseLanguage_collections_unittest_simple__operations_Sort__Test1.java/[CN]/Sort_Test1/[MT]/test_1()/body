{
  List<Integer> expected=ListSequence.<Integer>fromListAndArray(new ArrayList<Integer>(),1,2,3,4,5);
  List<Integer> list=ListSequence.<Integer>fromListAndArray(new ArrayList<Integer>(),2,4,1,5,3);
  List<Integer> list_sorted=ListSequence.<Integer>fromList(list).sort(new ISelector<Integer,Comparable<?>>(){
    public Comparable<?> select(    Integer it){
      return it;
    }
  }
,true).toListSequence();
  int index=0;
  for (  Integer i : ListSequence.<Integer>fromList(list_sorted)) {
    Assert.assertEquals(ListSequence.<Integer>fromList(expected).getElement(index),i);
    index++;
  }
}
