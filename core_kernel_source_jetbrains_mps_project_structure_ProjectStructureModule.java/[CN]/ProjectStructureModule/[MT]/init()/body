{
  if (INSTANCE != null) {
    throw new IllegalStateException("double initialization");
  }
  INSTANCE=this;
  ModelAccess.instance().runWriteAction(new Runnable(){
    public void run(){
      MPSModuleRepository.getInstance().addModule(ProjectStructureModule.this,new MPSModuleOwner(){
      }
);
    }
  }
);
}
