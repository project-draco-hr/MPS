{
  try {
    for (    SModelDescriptor model : ListSequence.fromList(SModelRepository.getInstance().getModelDescriptors())) {
      if (!(SModelStereotype.isUserModel(model))) {
        continue;
      }
      if (!(model instanceof EditableSModelDescriptor)) {
        continue;
      }
      if (model.getModule() == null || model.getModule().isPackaged()) {
        continue;
      }
      new MissingDependenciesFixer(model).fix(false);
      model.getModule().invalidateCaches();
    }
    ModelAccess.instance().runWriteAction(new Runnable(){
      public void run(){
        ClassLoaderManager.getInstance().reloadAll(new EmptyProgressMonitor());
      }
    }
);
  }
 catch (  Throwable t) {
    if (log.isErrorEnabled()) {
      log.error("User's action execute method failed. Action:" + "AddMissingImportsInGlobalScope",t);
    }
  }
}
