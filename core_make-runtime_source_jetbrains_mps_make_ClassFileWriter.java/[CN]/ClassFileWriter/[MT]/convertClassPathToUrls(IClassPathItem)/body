{
  final List<URL> urls=new ArrayList<>();
  for (  RealClassPathItem flatten : classPath.flatten()) {
    try {
      urls.add(new File(flatten.getPath()).toURI().toURL());
    }
 catch (    MalformedURLException e) {
      e.printStackTrace();
    }
  }
  return urls.toArray(new URL[urls.size()]);
}
