{
  if (myContextNode == null || !(SNodeOperations.isInstanceOf(node,MetaAdapterFactory.getConcept(new UUID(-935030926396207931l,-6610165693999523818l),1178285077437l,"jetbrains.mps.baseLanguage.structure.ClassifierMember")))) {
    if (LOG.isEnabledFor(Level.ERROR)) {
      LOG.error("isInScope(" + node + ") - context is null or not ClassifierMember");
    }
    return super.isInScope(node);
  }
  return myClassifierScope.getClassifierNodes().contains(SNodeOperations.getNodeAncestor(node,MetaAdapterFactory.getConcept(new UUID(-935030926396207931l,-6610165693999523818l),1107461130800l,"jetbrains.mps.baseLanguage.structure.Classifier"),false,false)) && isVisible(SNodeOperations.cast(node,MetaAdapterFactory.getConcept(new UUID(-935030926396207931l,-6610165693999523818l),1178285077437l,"jetbrains.mps.baseLanguage.structure.ClassifierMember")));
}
