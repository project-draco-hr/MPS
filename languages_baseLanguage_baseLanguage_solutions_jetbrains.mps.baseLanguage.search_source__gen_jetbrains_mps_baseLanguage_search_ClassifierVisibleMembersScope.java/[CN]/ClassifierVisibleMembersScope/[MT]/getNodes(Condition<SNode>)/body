{
  return myClassifierScope.getNodes(new Condition<SNode>(){
    @Override public boolean met(    SNode node){
      SNode member=SNodeOperations.as(node,MetaAdapterFactory.getConcept(new UUID(-935030926396207931l,-6610165693999523818l),1178285077437l,"jetbrains.mps.baseLanguage.structure.ClassifierMember"));
