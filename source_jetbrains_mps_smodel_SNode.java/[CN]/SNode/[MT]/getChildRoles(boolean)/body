{
  NodeReadAccessCaster.fireNodeReadAccessed(this);
  Set<String> result=new HashSet<String>();
  for (  SNode child : getChildren()) {
    String roleOf=getRoleOf(child);
    if (includeAttributeRoles || !(roleOf.contains(STEREOTYPE_DELIM))) {
      result.add(roleOf);
    }
  }
  return result;
}
