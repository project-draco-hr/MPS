{
  if (myRegisteredInModelFlag)   return;
  myRegisteredInModelFlag=true;
  if (model != myModel) {
    changeModel(model);
  }
 else {
    myModel.setNodeId(getId(),this);
    if (myChildren != null) {
      for (      SNode child : _children()) {
        child.registerInModel(model);
      }
    }
  }
}
