{
  if (myChildren == null) {
    myChildren=new ArrayList<SNode>(2);
    if (myChildrenLoader != null) {
      myModel.runLoadingAction(new Runnable(){
        public void run(){
          try {
            myChildrenLoader.loadChildren(SNode.this);
          }
 catch (          Throwable t) {
            LOG.error(t);
          }
        }
      }
);
      myChildrenLoader=null;
    }
  }
  return myChildren;
}
