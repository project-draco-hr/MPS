{
  if (myChildren == null) {
    myChildren=new ArrayList<SNode>();
    if (myChildrenLoader != null) {
      getModel().runLoadingAction(new Runnable(){
        public void run(){
          myChildrenLoader.loadChildren(SNode.this);
        }
      }
);
      for (      SNode node : myChildren) {
        node.registerInModel(node.getModel());
      }
    }
  }
  return myChildren;
}
