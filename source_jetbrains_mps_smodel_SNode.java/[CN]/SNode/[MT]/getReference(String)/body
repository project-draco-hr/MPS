{
  fireNodeReadAccess();
  SReference result=null;
  int count=0;
  if (myReferences != null) {
    for (    SReference reference : myReferences) {
      if (reference.getRole().equals(role)) {
        result=reference;
        count++;
      }
    }
  }
  if (count > 1) {
    LOG.errorWithTrace("ERROR: " + count + " referents for role '"+ role+ "' in "+ getDebugText());
  }
  return result;
}
