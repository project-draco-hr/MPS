{
  return levelCell != null && levelCell.isBig() && levelCell.getSNode() != null && haveSimilarLink(levelCell.getSNode()) && CellFinderUtil.findChildByCondition(levelCell,new Condition<EditorCell>(){
    @Override public boolean met(    EditorCell cell){
      return !cell.isReferenceCell() && cell instanceof EditorCell_Collection && myRole.equals(cell.getRole());
    }
  }
,forward(),true) != null;
}
