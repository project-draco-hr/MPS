{
  if (levelCell == null) {
    return false;
  }
  final SNode levelNode=levelCell.getSNode();
  return levelNode != null && haveSimilarLink(levelNode) && CellFinderUtil.findChildByCondition(levelCell,new Condition<EditorCell>(){
    @Override public boolean met(    EditorCell cell){
      return !cell.isReferenceCell() && cell instanceof EditorCell_Collection && myRole.equals(cell.getRole())&& cell.getSNode().equals(levelNode);
    }
  }
,forward(),true) != null;
}
