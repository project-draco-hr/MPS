{
  ModuleXml result=new ModuleXml(false);
  String[] param=URLDecoder.decode(s).split("\n");
  if (param.length > 0) {
    result.setRef(param[0]);
  }
  if (param.length > 1) {
    result.setDest(param[1]);
  }
  for (int i=2; i < param.length; ++i) {
    result.addText(param[i]);
  }
  return result;
}
