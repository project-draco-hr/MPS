{
  UIUtil.invokeLaterIfNeeded(new Runnable(){
    public void run(){
      if (myProject.isDisposed()) {
        return;
      }
      myMpsProject.getRepository().getModelAccess().executeCommand(new Runnable(){
        public void run(){
          dataModuleOptions.disposeModule();
        }
      }
);
    }
  }
);
}
