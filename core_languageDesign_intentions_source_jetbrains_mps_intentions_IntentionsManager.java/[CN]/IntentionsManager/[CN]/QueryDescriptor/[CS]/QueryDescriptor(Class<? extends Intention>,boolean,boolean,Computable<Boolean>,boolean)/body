{
  myIntentionClass=intentionClass;
  myInstantiate=instantiate;
  myEnabledOnly=enabledOnly;
  myCurrentNodeOnly=currentNodeOnly;
  if (terminated != null) {
    myTerminated=terminated;
  }
 else {
    myTerminated=new Computable<Boolean>(){
      public Boolean compute(){
        return false;
      }
    }
;
  }
}
