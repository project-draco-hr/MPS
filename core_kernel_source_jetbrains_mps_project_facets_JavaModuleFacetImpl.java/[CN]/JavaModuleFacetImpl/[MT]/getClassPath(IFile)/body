{
  String path=classes.getPath();
  if (path.contains("!")) {
    String[] split=path.split("!");
    if (split.length > 0) {
      if (!split[1].isEmpty() && !split[1].equals("/")) {
        LOG.warn("Can not transform directory " + path + " to proper classpath while calculating classpath for module "+ getModule());
      }
    }
    return split[0];
  }
  return path;
}
