{
  this.append("(");
  appendNode(SLinkOperations.getTarget(node,"type",true));
  this.append(") ");
  boolean needsParensAroundCastExpression=PrecedenceUtil.needsParensAroundCastExpression(node);
  if (needsParensAroundCastExpression) {
    this.append("(");
  }
  appendNode(SLinkOperations.getTarget(node,"expression",true));
  if (needsParensAroundCastExpression) {
    this.append(")");
  }
}
