{
  ModelAccess.instance().runReadAction(new Runnable(){
    public void run(){
      try {
        filter.start();
        ModelPersistence.saveModel(model.getSModel(),dataSource,persistence);
      }
 catch (      IOException e) {
        Assert.fail("Exception during test. See log for details");
        e.printStackTrace();
      }
 finally {
        filter.stop();
      }
    }
  }
);
}
