{
  myModel.getRootNodes().iterator();
  if (!myNodeMap.isEmpty()) {
    return getNodesImpl(conceptFqName,includeInherited);
  }
synchronized (myNodeMap) {
    if (myNodeMap.isEmpty()) {
      ConceptInstanceMap all=build(new ConceptNodeMapBuilder(myModel));
      all.trimValues();
      myNodeMap.merge(all);
    }
    return getNodesImpl(conceptFqName,includeInherited);
  }
}
