{
  if (editorCell.getSubstituteInfo() == null || editorCell.getSubstituteInfo() instanceof DefaultSubstituteInfo) {
    editorCell.setSubstituteInfo(new OldNewCompositeSubstituteInfo(myEditorContext,new SChildSubstituteInfo(editorCell,myOwnerNode,MetaAdapterFactory.getContainmentLink(0xb4dbff0c8c314a79L,0xa45a98e5fd0530e7L,0x4a1cc65caa54931bL,0x4a1cc65caa549328L,"field"),child),new DefaultChildSubstituteInfo(myOwnerNode,myContainmentLink.getDeclarationNode(),myEditorContext)));
  }
  if (editorCell.getRole() == null) {
    editorCell.setRole("field");
  }
}
