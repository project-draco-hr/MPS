{
  if (myGenerator.getSourceModel().getUID().equals(reference.getTargetModelUID())) {
    reference.getSourceNode().removeReference(reference);
    ReferenceInfo_CopiedInputNode refInfo=new ReferenceInfo_CopiedInputNode(reference.getRole(),reference.getSourceNode(),myInputNode,reference.getTargetNode());
    PostponedReference postponedReference=new PostponedReference(refInfo,myGenerator);
    reference.getSourceNode().addReference(postponedReference);
  }
}
