{
  String modelPath=PathManager.findModelPath(myProjectModelRoots,modelFQName);
  if (modelPath != null && (new File(modelPath)).exists()) {
    return modelPath;
  }
  modelPath=PathManager.findModelPath(myLibraryModelRoots,modelFQName);
  if (modelPath != null && (new File(modelPath)).exists()) {
    return modelPath;
  }
  return null;
}
