{
  if (source == null)   return null;
  StringBuffer buf=new StringBuffer(source);
  for (int i=0; i < buf.length(); i++) {
    Character c=buf.charAt(i);
    String s=ourSymbols.get(c);
    if (s != null) {
      buf.deleteCharAt(i);
      buf.insert(i,s);
      i+=s.length() - 1;
    }
  }
  return new String(buf);
}
