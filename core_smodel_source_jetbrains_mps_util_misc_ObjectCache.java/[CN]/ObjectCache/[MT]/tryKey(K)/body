{
  ++_attempts;
  if (key == _lastKey) {
    ++_hits;
    return _lastValue;
  }
  V result=_secondGenerationQueue.get(key);
  if (result == null) {
    result=_firstGenerationQueue.remove(key);
    if (result != null) {
      _secondGenerationQueue.put(key,result);
    }
  }
  if (result != null) {
    _lastKey=key;
    _lastValue=result;
    ++_hits;
  }
  return result;
}
