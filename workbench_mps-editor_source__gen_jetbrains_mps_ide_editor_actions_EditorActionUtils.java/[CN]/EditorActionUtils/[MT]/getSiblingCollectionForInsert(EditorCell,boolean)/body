{
  EditorCell nextLeaf=(forward ? APICellAdapter.getNextLeaf(cell) : APICellAdapter.getPrevLeaf(cell));
  if ((APICellAdapter.isBigCell(cell) || APICellAdapter.isLastPositionInBigCell(cell)) && ((forward ? hasSingleRolesAtRightBoundary(cell) : hasSingleRolesAtLeftBoundary(cell))) && nextLeaf != null) {
    return new ChildrenCollectionFinder(nextLeaf,cell,forward,true).find();
  }
  return null;
}
