{
  @SuppressWarnings("unchecked") QueryMethod<SNode> method=(QueryMethod<SNode>)getMethod(context,"cannot find method '%s' for INSERT macro");
  try {
    return method.invoke(context);
  }
 catch (  IllegalQueryMethodException e) {
    throw fromQueryAccessCode(e);
  }
catch (  InvocationTargetException e) {
    throw fromUserCode(myMethodName,context,e);
  }
}
