{
  try {
    createMethod().invoke(ctx);
  }
 catch (  ClassNotFoundException e) {
    ctx.showWarningMessage(null,String.format("cannot run script '%s' : no generated code found",myMethodName));
  }
catch (  NoSuchMethodException e) {
    ctx.showWarningMessage(null,String.format("cannot run script '%s' : no generated code found",myMethodName));
  }
}
