{
  if (myMethod == null) {
    QueryMethod<Boolean> m=null;
    try {
      m=QueryMethodGenerated.getQueryMethod(myTemplateNode.getModelReference(),myMethodName);
    }
 catch (    ClassNotFoundException e) {
      ctx.showWarningMessage(null,String.format("cannot find condition method '%s' : evaluate to %s",myMethodName,String.valueOf(myDefValue).toUpperCase()));
    }
catch (    NoSuchMethodException e) {
      ctx.showWarningMessage(null,String.format("cannot find condition method '%s' : evaluate to %s",myMethodName,String.valueOf(myDefValue).toUpperCase()));
    }
    myMethod=m != null ? m : new QueryMethod<Boolean>(){
      @Override public Boolean invoke(      IOperationContext context,      Object contextObject){
        return myDefValue;
      }
    }
;
  }
  return myMethod;
}
