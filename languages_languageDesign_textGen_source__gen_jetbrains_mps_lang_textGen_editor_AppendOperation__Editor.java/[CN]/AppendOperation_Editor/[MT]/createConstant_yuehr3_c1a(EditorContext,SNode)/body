{
  EditorCell_Constant editorCell=new EditorCell_Constant(editorContext,node,"redirect output to the identified text area");
  editorCell.setCellId("Constant_yuehr3_c1a");
  Style style=new StyleImpl();
  BaseStyleSheet_StyleSheet.apply_Comment(style,editorCell);
  style.set(StyleAttributes.SELECTABLE,0,false);
  editorCell.getStyle().putAll(style);
  editorCell.setDefaultText("");
  return editorCell;
}
