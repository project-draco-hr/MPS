{
  if (myParentNode == null) {
    return true;
  }
  if (concept == null) {
    return true;
  }
  SNodeReference outputConceptSourceNodeReference=concept.getSourceNode();
  if (outputConceptSourceNodeReference == null) {
    return true;
  }
  SNode outputConceptSourceNode=outputConceptSourceNodeReference.resolve(myRepository);
  if (outputConceptSourceNode == null) {
    return true;
  }
  return myLinkDeclarationNode == null || ModelConstraints.canBeParent(myParentNode,outputConceptSourceNode,myLinkDeclarationNode,null,null) && ModelConstraints.canBeAncestor(myParentNode,null,outputConceptSourceNode,null);
}
