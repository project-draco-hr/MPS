{
  myStage=0;
  myCreationStage=MapSequence.fromMap(new HashMap<Object,Integer>());
  for (  Node node : ListSequence.fromList(graph.getNodes())) {
    putStage(node);
  }
  for (  Edge edge : ListSequence.fromList(graph.getEdges())) {
    putStage(edge);
  }
  nextStage();
  graph.addListener(this);
}
