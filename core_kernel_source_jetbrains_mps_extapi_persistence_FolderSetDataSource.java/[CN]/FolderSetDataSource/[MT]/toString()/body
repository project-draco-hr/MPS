{
  StringBuilder sb=new StringBuilder("FolderSetDataSource(");
synchronized (LOCK) {
    boolean first=true;
    for (    String s : myPaths.keySet()) {
      if (first) {
        first=false;
      }
 else {
        sb.append(",");
      }
      if (sb.length() > 200) {
        sb.append("....");
        break;
      }
 else {
        sb.append(s);
      }
    }
  }
  sb.append(")");
  return sb.toString();
}
