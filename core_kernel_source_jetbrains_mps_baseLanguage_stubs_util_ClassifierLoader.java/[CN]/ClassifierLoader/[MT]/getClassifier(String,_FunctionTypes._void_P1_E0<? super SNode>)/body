{
  if (AbstractClassPathItem.isAnonymous(name)) {
    return;
  }
  IFile classfile=myDir.getDescendant(name + MPSExtentions.DOT_CLASSFILE);
  byte[] code=read(classfile);
  if (code == null) {
    return;
  }
  ClassReader reader=new ClassReader(code);
  ASMClass ac=new ASMClass(reader);
  SNode res=createClassifierForClass(name,reader);
  adder.invoke(res);
  myUpdater.updateClassifier(myModule,res,ac);
  updateInnerClassifiers(ac,res);
}
