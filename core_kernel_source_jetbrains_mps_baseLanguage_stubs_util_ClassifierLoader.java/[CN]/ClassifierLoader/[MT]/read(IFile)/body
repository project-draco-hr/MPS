{
  InputStream inp=null;
  try {
    inp=classfile.openInputStream();
    if (inp == null)     return null;
    byte[] result=new byte[(int)inp.available()];
    ReadUtil.read(result,inp);
    return result;
  }
 catch (  IOException e) {
    return null;
  }
 finally {
    if (inp != null) {
      try {
        inp.close();
      }
 catch (      IOException e) {
        LOG.error(e);
      }
    }
  }
}
