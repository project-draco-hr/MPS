{
  if (MPSCore.getInstance().isTestMode())   return;
  if (checker != null) {
synchronized (CHECKERS_LOCK) {
      myCheckers.add(checker);
    }
    addPendingAction(new Runnable(){
      public void run(){
        myCheckedOnceEditors.clear();
        myCheckedOnceInspectors.clear();
      }
    }
);
  }
}
