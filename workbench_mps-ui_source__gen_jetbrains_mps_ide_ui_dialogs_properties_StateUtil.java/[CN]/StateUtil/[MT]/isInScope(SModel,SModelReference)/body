{
  return ModelAccess.instance().runReadAction(new Computable<Boolean>(){
    public Boolean compute(){
      SModel targetModel=modelReference.resolve(MPSModuleRepository.getInstance());
      if (targetModel == null)       return false;
      SModule sourceModule=model.getModule();
      return new GlobalModuleDependenciesManager(sourceModule).getModules(Deptype.VISIBLE).contains(targetModel.getModule());
    }
  }
);
}
