{
  FileInputStream is=new FileInputStream(f);
  File target;
  if (to.isDirectory()) {
    target=new File(to,f.getName());
  }
 else {
    target=to;
  }
  if (!to.getParentFile().exists()) {
    to.getParentFile().mkdirs();
  }
  OutputStream os=new FileOutputStream(target);
  os.write(ReadUtil.read(is));
  is.close();
  os.close();
}
