{
  List<SNode> res=new ArrayList<SNode>();
  boolean foundStart=false;
  for (  SNode stmt : ((List<SNode>)SLinkOperations.getTargets(slist,"statement",true))) {
    if (stmt == start) {
      foundStart=true;
    }
    if (foundStart) {
      if (isControlStatement(stmt)) {
        break;
      }
      if (!(isIgnoredStatement(stmt))) {
        res.add(stmt);
      }
    }
  }
  return res;
}
