{
  SNodePointer nodePointer=tracerNode.getNodePointer();
  if (nodePointer != null) {
    SNode node=nodePointer.getNode();
    if (node != null) {
      Icon icon=IconManager.getIconFor(node);
      if (icon != null) {
        return icon;
      }
    }
  }
  return jetbrains.mps.ide.projectPane.Icons.DEFAULT_ICON;
}
