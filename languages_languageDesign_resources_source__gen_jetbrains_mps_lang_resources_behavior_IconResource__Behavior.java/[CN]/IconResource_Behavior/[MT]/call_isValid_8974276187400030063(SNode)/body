{
  SModule module=SNodeOperations.getModel(thisNode).getModule();
  if (!(module instanceof AbstractModule)) {
    return false;
  }
  MacroHelper macroHelper=MacrosFactory.forModule((AbstractModule)module);
  if (macroHelper == null) {
    return false;
  }
  String path=macroHelper.expandPath(SPropertyOperations.getString(thisNode,MetaAdapterFactory.getProperty(new UUID(-7480838662350550057l,-7394046985400605979l),8974276187400029898l,8974276187400029899l,"path")));
  if (path == null) {
    return false;
  }
  IFile file=FileSystem.getInstance().getFileByPath(path);
  if (!(file.exists())) {
    return false;
  }
  try {
    new ImageIcon(path);
    return true;
  }
 catch (  Throwable t) {
    return false;
  }
}
