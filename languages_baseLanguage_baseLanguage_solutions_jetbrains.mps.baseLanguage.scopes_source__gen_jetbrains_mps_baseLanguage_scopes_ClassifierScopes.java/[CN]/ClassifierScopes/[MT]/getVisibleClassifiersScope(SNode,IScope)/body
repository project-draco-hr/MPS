{
  return new FilteringScope(getReachableClassifiersScope(SNodeOperations.getModel(contextNode),scope)){
    @Override public boolean isExcluded(    SNode node){
      return !(VisibilityUtil.isVisible(contextNode,SNodeOperations.cast(node,"jetbrains.mps.baseLanguage.structure.IVisible")));
    }
  }
;
}
