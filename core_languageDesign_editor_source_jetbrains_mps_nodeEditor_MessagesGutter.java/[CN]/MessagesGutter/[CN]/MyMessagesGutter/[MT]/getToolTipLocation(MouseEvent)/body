{
  int y=event.getY();
  List<EditorMessage> messages=getMessagesAt(y);
  if (messages.size() > 0) {
    EditorMessage msg=messages.get(messages.size() - 1);
    int start=getMessageStart(msg);
    int height=getMessageHeight(msg);
    return new Point(event.getX(),start + height / 2);
  }
  return null;
}
