{
  if (isManagerThread()) {
    myThread.processCommand(command);
  }
 else {
    final CountDownLatch countDown=new CountDownLatch(1);
    schedule(new _FunctionTypes._void_P0_E0(){
      public void invoke(){
        try {
          command.invoke();
        }
  finally {
          countDown.countDown();
        }
      }
    }
);
    try {
      countDown.await();
    }
 catch (    InterruptedException ignore) {
    }
  }
}
