{
  if (isManagerThread()) {
    myThread.processCommand(command);
  }
 else {
    final CountDownLatch countDown=new CountDownLatch(1);
    schedule(new CommandDecorator(command,new _FunctionTypes._void_P0_E0(){
      public void invoke(){
        countDown.countDown();
      }
    }
));
    try {
      countDown.await();
    }
 catch (    InterruptedException ignore) {
    }
  }
}
