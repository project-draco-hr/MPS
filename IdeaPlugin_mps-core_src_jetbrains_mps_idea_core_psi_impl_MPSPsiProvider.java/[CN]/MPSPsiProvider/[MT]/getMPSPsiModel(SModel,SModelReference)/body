{
  if (MPS2PsiMapperUtil.hasCorrespondingPsi(model))   return null;
synchronized (model) {
    MPSPsiModel result=models.get(modelRef);
    if (result == null) {
      result=new MPSPsiModel(modelRef,PsiManager.getInstance(myProject));
      result.reload(model);
      models.put(modelRef,result);
    }
    return result;
  }
}
