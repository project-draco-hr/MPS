{
  if (MPS2PsiMapperUtil.hasCorrespondingPsi(model))   return null;
  MPSPsiModel result;
  result=new MPSPsiModel(modelRef,PsiManager.getInstance(myProject));
  final MPSPsiModel existing=models.putIfAbsent(modelRef,result);
  result.reload(model);
  if (existing != null) {
    result=existing;
  }
  return result;
}
