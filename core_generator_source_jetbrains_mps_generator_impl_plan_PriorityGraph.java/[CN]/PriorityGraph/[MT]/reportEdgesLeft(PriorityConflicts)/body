{
  CycleDetector cd=new CycleDetector();
  HashSet<MappingPriorityRule> rules=new HashSet<MappingPriorityRule>();
  for (  Entry edge : myRulePriorityEntries) {
    if (edge.isTrivial()) {
      continue;
    }
    cd.feed(edge);
    rules.addAll(edge.getRules());
  }
  conflicts.registerLeftovers(rules);
}
