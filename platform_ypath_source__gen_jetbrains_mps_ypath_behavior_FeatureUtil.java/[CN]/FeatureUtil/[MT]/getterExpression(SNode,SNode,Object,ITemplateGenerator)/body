{
  IGenericParameterizedFeatureDesign gfd=(IGenericParameterizedFeatureDesign)getFeatureDesign(feature);
  if (gfd != null) {
    try {
      SNode expr=gfd.getterExpression(srcExpr,param,generator);
      return expr;
    }
 catch (    Throwable t) {
      t.printStackTrace();
    }
  }
  return null;
}
