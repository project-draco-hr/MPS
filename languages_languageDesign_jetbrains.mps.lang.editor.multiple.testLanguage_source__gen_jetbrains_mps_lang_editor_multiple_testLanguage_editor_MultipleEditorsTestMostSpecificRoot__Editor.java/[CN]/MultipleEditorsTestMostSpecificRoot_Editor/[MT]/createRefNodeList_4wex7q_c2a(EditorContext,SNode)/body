  style.set(StyleAttributes.INDENT_LAYOUT_INDENT,true);
  style.set(StyleAttributes.INDENT_LAYOUT_NEW_LINE,true);
  editorCell.getStyle().putAll(style);
  editorCell.setRole(handler.getElementRole());
