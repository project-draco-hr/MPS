{
  if (stype == null) {
    return false;
  }
  try {
    return TypeChecker.getInstance().getSubtypingManager().isSubtype(type,stype,false);
  }
 catch (  Throwable thr) {
    if (LOG.isEnabledFor(Priority.ERROR)) {
      LOG.error("Error executing isStrongSubtype(): ",thr);
    }
  }
  return false;
}
