{
  try {
    DefaultSModelDescriptor model=(DefaultSModelDescriptor)myModel;
    ModelPersistence.saveModel(myModelBackup,((StreamDataSource)myModel.getSource()),model.getPersistenceVersion());
    refreshVfs();
  }
 catch (  IOException e) {
    e.printStackTrace();
    Assert.fail();
  }
  myModule.updateModelsSet();
  myModel=getModel();
}
