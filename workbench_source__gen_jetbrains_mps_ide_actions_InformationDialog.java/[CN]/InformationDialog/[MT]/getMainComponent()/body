{
  ModelAccess.instance().runReadAction(new Runnable(){
    public void run(){
      collectInformation(getRefactoringContext().getStructureModificationData().getConceptFeatures());
    }
  }
);
  JPanel mainPanel=new JPanel(new BorderLayout());
