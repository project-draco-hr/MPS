{
  InputStream in=null;
  try {
    in=file.openInputStream();
    InputSource source=new InputSource(new InputStreamReader(in,"UTF-8"));
    loadDescriptor(result,source);
  }
 catch (  UnsupportedEncodingException e) {
    LOG.error(e);
  }
catch (  IOException e) {
    LOG.error(e);
  }
 finally {
    if (in != null) {
      try {
        in.close();
      }
 catch (      IOException e) {
        LOG.error(e);
      }
    }
  }
}
