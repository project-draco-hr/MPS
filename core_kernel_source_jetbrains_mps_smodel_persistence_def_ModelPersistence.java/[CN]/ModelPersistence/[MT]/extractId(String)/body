{
  if (tag == null) {
    return null;
  }
  String idString=" id=\"";
  int index=tag.lastIndexOf(idString);
  if (index >= 0) {
    int offset=index + idString.length();
    index=offset;
    while (index < tag.length() && Character.isDigit(tag.codePointAt(index))) {
      index++;
    }
    if (index < tag.length() && tag.charAt(index) == '"') {
      return tag.substring(offset,index);
    }
  }
  return null;
}
