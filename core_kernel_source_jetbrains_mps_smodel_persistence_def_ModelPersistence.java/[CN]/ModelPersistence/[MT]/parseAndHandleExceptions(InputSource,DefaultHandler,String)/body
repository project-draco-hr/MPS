{
  try {
    JDOMUtil.createSAXParser().parse(source,handler);
  }
 catch (  BreakParseSAXException e) {
  }
catch (  ParserConfigurationException e) {
    LOG.error(e);
    throw new ModelReadException("Couldn't read " + what + ": "+ e.getMessage(),e);
  }
catch (  SAXException e) {
    throw new ModelReadException("Couldn't read " + what + ": "+ e.getMessage(),e);
  }
catch (  IOException e) {
    throw new ModelReadException("Couldn't read " + what + ": "+ e.getMessage(),e);
  }
}
