{
  IThread thread=getThread();
  if (thread == null) {
    return null;
  }
  IStackFrame frame=null;
  if (thread.getFramesCount() > 0) {
    frame=myAbstractDebugSession.getDebuggableFramesSelector().findDeepestDebuggableFrame(thread.getFrames());
  }
 else {
    frame=null;
  }
  return frame;
}
