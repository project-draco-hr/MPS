{
  if (!myInitialized) {
    initCache();
  }
  List<SNode> result=new LinkedList<SNode>();
  WeakHashMap<ConceptDeclaration,List<WeakReference<SNode>>> map=myNodesNoInheritance;
  if (includeInherited) {
    map=myNodesAll;
  }
  if (map.containsKey(concept)) {
    for (    WeakReference<SNode> n : map.get(concept)) {
      SNode node=n.get();
      if (node != null) {
        result.add(node);
      }
    }
  }
  return result;
}
