{
  List<SNode> result=new ArrayList<SNode>();
  ConceptDeclaration concept=SModelUtil.findConceptDeclaration(cls,GlobalScope.getInstance());
  if (myNodes.containsKey(concept)) {
    for (    WeakReference<SNode> n : getListFor(concept)) {
      SNode node=n.get();
      if (node != null) {
        result.add(node);
      }
    }
  }
  return result;
}
