{
  ModelAccess.instance().runUndoTransparentCommand(new Runnable(){
    public void run(){
      try {
        for (        PsiListener l : myListeners) {
          l.psiChanged(myCollectedData);
        }
      }
  finally {
        myCollectedData=new PsiChangeData();
        myTimerTask=null;
      }
    }
  }
,myMPSProject);
}
