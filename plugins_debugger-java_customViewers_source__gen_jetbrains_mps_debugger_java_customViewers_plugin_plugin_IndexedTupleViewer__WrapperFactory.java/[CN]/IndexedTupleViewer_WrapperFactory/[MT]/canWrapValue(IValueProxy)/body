{
  return EvaluationUtils.consumeEvaluationException(new EvaluationUtils.EvaluationInvocatable<Boolean>(){
    public Boolean invoke() throws EvaluationException {
      Value value=proxy.getJDIValue();
      if (value == null) {
        return false;
      }
      if (!(EvaluationUtils.getInstance().instanceOf(value.type(),"Ljetbrains/mps/baseLanguage/tuples/runtime/MultiTuple;",value.virtualMachine()))) {
        return false;
      }
      return true;
    }
  }
,false);
}
