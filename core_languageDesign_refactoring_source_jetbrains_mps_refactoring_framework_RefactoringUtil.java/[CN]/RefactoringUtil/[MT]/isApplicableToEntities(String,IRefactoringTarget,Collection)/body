{
  for (  Object entity : entities) {
    boolean applicable;
    try {
      applicable=target.isApplicable(entity);
    }
 catch (    Throwable t) {
      LOG.error("An error occured while executing " + refactoringName + ".isApplicable(). This refactoring will not be available.",t);
      applicable=false;
    }
    if (!applicable) {
      return false;
    }
  }
  return true;
}
