{
  final Entry<K,V> top=_top;
  if (top != e) {
    final Entry<K,V> prev=(Entry<K,V>)e.getPrevious();
    final Entry<K,V> next=(Entry<K,V>)e.getNext();
    prev.setNext(next);
    if (next != null) {
      next.setPrevious(prev);
    }
 else {
      _back=prev;
    }
    top.setPrevious(e);
    e.setNext(top);
    e.setPrevious(null);
    _top=e;
  }
}
