{
  final Entry<K,V> prev=(Entry<K,V>)e.getPrevious();
  final Entry<K,V> next=(Entry<K,V>)e.getNext();
  if (prev != null) {
    prev.setNext(next);
  }
  if (next != null) {
    next.setPrevious(prev);
  }
  if (_back == e) {
    _back=(Entry<K,V>)e.getPrevious();
  }
  if (_top == e) {
    _top=(Entry<K,V>)e.getNext();
  }
}
