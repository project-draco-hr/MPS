{
  SNode coerceStatement=SNodeOperations.getAncestor(SLinkOperations.getTarget(_parameterObject.getNode(),"patternVarDecl",false),"jetbrains.mps.bootstrap.helgins.structure.CoerceStatement",false,false);
  if (coerceStatement != null) {
