{
  try {
    Set<EditorMessage> messages=myChecker.createMessagesProtected(editorComponent.getEditedNode(),myEvents,incremental,editorComponent.getEditorContext(),cancellable,applyQuickFixes);
    boolean changed=myChecker.areMessagesChangedProtected();
    return new Completed(changed,messages);
  }
 catch (  IndexNotReadyException e) {
    myChecker.clearProtected(editorComponent.getEditedNode(),editorComponent);
    throw e;
  }
}
