{
  super("DebugStepsMenu",ID);
  this.setIsInternal(false);
  this.setPopup(false);
  try {
    DebugStepsMenu_ActionGroup.this.addAction("jetbrains.mps.debugger.api.ui.actions.StepOver_Action");
    DebugStepsMenu_ActionGroup.this.addAction("jetbrains.mps.debugger.api.ui.actions.StepInto_Action");
    DebugStepsMenu_ActionGroup.this.addAction("jetbrains.mps.debugger.api.ui.actions.StepOut_Action");
  }
 catch (  Throwable t) {
    LOG.error("User group error",t);
  }
}
