{
  byte[] code=readClass(file);
  if (code == null) {
    return;
  }
  ASMClass ac=new ASMClass(new ClassReader(code));
  SNode res=new ClassifierUpdater(ac,mySkipPrivate,myReferenceFactory).create(getClassName(file));
  if (res != null) {
    adder.invoke(res);
    updateInnerClassifiers(file,ac,res);
  }
}
