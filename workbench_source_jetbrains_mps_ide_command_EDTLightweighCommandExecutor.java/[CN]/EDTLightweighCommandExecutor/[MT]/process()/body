{
synchronized (myLock) {
    if (myToExecute.isEmpty()) {
      return;
    }
  }
  SwingUtilities.invokeLater(new Runnable(){
    public void run(){
      CommandProcessor.instance().tryToExecuteLightweightCommand(new Runnable(){
        public void run(){
          for (          Runnable r : getToExecute()) {
            try {
              r.run();
            }
 catch (            Throwable t) {
              LOG.error(t);
            }
          }
        }
      }
);
    }
  }
);
}
