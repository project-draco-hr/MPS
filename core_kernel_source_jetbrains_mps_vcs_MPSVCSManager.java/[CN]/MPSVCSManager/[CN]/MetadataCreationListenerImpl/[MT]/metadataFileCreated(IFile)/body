{
  if (ifile != null) {
    VirtualFile vfile=VFileSystem.getFile(ifile);
    if (vfile != null) {
      AbstractVcs vcs=null;
      try {
        vcs=ProjectLevelVcsManager.getInstance(myProject).getVcsFor(vfile);
      }
 catch (      Throwable t) {
        LOG.error(t);
      }
      if (vcs != null) {
        addInternal(Collections.singletonList(vfile));
      }
    }
  }
}
