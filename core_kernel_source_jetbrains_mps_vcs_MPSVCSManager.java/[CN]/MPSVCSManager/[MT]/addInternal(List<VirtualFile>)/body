{
  boolean result=true;
  invokeLater(new Runnable(){
    public void run(){
      ApplicationManager.getApplication().runWriteAction(new Runnable(){
        public void run(){
          for (          VirtualFile vf : inVCS) {
            if (vf == null) {
              continue;
            }
            sceduleUnversionedFileForAdditionInternal(vf);
          }
        }
      }
);
    }
  }
);
  return result;
}
