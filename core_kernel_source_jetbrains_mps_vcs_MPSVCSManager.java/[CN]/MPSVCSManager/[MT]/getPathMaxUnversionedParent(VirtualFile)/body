{
  List<VirtualFile> path=new LinkedList<VirtualFile>();
  path.add(vf);
  while (true) {
    VirtualFile parent=path.get(0).getParent();
    if (parent == null) {
      return Collections.EMPTY_LIST;
    }
    if (isUnderVCS(myProject,parent)) {
      break;
    }
 else {
      path.add(0,parent);
    }
  }
  return path;
}
