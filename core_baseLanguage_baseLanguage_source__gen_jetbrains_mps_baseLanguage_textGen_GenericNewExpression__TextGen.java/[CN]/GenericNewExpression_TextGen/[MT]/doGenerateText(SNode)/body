{
  this.append("new ");
  if ((SLinkOperations.getTarget(node,"creator",true) == null)) {
    this.foundError();
    this.append("???");
  }
 else {
    TextGenManager.instance().appendNodeText(this.getContext(),this.getBuffer(),SLinkOperations.getTarget(node,"creator",true),this.getSNode());
  }
}
