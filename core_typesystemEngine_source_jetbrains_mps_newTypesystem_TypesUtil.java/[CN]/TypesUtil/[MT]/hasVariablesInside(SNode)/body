{
  if (TypesUtil.isVariable(node)) {
    return true;
  }
  for (  SNode child : node.getChildren()) {
    if (TypesUtil.hasVariablesInside(child)) {
      return true;
    }
  }
  for (  SNode referent : getNodeReferents(node)) {
    if (referent != null && TypesUtil.isVariable(referent)) {
      return true;
    }
  }
  return false;
}
