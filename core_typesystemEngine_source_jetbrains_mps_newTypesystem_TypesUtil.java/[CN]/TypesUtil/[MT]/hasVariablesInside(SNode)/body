{
  if (isVariable(node)) {
    return true;
  }
  for (  SNode child : node.getChildren()) {
    if (hasVariablesInside(child)) {
      return true;
    }
  }
  for (  SNode referent : getNodeReferents(node)) {
    if (referent != null && HUtil.isRuntimeTypeVariable(referent)) {
      return true;
    }
  }
  return false;
}
