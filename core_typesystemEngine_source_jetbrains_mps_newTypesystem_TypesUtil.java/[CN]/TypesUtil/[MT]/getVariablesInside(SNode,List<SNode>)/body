{
  if (node == null) {
    return;
  }
  if (isVariable(node)) {
    result.add(node);
    return;
  }
  for (  SNode child : node.getChildren()) {
    getVariablesInside(child,result);
  }
  for (  SNode referent : node.getReferents()) {
    if (referent != null && isVariable(referent)) {
      result.add(referent);
    }
  }
}
