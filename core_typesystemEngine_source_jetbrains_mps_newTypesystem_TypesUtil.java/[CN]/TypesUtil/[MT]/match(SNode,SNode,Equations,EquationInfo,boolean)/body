{
  if (left == right) {
    return true;
  }
  if (left == null || right == null) {
    return false;
  }
  boolean result=MatchingUtil.matchNodes(left,right,typeMatchModifier,false);
  if (!checkOnly && result) {
    if (equations != null) {
      equations.addEquations(typeMatchModifier.getChildEqs(),info);
    }
  }
  return result;
}
