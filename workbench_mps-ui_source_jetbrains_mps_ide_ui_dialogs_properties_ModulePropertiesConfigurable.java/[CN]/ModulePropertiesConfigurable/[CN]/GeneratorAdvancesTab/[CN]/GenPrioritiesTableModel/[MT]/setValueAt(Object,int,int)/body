{
  MappingPriorityRule rule=myMappingPriorityRules.get(rowIndex);
  if (columnIndex == 0 && aValue instanceof MappingConfig_AbstractRef)   rule.setLeft((MappingConfig_AbstractRef)aValue);
  if (columnIndex == 1 && aValue instanceof RuleType)   rule.setType((RuleType)aValue);
  if (columnIndex == 2 && aValue instanceof MappingConfig_AbstractRef)   rule.setRight((MappingConfig_AbstractRef)aValue);
}
