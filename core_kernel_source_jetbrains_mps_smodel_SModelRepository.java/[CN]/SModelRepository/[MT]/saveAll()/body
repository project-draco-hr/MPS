{
  LOG.assertInCommand();
  ModelAccess.assertLegalWrite();
  for (  SModelDescriptor modelDescriptor : myModelDescriptors) {
    if (modelDescriptor.isChanged()) {
      try {
        modelDescriptor.save();
        modelDescriptor.setChanged(false);
      }
 catch (      Throwable t) {
        LOG.error(t);
      }
    }
  }
}
