{
  checkNotRegistered();
  pluginId=memento.get("pluginId");
  myModules.clear();
  for (  Memento mm : memento.getChildren("module")) {
    String ref=mm.get("ref");
    if (ref == null) {
      continue;
    }
    myModules.add(jetbrains.mps.project.structure.modules.ModuleReference.fromString(ref));
  }
}
