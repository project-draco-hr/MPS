{
  List<Integer> nums=null;
  Iterable<Integer> evenNums=ListSequence.<Integer>fromList(nums).where(new IWhereFilter<Integer>(){
    public boolean accept(    Integer n){
      return n % 2 == 0;
    }
  }
);
  Iterable<String> strings=Sequence.<Integer>fromIterable(evenNums).<String>select(new ISelector<Integer,String>(){
    public String select(    Integer it){
      return "" + it;
    }
  }
);
  Assert.assertEquals(0,Sequence.<String>fromIterable(strings).count());
}
