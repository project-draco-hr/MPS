{
  CollectionUtil.map(CollectionUtil.filter(((EditorCell_Collection)context.getNodeEditorComponent().getRootCell()).dfsCells(),new Condition<EditorCell>(){
    public boolean met(    EditorCell object){
      return object.isFolded();
    }
  }
),new Mapper<EditorCell,Void>(){
    public Void map(    EditorCell editorCell){
      ((EditorCell_Collection)editorCell).unfold();
      return null;
    }
  }
);
}
