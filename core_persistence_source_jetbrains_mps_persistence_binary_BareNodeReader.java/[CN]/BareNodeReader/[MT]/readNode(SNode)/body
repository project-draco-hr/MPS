{
  SNode node=instantiate(parent);
  if (myIn.readByte() != '{') {
    throw new IOException("bad stream, no '{'");
  }
  readProperties(node);
  readUserObjects(node);
  readReferences(node);
  readChildren(node);
  if (myIn.readByte() != '}') {
    throw new IOException("bad stream, no '}'");
  }
  return node;
}
