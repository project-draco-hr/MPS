{
  final Project project=startTestOnProjectCopy(source,destinationDir,projectName,plugins);
  if (project == null) {
    return false;
  }
  try {
    return pr.execute(project);
  }
 catch (  Throwable t) {
    t.printStackTrace();
    return false;
  }
 finally {
    finishTestOnProjectCopy(project,destinationDir);
  }
}
