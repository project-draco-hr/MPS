{
  myCell=cell;
  myCells=getIndentLeafs(myCell);
  myPosition=0;
  myX=myCell.getX();
  myWidth=0;
  myHeight=0;
  myLineWidth=0;
  myLineAscent=0;
  myLineDescent=0;
  myTopInset=0;
  myBottomInset=0;
  EditorSettings settings=EditorSettings.getInstance();
  myMaxWidth=cell.getRootParent().getX() + settings.getVerticalBoundWith();
}
