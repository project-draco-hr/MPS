{
  List<SNode> result=new ArrayList<SNode>();
  for (  QueryScope scope : myScopes) {
    SNode r=scope.resolve(anchor,str);
    if (r != null) {
      ListSequence.fromList(result).addElement(r);
    }
  }
  return (ListSequence.fromList(result).count() == 1 ? ListSequence.fromList(result).first() : null);
}
