{
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < s.length(); i++) {
    char c=s.charAt(i);
    if (Character.isDigit(c) && sb.length() > 0) {
      sb.append(c);
    }
 else     if (Character.isJavaIdentifierStart(c) && c != '$') {
      sb.append(c);
    }
 else {
      sb.append('_');
    }
  }
  return sb.toString();
}
