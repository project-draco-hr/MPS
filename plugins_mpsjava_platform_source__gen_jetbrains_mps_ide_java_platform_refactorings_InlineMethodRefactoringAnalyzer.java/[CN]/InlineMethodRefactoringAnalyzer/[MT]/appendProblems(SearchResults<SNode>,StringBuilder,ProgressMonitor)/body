{
  List<SNode> myOverriding=MethodRefactoringUtils.findOverridingMethods(myMethod,progressMonitor);
  errors.append(getOverridingErrors(myOverriding));
  if (ussages != null) {
    for (    SearchResult<SNode> res : ListSequence.fromList(ussages.getSearchResults())) {
      appendRefactoringProblems(res.getObject(),errors);
    }
  }
 else {
    appendRefactoringProblems(myCall,errors);
  }
}
