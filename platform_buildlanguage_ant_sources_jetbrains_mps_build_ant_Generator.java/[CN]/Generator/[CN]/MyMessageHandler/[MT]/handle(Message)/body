{
switch (msg.getKind()) {
case ERROR:
    System.out.println("ERROR: " + msg.getText());
  myErrors.add(msg.getText());
break;
case WARNING:
System.out.println("WARN:  " + msg.getText());
myWarnings.add(msg.getText());
break;
case INFORMATION:
System.out.println("INFO:  " + msg.getText());
break;
}
}
