{
  StringBuffer s=new StringBuffer("Generating models:");
  for (  SModelDescriptor m : models) {
    s.append("\n    ");
    s.append(m);
  }
  info(s.toString());
  GeneratorManager gm=project.getComponentSafe(GeneratorManager.class);
  gm.generateModels(models,new ProjectOperationContext(project),myGenerationType,new EmptyProgressIndicator(),myMessageHandler);
}
