{
  if (index != 0) {
    if (myDeltaOld != 0 || myDeltaNew != 0) {
      manager.addChangeItem(myDeltaOld,myDeltaNew);
      myDeltaOld=0;
      myDeltaNew=0;
    }
    manager.dec(index);
  }
  myOld-=index;
  myNew-=index;
  if (afterVertical) {
    myNew--;
    myDeltaNew++;
  }
 else {
    myOld--;
    myDeltaOld++;
  }
}
