{
  if (ListSequence.fromList(SLinkOperations.getTargets(node,"conceptLink",true)).isNotEmpty() || ListSequence.fromList(SLinkOperations.getTargets(node,"conceptProperty",true)).isNotEmpty()) {
    return true;
  }
  Iterable<SNode> nodes=(Iterable<SNode>)new ConceptAndSuperConceptsScope(node).getNodes(new Condition<SNode>(){
    public boolean met(    SNode n){
      return SNodeOperations.isInstanceOf(n,"jetbrains.mps.lang.structure.structure.ConceptLinkDeclaration") || SNodeOperations.isInstanceOf(n,"jetbrains.mps.lang.structure.structure.ConceptPropertyDeclaration");
    }
  }
);
  return Sequence.fromIterable(nodes).any(new IWhereFilter<SNode>(){
    public boolean accept(    SNode it){
      SNode n=SNodeOperations.as(it,"jetbrains.mps.lang.core.structure.IDeprecatable");
      return n == null || !(BehaviorReflection.invokeVirtual(Boolean.TYPE,n,"virtual_isDeprecated_1224609060727",new Object[]{}));
    }
  }
);
}
