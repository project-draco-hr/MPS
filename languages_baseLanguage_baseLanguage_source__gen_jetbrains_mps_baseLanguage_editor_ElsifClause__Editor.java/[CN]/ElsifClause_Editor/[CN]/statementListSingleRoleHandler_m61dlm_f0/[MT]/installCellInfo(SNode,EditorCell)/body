{
  editorCell.setSubstituteInfo(new OldNewCompositeSubstituteInfo(myEditorContext,new SChildSubstituteInfo(editorCell,myOwnerNode,MetaAdapterFactory.getContainmentLink(0xf3061a5392264cc5L,0xa443f952ceaf5816L,0x118ceceb41aL,0x118ced0f8fdL,"statementList"),child),new DefaultChildSubstituteInfo(myOwnerNode,myContainmentLink.getDeclarationNode(),myEditorContext)));
  if (editorCell.getRole() == null) {
    editorCell.setRole("statementList");
  }
  Style style=new StyleImpl();
  SNode node=myOwnerNode;
  EditorContext editorContext=myEditorContext;
  style.set(StyleAttributes.INDENT_LAYOUT_INDENT,0,true);
  style.set(StyleAttributes.INDENT_LAYOUT_NEW_LINE,0,true);
  editorCell.getStyle().putAll(style);
}
