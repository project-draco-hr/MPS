{
  Direction2D dir=getStartDirection(layout,node,edge);
  boolean wasSplitted=false;
  for (  Edge sourceEdge : ListSequence.<Edge>fromList(node.getEdges())) {
    if (sourceEdge != edge && getStartDirection(layout,node,sourceEdge) == dir) {
      wasSplitted=true;
    }
  }
  return wasSplitted;
}
