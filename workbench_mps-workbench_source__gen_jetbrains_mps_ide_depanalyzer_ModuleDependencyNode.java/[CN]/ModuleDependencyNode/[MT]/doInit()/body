{
  for (  DepPath c : Sequence.fromIterable(myCycles)) {
    Iterator<DepLink> itr=Sequence.fromIterable(c.elements()).iterator();
    itr.next();
    MPSTreeNode parent=this;
    while (itr.hasNext()) {
      DependencyTreeNode dtn=new DependencyTreeNode(itr.next());
      parent.add(dtn);
      parent=dtn;
    }
  }
  myInitialized=true;
}
