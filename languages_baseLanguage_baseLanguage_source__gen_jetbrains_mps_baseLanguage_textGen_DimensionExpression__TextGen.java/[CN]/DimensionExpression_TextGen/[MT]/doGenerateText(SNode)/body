{
  if ((SLinkOperations.getTarget(node,"expression",true) != null)) {
    this.append("[");
    TextGenManager.instance().appendNodeText(this.getContext(),this.getBuffer(),SLinkOperations.getTarget(node,"expression",true),this.getSNode());
    this.append("]");
  }
 else {
    this.append("[]");
  }
}
