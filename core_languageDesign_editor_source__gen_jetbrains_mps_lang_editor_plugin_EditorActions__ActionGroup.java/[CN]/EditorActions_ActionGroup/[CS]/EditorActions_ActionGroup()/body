{
  super("",ID);
  this.setIsInternal(false);
  this.setPopup(false);
  try {
{
      IModule language=MPSModuleRepository.getInstance().getModule(new ModuleReference("jetbrains.mps.lang.editor"));
      this.add(ActionFactory.getInstance().acquireRegisteredAction(language.getClass("jetbrains.mps.lang.editor.plugin.CopyThisDown_Action"),language.getModuleFqName(),null));
    }
  }
 catch (  Throwable t) {
    LOG.error("User group error",t);
  }
}
