{
  if (!(editorContext.getSelectedCell() instanceof EditorCell_Label)) {
    return false;
  }
  if (!(SNodeOperations.isInstanceOf(node,MetaAdapterFactory.getConcept(new UUID(5160134014823646133l,-7982110198386724200l),6666499814681541916l,"jetbrains.mps.core.xml.structure.XmlValuePart"))) || !(SNodeOperations.isInstanceOf(SNodeOperations.getParent(node),MetaAdapterFactory.getConcept(new UUID(5160134014823646133l,-7982110198386724200l),6666499814681447923l,"jetbrains.mps.core.xml.structure.XmlAttribute")))) {
    return false;
  }
  return isNotEmptyString(SPropertyOperations.getString(node,MetaAdapterFactory.getProperty(new UUID(5160134014823646133l,-7982110198386724200l),6666499814681541919l,6666499814681541920l,"text")));
}
