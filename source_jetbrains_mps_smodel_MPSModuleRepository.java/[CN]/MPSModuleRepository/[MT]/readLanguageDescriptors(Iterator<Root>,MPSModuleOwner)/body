{
  while (roots.hasNext()) {
    Root root=roots.next();
    File moduleRoot=new File(root.getPath());
    if (moduleRoot.exists()) {
      readLanguageDescriptors(moduleRoot,owner);
    }
 else {
      String error="Couldn't load languages from " + moduleRoot.getAbsolutePath() + "\nDirectory doesn't exist: ";
      LOG.error(error);
    }
  }
}
