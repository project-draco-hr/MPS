{
  if (prefix == null) {
    prefix="";
  }
  if (suffix == null) {
    suffix="";
  }
  int preparedNameStart=((StringUtils.isNotEmpty(prefix) && fullName.startsWith(prefix)) ? prefix.length() : 0);
  int preparedNameEnd=((StringUtils.isNotEmpty(suffix) && fullName.endsWith(suffix)) ? fullName.indexOf(suffix) : fullName.length());
  if (preparedNameStart >= preparedNameEnd) {
    return fullName;
  }
  return fullName.substring(preparedNameStart,preparedNameEnd);
}
