{
  if (value == null) {
    return null;
  }
  Iterator<FocusPolicy> constants=FocusPolicy.getConstants().iterator();
  while (constants.hasNext()) {
    FocusPolicy constant=constants.next();
    if (value.equals(constant.getName())) {
      return constant.getValueAsString();
    }
  }
  return null;
}
