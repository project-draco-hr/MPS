{
  NodeReadAccessCaster.runReadTransparentAction(new Runnable(){
    public void run(){
      myLinkDeclaration=link;
      if (myLinkDeclaration != null) {
        myIsReferenceCell=link.getMetaClass() != LinkMetaclass.aggregation;
      }
 else {
        myIsReferenceCell=false;
      }
    }
  }
);
}
