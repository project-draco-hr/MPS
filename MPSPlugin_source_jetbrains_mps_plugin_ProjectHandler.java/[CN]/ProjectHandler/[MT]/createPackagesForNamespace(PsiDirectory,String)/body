{
  PsiDirectory current=dir;
  try {
    String[] elements=namespace.split("\\.");
    for (    String el : elements) {
      PsiDirectory next=current.findSubdirectory(el);
      if (next == null) {
        next=current.createSubdirectory(el);
      }
      current=next;
    }
  }
 catch (  IncorrectOperationException e) {
    e.printStackTrace();
  }
  return current;
}
