{
  executeWriteAction(new Runnable(){
    public void run(){
      final JavaPsiFacade javaPsi=JavaPsiFacade.getInstance(myProject);
      PsiClass cls=javaPsi.findClass(fqName,GlobalSearchScope.allScope(myProject));
      if (cls == null)       return;
      cls.navigate(true);
      activateProjectWindow();
    }
  }
);
}
