{
  try {
    new WriteCommandAction(myProject){
      protected void run(      Result result) throws Throwable {
        doRenameClass(packageName(oldClassFQName) + ".structure." + shortName(oldClassFQName),newClassName);
        doRenameClass(packageName(oldClassFQName) + ".editor." + shortName(oldClassFQName)+ "_Editor",newClassName + "_Editor");
      }
    }
.execute();
    buildModule(sourceLangSourcePath);
  }
 catch (  Throwable e) {
    e.printStackTrace();
  }
}
