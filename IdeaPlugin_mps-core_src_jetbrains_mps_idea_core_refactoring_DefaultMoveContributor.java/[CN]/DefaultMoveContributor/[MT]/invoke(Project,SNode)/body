{
  final MPSProject mpsProject=project.getComponent(MPSProject.class);
  final List<SNode> targets=Arrays.asList(new SNode[]{node});
  MoveNodesExecute.execute(mpsProject,targets,new ExecuteRefactoring(){
    @Override public void run(    Object newLocation){
      RefactoringAccess.getInstance().getRefactoringFacade().execute(RefactoringContext.createRefactoringContext(new DefaultMoveRefactoring(),Arrays.asList("target"),Arrays.asList(newLocation),targets,mpsProject));
    }
  }
);
}
