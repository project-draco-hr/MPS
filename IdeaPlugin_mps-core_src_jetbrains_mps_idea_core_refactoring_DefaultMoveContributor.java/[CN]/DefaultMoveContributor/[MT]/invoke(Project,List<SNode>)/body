{
  final MPSProject mpsProject=project.getComponent(MPSProject.class);
  MoveNodesExecute.execute(mpsProject,nodes,new ExecuteRefactoring(){
    @Override public void run(    Object newLocation){
      RefactoringAccessEx.getInstance().getRefactoringFacade().execute(RefactoringContext.createRefactoringContext(new DefaultMoveRefactoring(nodes,newLocation),Arrays.asList("target"),Arrays.asList(newLocation),nodes,mpsProject));
    }
  }
);
}
