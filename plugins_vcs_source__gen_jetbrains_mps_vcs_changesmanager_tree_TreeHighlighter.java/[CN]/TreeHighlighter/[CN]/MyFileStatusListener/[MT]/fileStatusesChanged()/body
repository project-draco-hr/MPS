{
  ModelAccess.instance().runReadAction(new Runnable(){
    public void run(){
synchronized (myFeatureToNodes) {
        for (        Feature f : SetSequence.fromSet(myFeatureToNodes.keySet())) {
          if (f instanceof ModelFeature) {
            rehighlightFeatureAndDescendants(f);
          }
        }
      }
    }
  }
);
}
