{
  SuspendContext newContext=previousState.myContext;
  if (newContext == null) {
    return null;
  }
  JavaThread thread=getThread();
  if (thread == null) {
    return null;
  }
  if (!(newContext.suspends(thread.getThread()))) {
    newContext=null;
    for (    SuspendContext context : getAllPausedContexts()) {
      if (context.suspends(thread.getThread())) {
        newContext=context;
        break;
      }
    }
  }
  return newContext;
}
