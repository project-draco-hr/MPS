{
  if (moduleName == null) {
    return Sequence.fromIterable(Collections.<SNode>emptyList());
  }
  IModule module=MPSModuleRepository.getInstance().getModuleByUID(moduleName);
  if (module == null) {
    return Sequence.fromIterable(Collections.<SNode>emptyList());
  }
  return excludeAbstract(TestRunUtil.getModuleTests(module));
}
