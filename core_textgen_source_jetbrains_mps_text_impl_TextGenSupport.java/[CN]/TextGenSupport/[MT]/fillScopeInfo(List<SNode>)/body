{
  final TraceInfoCollector tic=getTraceInfoCollector();
  if (tic == null) {
    return;
  }
  TextMark m=myContext.getBuffer().popMark();
  final ScopePositionInfo pi=tic.createScopePosition(m,myContext.getPrimaryInput());
  for (  SNode var : vars) {
    if (var != null) {
      pi.addVarInfo(var);
    }
  }
}
