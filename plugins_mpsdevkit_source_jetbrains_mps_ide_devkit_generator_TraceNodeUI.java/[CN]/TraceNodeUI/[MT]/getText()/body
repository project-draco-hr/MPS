{
  if (myText != null) {
    return myText;
  }
  if (myTargetNode == null) {
    return getName();
  }
  if (myKind == Kind.APPROXIMATE_OUTPUT || myKind == Kind.APPROXIMATE_INPUT) {
    return "[approximate location] " + myTargetNode.toString();
  }
 else {
    return myTargetNode.toString();
  }
}
