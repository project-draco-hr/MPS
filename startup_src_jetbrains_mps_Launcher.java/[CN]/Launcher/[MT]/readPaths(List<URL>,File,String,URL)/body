{
  try {
    Scanner sc;
    for (sc=new Scanner(paths,"UTF-8"); sc.hasNextLine(); ) {
      File dir=new File(homePath,sc.nextLine());
      if (dir.isDirectory()) {
        final URL url=dir.toURI().toURL();
        if (!selfRootUrl.equals(url)) {
          classPath.add(url);
        }
      }
    }
    sc.close();
  }
 catch (  FileNotFoundException ignore) {
  }
}
