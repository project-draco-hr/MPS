{
  getEnvironment().createEmptyProject();
  Project mpsProject=getEnvironment().openProject(new File(PathManager.getHomePath()));
  testWait();
  mpsProject.dispose();
  testWait();
  getEnvironment().createEmptyProject();
  mpsProject=getEnvironment().openProject(new File(PathManager.getHomePath()));
  testWait();
  mpsProject.dispose();
  testWait();
}
