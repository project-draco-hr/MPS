{
  try {
    final Method method=ExecuteCalculator_Action.this.getMainMethod(_params);
    Thread thread=new Thread(new Runnable(){
      @Override public void run(){
        try {
          String[] args=new String[0];
          method.invoke(null,new Object[]{args});
        }
 catch (        Throwable e) {
          e.printStackTrace();
        }
      }
    }
);
    thread.start();
  }
 catch (  Throwable e) {
    e.printStackTrace();
  }
}
