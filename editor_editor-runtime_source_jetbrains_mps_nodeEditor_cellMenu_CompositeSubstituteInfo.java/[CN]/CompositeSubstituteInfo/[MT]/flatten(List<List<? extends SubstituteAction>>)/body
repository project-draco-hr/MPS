{
  int size=0;
  for (  List<? extends SubstituteAction> actionList : actionLists) {
    size+=actionList.size();
  }
  List<SubstituteAction> result=new ArrayList<SubstituteAction>(size);
  for (  List<? extends SubstituteAction> actionList : actionLists) {
    result.addAll(actionList);
  }
  return result;
}
