{
  int size=0;
  for (  List<INodeSubstituteAction> actionList : actionLists) {
    size+=actionList.size();
  }
  List<INodeSubstituteAction> result=new ArrayList<INodeSubstituteAction>(size);
  for (  List<INodeSubstituteAction> actionList : actionLists) {
    result.addAll(actionList);
  }
  return result;
}
