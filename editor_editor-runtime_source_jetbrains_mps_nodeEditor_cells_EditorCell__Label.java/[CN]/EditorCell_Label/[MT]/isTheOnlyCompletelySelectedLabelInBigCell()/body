{
  jetbrains.mps.openapi.editor.cells.EditorCell containingBigCell=CellTraversalUtil.getContainingBigCell(this);
  return containingBigCell != null && CellTraversalUtil.getFirstLeaf(containingBigCell) == this && CellTraversalUtil.getLastLeaf(containingBigCell) == this && getText().equals(getSelectedText());
}
