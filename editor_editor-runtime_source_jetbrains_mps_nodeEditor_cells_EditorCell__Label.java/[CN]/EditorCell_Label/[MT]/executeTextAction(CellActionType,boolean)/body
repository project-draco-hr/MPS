{
switch (type) {
case DELETE:
case BACKSPACE:
    break;
default :
  return false;
}
myCaretIsVisible=true;
if (!isEditable()) {
return false;
}
boolean result=getContext().executeCommand(new ProcessTextActionCommand(type,allowErrors));
if (result) {
getContext().flushEvents();
getEditor().relayout();
}
return result;
}
