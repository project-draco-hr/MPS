{
  if (!(context.getSelectedCell() instanceof EditorCell_Label))   return false;
  EditorCell_Label label=(EditorCell_Label)context.getSelectedCell();
  SNode node=label.getSNode();
  return node != null && label.canPasteText() && label.isEditable() && (label.isTheOnlyCompletelySelectedLabelInBigCell() || isFirstCaretPosition() && !getTextLine().hasNonTrivialSelection() || isLastCaretPosition() && !getTextLine().hasNonTrivialSelection() ? CopyPasteUtil.isStringOnTopOfClipboard() : TextPasteUtil.hasStringInClipboard());
}
