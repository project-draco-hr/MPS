{
  if (project == null) {
    return;
  }
  if (!(project.exists())) {
    throw new IllegalArgumentException("The project file '" + project + "' does not exist");
  }
  if (project.isDirectory()) {
    load(project,new File(project,".mps/modules.xml"));
  }
 else {
    load(project,project);
  }
}
