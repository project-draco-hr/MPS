{
  if (myTopConcept == null) {
    return Collections.emptyList();
  }
  List<SNode> result=new ArrayList<SNode>();
  for (  SNode node : getConcepts()) {
    if (node == null) {
      continue;
    }
    for (    SNode n : SNodeOperations.getDescendants(node,condition,true)) {
      result.add(n);
    }
  }
  return result;
}
