{
  if (myClassLoader == null)   return null;
  try {
    fqName=InternUtil.intern(fqName);
    try {
synchronized (LOADING_LOCK) {
        return myClassLoader.loadClass(fqName);
      }
    }
 catch (    ClassNotFoundException e) {
      return null;
    }
  }
 catch (  Throwable t) {
    LOG.error(t);
    return null;
  }
}
