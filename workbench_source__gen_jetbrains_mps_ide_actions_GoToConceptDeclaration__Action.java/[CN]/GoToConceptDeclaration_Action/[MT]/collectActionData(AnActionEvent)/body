{
  try {
    this.context=new ActionEventData(event).getOperationContext();
    if (this.context == null) {
      return false;
    }
{
      SNode node=new ActionEventData(event).getNode();
      if (node != null) {
      }
      this.node=node;
    }
    if (this.node == null) {
      return false;
    }
  }
 catch (  Throwable t) {
    return false;
  }
  return true;
}
