{
  for (  SNode mpsBuild : ListSequence.fromList(SNodeOperations.getDescendants(_context.getNode(),"jetbrains.mps.build.custommps.structure.MPSBuild",false))) {
    if (ListSequence.fromList(SNodeOperations.getDescendants(mpsBuild,"jetbrains.mps.build.custommps.structure.LibraryFolder",false)).isNotEmpty()) {
      return true;
    }
  }
  return false;
}
