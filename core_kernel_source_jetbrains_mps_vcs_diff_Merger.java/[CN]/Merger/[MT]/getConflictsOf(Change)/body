{
  Set<Change> result=new HashSet<Change>();
  if (!myConflictingChanges.contains(change)) {
    return result;
  }
  for (  Conflict c : myConflicts) {
    if (c.getC1() == change) {
      result.add(c.getC2());
      result.addAll(getChangeGroupOf(c.getC2()));
    }
    if (c.getC2() == change) {
      result.add(c.getC1());
      result.addAll(getChangeGroupOf(c.getC1()));
    }
  }
  return result;
}
