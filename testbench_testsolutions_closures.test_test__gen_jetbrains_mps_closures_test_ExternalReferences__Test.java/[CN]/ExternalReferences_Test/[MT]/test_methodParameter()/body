{
  Worker wrk=new Worker(){
    @Override public String doWork(    final Integer d){
      return new _FunctionTypes._return_P0_E0<String>(){
        public String invoke(){
          return "Done: " + d;
        }
      }
.invoke();
    }
  }
;
  Assert.assertEquals("Done: 1234",wrk.doWork(1234));
}
