{
  if (this.myNode == node) {
    return;
  }
  if (node == null) {
    if (this.myNode == null) {
      this.setText(null);
    }
  }
 else {
    this.myNode=node;
    ModelAccess.instance().runReadAction(new Runnable(){
      public void run(){
        MainNodeChooser.this.setText(INamedConcept_Behavior.call_getFqName_1213877404258(node));
      }
    }
);
  }
  this.fireNodeChanged();
}
