{
  if (SModelStereotype.JAVA_STUB.equals(sm.getStereotype())) {
    return false;
  }
  if (sm.getModelFile() == null) {
    return false;
  }
  if (Language.isAccessoryModel(sm)) {
    return false;
  }
  if (sm.getSModel().getRoots().size() == 0) {
    return false;
  }
  return sm.lastChangeTime() >= getLastGenerationTime(sm);
}
