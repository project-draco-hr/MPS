{
  loadNewStubSolutions();
  updateNotChangedStubPaths();
  disposeStubManagers();
  reloadStubModels();
  CleanupManager.getInstance().cleanup();
  MPSModuleRepository.getInstance().invalidateCaches();
  updateLoadedStubPaths();
  System.out.printf("\n\n=======RELOAD CYCLE COMPLETED===========================================\n\n");
}
