{
  if (myHashCode == null) {
    int result=0;
    if (myModelUID != null) {
      result=myModelUID.hashCode() + myNodeId.hashCode();
    }
 else     if (myNodeRef != null) {
      SNode node=myNodeRef.get();
      if (node != null) {
        result=node.getModel().getUID().hashCode() + node.getId().hashCode();
      }
    }
    myHashCode=result;
  }
  return myHashCode;
}
