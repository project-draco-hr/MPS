{
  List<IWrapper> path=new LinkedList<IWrapper>();
  int pathLength=0;
  IWrapper type=type_;
  while (getParent(type) != null) {
    path.add(type);
    pathLength++;
    type=getParent(type);
  }
  if (pathLength > 1) {
    for (    IWrapper typeOnPath : path) {
      setParent(typeOnPath,type);
    }
  }
  return type;
}
