{
  SNode module=(model != null ? SModelOperations.createNewRootNode(model,"jetbrains.mps.lang.project.structure.Module",null) : SConceptOperations.createNewNode("jetbrains.mps.lang.project.structure.Module",null));
  SPropertyOperations.set(module,"namespace",descriptor.getNamespace());
  SPropertyOperations.set(module,"uuid",descriptor.getUUID());
  SPropertyOperations.set(module,"compileInMPS","" + descriptor.getCompileInMPS());
  return module;
}
