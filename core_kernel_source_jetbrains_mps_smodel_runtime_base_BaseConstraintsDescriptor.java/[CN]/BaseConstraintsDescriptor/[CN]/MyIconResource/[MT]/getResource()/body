{
  String iconPath=MacrosFactory.forModule((AbstractModule)myNode.getConcept().getDeclarationNode().getModel().getModule()).expandPath(getAlternativeIcon(myNode));
  if (iconPath == null) {
    return null;
  }
  IFile file=FileSystem.getInstance().getFileByPath(iconPath);
  if (!file.exists()) {
    return null;
  }
  try {
    return file.openInputStream();
  }
 catch (  IOException e) {
    return null;
  }
}
