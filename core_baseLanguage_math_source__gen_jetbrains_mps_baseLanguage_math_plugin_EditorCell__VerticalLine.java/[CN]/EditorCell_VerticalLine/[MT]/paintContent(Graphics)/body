{
  if (this.isSelected()) {
    g.setColor(Color.BLACK);
    EditorCell_Collection parent=this.getParent();
    int x=parent.getX();
    int y=parent.getY();
    int width=parent.getWidth();
    int height=parent.getHeight();
    g.fillRect(x + width,y,1,height);
    g.fillRect(x,y + height,width,1);
  }
}
