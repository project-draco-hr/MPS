{
  if (language == null) {
    return null;
  }
  try {
    return (Class<T>)ClassLoaderManager.getInstance().getClass(language,className);
  }
 catch (  Throwable e) {
    LOG.debug(String.format("error loading class '%s' from module %s",className,language.getModuleName()),e);
  }
  return null;
}
