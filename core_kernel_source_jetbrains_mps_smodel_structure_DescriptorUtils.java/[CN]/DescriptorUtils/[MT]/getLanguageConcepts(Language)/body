{
  List<String> result=new ArrayList<String>();
  for (  SNode node : new NodesIterable(LanguageAspect.STRUCTURE.get(language))) {
    if ("jetbrains.mps.lang.structure.structure.ConceptDeclaration".equals(node.getConcept().getId()) || "jetbrains.mps.lang.structure.structure.InterfaceConceptDeclaration".equals(node.getConcept().getId())) {
      result.add(NameUtil.nodeFQName(node));
    }
  }
  return result;
}
