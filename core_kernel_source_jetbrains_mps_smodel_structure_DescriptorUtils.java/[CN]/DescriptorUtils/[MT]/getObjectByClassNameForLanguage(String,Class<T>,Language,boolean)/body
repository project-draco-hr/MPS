{
  try {
    if (language == null) {
      return null;
    }
    Class clazz=language.getClass(className);
    if (clazz == null) {
      return null;
    }
    Object o=clazz.newInstance();
    if (!castTo.isInstance(o)) {
      return null;
    }
    return castTo.cast(o);
  }
 catch (  Throwable e) {
    LOG.debug("error loading class\"" + className + "\"",e);
  }
  return null;
}
