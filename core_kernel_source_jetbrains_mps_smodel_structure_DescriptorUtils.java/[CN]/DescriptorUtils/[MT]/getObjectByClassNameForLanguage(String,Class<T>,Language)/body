{
  try {
    Object o=getObjectByClassNameForLanguage(className,language);
    if (o == null || !castTo.isInstance(o)) {
      return null;
    }
    return castTo.cast(o);
  }
 catch (  Throwable e) {
    LOG.debug(String.format("failed to cast class '%s' to %s",className,castTo.getName()));
  }
  return null;
}
