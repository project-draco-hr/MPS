{
  try {
    if (language == null) {
      return null;
    }
    Class clazz=language.getClass(className);
    if (clazz == null) {
      return null;
    }
    return clazz.newInstance();
  }
 catch (  Throwable e) {
    LOG.debug("error loading class\"" + className + "\"",e);
  }
  return null;
}
