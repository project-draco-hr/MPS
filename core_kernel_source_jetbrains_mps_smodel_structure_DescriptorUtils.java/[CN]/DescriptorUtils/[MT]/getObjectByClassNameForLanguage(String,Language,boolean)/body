{
  try {
    if (language == null) {
      return null;
    }
    if (avoidLogErrors) {
      ClassLoader cl=ClassLoaderManager.getInstance().getClassLoaderFor(language,false);
      if (cl == null) {
        return null;
      }
    }
    Class clazz=language.getClass(className);
    if (clazz == null) {
      return null;
    }
    return clazz.newInstance();
  }
 catch (  Throwable e) {
  }
  return null;
}
