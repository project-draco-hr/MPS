{
  Integer index=myIndexMap.get(id);
  if (index == null) {
    return null;
  }
switch (((int)index)) {
case 0:
    return myConceptSimpleConceptDeclaration;
case 1:
  return myConceptSimplePropertyDeclaration;
case 2:
return myConceptStructureAspectDeclaration;
default :
throw new IllegalStateException();
}
}
