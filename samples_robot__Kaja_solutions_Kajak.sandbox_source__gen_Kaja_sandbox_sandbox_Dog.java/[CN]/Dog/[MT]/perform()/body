{
  if (canMove()) {
    moveKaja();
    pause();
  }
 else {
    reportError("Oops, There's a wall in front of me. I can't make a step forward.");
  }
  lay_routine();
  turnAround_routine();
  fetch_routine();
  if (!(isFull())) {
    addMark();
    pause();
  }
 else {
    reportError("Cannot drop. The cell is already full.");
  }
  turnAround_routine();
  runArround_routine();
}
