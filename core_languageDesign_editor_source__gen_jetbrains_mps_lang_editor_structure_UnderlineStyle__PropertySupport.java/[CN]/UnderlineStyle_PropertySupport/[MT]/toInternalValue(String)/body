{
  if (value == null) {
    return null;
  }
  Iterator<UnderlineStyle> constants=UnderlineStyle.getConstants().iterator();
  while (constants.hasNext()) {
    UnderlineStyle constant=constants.next();
    if (value.equals(constant.getName())) {
      return constant.getValueAsString();
    }
  }
  return null;
}
