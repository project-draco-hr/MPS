{
  if (value == null) {
    return true;
  }
  Iterator<UnderlineStyle> constants=ListSequence.fromList(UnderlineStyle.getConstants()).iterator();
  while (constants.hasNext()) {
    UnderlineStyle constant=constants.next();
    if (value.equals(constant.getName())) {
      return true;
    }
  }
  return false;
}
