{
  SAXBuilder saxBuilder=createBuilder();
  try {
    return saxBuilder.build(source);
  }
 catch (  JDOMException e) {
    LOG.error("FAILED TO LOAD FILE : " + source.toString());
    throw e;
  }
catch (  IOException e) {
    LOG.error("FAILED TO LOAD FILE : " + source.toString());
    throw e;
  }
}
