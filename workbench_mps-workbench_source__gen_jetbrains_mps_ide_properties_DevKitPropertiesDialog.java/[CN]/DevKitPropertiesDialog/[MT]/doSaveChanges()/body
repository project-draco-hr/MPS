{
  if (!(checkValidity())) {
    return false;
  }
  getProject().getRepository().getModelAccess().executeCommand(new Runnable(){
    public void run(){
      myProperties.saveTo(myDevKit.getModuleDescriptor());
      myDevKit.setDevKitDescriptor(myDevKit.getModuleDescriptor(),true);
      myDevKit.save();
    }
  }
);
  return true;
}
