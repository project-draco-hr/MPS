{
  Set<ProgramState> result=new HashSet<ProgramState>();
  if (this != getProgram().getStart()) {
    Instruction prev=getProgram().get(getIndex() - 1);
    if (!(prev instanceof RetInstruction) && !(prev instanceof JumpInstruction) && !(prev instanceof EndTryInstruction && s.isReturnMode())&& !(prev instanceof StopRetInstruction)) {
      result.add(new ProgramState(prev,s.isReturnMode()));
    }
    if (prev instanceof StopRetInstruction && !s.isReturnMode()) {
      result.add(new ProgramState(prev,true));
      result.add(new ProgramState(prev,false));
    }
  }
  for (  Instruction jump : myJumps) {
    result.add(new ProgramState(jump,s.isReturnMode()));
  }
  return result;
}
