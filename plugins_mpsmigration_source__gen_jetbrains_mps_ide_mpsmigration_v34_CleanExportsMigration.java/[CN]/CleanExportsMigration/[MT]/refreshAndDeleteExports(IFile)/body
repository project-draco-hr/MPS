{
  if (f == null) {
    return;
  }
  if (f instanceof CachingFile) {
    ((CachingFile)f).refresh(new IdeaCachingContext(true,false));
  }
  if (f.isDirectory()) {
    for (    IFile cf : ListSequence.fromList(f.getChildren())) {
      refreshAndDeleteExports(cf);
    }
  }
 else   if (f.getName().equals("exports")) {
    f.delete();
  }
}
