{
  if (rule instanceof TemplateRootMappingRuleInterpreted) {
    return wrapped.applyRule(rule,environment,context);
  }
  try {
    tracer.push(taskName("root mapping rule",rule.getRuleNode().getNode()),true);
    return wrapped.applyRule(rule,environment,context);
  }
  finally {
    tracer.pop();
  }
}
