{
  if (rule instanceof TemplateCreateRootRuleInterpreted) {
    return wrapped.applyRule(rule,environment);
  }
  try {
    tracer.push(taskName("create root rule",rule.getRuleNode().resolve(MPSModuleRepository.getInstance())),true);
    return wrapped.applyRule(rule,environment);
  }
  finally {
    tracer.pop();
  }
}
