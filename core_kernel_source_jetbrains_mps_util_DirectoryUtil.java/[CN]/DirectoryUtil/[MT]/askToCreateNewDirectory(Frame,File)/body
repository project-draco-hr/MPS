{
  if (!dir.exists()) {
    int result=JOptionPane.showConfirmDialog(mainFrame,"Do you want to create directory " + dir,"Directory doesn't exist",JOptionPane.YES_NO_OPTION,JOptionPane.INFORMATION_MESSAGE);
    if (result != JOptionPane.OK_OPTION) {
      return false;
    }
    dir.mkdirs();
  }
  return true;
}
