{
  Project project=myEnvironment.getProject();
  File file=new File(Gcc_Command.getExecutableFile(SNodeOperations.cast(myRunConfiguration.getNode().getNode(),"jetbrains.mps.nanoc.structure.File")).getAbsolutePath());
  if (myDebuggerSettings.isEmpty()) {
{
      ProcessHandler _processHandler=new Exec_Command().createProcess(file);
      final ConsoleView _consoleView=ConsoleCreator.createConsoleView(project,false);
      _processHandler.addProcessListener(new ConsoleProcessListener(_consoleView));
      return new DefaultExecutionResult(_processHandler,new DefaultExecutionConsole(_consoleView.getComponent(),new _FunctionTypes._void_P0_E0(){
        public void invoke(){
          _consoleView.dispose();
        }
      }
));
    }
  }
 else {
{
      ProcessHandler _processHandler=new Gdb_Command().createProcess(file,project);
      final ConsoleView _consoleView=ConsoleCreator.createConsoleView(project,false);
      _processHandler.addProcessListener(new ConsoleProcessListener(_consoleView));
      return new DefaultExecutionResult(_processHandler,new DefaultExecutionConsole(_consoleView.getComponent(),new _FunctionTypes._void_P0_E0(){
        public void invoke(){
          _consoleView.dispose();
        }
      }
));
    }
  }
}
