{
  SubtypingCache cache=myTypeChecker.getSubtypingCache();
  if (cache != null) {
    Boolean answer=cache.getAnswer(subType,superType,isWeak);
    if (answer != null) {
      hits++;
      return answer;
    }
  }
  cache=myTypeChecker.getGlobalSubtypingCache();
  if (cache != null) {
    Boolean answer=cache.getAnswer(subType,superType,isWeak);
    if (answer != null) {
      hits++;
      return answer;
    }
  }
  misses++;
  return null;
}
