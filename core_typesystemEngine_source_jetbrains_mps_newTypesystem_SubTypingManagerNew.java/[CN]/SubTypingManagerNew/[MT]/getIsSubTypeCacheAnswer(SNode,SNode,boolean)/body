{
  SubtypingCache cache=myTypeChecker.getSubtypingCache();
  if (cache != null) {
    Boolean answer=cache.getIsSubtype(subType,superType,isWeak);
    if (answer != null) {
      return answer;
    }
  }
  cache=myTypeChecker.getGlobalSubtypingCache();
  if (cache != null) {
    Boolean answer=cache.getIsSubtype(subType,superType,isWeak);
    if (answer != null) {
      return answer;
    }
  }
  return null;
}
