{
  String packageName=SModelStereotype.withoutStereotype(sm.getModelName());
  String queriesClassName=packageName + ".QueriesGenerated";
  SModule module=sm.getModule();
  if (module == null) {
    reportErrorWhileClassLoading(queriesClassName,suppressErrorLogging,"couldn't find class 'QueriesGenerated': no module for model '" + sm.getReference() + "'");
  }
  if (!ClassLoaderManager.getInstance().canLoad(module)) {
    reportErrorWhileClassLoading(queriesClassName,suppressErrorLogging,"couldn't find class 'QueriesGenerated': module " + module.getModuleName() + " not classloading");
  }
  Class queriesClass=ClassLoaderManager.getInstance().getClass(module,queriesClassName);
  if (queriesClass == null) {
    reportErrorWhileClassLoading(queriesClassName,suppressErrorLogging,"couldn't find class 'QueriesGenerated' for model '" + sm.getReference() + "' : TRY TO GENERATE");
  }
  return queriesClass;
}
