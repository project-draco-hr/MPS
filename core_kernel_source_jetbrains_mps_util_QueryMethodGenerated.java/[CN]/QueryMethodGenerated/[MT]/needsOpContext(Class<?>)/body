{
  Boolean rv=myNeedOpContext.get(cls);
  if (rv != null) {
    return rv;
  }
  boolean result=true;
  for (  Field f : cls.getDeclaredFields()) {
    if ("NEEDS_OPCONTEXT".equals(f.getName())) {
      result=false;
      break;
    }
  }
  myNeedOpContext.putIfAbsent(cls,result);
  return result;
}
