{
  String packageName=SModelStereotype.withoutStereotype(sm.getModelName());
  String queriesClassName=packageName + ".QueriesGenerated";
  SModule module=sm.getModule();
  if (module == null) {
    reportErrorWhileClassLoading(queriesClassName,suppressErrorLogging,"couldn't find class 'QueriesGenerated': no module for model '" + sm.getSModelReference() + "'");
  }
  if (!(module instanceof IClassLoadingModule)) {
    reportErrorWhileClassLoading(queriesClassName,suppressErrorLogging,"couldn't find class 'QueriesGenerated': module " + module.getModuleName() + " not implements IClassLoadingModule");
  }
  Class queriesClass=((IClassLoadingModule)module).getClass(queriesClassName);
  if (queriesClass == null) {
    reportErrorWhileClassLoading(queriesClassName,suppressErrorLogging,"couldn't find class 'QueriesGenerated' for model '" + sm.getSModelReference() + "' : TRY TO GENERATE");
  }
  return queriesClass;
}
