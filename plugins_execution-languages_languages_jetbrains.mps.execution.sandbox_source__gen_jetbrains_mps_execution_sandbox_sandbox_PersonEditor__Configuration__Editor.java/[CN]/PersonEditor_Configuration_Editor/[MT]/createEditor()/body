{
  JPanel panel=new JPanel(new GridBagLayout());
  myNameField=new JTextField();
  myEmailField=new JTextField();
  panel.add(new JLabel("Name"),LayoutUtil.createLabelConstraints(0));
  panel.add(myNameField,LayoutUtil.createFieldConstraints(1));
  panel.add(new JLabel("Email"),LayoutUtil.createLabelConstraints(2));
  panel.add(myEmailField,LayoutUtil.createFieldConstraints(3));
  return panel;
}
