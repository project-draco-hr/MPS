{
  if ((oldAdapter == null)) {
    return;
  }
  Map<Binding,SNode> map=myReferentsCreator.myBindingMap;
  for (  Binding binding : new HashSet<Binding>(map.keySet())) {
    SNode result=map.get(binding);
    if ((result != null) && result == oldAdapter) {
      map.put(binding,newAdapter);
    }
  }
}
