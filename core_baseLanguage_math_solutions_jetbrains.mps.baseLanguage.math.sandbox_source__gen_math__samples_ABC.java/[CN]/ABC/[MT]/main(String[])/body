{
  Matrix<Double> dv=new Matrix<Double>(new Matrix<Integer>(new Integer[][]{{1},{2},{3}},Matrix.soInt),Matrix.soDouble);
  Matrix<Double> s=new Matrix<Double>(false,Matrix.soDouble,new Matrix<Double>(new Double[][]{{3.0},{Math.pow(3,2)},{4.0}},Matrix.soDouble),new Matrix(new Matrix<Double>(new Double[][]{{Math.sin(1.0)},{1.0},{Math.pow(2.0,((1.0) / (2)))}},Matrix.soDouble),dv,MatrixOperation.MatrixAdd,Matrix.soDouble),new Matrix<Double>(new Double[][]{{1.0},{3 + ((1.0) / (2))},{Math.exp(1.0)}},Matrix.soDouble),new Matrix<Double>(new Matrix<Integer>(new Integer[][]{{1},{2},{3}},Matrix.soInt),Matrix.soDouble));
  System.out.println(s);
  try {
    int i=1;
    while (i != 0) {
      i=System.in.read();
    }
  }
 catch (  IOException e) {
    System.out.println("Error!");
  }
}
