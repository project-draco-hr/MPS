{
  if (value == null) {
    return true;
  }
  Iterator<CellKeyMapCaretPolicy> constants=CellKeyMapCaretPolicy.getConstants().iterator();
  while (constants.hasNext()) {
    CellKeyMapCaretPolicy constant=constants.next();
    if (value.equals(constant.getName())) {
      return true;
    }
  }
  return false;
}
