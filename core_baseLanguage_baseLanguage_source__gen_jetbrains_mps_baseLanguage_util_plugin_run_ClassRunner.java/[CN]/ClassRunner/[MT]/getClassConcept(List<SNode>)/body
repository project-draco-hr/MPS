{
  SNode classConcept=null;
  if (nodes != null) {
    for (    SNode node : ListSequence.fromList(nodes)) {
      SNode n=SNodeOperations.getAncestor(node,"jetbrains.mps.baseLanguage.structure.ClassConcept",true,false);
      if ((n != null)) {
        classConcept=n;
        break;
      }
    }
  }
  return classConcept;
}
