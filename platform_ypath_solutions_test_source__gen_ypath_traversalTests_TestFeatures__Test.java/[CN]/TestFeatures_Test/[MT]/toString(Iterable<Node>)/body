{
  StringBuilder sb=new StringBuilder();
  String sep="";
{
    ICursor<Node> _zCursor=CursorFactory.createCursor(nodes);
    try {
      while (_zCursor.moveToNext()) {
        Node n=_zCursor.getCurrent();
        sb.append(sep).append(n.getNodeName());
        sep=", ";
      }
    }
  finally {
      _zCursor.release();
    }
  }
  return sb.toString();
}
