  copyToProject(APPLLICATION_PLUGINS_DIR + "/mps-core/lib","IdeaPlugin/mps-core/lib");
  copyToProject(APPLLICATION_PLUGINS_DIR + "/mps-core/languages","IdeaPlugin/mps-core/languages");
