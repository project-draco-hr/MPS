{
  SNode myType=TypeChecker.getInstance().getTypeOf(_context.getNode());
  SNode parentType=MathUtil.getUnboxedElementType(TypeChecker.getInstance().getTypeOf(SNodeOperations.getParent(_context.getNode())));
