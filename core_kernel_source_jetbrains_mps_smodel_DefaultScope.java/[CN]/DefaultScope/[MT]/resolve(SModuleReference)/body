{
  if (reference == null) {
    return null;
  }
  SModule module=reference.resolve(MPSModuleRepository.getInstance());
  if (module == null) {
    return null;
  }
synchronized (LOCK) {
    initialize();
    if (myVisibleModules.contains(module) || myUsedLanguages.contains(module) || myUsedDevkits.contains(module)) {
      return module;
    }
 else {
      return null;
    }
  }
}
