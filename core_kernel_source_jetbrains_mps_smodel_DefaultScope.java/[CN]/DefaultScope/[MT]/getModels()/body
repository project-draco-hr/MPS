{
  List<SModel> result=new ArrayList<SModel>();
synchronized (LOCK) {
    initialize();
    for (    IModule module : myVisibleModules) {
      result.addAll(module.getOwnModelDescriptors());
    }
    for (    IModule module : myUsedLanguages) {
      result.addAll(module.getOwnModelDescriptors());
    }
  }
  return result;
}
