{
  for (  SReference ref : node.getReferences()) {
    SNode targetNode=ref.getTargetNode();
    if (targetNode != null) {
      SNode targetRoot=targetNode.getContainingRoot();
      if (targetRoot != null) {
        Integer targetIndex=rootIndex.get(targetRoot);
        if (targetIndex != null) {
          dependsOn[targetIndex]=1;
        }
      }
    }
  }
}
