{
  if (isInitialized()) {
    myFastNodeFinder=null;
    myModelListeners.addAll(mySModel.getListeners());
    myModelCommandListeners.addAll(mySModel.getCommandListeners());
    mySModel.dispose();
    mySModel=myModelRootManager.refresh(this);
    if (mySModel != null) {
      addListenersToNewModel();
    }
  }
}
