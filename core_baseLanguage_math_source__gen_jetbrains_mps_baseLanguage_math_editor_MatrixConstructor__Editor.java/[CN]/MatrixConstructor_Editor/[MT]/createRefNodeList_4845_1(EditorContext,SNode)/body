    style.set(StyleAttributes.DEFAULT_BASE_LINE,DefaultBaseLine.CENTER);
    style.set(StyleAttributes.TABLE_COMPONENT,TableComponent.VERTICAL_COLLECTION);
  }
  editorCell.setRole(handler.getElementRole());
