{
  List<SNode> result=new ArrayList<SNode>();
  for (  ILazyTab tab : myTabbedPane.getTabs()) {
    tab.getComponent();
    for (    AbstractEditorComponent aec : tab.getEditorComponents()) {
      if (aec.getEditedNode() != null) {
        result.add(aec.getEditedNode());
      }
    }
  }
  return result;
}
