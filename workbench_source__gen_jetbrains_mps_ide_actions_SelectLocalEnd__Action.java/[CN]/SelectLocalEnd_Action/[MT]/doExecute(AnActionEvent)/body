{
  try {
    if (SelectLocalEnd_Action.this.editorCell instanceof EditorCell_Label && !(SelectLocalEnd_Action.this.editorCell.isLastCaretPosition()) && ((EditorCell_Label)SelectLocalEnd_Action.this.editorCell).isLastPositionAllowed()) {
      SelectLocalEnd_Action.this.editorCell.executeAction(CellActionType.SELECT_LOCAL_END);
    }
 else     if (event.getInputEvent() instanceof KeyEvent) {
      KeyEvent keyEvent=(KeyEvent)event.getInputEvent();
      NodeRangeSelection rangeSelection=SelectLocalEnd_Action.this.editorComponent.getNodeRangeSelection();
      if (rangeSelection.isActive()) {
        rangeSelection.processKeyPressed(SelectLocalEnd_Action.this.editorContext,keyEvent);
      }
 else {
        rangeSelection.activate(keyEvent);
      }
    }
  }
 catch (  Throwable t) {
    if (log.isErrorEnabled()) {
      log.error("User's action execute method failed. Action:" + "SelectLocalEnd",t);
    }
  }
}
