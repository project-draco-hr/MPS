{
  if (!(editorCells.getX() <= x && x < editorCells.getX() + editorCells.getWidth() && editorCells.getY() <= y && y < editorCells.getY() + editorCells.getHeight())) {
    return null;
  }
  for (  EditorCell editorCell : editorCells) {
    EditorCell cell=editorCell.findCell(x,y);
    if (cell != null) {
      return cell;
    }
  }
  return null;
}
