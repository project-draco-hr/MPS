{
  EditorCell contextCell=editorContext.getContextCell();
  if ((contextCell == null)) {
    return false;
  }
  SNode contextNode=contextCell.getSNode();
  if (contextNode == null) {
    return false;
  }
  if (contextNode.isInstanceOfConcept("jetbrains.mpslite.structure.Line")) {
    return true;
  }
  return false;
}
