{
  this.appendNewLine();
  this.appendWithIndent("{");
  this.increaseDepth();
  TextGenManager.instance().appendNodeText(this.getContext(),this.getBuffer(),SLinkOperations.getTarget(node,"statementList",true),this.getSNode());
  this.decreaseDepth();
  this.appendNewLine();
  this.appendWithIndent("}");
  this.appendNewLine();
}
