{
  BufferLayoutBuilder lb=new BufferLayoutBuilder();
  lb.add("HEADER");
  lb.add("IMPORTS");
  lb.add("SEPARATOR");
  lb.add("BODY");
  lb.activate("BODY");
  RegularTextUnit2 rv=new RegularTextUnit2(node,filename,null);
  rv.setBufferLayout(lb.create());
  rv.addContextObject("ctx",Auxiliary.contextObjectInstance_ctx(node));
  return rv;
}
