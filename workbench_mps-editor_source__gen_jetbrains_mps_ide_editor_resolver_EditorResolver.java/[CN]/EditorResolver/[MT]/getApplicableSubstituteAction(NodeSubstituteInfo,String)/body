{
  INodeSubstituteAction result=null;
  for (  INodeSubstituteAction nextAction : ListSequence.fromList(substituteInfo.getMatchingActions(resolveInfo,true))) {
    if (nextAction.canSubstitute(resolveInfo)) {
      if (result != null) {
        return null;
      }
      result=nextAction;
    }
  }
  return result;
}
