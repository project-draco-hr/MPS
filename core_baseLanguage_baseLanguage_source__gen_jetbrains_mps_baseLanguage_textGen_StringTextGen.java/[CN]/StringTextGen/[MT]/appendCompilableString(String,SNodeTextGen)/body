{
  String result=s;
  if (s.indexOf('"') >= 0 || s.indexOf('\n') >= 0 || s.indexOf('\r') >= 0) {
    boolean needsEscaping=false;
    for (int i=0; i < s.length(); i++) {
      if (isBadDoubleQuote(s,i,textGen) || result.charAt(i) == '\r' || result.charAt(i) == '\n') {
        needsEscaping=true;
        break;
      }
    }
    if (needsEscaping) {
      StringBuilder stringBuilder=new StringBuilder();
      for (int i=0; i < s.length(); i++) {
        if (isBadDoubleQuote(s,i,textGen)) {
          stringBuilder.append("\\\"");
        }
 else         if (result.charAt(i) == '\r') {
          stringBuilder.append("\\r");
        }
 else         if (result.charAt(i) == '\n') {
          stringBuilder.append("\\n");
        }
 else {
          stringBuilder.append(s.charAt(i));
        }
      }
      result=stringBuilder.toString();
    }
  }
  textGen.append(replaceNonAsciiSymbolsWithUnicodeSymbols(s,textGen));
}
