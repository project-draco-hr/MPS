{
  try {
    BookmarkManager bookmarkManager=((Project)MapSequence.fromMap(_params).get("project")).getComponent(BookmarkManager.class);
    bookmarkManager.setBookmark(((SNode)MapSequence.fromMap(_params).get("node")),0);
  }
 catch (  Throwable t) {
    if (log.isErrorEnabled()) {
      log.error("User's action execute method failed. Action:" + "SetBookmark0",t);
    }
  }
}
