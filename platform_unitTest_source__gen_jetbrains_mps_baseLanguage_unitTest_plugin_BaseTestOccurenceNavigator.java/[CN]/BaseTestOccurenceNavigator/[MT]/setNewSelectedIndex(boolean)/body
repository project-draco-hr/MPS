{
  boolean founded=false;
  DefaultMutableTreeNode node=this.testTree.getCurrentNode();
  do {
    MPSTreeNode temp=(MPSTreeNode)(next ? node.getNextNode() : node.getPreviousNode());
    this.selected=this.selected + (next ? 1 : -1);
    if (this.accept(temp)) {
      founded=true;
    }
    node=temp;
  }
 while (!(founded));
}
