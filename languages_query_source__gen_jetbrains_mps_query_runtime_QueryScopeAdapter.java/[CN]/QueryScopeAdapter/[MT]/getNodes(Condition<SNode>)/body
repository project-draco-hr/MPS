{
  List<SNode> result=scope.getAvailableElements(null);
  if (condition != null) {
    Iterator<SNode> it=ListSequence.<SNode>fromList(result).iterator();
    while (it.hasNext()) {
      SNode node=it.next();
      if (!(condition.met(node))) {
        it.remove();
      }
    }
  }
  return result;
}
