{
  ModelAccess.instance().runWriteAction(new Runnable(){
    public void run(){
      OutputStream outputStream=null;
      try {
        outputStream=file.getOutputStream(this);
        outputStream.write(result);
      }
 catch (      IOException e) {
        LOG.error(e);
      }
 finally {
        if (outputStream != null) {
          try {
            outputStream.close();
          }
 catch (          IOException e) {
          }
        }
      }
      reloadModel(result,file);
    }
  }
);
}
