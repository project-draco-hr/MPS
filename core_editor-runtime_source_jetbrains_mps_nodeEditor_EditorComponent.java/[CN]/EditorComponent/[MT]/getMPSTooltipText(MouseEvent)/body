{
  return ModelAccess.instance().tryRead(new Computable<String>(){
    public String compute(){
      if (myRootCell == null) {
        return null;
      }
      EditorCell cell=myRootCell.findLeaf(event.getX(),event.getY());
      if (cell == null) {
        return null;
      }
      return getMessageTextFor(cell);
    }
  }
);
}
