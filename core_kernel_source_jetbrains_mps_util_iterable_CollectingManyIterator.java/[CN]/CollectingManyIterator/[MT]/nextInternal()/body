{
  if (currentInner != null && currentInner.hasNext()) {
    return currentInner.next();
  }
  while (currentOuter.hasNext()) {
    IN element=currentOuter.next();
    currentInner=element != null ? translate(element) : null;
    if (currentInner != null && currentInner.hasNext()) {
      return currentInner.next();
    }
  }
  return null;
}
