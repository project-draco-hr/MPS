{
  editorCell.setSubstituteInfo(new OldNewCompositeSubstituteInfo(myEditorContext,new SChildSubstituteInfo(editorCell,myOwnerNode,MetaAdapterFactory.getContainmentLink(0xcb6d57037c8e46a9L,0xb993c1373dc0942fL,0x7ce1116e3a6ff2a8L,0x7ce1116e3a6ff2acL,"conceptToWrap"),child),new DefaultChildSubstituteInfo(myOwnerNode,myContainmentLink.getDeclarationNode(),myEditorContext)));
  if (editorCell.getRole() == null) {
    editorCell.setRole("conceptToWrap");
