{
  Expression program=new IfExpression(new NopExpression(),new AssignmentExpression("a",new NopExpression()),new AssignmentExpression("b",new NopExpression()));
  Program p=new ExpressionProgramBuilder().buildProgram(program);
  Assert.assertEquals("0: ifjump 3\n" + "1: write a\n" + "2: jump 4\n"+ "3: write b\n"+ "4: nop\n",p.toString());
}
