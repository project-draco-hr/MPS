{
  Expression program=new WhileExpression(new VariableExpression("a"),new AssignmentExpression("b",new NopExpression()));
  Program p=new ExpressionProgramBuilder().buildProgram(program);
  Assert.assertEquals("0: read a\n" + "1: ifjump 3\n" + "2: write b\n"+ "3: nop\n"+ "4: end\n",p.toString());
}
