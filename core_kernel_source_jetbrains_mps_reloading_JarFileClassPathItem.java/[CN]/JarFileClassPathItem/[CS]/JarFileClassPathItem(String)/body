{
  if (path.endsWith("!/")) {
    path=path.substring(0,path.length() - 2);
  }
  try {
    myFile=transformFile(FileSystem.getInstance().getFileByPath(path));
    myPrefix="jar:" + myFile.toURI().toURL() + "!/";
    myZipFile=new ZipFile(myFile);
  }
 catch (  IOException e) {
    LOG.error("invalid class path: " + path,e);
  }
}
