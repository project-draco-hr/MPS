{
  checkValidity();
  ensureInitialized();
  ZipEntry entry=myEntries.get(name);
  if (entry == null)   return null;
  InputStream inp=null;
  try {
    inp=myZipFile.getInputStream(entry);
    return ClassifierKind.getClassifierKind(inp);
  }
 catch (  IOException e) {
    return null;
  }
 finally {
    if (inp != null) {
      try {
        inp.close();
      }
 catch (      IOException e) {
        LOG.error(e);
      }
    }
  }
}
