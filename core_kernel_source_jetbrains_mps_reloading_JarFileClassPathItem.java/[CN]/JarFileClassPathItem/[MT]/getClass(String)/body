{
  ZipEntry entry=myEntries.get(name);
  if (entry == null)   return null;
  try {
    InputStream inp=myZipFile.getInputStream(entry);
    byte[] result=new byte[(int)entry.getSize()];
    ReadUtil.read(result,inp);
    inp.close();
    return result;
  }
 catch (  IOException e) {
    return null;
  }
}
