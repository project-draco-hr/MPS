{
  checkValidity();
  ensureInitialized();
  ZipEntry entry=myEntries.get(name);
  if (entry == null)   return null;
  InputStream inp=null;
  ZipFile zf=null;
  try {
    zf=new ZipFile(myFile);
    inp=zf.getInputStream(entry);
    if (inp == null)     return null;
    byte[] result=new byte[(int)entry.getSize()];
    ReadUtil.read(result,inp);
    return result;
  }
 catch (  IOException e) {
    LOG.error(e);
    return null;
  }
 finally {
    if (zf != null) {
      try {
        zf.close();
      }
 catch (      IOException e) {
        LOG.error(e);
      }
    }
    if (inp != null) {
      try {
        inp.close();
      }
 catch (      IOException e) {
        LOG.error(e);
      }
    }
  }
}
