{
  ZipFile zf=null;
  try {
    zf=new ZipFile(myFile);
    if (zf.getEntry(name) == null)     return null;
    return new URL(myPrefix + name);
  }
 catch (  MalformedURLException e) {
    LOG.error(null,e);
    return null;
  }
catch (  IOException e) {
    LOG.error(null,e);
    return null;
  }
 finally {
    if (zf != null) {
      try {
        zf.close();
      }
 catch (      IOException e) {
        LOG.error(null,e);
      }
    }
  }
}
