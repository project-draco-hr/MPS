{
  return SNodeOperations.isInstanceOf(parentNode,"jetbrains.mps.baseLanguage.structure.DotExpression") && SNodeOperations.isInstanceOf(SLinkOperations.getTarget(SNodeOperations.cast(parentNode,"jetbrains.mps.baseLanguage.structure.DotExpression"),"operand",true),"jetbrains.mps.lang.migration.structure.ForEachMatchingExpression");
