{
  if (myErrorView == null) {
    return;
  }
  if (eq_ljwq4f_a0b0p(myErrorView.bounds().get().dimension,bounds.dimension)) {
    if (neq_ljwq4f_a0a0b0p(myErrorView.bounds().get().origin,bounds.origin)) {
      myErrorView.moveTo(bounds.origin);
    }
  }
 else {
    updateSelectionPolyline(myErrorView,bounds);
  }
  myErrorView.invalidate();
}
