{
  this.myComboBox.removeAllItems();
  this.myComboBox.addItem(null);
  for (  ModuleReference modRef : (this.myForModel ? this.myProps.getModulesForModel(row) : this.myProps.getModulesForLanguage(row))) {
    this.myComboBox.addItem(modRef);
  }
  return super.getTableCellEditorComponent(table,value,isSelected,row,column);
}
