{
  Worker wrk=new Worker(){
    public int field;
    public String doWork(    Integer d){
      this.field=d;
      return new FunctionTypes._R<String>(){
        public String invoke(){
          return "Done: " + field;
        }
      }
.invoke();
    }
  }
;
  Assert.assertEquals("Done: 4321",wrk.doWork(4321));
}
