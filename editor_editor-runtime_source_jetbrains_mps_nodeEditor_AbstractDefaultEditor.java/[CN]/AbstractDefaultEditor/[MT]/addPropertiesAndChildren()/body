{
  addLabel("{");
  addStyle(StyleAttributes.MATCHING_LABEL,"body-brace");
  addNewLine();
  pushCollection();
  setIndent(collectionStack.peek());
  addProperties();
  addLabel("");
  addNewLine();
  addChildren();
  popCollection();
  addLabel("}");
  addStyle(StyleAttributes.MATCHING_LABEL,"body-brace");
}
