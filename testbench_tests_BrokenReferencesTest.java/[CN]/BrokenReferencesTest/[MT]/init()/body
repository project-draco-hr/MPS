{
  HELPER=new BrokenReferencesTestHelper();
  TOKEN=HELPER.init(new String[][]{{"samples_home",System.getProperty("user.dir") + "/samples"}});
  List<File> path=Collections.singletonList(new File(System.getProperty("user.dir")));
