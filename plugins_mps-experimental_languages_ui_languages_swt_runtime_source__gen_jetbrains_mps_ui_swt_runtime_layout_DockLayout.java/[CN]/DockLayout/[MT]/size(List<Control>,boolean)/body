{
switch ((ctrls == null ? 0 : ctrls.size())) {
case 0:
    return DockLayout.ZERO;
case 1:
  return this.size(ctrls.get(0),flushCache);
default :
Point res=new Point(0,0);
for (Control ctrl : ctrls) {
Point sz=ctrl.computeSize(SWT.DEFAULT,SWT.DEFAULT,flushCache);
res.x+=sz.x;
res.y+=sz.y;
}
return res;
}
}
