{
  if (!cacheFile.exists()) {
    return null;
  }
  SModelReference mr=myFilesToModels.get(cacheFile);
  if (mr == null) {
    return null;
  }
  T rv=myCache.get(mr);
  if (rv != null) {
    return rv;
  }
  SModel model=myModelRepository.getModelDescriptor(mr);
  if (model == null) {
    return null;
  }
  return readAndUpdateCache(cacheFile,model);
}
