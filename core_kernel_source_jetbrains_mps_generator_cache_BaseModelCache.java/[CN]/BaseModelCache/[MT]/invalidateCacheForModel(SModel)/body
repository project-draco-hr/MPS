{
  final SModelReference mr=md.getReference();
  if (!myCache.containsKey(mr)) {
    return;
  }
  IFile f=null;
  for (  Entry<IFile,SModelReference> e : myFilesToModels.entrySet()) {
    if (mr.equals(e.getValue())) {
      f=e.getKey();
    }
  }
  if (f != null) {
    invalidateCacheForFile(f);
  }
}
