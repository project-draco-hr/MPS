{
  List<SNode> _results=new ArrayList<SNode>();
{
    SNode current=node;
    ListOperations.addElement(_results,current);
    while (current != null) {
      current=SLinkOperations.getTarget(SLinkOperations.getTarget(current,"superclass",true),"classifier",false);
      if (current != null) {
        ListOperations.addElement(_results,current);
      }
    }
  }
  return _results;
}
