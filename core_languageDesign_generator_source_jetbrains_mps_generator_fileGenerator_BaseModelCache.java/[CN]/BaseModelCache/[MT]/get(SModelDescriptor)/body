{
synchronized (myCache) {
    if (myCache.containsKey(sm)) {
      return myCache.get(sm);
    }
    IFile cacheFile=getCacheFile(sm);
    if (cacheFile == null || !cacheFile.exists())     return null;
    InputStream is=null;
    try {
      is=cacheFile.openInputStream();
      T result=load(is);
      myCache.put(sm,result);
      return result;
    }
 catch (    IOException e) {
      LOG.error(e);
    }
 finally {
      try {
        if (is != null) {
          is.close();
        }
      }
 catch (      IOException e) {
        LOG.error(e);
      }
    }
    myCache.put(sm,null);
    return null;
  }
}
