{
  if (myTypeVars == null) {
    return (parentState == null ? null : parentState.resolveTypeVar(name));
  }
  if (myTypeVars == null || !(MapSequence.fromMap(myTypeVars).containsKey(name))) {
    return (parentState == null ? null : parentState.resolveTypeVar(name));
  }
 else {
    SNode typeVar=MapSequence.fromMap(myTypeVars).get(name);
    SNode typeVarRef=SConceptOperations.createNewNode(SNodeOperations.asInstanceConcept(MetaAdapterFactory.getConcept(0xf3061a5392264cc5L,0xa443f952ceaf5816L,0x102467229d8L,"jetbrains.mps.baseLanguage.structure.TypeVariableReference")));
    SReference ref;
    if (typeVar != null) {
      ref=new StaticReference("typeVariableDeclaration",typeVarRef,typeVar);
    }
 else {
      ref=new DynamicReference("typeVariableDeclaration",typeVarRef,null,name);
    }
    typeVarRef.setReference(ref.getRole(),ref);
    return typeVarRef;
  }
}
