{
  return ((SLinkOperations.getTarget(_context.getNode(),"mappingLabel",false) != null) && (SPropertyOperations.getString(SLinkOperations.getTarget(_context.getNode(),"mappingLabel",false),"name") != null && SPropertyOperations.getString(SLinkOperations.getTarget(_context.getNode(),"mappingLabel",false),"name").length() > 0) || (SLinkOperations.getTarget(_context.getNode(),"sourceNodeQuery",true) != null) ? "context" + ((int[])_context.getVariable("varindex"))[0] : ((String)_context.getVariable("contextVar")));
}
