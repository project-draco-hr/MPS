{
  String resolveInfo=templateRef.getResolveInfo();
  boolean willNotPassTargetId=resolveInfo != null && preferResolveInfo;
  return newInstance(role,sourceNode,willNotPassTargetId ? null : templateRef.getTargetNodeId(),templateRef.getExtResolveInfo(),templateRef.getTargetModelUID(),resolveInfo,templateRef.getTargetClassResolveInfo());
}
