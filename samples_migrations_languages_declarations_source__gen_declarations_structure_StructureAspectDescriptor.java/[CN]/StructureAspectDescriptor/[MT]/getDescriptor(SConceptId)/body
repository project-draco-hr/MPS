{
  Integer index=myIndexMap.get(id);
  if (index == null) {
    return null;
  }
switch (((int)index)) {
case 0:
    return myConceptNewComponent;
case 1:
  return myConceptNewComponentMember;
case 2:
return myConceptOldComponent;
case 3:
return myConceptOldComponentMember;
default :
throw new IllegalStateException();
}
}
