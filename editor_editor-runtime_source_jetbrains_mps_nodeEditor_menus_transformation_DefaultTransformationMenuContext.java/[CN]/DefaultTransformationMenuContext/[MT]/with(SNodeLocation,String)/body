{
  if (nodeLocation == null) {
    nodeLocation=myNodeLocation;
  }
  if (menuLocation == null) {
    menuLocation=myMenuLocation;
  }
  if (myNodeLocation.equals(nodeLocation) && myMenuLocation.equals(menuLocation)) {
    return this;
  }
  return new DefaultTransformationMenuContext(myMenuItemFactory,menuLocation,myEditorContext,nodeLocation,createSuitableForConstraintsPredicate(nodeLocation,myEditorContext.getRepository()));
}
