{
  return new Junit_Command().setVirtualMachineParameter(javaRunParameters.vmOptions()).setJrePath(((boolean)javaRunParameters.useAlternativeJre() ? javaRunParameters.jrePath() : null)).setWorkingDirectory((StringUtils.isEmpty(javaRunParameters.workingDirectory()) ? null : new File(javaRunParameters.workingDirectory()))).createProcess(tests);
}
