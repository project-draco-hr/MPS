{
  for (  SReference ref : (List<SReference>)SNodeOperations.getReferences(node)) {
    if (ref.isExternal()) {
      continue;
    }
    SNode targetNode=ref.getTargetNode();
    if (targetNode != null) {
      Integer targetIndex=rootIndex.get(targetNode.getTopmostAncestor());
      if (targetIndex != null) {
        dependsOn[targetIndex]=1;
      }
    }
  }
}
