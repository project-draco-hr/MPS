{
  try {
    for (    SModelDescriptor model : ListSequence.<SModelDescriptor>fromList(SModelRepository.getInstance().getModelDescriptors())) {
      if (!(SModelStereotype.isUserModel(model))) {
        continue;
      }
      if (!(model instanceof EditableSModelDescriptor)) {
        return;
      }
      ((EditableSModelDescriptor)model).setChanged(true);
    }
    SModelRepository.getInstance().saveAll();
  }
 catch (  Throwable t) {
    if (log.isErrorEnabled()) {
      log.error("User's action execute method failed. Action:" + "ReSaveAllModels",t);
    }
  }
}
