{
  if (USE_NULL_SEQUENCE) {
    if (coll == null) {
      return NullCollectionSequence.instance();
    }
  }
  if (coll instanceof ICollectionSequence<?>) {
    return (ICollectionSequence<U>)coll;
  }
  return new CollectionSequence<U>(){
    protected Collection<U> getCollection(){
      return coll;
    }
  }
;
}
