{
  if (getPatternEditor().processKeyTyped(keyEvent)) {
    if (myPopupActivated) {
      rebuildMenuEntries();
      if (getEditorWindow() != null && !MPSCore.getInstance().isTestMode()) {
        relayoutPopupMenu();
      }
      tryToApplyIntelligentInput();
    }
    return true;
  }
  return false;
}
