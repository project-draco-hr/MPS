{
  for (  T rule : rules) {
    ConceptDeclaration concept1=SModelUtil_new.findConceptDeclaration(rule.getApplicableConceptFQName1(),GlobalScope.getInstance());
    ConceptDeclaration concept2=SModelUtil_new.findConceptDeclaration(rule.getApplicableConceptFQName2(),GlobalScope.getInstance());
    Pair<AbstractConceptDeclaration,AbstractConceptDeclaration> pair=new Pair<AbstractConceptDeclaration,AbstractConceptDeclaration>(concept1,concept2);
    Set<T> existingRules=myRules.get(pair);
    if (existingRules == null) {
      existingRules=new HashSet<T>();
      myRules.put(pair,existingRules);
    }
    existingRules.add(rule);
  }
}
