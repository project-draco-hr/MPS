{
  XmlFastScanner s=new XmlFastScanner("<l1 a=1><l2 b=2><l3 c=3/><l3/><l3/></l2><l2><l3></l3></l2></l1>".toCharArray());
  assertNextTag(s,OPEN_TAG,0);
  assertNextTag(s,OPEN_TAG,1);
  assertNextTag(s,SIMPLE_TAG,2);
  assertNextTag(s,SIMPLE_TAG,2);
  assertNextTag(s,SIMPLE_TAG,2);
  assertNextTag(s,CLOSE_TAG,1);
  assertNextTag(s,OPEN_TAG,1);
  assertNextTag(s,OPEN_TAG,2);
  assertNextTag(s,CLOSE_TAG,2);
  assertNextTag(s,CLOSE_TAG,1);
  assertNextTag(s,CLOSE_TAG,0);
  print(s.reset());
}
