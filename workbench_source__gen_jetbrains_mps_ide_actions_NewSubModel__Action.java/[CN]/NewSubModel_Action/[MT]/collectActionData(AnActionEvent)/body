{
  if (!(super.collectActionData(event))) {
    return false;
  }
  this.ideaProject=event.getData(MPSDataKeys.PROJECT);
  if (this.ideaProject == null) {
    return false;
  }
  this.context=event.getData(MPSDataKeys.OPERATION_CONTEXT);
  if (this.context == null) {
    return false;
  }
  this.model=event.getData(MPSDataKeys.CONTEXT_MODEL);
  if (this.model == null) {
    return false;
  }
  this.virtPackage=event.getData(MPSDataKeys.VIRTUAL_PACKAGE);
  if (this.virtPackage == null) {
    return false;
  }
  return true;
}
