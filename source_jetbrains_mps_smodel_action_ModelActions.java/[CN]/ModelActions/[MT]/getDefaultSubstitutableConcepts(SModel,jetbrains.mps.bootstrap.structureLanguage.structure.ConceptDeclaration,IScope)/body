{
  return sourceModel.conceptAdaptersFromModelLanguages(new Condition<ConceptDeclaration>(){
    public boolean met(    ConceptDeclaration node){
      return ChildSubstituteActionsHelper.isDefaultSubstitutableConcept((jetbrains.mps.bootstrap.structureLanguage.structure.ConceptDeclaration)node,targetConcept,scope);
    }
  }
,scope);
}
