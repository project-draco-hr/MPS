{
  op1=roundFloor(op1,type);
  op2=roundFloor(op2,type);
  boolean result;
  if (op1 == null || op2 == null) {
switch (cmp) {
case EQ:
      result=op1 == op2;
    break;
case NE:
  result=op1 != op2;
break;
default :
throw new NullPointerException();
}
}
 else {
switch (cmp) {
case EQ:
result=op1.compareTo(op2) == 0;
break;
case NE:
result=op1.compareTo(op2) != 0;
break;
case LT:
result=op1.compareTo(op2) < 0;
break;
case GT:
result=op1.compareTo(op2) > 0;
break;
case LE:
result=op1.compareTo(op2) <= 0;
break;
case GE:
result=op1.compareTo(op2) >= 0;
break;
default :
throw new UnsupportedOperationException("Unsupported compare type: " + cmp);
}
}
return result;
}
