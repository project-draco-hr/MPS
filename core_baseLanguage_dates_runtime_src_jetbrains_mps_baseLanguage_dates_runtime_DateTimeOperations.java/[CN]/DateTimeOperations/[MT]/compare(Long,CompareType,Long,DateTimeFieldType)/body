{
  op1=roundFloor(op1,type);
  op2=roundFloor(op2,type);
  if (op1 == null) {
    op1=Long.MIN_VALUE;
  }
  if (op2 == null) {
    op2=Long.MIN_VALUE;
  }
  boolean result;
switch (cmp) {
case EQ:
    result=op1.compareTo(op2) == 0;
  break;
case NE:
result=op1.compareTo(op2) != 0;
break;
case LT:
result=op1.compareTo(op2) < 0;
break;
case GT:
result=op1.compareTo(op2) > 0;
break;
case LE:
result=op1.compareTo(op2) <= 0;
break;
case GE:
result=op1.compareTo(op2) >= 0;
break;
default :
throw new UnsupportedOperationException("Unsupported compare type: " + cmp);
}
return result;
}
