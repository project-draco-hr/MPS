{
  while (true) {
    Runnable task;
synchronized (myQueue) {
      while (myQueue.isEmpty()) {
        try {
          myQueue.wait();
        }
 catch (        InterruptedException e) {
          return;
        }
      }
      task=myQueue.poll();
    }
    try {
      task.run();
    }
 catch (    Throwable e) {
      if (log.isErrorEnabled()) {
        log.error("Error in " + getName(),e);
      }
    }
  }
}
