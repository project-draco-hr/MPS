{
  if (myChanges.isEmpty()) {
    myMinY=cell.getY();
    myMaxY=cell.getY() + cell.getHeight();
  }
 else {
    if (myMinY > cell.getY()) {
      myMinY=cell.getY();
    }
    if (myMaxY < cell.getY() + cell.getHeight()) {
      myMaxY=cell.getY() + cell.getHeight();
    }
  }
  myChanges.add(message);
}
