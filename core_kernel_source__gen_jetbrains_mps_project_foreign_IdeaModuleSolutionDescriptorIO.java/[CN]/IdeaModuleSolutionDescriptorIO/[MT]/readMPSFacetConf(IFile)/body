{
  try {
    IdeaModuleConfiguration imc=IdeaModuleConfiguration.readFile(file);
    return imc.getMPSFacetConfiguration();
  }
 catch (  FacetConfigurationFormatException e) {
    throw new DescriptionReadException(e);
  }
catch (  IOException e) {
    throw new DescriptionReadException(e);
  }
catch (  JDOMException e) {
    throw new DescriptionReadException(e);
  }
}
