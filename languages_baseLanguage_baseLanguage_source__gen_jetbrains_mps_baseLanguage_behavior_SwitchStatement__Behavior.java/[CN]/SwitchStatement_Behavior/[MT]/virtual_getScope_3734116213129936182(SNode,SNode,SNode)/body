    }
    List<SNode> variables=new ArrayList<SNode>();
    for (    SNode caseNode : SLinkOperations.getChildren(thisNode,MetaAdapterFactory.getContainmentLink(0xf3061a5392264cc5L,0xa443f952ceaf5816L,0x10ef02a8c6aL,0x10ef02edcafL,"case"))) {
