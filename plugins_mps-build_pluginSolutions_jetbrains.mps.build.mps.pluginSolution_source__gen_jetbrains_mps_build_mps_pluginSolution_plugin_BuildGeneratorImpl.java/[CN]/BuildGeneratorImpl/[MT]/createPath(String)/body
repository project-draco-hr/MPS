    SLinkOperations.setNewChild(compositePart,"tail","jetbrains.mps.build.structure.BuildCompositePath");
    compositePart=SLinkOperations.getTarget(compositePart,"tail",true);
  }
  SNodeOperations.deleteNode(compositePart);
