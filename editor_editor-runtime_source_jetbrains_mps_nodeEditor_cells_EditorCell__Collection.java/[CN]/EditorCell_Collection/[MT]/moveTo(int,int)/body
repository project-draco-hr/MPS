{
  if (x == myX && y == myY) {
    return;
  }
  int xOld=myX;
  int yOld=myY;
  super.moveTo(x,y);
  if (wasRelayoutRequested()) {
    return;
  }
  for (  jetbrains.mps.nodeEditor.cells.EditorCell myEditorCell : getCells()) {
    myEditorCell.moveTo(myEditorCell.getX() + x - xOld,myEditorCell.getY() + y - yOld);
  }
  getCellLayout().move(this,x - xOld,y - yOld);
}
