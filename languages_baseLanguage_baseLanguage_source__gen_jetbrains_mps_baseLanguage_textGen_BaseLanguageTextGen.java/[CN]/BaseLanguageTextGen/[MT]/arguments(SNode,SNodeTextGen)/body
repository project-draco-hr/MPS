{
  textGen.append("(");
{
    Iterable<SNode> collection=SLinkOperations.getTargets(methodCall,"actualArgument",true);
    final SNode lastItem=Sequence.fromIterable(collection).last();
    for (    SNode item : collection) {
      textGen.appendNode(item);
      if (item != lastItem) {
        textGen.append(", ");
      }
    }
  }
  textGen.append(")");
}
