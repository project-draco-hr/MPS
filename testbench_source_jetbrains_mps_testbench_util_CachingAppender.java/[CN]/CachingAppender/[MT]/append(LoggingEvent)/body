{
  if (!isExpected(loggingEvent)) {
    events++;
    messages.add(loggingEvent.getRenderedMessage());
    String[] stackTrace=loggingEvent.getThrowableStrRep();
    if (stackTrace != null) {
      messages.add("++ =============StackTrace================");
      messages.addAll(Arrays.asList(stackTrace));
      messages.add("-- =============StackTrace================");
    }
  }
}
