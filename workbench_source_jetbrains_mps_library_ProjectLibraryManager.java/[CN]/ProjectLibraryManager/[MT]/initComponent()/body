{
  if (!ThreadUtils.isEventDispatchThread()) {
    ApplicationManager.getApplication().invokeAndWait(new Runnable(){
      @Override public void run(){
        ProjectLibraryManager.super.initComponent();
      }
    }
,ModalityState.defaultModalityState());
  }
 else {
    ProjectLibraryManager.super.initComponent();
  }
}
