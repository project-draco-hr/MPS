{
  for (  SModel model : getModels()) {
    if (reference.getModelId() != null) {
      if (model.getModelReference().equals(reference)) {
        return model;
      }
    }
 else {
      if (model.getModelName().equals(reference.getModelName())) {
        return model;
      }
    }
  }
  return null;
}
