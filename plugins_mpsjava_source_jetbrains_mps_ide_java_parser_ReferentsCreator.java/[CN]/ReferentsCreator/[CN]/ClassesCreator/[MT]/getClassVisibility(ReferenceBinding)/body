{
  SModel model=myReferentsCreator.myCurrentModel;
  if (b.isPublic()) {
    return PublicVisibility.newInstance(model);
  }
 else   if (b.isPrivate()) {
    return PrivateVisibility.newInstance(model);
  }
 else   if (b.isProtected()) {
    return ProtectedVisibility.newInstance(model);
  }
 else {
    return null;
  }
}
