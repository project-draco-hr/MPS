    if (ListSequence.fromList(SLinkOperations.getTargets(SLinkOperations.getTarget(ifStmt,"ifTrue",true),"statement",true)).last() == thisNode || SLinkOperations.getTarget(ifStmt,"ifFalseStatement",true) == thisNode) {
