{
  try {
    IFacet javaCompileFacet=JavaCompile_Facet.class.newInstance();
    IFacet reloadClassesFacet=ReloadClasses_Facet.class.newInstance();
    FacetRegistry.getInstance().register(BaseLanguageFacets_CustomApplicationPlugin.this.blNamespace,javaCompileFacet);
    FacetRegistry.getInstance().register(BaseLanguageFacets_CustomApplicationPlugin.this.blNamespace,reloadClassesFacet);
  }
 catch (  Throwable t) {
    throw new RuntimeException(t);
  }
}
