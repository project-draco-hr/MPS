{
  if (mySource.getReference(myRole) != null && mySource.getReferenceTarget(myRole) == null) {
    mySource.removeReference(mySource.getReference(myRole));
  }
 else {
    List<SNode> nodes=new ArrayList<SNode>();
    nodes.add(mySource);
    for (    SNode node : nodes) {
      node.delete();
    }
  }
}
