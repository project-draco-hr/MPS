{
  if (mySModel == null) {
    updateDiskTimestamp();
    return;
  }
  final jetbrains.mps.smodel.SModel result=((jetbrains.mps.smodel.SModel)getSource().loadSModel((IModule)myModule,this));
  updateDiskTimestamp();
  replaceModel(new Runnable(){
    @Override public void run(){
      mySModel=result;
    }
  }
);
}
