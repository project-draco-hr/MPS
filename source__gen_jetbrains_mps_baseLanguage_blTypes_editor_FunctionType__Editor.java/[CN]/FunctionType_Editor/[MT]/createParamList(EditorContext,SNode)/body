{
  if (this.myParamListHandler == null) {
    this.myParamListHandler=new FunctionType_Editor_ParamListHandler(node,"param",context);
  }
  EditorCell_Collection editorCell=this.myParamListHandler.createCells(context,new CellLayout_Horizontal(),false);
  editorCell.setSelectable(false);
  editorCell.setDrawBorder(false);
  editorCell.setGridLayout(false);
  editorCell.setDrawBrackets(false);
  editorCell.setBracketsColor(Color.black);
  editorCell.setUsesBraces(false);
  editorCell.setCanBeFolded(false);
  editorCell.putUserObject(EditorCell.ROLE,this.myParamListHandler.getElementRole());
  editorCell.setLayoutConstraint("");
  ((EditorCell_Basic)editorCell).setParseable(false);
  return editorCell;
}
