{
  if (type == null)   return null;
  if (type instanceof RuntimeTypeVariable && !(type instanceof RuntimeErrorType)) {
    return ((SNode)type).getName();
  }
  if (type instanceof String) {
    return (String)type;
  }
  if (type instanceof SNode) {
    return toString_1((SNode)type);
  }
  return null;
}
