{
  if (value == null) {
    return true;
  }
  Iterator<OrangeTaste> constants=ListSequence.fromList(OrangeTaste.getConstants()).iterator();
  while (constants.hasNext()) {
    OrangeTaste constant=constants.next();
    if (value.equals(constant.getName())) {
      return true;
    }
  }
  return false;
}
