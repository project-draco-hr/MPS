{
  try {
    myOwner=owner;
    myConverter=ConverterFactory.createClassPathConverter(this,root,getClassPathItem(),owner);
    Set<SModelDescriptor> result=new HashSet<SModelDescriptor>();
    addPackageModelDescriptors(result,root,root.getPrefix());
    return result;
  }
  finally {
    myOwner=null;
  }
}
