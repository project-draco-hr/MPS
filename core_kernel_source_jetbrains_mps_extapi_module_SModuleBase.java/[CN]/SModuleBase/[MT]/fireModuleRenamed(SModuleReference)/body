{
  assertCanChange();
  for (  SModuleListener listener : myListeners) {
    try {
      if (listener instanceof SModuleListenerBase) {
        ((SModuleListenerBase)listener).moduleRenamed(this,oldRef);
      }
    }
 catch (    VirtualMachineError e) {
      throw e;
    }
catch (    Throwable t) {
      LOG.error("",t);
    }
  }
}
