{
  Component dialog=openDialog();
  checkProjectStep();
  pressButton(dialog,"Next");
  flushAWT();
  checkLanguageStep();
  pressButton(dialog,"Next");
  flushAWT();
  checkSolutionStep();
  pressButton(dialog,"Finish");
  flushAWT();
  checkDialogDisposed();
  checkNoAdditionalDialogs();
  flushAWT();
  Project p=waitProjectLoaded();
  checkProject(p);
  flushAWT();
}
