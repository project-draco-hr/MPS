{
  if (m == null) {
    return;
  }
  ModelAccess.instance().runReadAction(new Runnable(){
    public void run(){
      for (      IModule module : myThis.getProject().getScope().getVisibleModules()) {
        if (module.getModuleFqName().equals(m)) {
          myThis.setModule(module);
          break;
        }
      }
    }
  }
);
}
