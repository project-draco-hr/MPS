{
  if (m == null) {
    myThis.setModule(null);
  }
  ModelAccess.instance().runReadAction(new Runnable(){
    public void run(){
      for (      IModule module : GlobalScope.getInstance().getVisibleModules()) {
        if (module.getModuleFqName().equals(m)) {
          myThis.setModule(module);
          return;
        }
      }
      myThis.setModule(null);
    }
  }
);
}
