{
  if (needsReloading()) {
    LOG.warning("Model file " + getSModel().getSModelReference().getSModelFqName() + " was modified externally!\n"+ "You might want to turn \"Synchronize files on frame activation/deactivation\" option on to avoid conflicts.");
    resolveDiskConflict();
    return false;
  }
  if (needsReloading())   return false;
  return true;
}
