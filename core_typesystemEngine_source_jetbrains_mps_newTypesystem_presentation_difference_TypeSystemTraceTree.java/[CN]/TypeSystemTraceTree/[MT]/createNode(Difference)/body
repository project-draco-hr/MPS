{
  TypeSystemTraceTreeNode result=new TypeSystemTraceTreeNode(diff,myOperationContext);
  if (diff.getChildren() != null) {
    for (    Difference child : diff.getChildren()) {
      TypeSystemTraceTreeNode node=createNode(child);
      if (showNode(child)) {
        result.add(node);
      }
    }
  }
  return result;
}
