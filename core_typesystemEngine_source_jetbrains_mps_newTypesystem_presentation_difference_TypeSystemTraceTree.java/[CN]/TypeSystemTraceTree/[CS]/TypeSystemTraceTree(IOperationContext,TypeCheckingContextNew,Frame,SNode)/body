{
  myOperationContext=operationContext;
  myTypeCheckingContextNew=tcc;
  myDifference=tcc.getOperation();
  myFrame=frame;
  mySelectedNode=node;
  myNodes=new HashSet<SNode>();
  myNodes.add(node);
  if (mySelectedNode != null) {
    getEquivalentVars(myDifference);
  }
  this.rebuildNow();
  expandAll();
}
