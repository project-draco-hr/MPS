{
  String str=null;
  str=psiTypeElem.getText();
  int firstBracket=str.indexOf("<");
  if (firstBracket > 0) {
    str=str.substring(0,firstBracket);
  }
  PsiType psiType=psiTypeElem.getType();
  if (psiType instanceof PsiArrayType && !(str.endsWith("[]"))) {
    str=str + "[]";
  }
  return str;
}
