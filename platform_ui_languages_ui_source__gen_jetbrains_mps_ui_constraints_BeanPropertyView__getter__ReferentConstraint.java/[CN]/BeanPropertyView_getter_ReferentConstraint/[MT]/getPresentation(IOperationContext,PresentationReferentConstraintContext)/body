{
  String gn=SPropertyOperations.getString(_context.getParameterNode(),"name");
  if (gn == null) {
    return null;
  }
  if (gn.startsWith("is")) {
    return Introspector.decapitalize(gn.substring(2));
  }
  if (gn.startsWith("get")) {
    return Introspector.decapitalize(gn.substring(3));
  }
  return null;
}
