{
  Iterator<SNode> iterator=Sequence.fromIterable(AttributeOperations.getChildNodesAndAttributes(parent,containmentLink)).iterator();
  SNode prev=null;
  SNode next;
  while (iterator.hasNext()) {
    next=iterator.next();
    if (next == anchor) {
      return prev;
    }
    prev=next;
  }
  return null;
}
