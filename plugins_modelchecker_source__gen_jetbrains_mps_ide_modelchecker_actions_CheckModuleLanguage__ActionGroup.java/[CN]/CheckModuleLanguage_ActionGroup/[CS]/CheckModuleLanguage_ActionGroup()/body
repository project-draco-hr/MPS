{
  super("CheckModuleLanguage",ID);
  this.setIsInternal(false);
  this.setPopup(false);
  try {
    CheckModuleLanguage_ActionGroup.this.addParameterizedAction(new CheckModule_Action("Language"),PluginId.getId("jetbrains.mps.ide.modelchecker"),"Language");
