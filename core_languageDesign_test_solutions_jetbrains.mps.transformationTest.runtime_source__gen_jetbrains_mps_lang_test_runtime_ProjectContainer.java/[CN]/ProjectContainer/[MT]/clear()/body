{
  final Map<String,MPSProject> projects=new HashMap<String,MPSProject>();
  try {
    SwingUtilities.invokeAndWait(new Runnable(){
      public void run(){
        for (        MPSProject project : projects.values()) {
          project.dispose();
        }
        MapSequence.fromMap(projects).clear();
      }
    }
);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
