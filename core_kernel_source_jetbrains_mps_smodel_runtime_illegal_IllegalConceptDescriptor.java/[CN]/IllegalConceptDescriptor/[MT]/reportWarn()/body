{
  if (myReported) {
    return;
  }
  myReported=true;
  String msg="IllegalConceptDescriptor created for concept " + myConcept + ". Please check the language "+ myConcept.getLanguage()+ " is built and compiled.";
  if (!ourReportedConcepts.contains(myConcept)) {
    ourReportedConcepts.add(myConcept);
    LOG.warn(msg);
  }
}
