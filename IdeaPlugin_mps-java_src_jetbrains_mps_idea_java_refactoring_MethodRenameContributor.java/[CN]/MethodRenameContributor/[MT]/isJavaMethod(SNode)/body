{
  SConcept concept=node.getConcept();
  return concept.isSubConceptOf(MetaAdapterFactoryByName.getConcept("jetbrains.mps.baseLanguage.structure.InstanceMethodDeclaration")) || concept.isSubConceptOf(MetaAdapterFactoryByName.getConcept("jetbrains.mps.baseLanguage.structure.StaticMethodDeclaration")) || concept.isSubConceptOf(MetaAdapterFactoryByName.getConcept("jetbrains.mps.baseLanguage.structure.ConstructorDeclaration"));
}
