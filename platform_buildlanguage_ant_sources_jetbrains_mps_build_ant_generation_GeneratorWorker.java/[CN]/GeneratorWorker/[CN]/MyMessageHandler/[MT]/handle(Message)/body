{
switch (msg.getKind()) {
case ERROR:
    GeneratorWorker.this.error(msg.getText());
  myGenerationErrors.add(msg.getText());
break;
case WARNING:
GeneratorWorker.this.warning(msg.getText());
myGenerationWarnings.add(msg.getText());
break;
case INFORMATION:
GeneratorWorker.this.verbose(msg.getText());
break;
}
}
