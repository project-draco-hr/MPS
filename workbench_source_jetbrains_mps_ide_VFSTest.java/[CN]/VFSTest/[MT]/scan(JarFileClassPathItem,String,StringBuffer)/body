{
  Set<String> subpackages=pathItem.getSubpackages(namespace);
  for (  String s : subpackages) {
    sb.append(s);
    sb.append("\n");
    scan(pathItem,s,sb);
  }
  Set<String> classes=pathItem.getAvailableClasses(namespace);
  for (  String clname : classes) {
    sb.append(clname);
    sb.append("\n");
  }
}
