{
  if (myText.getText().length() == 0) {
    myText.setBackground(Color.white);
    setPrevNextEnabled(false);
    myText.requestFocus();
    return;
  }
  selectCell();
  if (myCells.isEmpty()) {
    myText.setBackground(myBadSequenceColor);
    setPrevNextEnabled(false);
    return;
  }
  if (myText.getBackground() == myBadSequenceColor) {
    myText.setBackground(Color.white);
  }
  setPrevNextEnabled(true);
}
