{
  if (editorCell.getSubstituteInfo() == null || editorCell.getSubstituteInfo() instanceof DefaultSubstituteInfo) {
    editorCell.setSubstituteInfo(new OldNewCompositeSubstituteInfo(myEditorContext,new SChildSubstituteInfo(editorCell,myOwnerNode,MetaAdapterFactory.getContainmentLink(0x6106f6117a7442d1L,0x80deedc5c602bfd1L,0x58940e88f3ef74c9L,0x10efa534a50ae17fL,"input"),child),new DefaultChildSubstituteInfo(myOwnerNode,myContainmentLink.getDeclarationNode(),myEditorContext)));
  }
  if (editorCell.getRole() == null) {
    editorCell.setRole("input");
  }
}
