{
  ThreadReference thread=event.thread();
  if (thread == null) {
    return defaultStepType();
  }
  try {
    if (thread.frameCount() == 0) {
      return defaultStepType();
    }
    StackFrame frame=thread.frame(0);
    return nextStep(thread,frame);
  }
 catch (  IncompatibleThreadStateException e) {
    return defaultStepType();
  }
}
