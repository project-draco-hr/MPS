          _reporter_2309309498.addIntentionProvider(intentionProvider);
        }
      }
    }
  }
  for (  SProperty prop : Sequence.fromIterable(node.getProperties())) {
    if (((SPropertyAdapter)prop).getPropertyDescriptor() == null) {
