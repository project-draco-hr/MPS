{
  if (myProcessingBans == 0) {
    LOG.assertLog(myOperation == null,"Vcs operations scheduler has not processed tasks:\n" + myOperation + "\nThat's weird cause processing is allowed.");
    processTask(task);
  }
 else {
    if (myOperation == null) {
      myOperation=task;
    }
 else {
      myOperation.absorb(task);
    }
  }
}
