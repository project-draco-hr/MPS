{
  try {
    final Wrappers._T<SModel> sModel=new Wrappers._T<SModel>();
    ModelAccess.instance().runReadAction(new Runnable(){
      public void run(){
        sModel.value=ModelPersistence.readModel(modelContent,false);
      }
    }
);
    return sModel.value;
  }
 catch (  Exception e) {
