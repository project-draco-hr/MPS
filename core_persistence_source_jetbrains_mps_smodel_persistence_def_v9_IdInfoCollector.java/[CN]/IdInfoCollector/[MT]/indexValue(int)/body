{
  char[] rv=new char[6];
  rv[5]=ourIndexChars[v & 0x3F];
  v>>=6;
  rv[4]=ourIndexChars[v & 0x3F];
  v>>=6;
  rv[3]=ourIndexChars[v & 0x3F];
  v>>=6;
  rv[2]=ourIndexChars[v & 0x3F];
  v>>=6;
  rv[1]=ourIndexChars[v & 0x3F];
  v>>=6;
  v&=0x3;
  if (v != 0) {
    rv[0]=ourIndexChars[v];
    return new String(rv);
  }
  return new String(rv,1,5);
}
