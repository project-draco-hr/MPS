{
  ListOperations.addElement(_results,node);
  if (SNodeOperations.getAncestor(node,"jetbrains.mps.baseLanguage.structure.ClassConcept",false,false) != null) {
    for (    SNode fieldNode : this.executeFinder("jetbrains.mps.baseLanguage.findUsages.OverridingFields_Finder",node,scope,new NullAdaptiveProgressMonitor())) {
