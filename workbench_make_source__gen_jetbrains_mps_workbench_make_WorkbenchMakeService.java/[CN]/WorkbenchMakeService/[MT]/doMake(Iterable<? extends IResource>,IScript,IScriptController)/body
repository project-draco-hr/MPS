{
  Future<IResult> result=null;
  try {
    awaitCurrentProcess();
    result=_doMake(inputRes,script,controller);
  }
  finally {
    if (result == null || result.isDone()) {
      if (!(currentSessionStickyMark.isMarked())) {
        currentSessionStickyMark.set(null,false);
      }
    }
  }
  return result;
}
