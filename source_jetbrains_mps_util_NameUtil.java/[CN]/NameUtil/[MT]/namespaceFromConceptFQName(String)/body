{
  if (fqName == null) {
    return fqName;
  }
  int offset=fqName.lastIndexOf('.');
  if (offset < 0) {
    return "";
  }
  String namespace=fqName.substring(0,offset);
  int i=namespace.lastIndexOf(".structure");
  if (i >= 0) {
    namespace=namespace.substring(0,i);
  }
  return namespace;
}
