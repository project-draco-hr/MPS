{
  try {
    Object inst=new FacetManifest();
    Class<?> mf=inst.getClass();
    Facets_CustomApplicationPlugin.this.manifest=inst;
    Method mth=mf.getMethod("registerFacets",FacetRegistry.class);
    mth.invoke(inst,FacetRegistry.getInstance());
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
