{
  myEventsHandler.addEventListener(new GDBEventsAdapter(){
    @Override public void breakpointHit(    AsyncAnswer answer,    SimpleConsoleProcessHandler gdbProcess){
      StackInfoRequest request=new StackInfoRequest(){
        @Override public void onRequestFulfilled(        ResultAnswer answer,        List<StreamAnswer> receivedStreamAnswers){
          processStackOnPause(answer);
        }
      }
;
      myRequestManager.createRequest(request);
    }
  }
);
}
