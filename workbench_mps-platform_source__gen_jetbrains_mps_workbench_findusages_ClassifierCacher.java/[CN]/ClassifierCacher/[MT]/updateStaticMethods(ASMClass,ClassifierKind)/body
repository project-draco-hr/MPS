{
  for (  ASMMethod m : ac.getDeclaredMethods()) {
    if (!(m.isStatic())) {
      continue;
    }
    if (m.isCompilerGenerated()) {
      continue;
    }
    if (kind == ClassifierKind.ENUM && isGeneratedEnumMethod(m)) {
      continue;
    }
    instance("jetbrains.mps.baseLanguage.structure.StaticMethodDeclaration");
    createVisibility(m);
    updateBaseMethod(m);
  }
}
