{
  new Thread(){
    @Override public void run(){
      try {
        for (int c; (c=from.read()) >= 0; ) {
          to.write(c);
        }
      }
 catch (      IOException e) {
      }
      try {
        barrier.await();
      }
 catch (      InterruptedException e) {
      }
catch (      BrokenBarrierException e) {
      }
    }
  }
.start();
}
