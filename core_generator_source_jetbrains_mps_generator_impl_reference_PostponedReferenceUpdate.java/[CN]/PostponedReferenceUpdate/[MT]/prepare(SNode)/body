{
  for (  SReference ref : node.getReferences()) {
    if (ref instanceof PostponedReference) {
      PostponedReference pr=(PostponedReference)ref;
      pr.initReplacementReference(myGenerator);
      myRefs.add(pr);
    }
  }
  for (  SNode child : node.getChildren()) {
    prepare(child);
  }
}
