{
  if (pattern.startsWith("@")) {
    pattern=pattern.substring(1);
  }
  ArrayList<String> result=new ArrayList<String>();
  try {
    for (    String name : names) {
      if (isCancelled.compute()) {
        break;
      }
      if (matcher.nameMatches(pattern,name)) {
        result.add(name);
      }
    }
  }
 catch (  Exception e) {
  }
  return result;
}
