{
  Component dialog=openNewProjectDialog(test,null);
  test.pressButton(dialog,"Next");
  test.flushAWT();
  test.pressButton(dialog,"Next");
  test.flushAWT();
  test.pressButton(dialog,"Finish");
  test.flushAWT();
  MPSProject project=waitProjectLoaded(test);
  test.flushAWT();
  return project;
}
