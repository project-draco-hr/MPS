{
  if (node == null) {
    myBuffer.append("???");
    if (mySNode != null) {
      myBuffer.foundError("possible broken reference in " + SNodeOperations.getDebugText(mySNode),mySNode,null);
    }
    return;
  }
  TextGen.getTextGenForNode(node).generateText(new TextGenTransitionContext(node,myBuffer));
}
