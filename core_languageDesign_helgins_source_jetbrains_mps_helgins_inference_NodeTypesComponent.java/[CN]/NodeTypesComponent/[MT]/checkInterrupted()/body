{
  try {
    if (myIsInterrupted) {
      try {
        solveInequationsAndExpandTypes();
        performActionsAfterChecking();
      }
  finally {
        myNotSkippedNodes.clear();
        clearEquationManager();
        if (myRootNode.isRoot()) {
          myTypeChecker.markAsChecked(myRootNode);
        }
      }
    }
  }
  finally {
    myIsInterrupted=false;
  }
}
