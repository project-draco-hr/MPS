{
  super("Set return type in constructors");
  this.addRefactoring(new AbstractMigrationRefactoring(operationContext){
    public String getName(){
      return "Set return type in constructors";
    }
    public String getAdditionalInfo(){
      return "Set return type in constructors";
    }
    public String getFqNameOfConceptToSearchInstances(){
      return "jetbrains.mps.baseLanguage.structure.ConstructorDeclaration";
    }
    public boolean isApplicableInstanceNode(    SNode node){
      return (SLinkOperations.getTarget(node,"returnType",true) == null);
    }
    public void doUpdateInstanceNode(    SNode node){
      SLinkOperations.setNewChild(node,"returnType","jetbrains.mps.baseLanguage.structure.VoidType");
    }
    public boolean isShowAsIntention(){
      return true;
    }
  }
);
}
