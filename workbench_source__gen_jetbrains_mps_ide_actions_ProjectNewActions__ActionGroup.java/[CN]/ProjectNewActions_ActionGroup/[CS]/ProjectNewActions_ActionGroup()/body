{
  super("New",ID);
  this.setIsAlwaysVisible(true);
  this.setIsInternal(false);
  this.setPopup(true);
  try {
    this.add(new NewSolutionAction());
    this.add(new NewLanguageAction());
    this.add(new NewDevKitAction());
  }
 catch (  Throwable t) {
    LOG.error("User group error",t);
  }
}
