{
  if (myAdded) {
    throw new IllegalStateException("Trying to add tree node which have already been added");
  }
  try {
    onAdd();
  }
 catch (  Throwable t) {
    LOG.error(t);
  }
  myAdded=true;
  if (!isInitialized()) {
    return;
  }
  for (int i=0; i < getChildCount(); i++) {
    MPSTreeNode node=(MPSTreeNode)getChildAt(i);
    node.addThisAndChildren();
  }
}
