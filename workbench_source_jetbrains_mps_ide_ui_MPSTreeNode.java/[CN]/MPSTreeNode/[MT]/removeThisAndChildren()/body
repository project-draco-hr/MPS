{
  if (!myAdded) {
    throw new IllegalStateException("Trying to remove tree node which have not been added");
  }
  try {
    onRemove();
  }
 catch (  Throwable t) {
    LOG.error(t);
  }
  myAdded=false;
  if (!isInitialized()) {
    return;
  }
  for (  MPSTreeNode node : this) {
    node.removeThisAndChildren();
  }
}
