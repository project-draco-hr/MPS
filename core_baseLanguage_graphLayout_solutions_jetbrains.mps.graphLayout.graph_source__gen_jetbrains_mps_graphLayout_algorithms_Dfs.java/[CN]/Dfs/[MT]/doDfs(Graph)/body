{
  myGraph=graph;
  myDfsState=new NodeMap<Integer>(graph);
  for (  Node node : ListSequence.fromList(myGraph.getNodes())) {
    MapSequence.fromMap(myDfsState).put(node,BEFORE);
  }
  for (  Node node : ListSequence.fromList(myGraph.getNodes())) {
    if (MapSequence.fromMap(myDfsState).get(node) == BEFORE) {
      processRoot();
      dfs(node);
    }
  }
}
