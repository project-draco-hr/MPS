{
  myOldOut=System.out;
  myOldErr=System.err;
  for (Enumeration<Appender> en=Logger.getRootLogger().getAllAppenders(); en.hasMoreElements(); )   en.nextElement().addFilter(myFilter);
  System.err.flush();
  System.setErr(new PrintStream(myErr));
  System.out.flush();
  System.setOut(new PrintStream(myOut));
}
