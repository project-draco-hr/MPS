{
  if (value == null) {
    return null;
  }
  Iterator<MqlSubstringKind> constants=ListSequence.fromList(MqlSubstringKind.getConstants()).iterator();
  while (constants.hasNext()) {
    MqlSubstringKind constant=constants.next();
    if (value.equals(constant.getName())) {
      return constant.getValueAsString();
    }
  }
  return null;
}
