{
  if (value == null) {
    return null;
  }
  Iterator<RestrictionKind> constants=RestrictionKind.getConstants().iterator();
  while (constants.hasNext()) {
    RestrictionKind constant=constants.next();
    if (value.equals(constant.getName())) {
      return constant.getValueAsString();
    }
  }
  return null;
}
