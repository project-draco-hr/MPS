{
  myModelAccess.runWriteAction(new Runnable(){
    public void run(){
      ApplicationManager.getApplication().getComponent(JdkStubSolutionManager.class).releaseSdk(myModule);
      if (myModule.isDisposed()) {
        return;
      }
      setModuleDescriptor(getModuleDescriptor());
    }
  }
);
}
