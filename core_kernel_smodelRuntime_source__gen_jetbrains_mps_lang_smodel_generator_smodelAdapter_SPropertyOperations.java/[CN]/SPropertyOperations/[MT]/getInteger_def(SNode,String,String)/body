{
  if (node != null) {
    String value=node.getProperty(propertyName);
    try {
      return Integer.parseInt(value);
    }
 catch (    Exception e) {
    }
  }
  if (defaultValue != null) {
    try {
      return Integer.parseInt(defaultValue);
    }
 catch (    Exception e) {
    }
  }
  return 0;
}
