{
  try {
{
      SNode node=context.getNode();
      if (node != null) {
        if (!(SNodeOperations.isInstanceOf(node,"jetbrains.mps.baseLanguage.structure.CommentedStatementsBlock"))) {
          node=null;
        }
      }
      this.node=node;
    }
    if (this.node == null) {
      return false;
    }
  }
 catch (  Throwable t) {
    return false;
  }
  return true;
}
