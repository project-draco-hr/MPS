{
  if (!(super.collectActionData(event))) {
    return false;
  }
{
    SNode node=event.getData(MPSDataKeys.SNODE);
    if (node != null) {
      if (!(SNodeOperations.isInstanceOf(node,"jetbrains.mps.lang.core.structure.BaseConcept"))) {
        node=null;
      }
    }
    this.node=node;
  }
  if (this.node == null) {
    return false;
  }
  return true;
}
