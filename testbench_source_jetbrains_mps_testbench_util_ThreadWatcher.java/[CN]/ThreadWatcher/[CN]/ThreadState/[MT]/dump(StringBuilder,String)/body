{
  String sep="";
  for (  long id : runningThreads.keys()) {
    sb.append(sep).append(pref).append("Running thread ").append(id).append(" \"").append(runningThreads.get(id).getThreadName()).append("\"");
    sep="\n";
  }
  for (  long id : allThreads.keys()) {
    if (!runningThreads.contains(id)) {
      sb.append(sep).append(pref).append("Inactive thread ").append(id).append(" \"").append(allThreads.get(id).getThreadName()).append("\"");
      sep="\n";
    }
  }
}
