{
  if (file == null) {
    myName="Dummy project";
    return;
  }
  if (!file.exists()) {
    myName=file.getName();
    return;
  }
  try {
    Document document=JDOMUtil.loadDocument(file);
    Element projectElement=document.getRootElement();
    loadProjectDescriptorFromElement(file,projectElement);
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
