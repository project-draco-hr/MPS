{
  CellProviderWithRole provider=aProvider;
  provider.setAuxiliaryCellProvider(new PrimitiveTypeRef_Editor._Inline());
  EditorCell editorCell=provider.createEditorCell(context);
  PrimitiveTypeRef_Editor.setupBasic_DescriptorReferenceCell(editorCell,node,context);
  if (editorCell instanceof EditorCell_Label) {
    PrimitiveTypeRef_Editor.setupLabel_DescriptorReferenceCell((EditorCell_Label)editorCell,node,context);
  }
  editorCell.setSubstituteInfo(provider.createDefaultSubstituteInfo());
  return editorCell;
}
