{
  return ModelAccess.instance().tryRead(new Computable<Point>(){
    public Point compute(){
      if (myRootCell == null) {
        return null;
      }
      EditorCell cell=myRootCell.findLeaf(event.getX(),event.getY());
      if (cell == null) {
        return null;
      }
      return new Point(cell.getX(),cell.getY() + cell.getHeight());
    }
  }
);
}
