{
  if (USE_NEW_TOOLTIPS) {
    MPSToolTipManager.getInstance().hideToolTip();
  }
 else {
    try {
      ToolTipManager toolTipManager=ToolTipManager.sharedInstance();
      toolTipManager.mousePressed(new MouseEvent(this,Event.MOUSE_MOVE,0,0,0,0,0,false));
    }
 catch (    Exception e) {
      LOG.error(e);
    }
  }
}
