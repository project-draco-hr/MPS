{
  EditorCell parent=cell;
  while (parent != null) {
    if (cell.getBounds().getMaxY() < parent.getBounds().getMaxY()) {
      return null;
    }
    List<HighlighterMessage> messages=parent.getMessages(HighlighterMessage.class);
    if (!messages.isEmpty()) {
      return messages.get(0);
    }
    parent=parent.getParent();
  }
  return null;
}
