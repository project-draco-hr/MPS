{
  return new BasePropertyConstraintsDescriptor(propertyName,container){
    @Override public boolean hasOwnGetter(){
      return getter != null;
    }
    @Override public boolean hasOwnSetter(){
      return setter != null;
    }
    @Override public boolean hasOwnValidator(){
      return validator != null;
    }
    @Override public Object getValue(    SNode node,    IScope scope){
      if (getter != null) {
        return getter.execPropertyGet(node,propertyName,scope);
      }
 else {
        return super.getValue(node,scope);
      }
    }
    @Override public void setValue(    SNode node,    String value,    IScope scope){
      if (setter != null) {
        setter.execPropertySet(node,propertyName,value,scope);
      }
 else {
        super.setValue(node,value,scope);
      }
    }
    @Override public boolean validateValue(    SNode node,    String value,    IScope scope){
      if (validator != null) {
        return validator.checkPropertyValue(node,propertyName,value,scope);
      }
 else {
        return super.validateValue(node,value,scope);
      }
    }
  }
;
}
