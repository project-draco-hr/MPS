{
  if (this.myStatementListListHandler_statementListList_ == null) {
    this.myStatementListListHandler_statementListList_=new TargetDeclaration_Editor._RefNodeListHandler3(node,"statementList",context);
  }
  EditorCell_Collection editorCell=this.myStatementListListHandler_statementListList_.createCells(context,new CellLayout_Vertical(),false);
  TargetDeclaration_Editor.setupBasic_StatementListList(editorCell,node,context);
  editorCell.setGridLayout(false);
  editorCell.setUsesBraces(false);
  editorCell.setCanBeFolded(false);
  editorCell.putUserObject(EditorCell.ROLE,this.myStatementListListHandler_statementListList_.getElementRole());
  return editorCell;
}
