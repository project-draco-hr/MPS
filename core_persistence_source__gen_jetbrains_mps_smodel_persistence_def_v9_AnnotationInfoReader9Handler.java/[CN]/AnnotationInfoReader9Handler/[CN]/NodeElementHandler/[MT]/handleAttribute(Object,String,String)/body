{
  Void result=(Void)resultObject;
  if ("id".equals(name)) {
    try {
      my_accumulatorField.pushNode(my_readHelperParam.getIdEncoder().parseNodeId(value),myLocator);
    }
 catch (    IdEncoder.EncodingException e) {
      throw new IllegalArgumentException(e);
    }
