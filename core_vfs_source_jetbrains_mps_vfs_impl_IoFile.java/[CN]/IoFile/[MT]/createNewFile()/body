{
  try {
    File parentFile=myFile.getParentFile();
    if (parentFile.exists()) {
      if (!parentFile.isDirectory()) {
        return false;
      }
    }
 else {
      if (!parentFile.mkdirs()) {
        return false;
      }
    }
    if (myFile.exists()) {
      return myFile.isFile();
    }
    return myFile.createNewFile();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
