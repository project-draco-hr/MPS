  this.add(new RemoveModuleFromProjectAction(),this);
  this.addSeparator();
  this.add(new MakeModuleAction(),this);
  this.add(new CleanModuleAction(),this);
  this.addSeparator();
  this.add(new SetModuleFolderAction(),this);
}
