{
  EditorCell rawCell=getRawCell(editorComponent,node);
  if (rawCell == null) {
    return null;
  }
  EditorCell child=rawCell.findChild(CellFinders.byCondition(new Condition<EditorCell>(){
    @Override public boolean met(    EditorCell cell){
      return role.equals(cell.getRole()) && node == cell.getSNode();
    }
  }
,true),true);
  if (child != null) {
    return child;
  }
  return rawCell;
}
