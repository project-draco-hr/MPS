{
  List<SNode> resNodes=ListSequence.fromList(ListSequence.<SNode>fromArray()).addSequence(SetSequence.fromSet(FindUsagesManager.getInstance().findInstances((AbstractConceptDeclaration)((AbstractConceptDeclaration)SNodeOperations.getAdapter(node)),scope,new FindUsagesManager.ProgressAdapter(indicator),false)));
  for (  SNode resNode : resNodes) {
    ListOperations.addElement(_results,resNode);
  }
}
