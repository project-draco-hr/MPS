{
  LOG.error(message);
  if (inputNode != null) {
    LOG.error("-- was input: " + inputNode.getDebugText(),inputNode);
  }
  if (templateNode != null) {
    LOG.error("-- was template: " + templateNode.getDebugText(),templateNode);
  }
  if (ruleNode != null) {
    LOG.error("-- was rule: " + ruleNode.getDebugText(),ruleNode);
  }
}
