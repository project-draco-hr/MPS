{
  return "Change to " + ((SConceptOperations.isExactly(myParameter,"jetbrains.mps.baseLanguage.collections.structure.ForEachStatement") ? "For Each" : SPropertyOperations.getString(myParameter,"conceptAlias"))) + ((SConceptOperations.isExactly(myParameter,"jetbrains.mps.baseLanguage.structure.ForeachStatement") ? "each" : ""))+ (SConceptOperations.isExactly(myParameter,"jetbrains.mps.baseLanguage.structure.DoWhileStatement") ? "-While" : "")+ " statement";
