{
  EditorCell result=first;
  while (result != null) {
    if (result.isSelectable()) {
      boolean common=true;
      for (      EditorCell cell : cells) {
        if (!result.isAncestorOf(cell) && result != cell) {
          common=false;
          break;
        }
      }
      if (common)       return result;
    }
    result=result.getParent();
  }
  return null;
}
