{
  T oldItem=myArray[index];
  T[] oldArray=myArray;
  T[] newArray=(T[])Array.newInstance(myItemType,oldArray.length - 1);
  System.arraycopy(oldArray,0,newArray,0,index);
  System.arraycopy(oldArray,index + 1,newArray,index,oldArray.length - index - 1);
  myArray=newArray;
  setArray(newArray);
  return oldItem;
}
