{
  File tmp=getTempDir();
  for (int i=0; ; ++i) {
    if (!new File(tmp,MPSTEMP + i).exists()) {
      File tmpDir=new File(tmp,MPSTEMP + i);
      boolean result=tmpDir.mkdir();
      if (!result) {
        throw new IllegalStateException("Could not create a directory " + tmpDir);
      }
      return tmpDir;
    }
  }
}
