{
  SModelUID modelUID=model.getUID();
  SModel newModel=new SModel(new SModelUID(modelUID.getLongName(),SModelStereotype.INTERNAL_COPY));
  for (  String language : model.getUserDefinedLanguageNamespaces())   newModel.addLanguage(language);
  for (  SModelUID importedModel : model.getImportedModelUIDs())   newModel.addImportedModel(importedModel);
  return newModel;
}
