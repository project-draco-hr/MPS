{
  ModelAccess.assertLegalRead();
  SModel result;
  boolean fireInitialized=false;
synchronized (myLoadingLock) {
    if (mySModel == null) {
      result=loadModel();
      doPostLoadStuff();
      fireInitialized=true;
      mySModel=result;
    }
 else {
      result=mySModel;
    }
  }
  if (fireInitialized) {
    result.fireModelInitialized();
  }
  return result;
}
