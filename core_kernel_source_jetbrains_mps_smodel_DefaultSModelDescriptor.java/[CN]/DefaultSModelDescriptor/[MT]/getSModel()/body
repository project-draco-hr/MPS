{
  SModel result;
  boolean fireInitialized=false;
synchronized (myLoadingLock) {
    if (mySModel == null) {
      mySModel=loadModel();
      updateAfterCreation(mySModel);
      doPostLoadStuff();
      fireInitialized=true;
    }
    result=mySModel;
  }
  if (fireInitialized) {
    result.fireModelInitialized();
  }
  return result;
}
