{
  if (!myModelRootManager.isFindUsagesSupported())   return false;
  Set<String> strings=(mySModel == null || !SModelRepository.getInstance().isChanged(mySModel)) ? Collections.singleton(language.getNamespace()) : Collections.<String>emptySet();
  if (!myModelRootManager.containsSomeString(this,strings))   return false;
  getSModel();
  if (mySModel != null) {
    return mySModel.hasLanguage(language.getModuleReference());
  }
  return false;
}
