{
  if (errorText != null) {
    StringTokenizer tokenizer=new StringTokenizer(errorText,"\n");
    errorText="";
    while (errorText.length() == 0 && tokenizer.hasMoreTokens()) {
      errorText=tokenizer.nextToken();
    }
    if (tokenizer.hasMoreTokens()) {
      errorText+="...";
    }
  }
  myErrorLabel.setText(errorText);
}
