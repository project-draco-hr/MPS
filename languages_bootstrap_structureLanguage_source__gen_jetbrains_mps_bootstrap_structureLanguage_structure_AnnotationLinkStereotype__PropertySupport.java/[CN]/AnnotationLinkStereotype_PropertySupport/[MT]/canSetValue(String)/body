{
  if (value == null) {
    return true;
  }
  Iterator<AnnotationLinkStereotype> constants=AnnotationLinkStereotype.getConstants().iterator();
  while (constants.hasNext()) {
    AnnotationLinkStereotype constant=constants.next();
    if (value.equals(constant.getName())) {
      return true;
    }
  }
  return false;
}
