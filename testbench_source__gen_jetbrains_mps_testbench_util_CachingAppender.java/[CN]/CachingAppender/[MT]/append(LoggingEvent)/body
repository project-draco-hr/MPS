{
  if (!(isExpected(loggingEvent))) {
    myEvents++;
    myMessages.add(loggingEvent.getRenderedMessage());
    String[] stackTrace=loggingEvent.getThrowableStrRep();
    if (stackTrace != null) {
      myMessages.add("++ =============StackTrace================");
      myMessages.addAll(Arrays.asList(stackTrace));
      myMessages.add("-- =============StackTrace================");
    }
  }
}
