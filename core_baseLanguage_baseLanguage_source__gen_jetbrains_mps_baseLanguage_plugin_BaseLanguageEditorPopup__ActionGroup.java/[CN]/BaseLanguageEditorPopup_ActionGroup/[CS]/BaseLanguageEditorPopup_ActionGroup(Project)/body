{
  super("",ID,true,false,null,project);
  this.setPopup(false);
  try {
    this.add(new CommentStatements_Action(project));
    this.add(new UncommentStatements_Action(project));
    this.add(new RunClassConcept_Action(project));
    this.addSeparator();
    this.add(new ExtractMethod_Action(project));
    this.add(new InlineMethod_Action(project));
    this.add(new ChangeMethodSignature_Action(project));
  }
 catch (  Throwable t) {
    LOG.error("User group error",t);
  }
}
