{
  String path=libRoot.getUrl();
  if (path.startsWith(JARSCHEME)) {
    path=path.substring(JARSCHEME.length());
  }
  if (path.startsWith(FILESCHEME)) {
    path=path.substring(FILESCHEME.length());
  }
  if (path.endsWith("!/")) {
    path=path.substring(0,path.length() - 2);
  }
  return path;
}
