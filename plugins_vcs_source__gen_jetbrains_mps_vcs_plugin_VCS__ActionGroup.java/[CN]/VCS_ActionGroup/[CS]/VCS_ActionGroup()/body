{
  super("VCS",ID);
  this.setIsInternal(false);
  this.setPopup(false);
  try {
    VCS_ActionGroup.this.addAction("jetbrains.mps.vcs.plugin.AddToVcs_Action");
    VCS_ActionGroup.this.addAction("jetbrains.mps.vcs.plugin.IgnoreInVcs_Action");
  }
 catch (  Throwable t) {
    LOG.error("User group error",t);
  }
}
