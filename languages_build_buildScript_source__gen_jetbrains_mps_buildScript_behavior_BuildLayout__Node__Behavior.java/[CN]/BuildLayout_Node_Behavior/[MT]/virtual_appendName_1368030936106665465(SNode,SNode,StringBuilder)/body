{
  if (SNodeOperations.isInstanceOf(parent,"jetbrains.mps.buildScript.structure.BuildLayout_Node")) {
    sb.append("/");
  }
  sb.append((SConceptPropertyOperations.getString(thisNode,"alias") != null ? SConceptPropertyOperations.getString(thisNode,"alias") : SPropertyOperations.getString(SNodeOperations.getConceptDeclaration(thisNode),"name")));
}
