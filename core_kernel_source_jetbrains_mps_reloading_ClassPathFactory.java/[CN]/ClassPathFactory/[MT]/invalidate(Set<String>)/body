{
  List<AbstractClassPathItem> invalidated=new ArrayList<AbstractClassPathItem>();
synchronized (LOCK) {
    for (    String path : paths) {
      if (myCache.containsKey(path)) {
        invalidated.add(myCache.remove(path));
      }
    }
  }
  for (  AbstractClassPathItem p : invalidated) {
    p.invalidate();
  }
}
