{
  if (file.isReadOnly()) {
    LOG.error("Can't write to " + file);
    return false;
  }
  ModelOutputStream os=null;
  try {
    os=new ModelOutputStream(file.openOutputStream());
    saveModel(model,os);
    return true;
  }
 catch (  IOException e) {
    LOG.error("Can't write to " + file,e);
  }
 finally {
    FileUtil.closeFileSafe(os);
  }
  return false;
}
