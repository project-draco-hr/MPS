{
  SConceptId id=getRoleId().getConceptId();
  ConceptDescriptor concept=ConceptRegistry.getInstance().getConceptDescriptor(id);
  return concept.isInterfaceConcept() ? MetaAdapterFactory.getInterfaceConcept(id,concept.getConceptFqName()) : MetaAdapterFactory.getConcept(id,concept.getConceptFqName());
}
