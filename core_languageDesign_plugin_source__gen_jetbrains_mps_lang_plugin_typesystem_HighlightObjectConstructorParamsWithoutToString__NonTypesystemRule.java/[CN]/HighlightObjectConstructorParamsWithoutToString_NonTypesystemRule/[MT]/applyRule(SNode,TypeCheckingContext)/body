{
  if (!(SNodeOperations.isInstanceOf(SLinkOperations.getTarget(parameter,"type",true),"jetbrains.mps.baseLanguage.structure.PrimitiveType"))) {
    if (SLinkOperations.getTarget(parameter,"toStringFunction",true) == null) {
