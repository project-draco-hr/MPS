{
  for (int i=0; i <= 1; i++) {
    if (args[i] instanceof Omega) {
      return args[1 - i];
    }
  }
  SNode leftType=(SNode)args[0];
  SNode rightType=(SNode)args[1];
  boolean mayBeString=false;
  if (args.length >= 3) {
    mayBeString=(Boolean)args[2];
  }
  return getBinaryOperationType(leftType,rightType,mayBeString);
}
