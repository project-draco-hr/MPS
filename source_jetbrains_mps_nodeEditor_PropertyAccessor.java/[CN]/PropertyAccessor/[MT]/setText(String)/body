{
  if (myWritable) {
    if (!myAllowEmptyText && text != null && text.length() == 0) {
      text=null;
    }
    if (isValidText(text)) {
      String propertyValue=toInternal(text);
      System.out.println("set internal value: " + myPropertyName + " = "+ propertyValue);
      myNode.setProperty(myPropertyName,propertyValue);
    }
  }
}
