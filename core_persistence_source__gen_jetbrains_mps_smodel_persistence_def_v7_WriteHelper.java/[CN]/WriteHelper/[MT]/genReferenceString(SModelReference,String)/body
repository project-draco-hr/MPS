{
  if (ref.equals(myModelRef)) {
    return encode(text);
  }
  String index=getImportIndex(ref);
  if (index == null) {
    if (LOG.isEnabledFor(Priority.ERROR)) {
      LOG.error("model " + ref + " not found in index");
    }
    return encode(text);
  }
  return index + MODEL_SEPARATOR_CHAR + encode(text);
}
