{
  final List<LibraryContributor> libContribs=ListSequence.fromList(new ArrayList<LibraryContributor>());
  ListSequence.fromList(libContribs).addSequence(Sequence.fromIterable(createLibContributors(config)));
  ListSequence.fromList(libContribs).addElement(EnvironmentUtils.createPluginLibContributor(config));
  LibraryInitializer libraryInitializer=myPlatformLoader.getMPSCore().getLibraryInitializer();
  libraryInitializer.load(libContribs);
  return libContribs;
}
