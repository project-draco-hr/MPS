{
  ModelRootDescriptor modelRoot=(ModelRootDescriptor)value;
  ModelRoot root=modelRoot.getRoot();
  ModelRootManager manager=(root != null ? root.getManager() : null);
  String representation=(root != null ? root.getPath() + " (" + ((manager == null ? "Default" : NameUtil.shortNameFromLongName(manager.getClassName())))+ ")" : "<" + modelRoot.getType() + ">");
  Component result=super.getListCellRendererComponent(list,representation,index,isSelected,cellHasFocus);
  return result;
}
