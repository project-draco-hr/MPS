{
  final int index=Arrays.binarySearch(myId2Index,conceptId.getIdValue());
switch (index) {
case 0:
    if (myIntentions0 == null) {
      myIntentions0=new IntentionFactory[3];
      myIntentions0[0]=new AddDefaultPropertyAttribute_Intention();
      myIntentions0[1]=new AddDefaultNodeAttribute_Intention();
      myIntentions0[2]=new AddDefaultReferenceAttribute_Intention();
    }
  return Arrays.asList(myIntentions0);
default :
return null;
}
}
