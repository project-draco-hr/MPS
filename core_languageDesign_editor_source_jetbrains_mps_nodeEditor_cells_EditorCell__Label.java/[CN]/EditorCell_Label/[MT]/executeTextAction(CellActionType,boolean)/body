{
switch (type) {
case DELETE:
case BACKSPACE:
    break;
default :
  return false;
}
myCaretIsVisible=true;
if (!isEditable()) {
return false;
}
boolean result=getEditorContext().executeCommand(new ProcessTextActionCommand(type,allowErrors));
if (result) {
getEditorContext().flushEvents();
}
return result;
}
