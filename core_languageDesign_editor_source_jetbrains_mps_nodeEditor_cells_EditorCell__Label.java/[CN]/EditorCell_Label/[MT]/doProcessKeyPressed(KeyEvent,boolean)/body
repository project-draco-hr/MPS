{
  int wasPosition=getCaretPosition();
  CellSide side;
  if (wasPosition == 0) {
    side=CellSide.LEFT;
  }
 else {
    side=CellSide.RIGHT;
  }
  myCaretIsVisible=true;
  if (isNotApplicableKeyEvent(keyEvent))   return false;
  if (processMutableKeyPressed(keyEvent,allowErrors)) {
    getEditorContext().flushEvents();
    getEditor().relayout();
    return true;
  }
  return false;
}
