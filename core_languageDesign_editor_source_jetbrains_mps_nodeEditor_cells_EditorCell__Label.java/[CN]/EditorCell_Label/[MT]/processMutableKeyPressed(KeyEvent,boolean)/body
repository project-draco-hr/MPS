{
  if (!isEditable()) {
    return false;
  }
  if (!isMutableKeystroke(keyEvent)) {
    return false;
  }
  final boolean[] result=new boolean[1];
  getEditorContext().executeCommand(new Runnable(){
    public void run(){
      result[0]=processMutableKeyPressed_impl(keyEvent,allowErrors);
      return;
    }
  }
);
  return result[0];
}
