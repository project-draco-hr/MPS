{
  if ((created == null)) {
    return;
  }
  try {
    ModuleLoader.createModuleChecker(created,visible,converter).check(ModuleChecker.CheckType.LOAD_IMPORTANT_PART);
  }
 catch (  ModuleLoaderException ex) {
    if (LOG.isEnabledFor(Priority.ERROR)) {
      LOG.error(ex.getMessage());
    }
  }
}
