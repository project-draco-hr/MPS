{
  List<ProjectMigration> mig=ProjectMigrationsRegistry.getInstance().getMigrations();
  if (mig.isEmpty()) {
    return null;
  }
  if (current == null) {
    return mig.iterator().next();
  }
  int index=mig.indexOf(current);
  if (index == mig.size() - 1) {
    return null;
  }
  return mig.get(index + 1);
}
