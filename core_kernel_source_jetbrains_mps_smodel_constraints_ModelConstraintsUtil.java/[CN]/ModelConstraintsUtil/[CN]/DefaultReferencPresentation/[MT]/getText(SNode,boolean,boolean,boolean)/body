{
  if (myProvider != null) {
    return myProvider.getPresentation(myOperationContext,new PresentationReferentConstraintContext(myContext.getModel(),myContext.getEnclosingNode(),myContext.getReferenceNode(),myContext.getLinkTarget(),node,myContext.getEnclosingLink(),visible,smartRef,inEditor));
  }
  return node.getPresentation();
}
