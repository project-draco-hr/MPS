{
  if (setterDescriptor == this) {
    node.setProperty(new SPropertyAdapterById(getProperty(),ConceptRegistry.getInstance().getConceptDescriptor(getContainer().getConceptId()).getPropertyDescriptor(getProperty()).getName()),value);
    return;
  }
  if (setterDescriptor != null) {
    setterDescriptor.setValue(node,value);
  }
 else {
    node.setProperty(new SPropertyAdapterById(getProperty(),ConceptRegistry.getInstance().getConceptDescriptor(getContainer().getConceptId()).getPropertyDescriptor(getProperty()).getName()),value);
  }
}
