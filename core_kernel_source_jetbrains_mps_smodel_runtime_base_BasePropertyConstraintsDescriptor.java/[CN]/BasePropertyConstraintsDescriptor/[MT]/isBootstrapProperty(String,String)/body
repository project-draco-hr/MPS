{
  String namespace=NameUtil.namespaceFromConceptFQName(fqName);
  if (namespace.equals("jetbrains.mps.lang.structure") && propertyName.equals(SNodeUtil.propertyName_INamedConcept_name) && !fqName.equals("jetbrains.mps.lang.structure.structure.AnnotationLinkDeclaration")) {
    return true;
  }
  if (fqName.equals("jetbrains.mps.lang.typesystem.structure.RuntimeTypeVariable")) {
    return true;
  }
  return false;
}
