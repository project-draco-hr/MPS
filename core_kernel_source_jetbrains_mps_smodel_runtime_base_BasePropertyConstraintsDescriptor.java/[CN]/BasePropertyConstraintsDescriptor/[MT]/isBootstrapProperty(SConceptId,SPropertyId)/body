{
  if (property.equals(SNodeUtil.propertyId_INamedConcept_name) && concept.getLanguageId().equals(MetaIdByDeclaration.ref2LangId(BootstrapLanguages.structureLanguageRef()))) {
    return true;
  }
  if (property.getConceptId().equals(SNodeUtil.conceptId_RuntimeTypeVariable)) {
    return true;
  }
  return false;
}
