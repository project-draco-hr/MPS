{
  ConstraintsDescriptor container=directConstraints.getContainer();
  return new BasePropertyConstraintsDescriptor(container,directConstraints.getName(),directConstraints.hasOwnGetter() ? directConstraints : getGetterUsingInheritance(container.getConceptFqName()),directConstraints.hasOwnSetter() ? directConstraints : getSetterUsingInheritance(container.getConceptFqName()),directConstraints.hasOwnValidator() ? directConstraints : getValidatorUsingInheritance(container.getConceptFqName()),directConstraints);
}
