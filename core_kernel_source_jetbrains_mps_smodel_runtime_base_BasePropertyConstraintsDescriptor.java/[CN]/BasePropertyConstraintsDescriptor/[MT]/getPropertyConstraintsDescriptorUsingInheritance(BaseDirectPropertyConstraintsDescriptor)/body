{
  ConstraintsDescriptor container=directConstraints.getContainer();
  String name=directConstraints.getName();
  return new BasePropertyConstraintsDescriptor(container,name,directConstraints.hasOwnGetter() ? directConstraints : getGetterUsingInheritance(container.getConceptFqName(),name),directConstraints.hasOwnSetter() ? directConstraints : getSetterUsingInheritance(container.getConceptFqName(),name),directConstraints.hasOwnValidator() ? directConstraints : getValidatorUsingInheritance(container.getConceptFqName(),name),directConstraints);
}
