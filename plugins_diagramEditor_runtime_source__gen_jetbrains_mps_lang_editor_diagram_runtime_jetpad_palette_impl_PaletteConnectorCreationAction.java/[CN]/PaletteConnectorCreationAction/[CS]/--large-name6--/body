{
  mySubstituteAction=action;
  myCanCreateConnectorCallback=canCreateConnector;
  mySetConnectorCallBack=setConnectorCallback;
  myEditorContext=editorContext;
  ModelAccess.instance().runReadAction(new Runnable(){
    public void run(){
