{
  Long current=System.currentTimeMillis();
  Assert.assertEquals(DateTimeOperations.print(current,DateTimeFormat.shortTime(),new Locale("ru","RU",""),DateTimeZone.forID("null")),DateTimeOperations.print((DateTimeOperations.minus(current,Period.hours(3))),DateTimeFormat.shortTime(),new Locale("ru","RU",""),DateTimeZone.forID("null")));
}
