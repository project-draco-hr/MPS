{
  myEditor=edtitor;
  myHandler=new ReloadAdapter(){
    public void onReload(){
      clear();
    }
  }
;
  edtitor.addRebuildListener(new RebuildListener(){
    public void editorRebuilt(    EditorComponent editor){
      rebuildMessages();
    }
  }
);
  ClassLoaderManager.getInstance().addReloadHandler(myHandler);
}
