{
  if (myDisposed) {
    return;
  }
  for (  EditorMessage msg : getMessages()) {
    if (msg.sameAs(message))     return;
  }
synchronized (myMessagesLock) {
    addMessage(message);
    invalidateMassagesCaches();
  }
  if (message.showInGutter()) {
    myEditor.getMessagesGutter().add(message);
  }
}
