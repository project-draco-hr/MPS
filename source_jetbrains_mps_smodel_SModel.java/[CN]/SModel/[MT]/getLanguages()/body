{
  ArrayList<Language> languages=new ArrayList<Language>();
  for (  String languageNamespace : myLanguages) {
    Language language=LanguageRepository.getInstance().getLanguage(languageNamespace);
    if (language != null) {
      languages.add(language);
    }
 else {
      LOG.error("Language \"" + languageNamespace + "\" was not loaded. Used by model \""+ getFQName()+ "\"\nAdd this language to the LANGUAGES section of the project properties");
    }
  }
  if (languages.isEmpty()) {
    LOG.error("Model \"" + getFQName() + "\" has no languages !!!");
  }
  return languages;
}
