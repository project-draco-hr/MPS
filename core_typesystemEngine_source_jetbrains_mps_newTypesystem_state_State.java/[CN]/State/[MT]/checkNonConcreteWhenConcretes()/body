{
  for (  Block block : getBlocks()) {
    if (block.getBlockKind().equals(BlockKind.WHEN_CONCRETE)) {
      WhenConcreteBlock wCBlock=(WhenConcreteBlock)block;
      if (!wCBlock.isSkipError()) {
        SNode node=myNodeMaps.getNode(wCBlock.getArgument());
        if (node != null) {
          SConcept concept=node.getConcept();
          boolean isRuntime=concept.equals(SConceptRepository.getInstance().getInstanceConcept("jetbrains.mps.lang.typesystem.structure.RuntimeTypeVariable"));
          if (!concept.isAbstract() && !isRuntime) {
            myTypeCheckingContext.reportWarning(node,"argument of WHEN CONCRETE block is never concrete",wCBlock.getNodeModel(),wCBlock.getNodeId(),null,new NodeMessageTarget());
          }
        }
      }
    }
  }
}
