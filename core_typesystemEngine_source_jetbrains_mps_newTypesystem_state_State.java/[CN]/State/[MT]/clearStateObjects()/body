{
  if (!(myTypeCheckingContext instanceof TracingTypecheckingContext) && myInequalitySystem == null) {
    for (    Entry<ConditionKind,ManyToManyMap<SNode,Block>> map : myBlocksAndInputs.entrySet()) {
      map.getValue().clear();
    }
    myBlocks.clear();
    myEquations.clear();
    myNodeMaps.clearTypesToNodes();
  }
  clearOperations();
}
