{
  myEquations.clear();
  myInequalities.clear();
  myNodeMaps.clear();
  myVariableIdentifier.clear();
  myBlocks.clear();
  myTargetNode=null;
  myTargetTypeCalculated=false;
  for (  ManyToManyMap map : myBlocksAndInputs.values()) {
    map.clear();
  }
  if (clearDiff) {
    clearOperations();
  }
  if (myInequalitySystem != null) {
    SNode hole=myHole;
    disposeHole();
    initHole(hole);
  }
}
