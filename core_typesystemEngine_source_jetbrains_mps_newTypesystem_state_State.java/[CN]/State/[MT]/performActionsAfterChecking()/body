{
  checkNonConcreteWhenConcretes();
  if (!myTypeCheckingContext.isInTraceMode()) {
    for (    Entry<ConditionKind,ManyToManyMap<SNode,Block>> map : myBlocksAndInputs.entrySet()) {
      map.getValue().clear();
    }
    myBlocks.clear();
    myEquations.clear();
    myNodeMaps.clearTypesToNodes();
  }
}
