{
  MPSPsiNodeBase firstChild=first;
  if (firstChild == node) {
    first=node.next;
    if (first != null) {
      first.prev=node.prev;
    }
  }
 else {
    node.prev.next=node.next;
    if (node.next != null) {
      node.next.prev=node.prev;
    }
 else {
      firstChild.prev=node.prev;
    }
  }
  node.prev=node.next=null;
  node.parent=null;
}
