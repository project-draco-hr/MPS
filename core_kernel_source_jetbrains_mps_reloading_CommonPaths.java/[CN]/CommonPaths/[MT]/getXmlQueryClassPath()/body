{
  String workbenchClasses=PathManager.getHomePath() + File.separator + "core"+ File.separator+ "kernel"+ File.separator+ "xmlQuery"+ File.separator+ "runtime"+ File.separator+ "classes";
  if (new File(workbenchClasses).exists()) {
    return ClassPathFactory.getInstance().createFromPath(workbenchClasses);
  }
  return null;
}
