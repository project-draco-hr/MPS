{
  String workbenchClasses=PathManager.getHomePath() + File.separator + "core"+ File.separator+ "kernel"+ File.separator+ "vcs"+ File.separator+ "svn"+ File.separator+ "classes";
  if (new File(workbenchClasses).exists()) {
    return AbstractClassPathItem.createFromPath(workbenchClasses);
  }
  return null;
}
