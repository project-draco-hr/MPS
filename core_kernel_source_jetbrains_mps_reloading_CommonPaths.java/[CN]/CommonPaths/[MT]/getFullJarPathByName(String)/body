{
  for (  URL url : Launcher.getBootstrapClassPath().getURLs()) {
    try {
      File file=new File(url.toURI());
      if (!file.exists())       continue;
      if (!file.getPath().endsWith(name))       continue;
      return file.getAbsolutePath();
    }
 catch (    URISyntaxException e) {
      LOG.error(e);
    }
  }
  return null;
}
