{
  path=PathManager.getHomePath() + path.replace('/',File.separatorChar);
  File file=new File(path);
  if (file.exists()) {
    try {
      if (file.isDirectory()) {
        item.add(new FileClassPathItem(path));
      }
 else {
        item.add(new JarFileClassPathItem(path));
      }
    }
 catch (    Throwable e) {
      LOG.error(e);
    }
  }
}
