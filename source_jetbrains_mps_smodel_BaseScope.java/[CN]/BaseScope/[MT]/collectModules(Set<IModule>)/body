{
  boolean changed=true;
  while (changed) {
    changed=false;
    for (    IModule module : new HashSet<IModule>(result)) {
      for (      IModule m : module.getExplicitlyVisibleModules()) {
        if (!result.contains(m)) {
          result.add(m);
          changed=true;
        }
      }
    }
  }
}
