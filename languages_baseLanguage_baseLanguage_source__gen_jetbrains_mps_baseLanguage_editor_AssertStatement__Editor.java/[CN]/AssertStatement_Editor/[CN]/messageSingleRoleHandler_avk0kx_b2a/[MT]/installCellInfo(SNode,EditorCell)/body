{
  if (editorCell.getSubstituteInfo() == null || editorCell.getSubstituteInfo() instanceof DefaultSubstituteInfo) {
    editorCell.setSubstituteInfo(new OldNewCompositeSubstituteInfo(myEditorContext,new SChildSubstituteInfo(editorCell,myOwnerNode,MetaAdapterFactory.getContainmentLink(0xf3061a5392264cc5L,0xa443f952ceaf5816L,0x10e50ecba3dL,0x10e50ed92e0L,"message"),child),new DefaultChildSubstituteInfo(myOwnerNode,myContainmentLink.getDeclarationNode(),myEditorContext)));
  }
  if (editorCell.getRole() == null) {
    editorCell.setRole("message");
  }
}
