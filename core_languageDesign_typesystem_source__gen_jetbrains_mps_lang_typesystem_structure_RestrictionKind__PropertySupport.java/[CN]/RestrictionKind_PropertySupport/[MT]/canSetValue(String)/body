{
  if (value == null) {
    return true;
  }
  Iterator<RestrictionKind> constants=ListSequence.fromList(RestrictionKind.getConstants()).iterator();
  while (constants.hasNext()) {
    RestrictionKind constant=constants.next();
    if (value.equals(constant.getName())) {
      return true;
    }
  }
  return false;
}
