{
  try {
    this.module=new ActionEventData(event).getModule();
    if (this.module == null) {
      return false;
    }
    this.context=new ActionEventData(event).getOperationContext();
    if (this.context == null) {
      return false;
    }
    this.scope=new ActionEventData(event).getScope();
    if (this.scope == null) {
      return false;
    }
  }
 catch (  Throwable t) {
    return false;
  }
  return true;
}
