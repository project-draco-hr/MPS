{
  if (node == null) {
    buffer.append("???");
    if (contextNode != null) {
      buffer.foundError("possible broken reference in " + org.jetbrains.mps.openapi.model.SNodeUtil.getDebugText(contextNode),contextNode,null);
    }
    return;
  }
  getTextGenForNode(node).doGenerateText(node,buffer);
}
