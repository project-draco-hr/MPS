{
  String pname="";
  if (parentPackageName != null) {
    for (    char[] part : parentPackageName) {
      pname+=new String(part) + ".";
    }
  }
  pname+=new String(packageName);
  return getClassPathItem().getAvailableRootClasses(pname).size() != 0 || getClassPathItem().getSubpackages(pname).size() != 0;
}
