{
  String pname="";
  if (parentPackageName != null) {
    for (    char[] part : parentPackageName) {
      pname+=new String(part) + ".";
    }
  }
  pname+=new String(packageName);
  return getClassPathItem().getAvailableRootClasses(pname).iterator().hasNext() || getClassPathItem().getSubpackages(pname).iterator().hasNext();
}
