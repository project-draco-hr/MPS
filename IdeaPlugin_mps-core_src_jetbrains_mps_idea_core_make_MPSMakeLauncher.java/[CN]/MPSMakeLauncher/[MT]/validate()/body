{
  this.myValid=false;
  if (!new File(myProject.getBaseDir().getPath()).exists()) {
    LOG.error("Work dir not found " + myProject.getBaseDir().getPath());
    return;
  }
  try {
    myCommandLine=buildCommandLine(collectClassPath());
  }
 catch (  Exception e) {
    LOG.error(null,e);
    return;
  }
  this.myValid=true;
}
