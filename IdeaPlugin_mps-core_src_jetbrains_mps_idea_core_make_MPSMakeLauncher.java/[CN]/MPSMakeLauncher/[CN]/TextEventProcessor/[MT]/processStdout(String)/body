{
  Scanner s=new Scanner(text);
  while (s.hasNextLine()) {
    String line=s.nextLine();
    if (line.indexOf(WRITTEN) == 0) {
      reportWrittenFile(line.substring(WRITTEN.length()));
    }
 else     if (line.indexOf(DELETED) == 0) {
      reportDeletedFile(line.substring(DELETED.length()));
    }
 else {
      info(myPrefix + " - " + line);
    }
  }
}
