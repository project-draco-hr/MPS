{
  Map<String,PropertyConstraintsDescriptor> properties=new HashMap();
  properties.put("hint",new BasePropertyConstraintsDescriptor("hint",this){
    @Override public boolean hasOwnValidator(){
      return true;
    }
    @Override public boolean validateValue(    SNode node,    String propertyValue,    IScope scope){
      String propertyName="hint";
      return (SPropertyOperations.getString(propertyValue)).matches("[a-zA-Z0-9$[_]]*");
    }
  }
);
  return properties;
}
