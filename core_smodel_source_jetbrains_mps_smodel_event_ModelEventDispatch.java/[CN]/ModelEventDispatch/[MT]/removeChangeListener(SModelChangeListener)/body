{
  if (l == null) {
    return;
  }
  LegacyNodeChangeListener wrap=null;
  for (  LegacyNodeChangeListener w : myLegacyChangeListeners) {
    if (w.wraps(l)) {
      wrap=w;
      break;
    }
  }
  if (wrap == null) {
    return;
  }
  myLegacyChangeListeners.remove(wrap);
  removeChangeListener(wrap);
}
