{
  return new TaskInfo(){
    @NotNull @Override public String getTitle(){
      return "Applying Migrations";
    }
    @Override public String getCancelText(){
      return null;
    }
    @Override public String getCancelTooltipText(){
      return null;
    }
    @Override public boolean isCancellable(){
      return false;
    }
    @NonNls @Override public String getProcessId(){
      return "migration";
    }
  }
;
}
