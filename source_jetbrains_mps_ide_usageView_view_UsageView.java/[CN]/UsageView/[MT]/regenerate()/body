{
  MPSProject project=myProjectFrame.getProject();
  if (project == null) {
    return;
  }
  Set<SModel> models=new HashSet<SModel>();
  collectModels(myResultProvider.getResults(mySearchQuery).getSearchResults(),models);
  GeneratorManager manager=project.getComponentSafe(GeneratorManager.class);
  for (  SModel model : models) {
    List<SModel> m=new ArrayList<SModel>();
    m.add(model);
    manager.generateModelsWithProgressWindow(m,BootstrapLanguages.getInstance().getBaseLanguage(),ModuleContext.create(model,myProjectFrame,false),IGenerationType.FILES,IGenerationScript.DEFAULT,false);
  }
}
