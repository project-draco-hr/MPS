{
  int start=getMessageStart(msg);
  int length=getMessageHeight(msg);
  int messageY;
switch (msg.getLocationOnCell()) {
case CENTER:
{
      messageY=start + (length / 2);
      break;
    }
case BOTTOM:
{
    messageY=start + length;
    break;
  }
case TOP:
{
  messageY=start;
  break;
}
default :
{
messageY=start + (length / 2);
}
}
return messageY;
}
