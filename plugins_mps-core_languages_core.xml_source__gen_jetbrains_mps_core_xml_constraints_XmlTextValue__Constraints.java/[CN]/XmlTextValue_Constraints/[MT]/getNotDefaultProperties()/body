{
  Map<String,PropertyConstraintsDescriptor> properties=new HashMap();
  properties.put("text",new BasePropertyConstraintsDescriptor("text",this){
    @Override public boolean hasOwnValidator(){
      return true;
    }
    @Override public boolean validateValue(    SNode node,    String propertyValue,    IScope scope){
      String propertyName="text";
      return XmlNameUtil.isAttValue((SPropertyOperations.getString(propertyValue)));
    }
  }
);
  return properties;
}
