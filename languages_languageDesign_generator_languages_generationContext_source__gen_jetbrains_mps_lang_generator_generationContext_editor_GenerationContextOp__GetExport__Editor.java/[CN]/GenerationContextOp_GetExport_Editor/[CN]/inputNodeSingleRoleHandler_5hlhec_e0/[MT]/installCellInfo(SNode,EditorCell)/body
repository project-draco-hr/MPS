{
  if (editorCell.getSubstituteInfo() == null || editorCell.getSubstituteInfo() instanceof DefaultSubstituteInfo) {
    editorCell.setSubstituteInfo(new OldNewCompositeSubstituteInfo(myEditorContext,new SChildSubstituteInfo(editorCell,myOwnerNode,MetaAdapterFactory.getContainmentLink(0xd7706f639be2479cL,0xa3daae92af1e64d5L,0x6db65d569fb2527L,0x1893693a07c29eb7L,"inputNode"),child),new DefaultChildSubstituteInfo(myOwnerNode,myContainmentLink.getDeclarationNode(),myEditorContext)));
  }
  if (editorCell.getRole() == null) {
    editorCell.setRole("inputNode");
  }
}
