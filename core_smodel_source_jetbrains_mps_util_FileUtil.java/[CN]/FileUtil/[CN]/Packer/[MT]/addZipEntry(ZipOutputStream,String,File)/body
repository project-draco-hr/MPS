{
  ZipEntry entry=new ZipEntry(path);
  entry.setTime(file.lastModified());
  FileInputStream is=null;
  try {
    out.putNextEntry(entry);
    if (file.isFile()) {
      is=new FileInputStream(file);
      byte[] bytes=ReadUtil.read(is);
      out.write(bytes);
    }
    out.closeEntry();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
 finally {
    if (is != null) {
      try {
        is.close();
      }
 catch (      IOException e) {
        e.printStackTrace();
      }
    }
  }
}
