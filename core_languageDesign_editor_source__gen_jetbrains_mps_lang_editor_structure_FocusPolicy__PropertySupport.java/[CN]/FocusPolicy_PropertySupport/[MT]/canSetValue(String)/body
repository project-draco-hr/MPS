{
  if (value == null) {
    return true;
  }
  Iterator<FocusPolicy> constants=ListSequence.fromList(FocusPolicy.getConstants()).iterator();
  while (constants.hasNext()) {
    FocusPolicy constant=constants.next();
    if (value.equals(constant.getName())) {
      return true;
    }
  }
  return false;
}
