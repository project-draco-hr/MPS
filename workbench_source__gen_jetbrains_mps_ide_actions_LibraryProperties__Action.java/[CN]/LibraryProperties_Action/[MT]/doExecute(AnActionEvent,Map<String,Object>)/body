{
  try {
    final Library lib=(Library)((IModule)MapSequence.fromMap(_params).get("module"));
    final Wrappers._T<LibraryPropertiesDialog> dialog=new Wrappers._T<LibraryPropertiesDialog>();
    ModelAccess.instance().runReadAction(new Runnable(){
      public void run(){
        dialog.value=StandardDialogs.createLibraryPropertiesDialog(lib,((IOperationContext)MapSequence.fromMap(_params).get("context")));
      }
    }
);
    dialog.value.showDialog();
  }
 catch (  Throwable t) {
    if (log.isErrorEnabled()) {
      log.error("User's action execute method failed. Action:" + "LibraryProperties",t);
    }
  }
}
