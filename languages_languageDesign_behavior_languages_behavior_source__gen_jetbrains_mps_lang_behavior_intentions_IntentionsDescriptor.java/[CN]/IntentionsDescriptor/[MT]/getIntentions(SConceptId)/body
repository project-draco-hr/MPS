{
  final int index=Arrays.binarySearch(myId2Index,conceptId.getIdValue());
switch (index) {
case 0:
    if (myIntentions0 == null) {
      myIntentions0=new IntentionFactory[4];
      myIntentions0[0]=new MakeAbstract_Intention();
      myIntentions0[1]=new MakeVirtual_Intention();
      myIntentions0[2]=new RemoveTVD_Intention();
      myIntentions0[3]=new MakeStatic_Intention();
    }
  return Arrays.asList(myIntentions0);
default :
return null;
}
}
