{
  BaseConcept parameterNode=(BaseConcept)BaseAdapter.fromNode((SNode)getParameterObject());
  if (parameterNode.getShortDescription() == null) {
    return "^" + NodePresentationUtil.descriptionText(parameterNode,true);
  }
  return "^" + NodePresentationUtil.descriptionText(parameterNode,true);
}
