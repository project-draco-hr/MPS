{
  SNode concept=SLinkOperations.getTarget(SNodeOperations.getAncestor(_context.getReferenceNode(),"jetbrains.mps.lang.constraints.structure.ConceptBehavior",true,false),"concept",false);
  List<SNode> concepts=SConceptOperations.getAllSubConcepts(concept,_context.getModel(),operationContext.getScope());
