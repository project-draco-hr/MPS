{
  try {
    ModuleDescriptor descriptor=loadModuleDescriptor(descriptorFile);
    if (descriptor != null) {
      processExcludes(descriptorFile,descriptor);
    }
    return descriptor;
  }
 catch (  Exception t) {
    LOG.error("Fail to load module from descriptor " + descriptorFile.getPath(),t);
  }
  return null;
}
