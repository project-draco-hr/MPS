{
  List<INodeSubstituteAction> actions=new LinkedList<INodeSubstituteAction>();
  for (  ISubstituteInfoPart menuPart : myParts) {
    try {
      actions.addAll(menuPart.createActions(myCellContext,getEditorContext()));
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
  return actions;
}
