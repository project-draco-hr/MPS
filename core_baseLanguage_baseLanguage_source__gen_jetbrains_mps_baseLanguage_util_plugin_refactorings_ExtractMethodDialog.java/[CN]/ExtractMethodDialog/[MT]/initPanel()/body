{
  this.myPanel=new JPanel(new GridBagLayout());
  Insets defaultInsets=new Insets(3,5,3,5);
  GridBagConstraints c=new GridBagConstraints();
  c.fill=GridBagConstraints.BOTH;
  c.insets=defaultInsets;
  c.gridx=0;
  c.gridy=0;
  c.weightx=1;
  c.weighty=0.1;
  c.gridwidth=2;
  this.myPanel.add(this.createMethodPanel(),c);
  c=new GridBagConstraints();
  c.fill=GridBagConstraints.BOTH;
  c.insets=defaultInsets;
  c.gridx=0;
  c.gridy=1;
  c.weightx=1;
  c.weighty=0.7;
  c.gridwidth=1;
  this.myPanel.add(this.createParametersPanel(),c);
  c=new GridBagConstraints();
  c.fill=GridBagConstraints.BOTH;
  c.insets=defaultInsets;
  c.gridx=1;
  c.gridy=1;
  c.weightx=0;
  c.weighty=0;
  c.gridwidth=1;
  this.myPanel.add(this.createVisibilityPanel(),c);
  c=new GridBagConstraints();
  c.fill=GridBagConstraints.BOTH;
  c.insets=defaultInsets;
  c.gridx=0;
  c.gridy=2;
  c.weightx=0;
  c.weighty=0.1;
  c.gridwidth=2;
  this.myPanel.add(this.createPreviewPanel(),c);
  c=new GridBagConstraints();
  c.fill=GridBagConstraints.BOTH;
  c.insets=defaultInsets;
  c.gridx=0;
  c.gridy=3;
  c.weightx=1;
  c.weighty=0.2;
  c.gridwidth=2;
  this.myPanel.add(this.createMessagesComponent(),c);
}
