{
  return runWriteAction(new Computable<T>(){
    public T compute(){
      final Object[] result=new Object[1];
      CommandProcessor.getInstance().executeCommand(null,new Runnable(){
        public void run(){
          result[0]=c.compute();
        }
      }
,name,null,policy);
      return (T)result[0];
    }
  }
);
}
