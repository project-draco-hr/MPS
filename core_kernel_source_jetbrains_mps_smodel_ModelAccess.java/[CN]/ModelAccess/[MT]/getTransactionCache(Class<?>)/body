{
  if (canWrite()) {
    return null;
  }
  ConcurrentMap<K,V> cache=(ConcurrentMap<K,V>)myTransactionCaches.get(clazz);
  if (cache != null) {
    return cache;
  }
  cache=(ConcurrentMap<K,V>)myTransactionCaches.putIfAbsent(clazz,new ConcurrentHashMap<Object,Object>());
  return cache != null ? cache : null;
}
