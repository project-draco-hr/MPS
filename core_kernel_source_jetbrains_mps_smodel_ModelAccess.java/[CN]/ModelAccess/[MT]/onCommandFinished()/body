{
synchronized (myListenersLock) {
    for (    ModelAccessListener l : myListeners) {
      try {
        l.beforeCommandFinished();
      }
 catch (      Throwable t) {
        LOG.error(t);
      }
    }
    for (    ModelAccessListener l : myListeners) {
      try {
        l.commandFinished();
      }
 catch (      Throwable t) {
        LOG.error(t);
      }
    }
  }
}
