{
  try {
    Class pluginClass=descriptor.first.getClass(descriptor.second);
    if (pluginClass == null) {
      LOG.error("Can't find a class : " + descriptor.second);
      return null;
    }
    return (BaseProjectPlugin)pluginClass.newInstance();
  }
 catch (  Throwable t) {
    LOG.error(t);
    return null;
  }
}
