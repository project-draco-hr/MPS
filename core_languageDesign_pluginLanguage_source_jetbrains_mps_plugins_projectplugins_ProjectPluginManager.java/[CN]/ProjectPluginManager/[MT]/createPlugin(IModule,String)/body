{
  try {
    Class pluginClass=contextModule.getClass(pluginClassFqName);
    if (pluginClass == null) {
      LOG.error("Can't find a class : " + pluginClassFqName);
      return null;
    }
    return (BaseProjectPlugin)pluginClass.newInstance();
  }
 catch (  Throwable t) {
    LOG.error(t);
    return null;
  }
}
