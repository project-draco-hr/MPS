{
  try {
    Class pluginClass=contextModule.getClass(pluginClassFqName);
    if (pluginClass == null) {
      LOG.error("Can't find a class : " + pluginClassFqName);
      return;
    }
    myPlugins.add((IProjectPlugin)pluginClass.newInstance());
  }
 catch (  Throwable t) {
    LOG.error(t);
  }
}
