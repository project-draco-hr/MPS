{
  Reader r=null;
  try {
    r=new BufferedReader(new FileReader(f));
    for (int c=0; c != -1; c=r.read()) {
      if (c == '\r' && r.read() == '\n') {
        return true;
      }
    }
  }
 catch (  FileNotFoundException e) {
    e.printStackTrace();
  }
catch (  IOException e) {
    e.printStackTrace();
  }
 finally {
    FileUtil.closeFileSafe(r);
  }
  return false;
}
