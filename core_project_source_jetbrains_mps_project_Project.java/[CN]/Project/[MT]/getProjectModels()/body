{
  List<SModel> result=new ArrayList<SModel>();
  for (  SModule module : getModules()) {
    Iterable<SModel> models=module.getModels();
    if (models instanceof Collection) {
      result.addAll((Collection<SModel>)models);
    }
 else {
      for (      SModel model : models) {
        result.add(model);
      }
    }
  }
  return result;
}
