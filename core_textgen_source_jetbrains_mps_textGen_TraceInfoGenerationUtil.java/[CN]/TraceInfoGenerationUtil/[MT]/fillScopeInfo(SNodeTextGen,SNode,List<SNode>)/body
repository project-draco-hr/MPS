{
  TextGenBuffer buffer=nodeTextGen.getBuffer();
  final TraceInfoCollector tic=getTraceInfoCollector(buffer);
  if (tic == null) {
    return;
  }
  final TextMark m=buffer.getRealBuffer().popMark();
  ScopePositionInfo info=tic.createScopePosition(m,node);
  for (  SNode var : vars) {
    if (var != null) {
      info.addVarInfo(var);
    }
  }
}
