{
  invalidate(path);
  IFile ifile=FileSystem.getInstance().getFileByPath(path);
  final EditableSModelDescriptor model=SModelRepository.getInstance().findModel(ifile);
  if (model == null) {
    return;
  }
  IModule module=model.getModule();
  if (module == null) {
    return;
  }
  s.addChangedModule(module);
}
