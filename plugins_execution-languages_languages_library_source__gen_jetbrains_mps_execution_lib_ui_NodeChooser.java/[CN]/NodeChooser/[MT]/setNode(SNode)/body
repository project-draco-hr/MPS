{
  if (check_qnl8bg_a0a0h(((SNodePointer)this.myNodePointer),this) == node) {
    return;
  }
  if (node == null) {
    if (this.myNodePointer == null) {
      this.setText(null);
      return;
    }
 else {
      myNodePointer=null;
    }
  }
 else {
    this.myNodePointer=new SNodePointer(node);
    this.setText(getFqName(node));
  }
  this.fireNodeChanged();
}
