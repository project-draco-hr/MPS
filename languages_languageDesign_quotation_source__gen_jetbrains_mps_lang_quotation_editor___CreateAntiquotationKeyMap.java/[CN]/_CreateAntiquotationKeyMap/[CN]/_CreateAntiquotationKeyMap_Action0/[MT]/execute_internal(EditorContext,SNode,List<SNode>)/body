 else   SNodeFactoryOperations.setNewAttribute(contextNode,new IAttributeDescriptor.NodeAttribute("jetbrains.mps.lang.quotation.structure.Antiquotation"),"jetbrains.mps.lang.quotation.structure.Antiquotation");
