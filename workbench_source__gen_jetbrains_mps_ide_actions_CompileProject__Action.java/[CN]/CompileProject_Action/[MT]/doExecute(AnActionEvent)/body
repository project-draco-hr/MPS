{
  try {
    Set<IModule> modules=SetSequence.fromSet(new LinkedHashSet<IModule>());
    SetSequence.fromSet(modules).addSequence(ListSequence.fromList(CompileProject_Action.this.project.getProjectSolutions()));
    SetSequence.fromSet(modules).addSequence(ListSequence.fromList(CompileProject_Action.this.project.getProjectLanguages()));
    SetSequence.fromSet(modules).addSequence(ListSequence.fromList(CompileProject_Action.this.project.getProjectDevKits()));
    ProgressManager.getInstance().run(new DefaultMakeTask(CompileProject_Action.this.ideaProject,"Making",modules,false));
  }
 catch (  Throwable t) {
    if (log.isErrorEnabled()) {
      log.error("User's action execute method failed. Action:" + "CompileProject",t);
    }
  }
}
