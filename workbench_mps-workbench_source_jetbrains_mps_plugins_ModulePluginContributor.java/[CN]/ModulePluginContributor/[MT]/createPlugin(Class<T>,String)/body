{
  try {
    Class<?> pluginClass=myModule.getOwnClass(className);
    return pluginClass.asSubclass(expectedClass).newInstance();
  }
 catch (  ClassNotFoundException e) {
    return null;
  }
catch (  Throwable t) {
    LOG.error("Failed to instantiate plugin component activator",t);
    return null;
  }
}
