{
  if (modelFile == null)   return;
  IFile metadataFile=getMetadataFile(modelFile);
  if (metadata.isEmpty()) {
    if (metadataFile.exists()) {
      metadataFile.delete();
    }
    return;
  }
  if (!metadataFile.exists()) {
    metadataFile.createNewFile();
  }
  DefaultMetadataPersistence.save(metadataFile,metadata);
}
