{
  SModel sm=SNodeOperations.getModel(node);
  if (sm == null || !(sm.getModule() instanceof Generator)) {
    return false;
  }
  EditorCell editorCell=editorContext.getSelectedCell();
  if (editorCell == null) {
    return false;
  }
  return QueriesUtil.isReferenceMacroApplicable(node,editorCell);
}
