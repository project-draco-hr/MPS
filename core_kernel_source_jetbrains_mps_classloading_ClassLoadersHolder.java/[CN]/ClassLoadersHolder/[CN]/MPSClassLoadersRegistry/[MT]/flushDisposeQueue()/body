{
  myModelAccess.runWriteInEDT(new Runnable(){
    @Override public void run(){
      LOG.debug("Disposing " + myDisposeQueue.size() + " class loaders");
      while (!myDisposeQueue.isEmpty()) {
        ModuleClassLoader classLoader=myDisposeQueue.poll();
        classLoader.dispose();
      }
    }
  }
);
}
