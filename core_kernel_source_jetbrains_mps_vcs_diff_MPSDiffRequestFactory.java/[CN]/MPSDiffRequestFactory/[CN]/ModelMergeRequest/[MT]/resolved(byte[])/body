{
  ((SimpleContent)getContents()[ORIGINAL]).setBOM(result);
  setResult(DialogWrapper.OK_EXIT_CODE);
  try {
    OutputStream outputStream=myFile.getOutputStream(this);
    outputStream.write(result);
    outputStream.close();
  }
 catch (  IOException e) {
    LOG.error(e);
  }
  reloadModel(result);
}
