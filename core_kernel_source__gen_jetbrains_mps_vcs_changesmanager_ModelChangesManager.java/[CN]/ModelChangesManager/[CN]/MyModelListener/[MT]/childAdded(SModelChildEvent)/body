              if (e.getParent().getSNodeId().equals(check_4410510979167128200(SNodeOperations.getParent(thisNodeInBase))) && e.getChildRole().equals(SNodeOperations.getContainingLinkRole(thisNodeInBase))) {
                return;
              }
            }
