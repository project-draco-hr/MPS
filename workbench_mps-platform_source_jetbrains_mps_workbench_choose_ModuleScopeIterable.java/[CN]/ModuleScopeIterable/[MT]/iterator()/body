{
  List<SModuleReference> refs=new ModelAccessHelper(myRepo).runReadAction(new Computable<List<SModuleReference>>(){
    @Override public List<SModuleReference> compute(){
      ArrayList<SModuleReference> rv=new ArrayList<SModuleReference>(100);
      for (      SModule m : myScope.getModules()) {
        rv.add(m.getModuleReference());
      }
      return rv;
    }
  }
);
  return refs.iterator();
}
