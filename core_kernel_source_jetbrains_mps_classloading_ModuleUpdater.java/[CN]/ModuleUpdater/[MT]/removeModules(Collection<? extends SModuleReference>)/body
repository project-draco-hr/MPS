{
synchronized (LOCK) {
    for (    SModuleReference mRef : mRefs) {
      if (myRefStorage.moduleRemoved(mRef) != null) {
        removeMRefFromModules(mRef,myModulesToAdd);
        removeMRefFromModules(mRef,myModulesToReload);
        myModulesToRemove.add(mRef);
        myChangedFlag=true;
      }
    }
  }
}
