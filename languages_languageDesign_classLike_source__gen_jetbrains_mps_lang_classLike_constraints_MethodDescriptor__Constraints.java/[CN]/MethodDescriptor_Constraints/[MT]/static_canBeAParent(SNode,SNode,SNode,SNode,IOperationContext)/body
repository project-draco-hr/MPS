{
  if (SConceptOperations.isExactly(childConcept,"jetbrains.mps.lang.classLike.structure.RequiredModifier")) {
    return Sequence.fromIterable(SNodeOperations.ofConcept(SLinkOperations.getTargets(node,"modifier",true),"jetbrains.mps.lang.classLike.structure.RequiredModifier")).isEmpty();
  }
  return true;
}
