{
  if (myBaseNode.equals(node))   return;
  if (myBaseNode.getNode() == null)   return;
  if (myBaseNode.getModel() == null || myBaseNode.getModel().getModule() == null)   return;
  if (ModuleRepositoryFacade.getInstance().getModule(myBaseNode.getModel().getModule().getModuleReference()) == null)   return;
  if (!isTabUpdateNeeded(node))   return;
  updateTabs();
}
