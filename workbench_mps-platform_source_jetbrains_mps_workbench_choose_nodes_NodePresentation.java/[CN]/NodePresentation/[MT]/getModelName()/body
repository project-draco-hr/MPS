{
  if (myNode == null) {
    return "null node";
  }
  if (myModelName == null) {
    myModelName=ModelAccess.instance().runReadAction(new Computable<String>(){
      @Override public String compute(){
        return myNode.getModel().getReference().getModelName();
      }
    }
);
  }
  return myModelName;
}
