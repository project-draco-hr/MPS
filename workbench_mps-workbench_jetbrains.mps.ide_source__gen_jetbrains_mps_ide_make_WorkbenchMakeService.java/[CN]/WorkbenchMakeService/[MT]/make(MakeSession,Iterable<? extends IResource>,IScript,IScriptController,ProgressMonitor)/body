{
  this.checkValidUsage();
  this.checkValidSession(session);
  Future<IResult> result=null;
  try {
    awaitCurrentProcess();
    result=_doMake(resources,script,controller,monitor);
  }
  finally {
    if (result == null || result.isDone()) {
      this.attemptCloseSession();
    }
  }
  return result;
}
