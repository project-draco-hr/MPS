{
  final SNode[] current=new SNode[1];
  CommandProcessor.instance().executeCommand(getEditorContext(),new Runnable(){
    public void run(){
      current[0]=getSelectedCell().getSNode();
      new IntelligentNodeMover(current[0]){
        boolean forward(){
          return false;
        }
      }
.move();
    }
  }
);
  selectNode(current[0]);
}
