{
  File pfile=new File(project);
  final MPSProject prj=TestMain.loadProject(pfile);
  GENERATION_HELPER.generate(prj);
  ThreadUtils.runInUIThreadAndWait(new Runnable(){
    public void run(){
      prj.dispose(false);
      IdeEventQueue.getInstance().flushQueue();
      System.gc();
    }
  }
);
}
