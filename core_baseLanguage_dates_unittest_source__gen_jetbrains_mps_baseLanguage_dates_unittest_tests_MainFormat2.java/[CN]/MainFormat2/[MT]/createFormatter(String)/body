{
  if ("hoursBeforeFixed".equals(formatterName)) {
    DateTimeFormatterBuilder builder=new DateTimeFormatterBuilder();
    builder.append(new OffsetDateTimePrinter(new DateTime(2000,4,12,19,0,0,0,DateTimeZone.forID("GMT+0")),DurationFieldType.hours(),DurationFieldType.minutes()));
    return builder.toFormatter();
  }
  return null;
}
