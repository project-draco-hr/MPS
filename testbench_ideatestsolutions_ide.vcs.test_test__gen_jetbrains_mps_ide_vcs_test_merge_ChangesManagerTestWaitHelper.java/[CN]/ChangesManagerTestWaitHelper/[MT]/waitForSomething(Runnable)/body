{
synchronized (myWaitLock) {
    myWaitCompleted=false;
    waitScheduling.run();
  }
  while (true) {
synchronized (myWaitLock) {
      if (myWaitCompleted) {
        break;
      }
      try {
        myWaitLock.wait();
      }
 catch (      InterruptedException e) {
        e.printStackTrace();
      }
    }
  }
}
