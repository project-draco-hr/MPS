{
  EditorCell_Collection editorCell=EditorCell_Collection.createHorizontal(context,node);
  SwitchNodeKindStatement_Editor.setupBasic_RowCell(editorCell,node,context);
  editorCell.setGridLayout(false);
  editorCell.setUsesBraces(false);
  editorCell.setCanBeFolded(false);
  editorCell.addEditorCell(this.createConstantCell(context,node,"switch"));
  editorCell.addEditorCell(this.createNodeExpressionCell(context,node));
  editorCell.addEditorCell(this.createConstantCell1(context,node,"kind"));
  editorCell.addEditorCell(this.createConstantCell2(context,node,"{"));
  return editorCell;
}
