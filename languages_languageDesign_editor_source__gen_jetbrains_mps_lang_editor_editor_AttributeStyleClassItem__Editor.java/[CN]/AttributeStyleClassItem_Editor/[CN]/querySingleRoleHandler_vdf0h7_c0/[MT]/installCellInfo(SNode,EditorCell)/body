{
  editorCell.setSubstituteInfo(new OldNewCompositeSubstituteInfo(myEditorContext,new SChildSubstituteInfo(editorCell,myOwnerNode,MetaAdapterFactory.getContainmentLink(0x18bc659203a64e29L,0xa83a7ff23bde13baL,0x3744c0f9eae0a402L,0x3744c0f9eae0a403L,"query"),child),new DefaultChildSubstituteInfo(myOwnerNode,myContainmentLink.getDeclarationNode(),myEditorContext)));
  if (editorCell.getRole() == null) {
    editorCell.setRole("query");
  }
  if (true) {
    editorCell.getStyle().set(StyleAttributes.FOCUS_POLICY,FocusPolicy.ATTRACTS_RECURSIVELY);
  }
