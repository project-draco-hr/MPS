{
  return new ActionListener(){
    public void actionPerformed(    ActionEvent event){
      BaseDialog dialog;
      if (context.isEmptyMaps()) {
        dialog=new InformationDialog(myContext,context);
      }
 else {
        dialog=new InformationDialogWithEditor(myContext,context);
      }
      dialog.showDialog();
    }
  }
;
}
