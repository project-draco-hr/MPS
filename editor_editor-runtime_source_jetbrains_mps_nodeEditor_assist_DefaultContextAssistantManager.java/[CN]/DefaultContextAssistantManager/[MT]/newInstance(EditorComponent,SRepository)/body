{
  SelectionMenuProvider executableContextAssistantItemsMenuProvider=new FilteringSelectionMenuProvider(new SelectionMenuProviderByCellAndConcept(MenuLocations.CONTEXT_ASSISTANT),new CanExecuteFilter());
  EditorContextAssistants assistants=new EditorContextAssistants(new AncestorOrSmallCellContextAssistantFinder(),executableContextAssistantItemsMenuProvider,component.getSelectionManager(),repository.getModelAccess());
  EditorContextAssistantsController controller=new EditorContextAssistantsController(assistants,component.getSelectionManager(),component.getUpdater());
  return new DefaultContextAssistantManager(assistants,controller);
}
