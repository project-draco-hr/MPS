{
  if (!(((IModule)MapSequence.<String,Object>fromMap(_params).get("module")) instanceof Language)) {
    return false;
  }
  Language language=(Language)((IModule)MapSequence.<String,Object>fromMap(_params).get("module"));
  SModelDescriptor structureModelDescriptor=language.getStructureModelDescriptor();
  if (structureModelDescriptor == null) {
    return false;
  }
  if (structureModelDescriptor.getSModel().rootsCount() == 0) {
    return false;
  }
  return true;
}
