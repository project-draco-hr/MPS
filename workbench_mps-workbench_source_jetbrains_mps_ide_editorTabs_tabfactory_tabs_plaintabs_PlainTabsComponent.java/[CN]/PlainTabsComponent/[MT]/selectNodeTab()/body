{
  if (myDisposed)   return;
  for (  PlainEditorTab t : myRealTabs) {
    if (t.getNode() != null && t.getNode().equals(getLastNode())) {
      myJbTabs.setSelectedIndex(myRealTabs.indexOf(t));
      return;
    }
  }
  for (  PlainEditorTab t : myRealTabs) {
    if (t.getNode() == null && t.getTab().equals(myLastEmptyTab)) {
      myJbTabs.setSelectedIndex(myRealTabs.indexOf(t));
      return;
    }
  }
}
