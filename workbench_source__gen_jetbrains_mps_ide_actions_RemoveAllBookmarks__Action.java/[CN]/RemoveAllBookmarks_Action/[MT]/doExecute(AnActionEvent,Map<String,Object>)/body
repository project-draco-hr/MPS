{
  try {
    ((Project)MapSequence.<String,Object>fromMap(_params).get("project")).getComponent(BookmarkManager.class).clearBookmarks();
  }
 catch (  Throwable t) {
    if (log.isErrorEnabled()) {
      log.error("User's action execute method failed. Action:" + "RemoveAllBookmarks",t);
    }
  }
}
