{
  if (myCells.size() == 0)   return;
  addToHistory();
  int index=Math.max(0,myCells.indexOf(myEditor.getSelectedCell()));
  if (index >= myCells.size() - 1) {
    index=0;
  }
 else {
    while (myCells.get(index).equals(myEditor.getSelectedCell())) {
      if (index >= myCells.size() - 1) {
        index=0;
        break;
      }
 else {
        index++;
      }
    }
  }
  changeSelection(index);
}
