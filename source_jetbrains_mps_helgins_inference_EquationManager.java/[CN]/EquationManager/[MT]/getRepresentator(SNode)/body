{
  List<SNode> path=new LinkedList<SNode>();
  int pathLength=0;
  SNode type=type_;
  while (getParent(type) != null) {
    path.add(type);
    pathLength++;
    type=getParent(type);
  }
  if (pathLength > 1) {
    for (    SNode typeOnPath : path) {
      setParent(typeOnPath,type);
    }
  }
  return getSmartType(type);
}
