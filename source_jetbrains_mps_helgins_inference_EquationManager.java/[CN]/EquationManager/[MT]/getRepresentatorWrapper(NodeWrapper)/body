{
  List<NodeWrapper> path=new LinkedList<NodeWrapper>();
  int pathLength=0;
  NodeWrapper type=type_;
  while (getParent(type) != null) {
    path.add(type);
    pathLength++;
    type=fromNode(getParent(type));
  }
  if (pathLength > 1) {
    for (    NodeWrapper typeOnPath : path) {
      setParent(typeOnPath,fromWrapper(type));
    }
  }
  return type;
}
