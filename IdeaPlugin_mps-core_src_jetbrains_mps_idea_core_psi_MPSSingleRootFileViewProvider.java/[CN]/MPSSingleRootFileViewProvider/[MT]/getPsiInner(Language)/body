{
  if (target != MPSLanguage.INSTANCE) {
    return null;
  }
  PsiFile psiFile=getCachedPsi(MPSLanguage.INSTANCE);
  if (psiFile == null || (psiFile instanceof MPSPsiNodeBase && !psiFile.isValid())) {
    psiFile=createFile();
    if (psiFile == null)     return null;
    forceCachedPsi(psiFile);
  }
  return psiFile;
}
