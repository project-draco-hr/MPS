{
  TracerNode checkNode=myCurrentTraceNode;
  while (checkNode != null) {
    if (checkNode.isThis(kind,node)) {
      myCurrentTraceNode=checkNode.getParent();
      return;
    }
    checkNode=checkNode.getParent();
  }
  LOG.errorWithTrace("tracer node not found. kind:" + kind + " node: "+ org.jetbrains.mps.openapi.model.SNodeUtil.getDebugText(node.getNode()));
  myCurrentTraceNode=null;
}
