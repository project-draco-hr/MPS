{
  List<Throwable> errors=new ArrayList<Throwable>();
  try {
    if (myPrev != null) {
      myPrev.evaluate();
    }
  }
 catch (  Throwable th) {
    errors.add(th);
  }
 finally {
    try {
      ActiveEnvironment.get().disposeEnvironment();
    }
 catch (    Throwable th) {
      errors.add(th);
    }
  }
  MultipleFailureException.assertEmpty(errors);
}
