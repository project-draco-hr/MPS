{
  if (index == 0) {
    return listIterator();
  }
 else {
    if (index < 0 || index > size()) {
      throw new IndexOutOfBoundsException();
    }
    if (index == size()) {
      ListItr itr=new ListItr(get(index - 1),index - 1);
      itr.next();
      return itr;
    }
 else {
      return new ListItr(get(index),index);
    }
  }
}
