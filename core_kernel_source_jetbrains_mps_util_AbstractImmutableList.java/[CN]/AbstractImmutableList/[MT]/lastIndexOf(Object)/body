{
  int lastIndex=-1;
  int count=0;
  T current=myFirst;
  if (mySize == 0 || o == null) {
    return -1;
  }
  while (current != null) {
    if (current.equals(o)) {
      lastIndex=count;
    }
    if (++count == mySize) {
      return lastIndex;
    }
    current=next(current);
  }
  return lastIndex;
}
