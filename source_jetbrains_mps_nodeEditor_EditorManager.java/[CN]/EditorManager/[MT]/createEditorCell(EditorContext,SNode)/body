{
  EditorCell editorCell=createEditorCell_internal(context,node);
  boolean hasBadReference=false;
  for (  SReference sr : node.getReferences()) {
    if (!sr.isGood()) {
      hasBadReference=true;
      break;
    }
  }
  if (hasBadReference)   editorCell.setOutlined(true);
  return editorCell;
}
