{
  stream.writeByte(31);
  stream.writeString(file);
  if (header instanceof SModelHeader) {
    stream.writeByte(55);
    ((SModelHeader)header).save(stream);
  }
 else   if (header instanceof BinaryModelHeader) {
    stream.writeByte(56);
    ((BinaryModelHeader)header).save(stream);
  }
 else {
    stream.writeByte(0x70);
  }
}
