{
  if (SNodeOperations.isInstanceOf(SLinkOperations.getTarget(thisNode,"visibility",true),"jetbrains.mps.baseLanguage.structure.PrivateVisibility") && contextClassifier != SNodeOperations.getParent(thisNode)) {
    return;
