{
  if (object == this)   return true;
  if (object instanceof Memento) {
    Memento m=(Memento)object;
    if (ObjectUtils.equals(mySelectionStack,m.mySelectionStack) && ObjectUtils.equals(myCollectionsWithEnabledBraces,m.myCollectionsWithEnabledBraces) && ObjectUtils.equals(myFolded,m.myFolded)) {
      return true;
    }
  }
  return false;
}
