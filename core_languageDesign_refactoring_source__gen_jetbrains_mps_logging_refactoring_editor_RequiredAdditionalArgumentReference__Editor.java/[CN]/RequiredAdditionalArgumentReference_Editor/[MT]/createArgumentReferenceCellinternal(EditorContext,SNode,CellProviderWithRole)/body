{
  CellProviderWithRole provider=aProvider;
  provider.setAuxiliaryCellProvider(new RequiredAdditionalArgumentReference_Editor._Inline());
  EditorCell editorCell=provider.createEditorCell(context);
  RequiredAdditionalArgumentReference_Editor.setupBasic_ArgumentReferenceCell(editorCell,node,context);
  if (editorCell instanceof EditorCell_Label) {
    RequiredAdditionalArgumentReference_Editor.setupLabel_ArgumentReferenceCell((EditorCell_Label)editorCell,node,context);
  }
  editorCell.setSubstituteInfo(provider.createDefaultSubstituteInfo());
  return editorCell;
}
