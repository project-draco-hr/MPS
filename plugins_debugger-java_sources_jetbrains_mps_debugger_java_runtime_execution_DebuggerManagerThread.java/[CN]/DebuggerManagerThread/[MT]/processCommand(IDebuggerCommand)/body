{
  assertIsManagerThread();
  try {
    if (myCommandQueue.isClosed()) {
      command.notifyCancelled();
    }
 else {
      command.run();
    }
  }
 catch (  VMDisconnectedException e) {
    LOG.debug("",e);
  }
catch (  RuntimeException e) {
    throw e;
  }
catch (  InterruptedException e) {
    throw new RuntimeException(e);
  }
catch (  Exception e) {
    LOG.error(e);
  }
}
