{
  this.condition=condition;
  current=original;
  if (current != null) {
    for (    SNode child : current.getChildren()) {
      queue.offer(child);
    }
  }
  if (!(includeFirst)) {
    current=nextInternal();
  }
  while (current != null && condition != null && !(current instanceof SNode && condition.met(((SNode)current)))) {
    current=nextInternal();
  }
}
