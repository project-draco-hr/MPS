{
  List<SNode> children=((List<SNode>)node.getChildren());
  if (includeAttributes) {
    return (children);
  }
  List<SNode> res=new ArrayList<SNode>();
  for (  SNode child : children) {
    if (child != null && AttributeOperations.isAttribute(child)) {
      res.add(child);
    }
  }
  return res;
}
