{
  Iterator<? extends jetbrains.mps.smodel.SNode> iterator=((Iterator<? extends jetbrains.mps.smodel.SNode>)node.getChildren().iterator());
  SNode firstChild=(iterator.hasNext() ? iterator.next() : null);
  return new SNodeOperations.DescendantsIterable(node,(includeFirst ? node : firstChild),cond);
}
