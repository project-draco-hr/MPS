{
  try {
    return GeneratorUtilEx.evaluateExpression(myExpression);
  }
 catch (  IllegalArgumentException ex) {
    context.getEnvironment().getLogger().error(myExpression.getReference(),String.format("cannot evaluate template argument #%d: %s",myArgIndex,ex.toString()),GeneratorUtil.describeInput(context));
  }
  return null;
}
