{
switch (axis) {
case ANCESTORS:
    return "\\\\ancestors";
case CHILDREN:
  return "\\";
case DESCENDANTS:
return "\\\\descendants";
case SELF_DESCENDANTS:
return "\\\\descendants+";
case PRECEDING_SIBLINGS:
return "\\\\preceding";
case PRECEDING_SIBLINGS_SELF:
return "\\\\preceding+";
case FOLLOWING_SIBLINGS:
return "\\\\following";
case SELF_FOLLOWING_SIBLINGS:
return "\\\\following+";
case SELF_ANCESTORS:
return "\\\\ancestors+";
default :
return "???";
}
}
