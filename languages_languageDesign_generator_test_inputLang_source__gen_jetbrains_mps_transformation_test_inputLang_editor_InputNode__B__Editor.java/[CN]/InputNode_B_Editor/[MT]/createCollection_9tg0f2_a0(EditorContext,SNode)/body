  style.set(StyleAttributes.DRAW_BRACKETS,0,true);
  style.set(StyleAttributes.BRACKETS_COLOR,0,StyleRegistry.getInstance().getSimpleColor(MPSColors.gray));
  editorCell.getStyle().putAll(style);
