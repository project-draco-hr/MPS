{
  if (myLineEmpty) {
    myLineWidth+=getIndent(cell) * getIndentWidth();
    myLineEmpty=false;
  }
 else {
    cell.setLeftGap(getSpacesWidth(1));
  }
  cell.setX(myX + myLineWidth);
  cell.setY(myY + myHeight);
  cell.relayout();
  myLineHeight=Math.max(myLineHeight,cell.getHeight());
  myLineWidth+=cell.getWidth();
}
