{
  g.setColor(c);
  for (  EditorCell leaf : getIndentLeafs(editorCells)) {
    g.fillRect(leaf.getX(),leaf.getY(),leaf.getWidth(),leaf.getHeight());
  }
  g.setColor(c.darker());
  for (  EditorCell leaf : getIndentLeafs(editorCells)) {
    g.drawRect(leaf.getX(),leaf.getY(),leaf.getWidth(),leaf.getHeight());
  }
}
