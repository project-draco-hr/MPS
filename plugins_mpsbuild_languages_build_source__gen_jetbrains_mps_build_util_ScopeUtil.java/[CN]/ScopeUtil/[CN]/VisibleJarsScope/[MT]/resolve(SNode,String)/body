{
  SNode result=null;
  for (  SNode n : getAllNodes()) {
    String name=getName(n);
    if (name.equals(refText)) {
      if (result == null) {
        result=(SNodeOperations.isInstanceOf(n,"jetbrains.mps.build.structure.BuildSource_SingleFile") ? n : SLinkOperations.getTarget(SNodeOperations.cast(n,"jetbrains.mps.build.structure.BuildLayout_Copy"),"fileset",true));
      }
 else {
        return null;
      }
    }
  }
  return result;
}
