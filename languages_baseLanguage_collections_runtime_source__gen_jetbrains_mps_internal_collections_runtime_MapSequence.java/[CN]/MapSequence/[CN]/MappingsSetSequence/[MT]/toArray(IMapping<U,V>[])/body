{
  int size=size();
  if (arr.length < size) {
    arr=(IMapping<U,V>[])ArrayUtils.newArrayInstance(arr.getClass().getComponentType(),size);
  }
  Iterator<IMapping<U,V>> it=iterator();
  for (int i=0; i < size; i++) {
    arr[i]=it.next();
  }
  if (arr.length > size) {
    arr[size]=null;
  }
  return arr;
}
