{
  myChanges=changes;
  myOldModel=oldModel;
  myNewModel=newModel;
  myAddedNodes.clear();
  myChangedNodes.clear();
  myDeletedNodes.clear();
  myChangedSubtree.clear();
  for (  Change change : changes) {
    SNodeId id=change.getAffectedNodeId();
    if (id != null) {
      addToChangeSubtree(change,change.getAffectedNodeId(),newModel,oldModel);
    }
  }
  rebuildNow();
  expandRoot();
  return this;
}
