{
  ModelChangesWatcher.setForceProcessingEnabled(true);
  checkAndEnable();
  modifyModel();
  saveAndCommit();
  uncommit();
  modifyExternally();
  rollback();
  removeModifiedRoot();
  addRoot();
  changeProperty();
  changeReference();
