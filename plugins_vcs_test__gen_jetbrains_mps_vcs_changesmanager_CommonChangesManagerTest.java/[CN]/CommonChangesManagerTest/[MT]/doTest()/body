  rollbackAllSerially();
  rollbackAllAtomically();
  createNewModel();
  deleteModelAndRollback();
  SwingUtilities.invokeAndWait(new Runnable(){
    public void run(){
      myIdeaProject.getComponent(InspectorTool.class).getInspector().editNode(null,null);
    }
  }
);
