{
  final Object lock=new Object();
  final Wrappers._boolean finished=new Wrappers._boolean(false);
  myRegistry.getCommandQueue().addTask(new Runnable(){
    public void run(){
synchronized (lock) {
        finished.value=true;
        lock.notify();
      }
    }
  }
);
  while (!(finished.value)) {
synchronized (lock) {
      try {
        lock.wait();
      }
 catch (      InterruptedException e) {
        e.printStackTrace();
      }
    }
  }
}
