{
  final Wrappers._boolean finished=new Wrappers._boolean(false);
  myRegistry.getCommandQueue().addTask(new Runnable(){
    public void run(){
synchronized (myWaitLock) {
        finished.value=true;
        myWaitLock.notify();
      }
    }
  }
);
  while (!(finished.value)) {
synchronized (myWaitLock) {
      try {
        myWaitLock.wait();
      }
 catch (      InterruptedException e) {
        e.printStackTrace();
      }
    }
  }
}
