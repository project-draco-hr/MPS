{
  SNode root=event.getRoot();
  if (!getLoadableNodes().contains(root)) {
    IOperationContext context=null;
    for (    Pair<SNode,IOperationContext> p : tryToLoadNodes()) {
      if (p.o1 == root) {
        context=p.o2;
        break;
      }
    }
    if (context != null) {
      addInnerTabChecked(event.getRoot(),context);
    }
  }
}
