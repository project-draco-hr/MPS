{
  if (node == null)   new ArrayList<SNode>();
  List<SNode> result=new ArrayList<SNode>();
  if (!inclusion && node != null) {
    node=node.getParent();
  }
  while (node != null) {
    if (_isInstanceOf(node,ancestorConceptFqNames)) {
      result.add(node);
    }
    node=node.getParent();
  }
  return result;
}
