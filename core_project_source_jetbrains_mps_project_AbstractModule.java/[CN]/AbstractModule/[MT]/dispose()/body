{
  assertCanChange();
  LOG.trace("Disposing the module " + this);
  ourFS.removeListener(this);
  for (  ModuleFacetBase f : myFacets) {
    f.dispose();
  }
  myFacets.clear();
  for (  ModelRoot m : mySModelRoots) {
    ((ModelRootBase)m).dispose();
  }
  mySModelRoots.clear();
  super.dispose();
}
