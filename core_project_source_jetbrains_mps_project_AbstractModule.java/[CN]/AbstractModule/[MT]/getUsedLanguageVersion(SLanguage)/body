{
  Integer res=getModuleDescriptor().getLanguageVersions().get(usedLanguage);
  if (res == null) {
    LOG.error("getUsedLanguageVersion can't find a version for language " + usedLanguage.getQualifiedName() + " in module "+ getModuleName()+ "."+ " This can either mean that the language is not imported into this module or that "+ "validateLanguageVersions was not called on this module in appropriate moment.",new Throwable());
    return usedLanguage.getLanguageVersion();
  }
  return res;
}
