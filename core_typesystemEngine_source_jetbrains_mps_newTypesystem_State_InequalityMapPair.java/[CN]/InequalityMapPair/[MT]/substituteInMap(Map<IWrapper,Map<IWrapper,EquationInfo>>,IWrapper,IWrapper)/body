{
  Map<IWrapper,EquationInfo> values=baseMap.remove(var);
  baseMap.put(type,values);
  if (values != null) {
    for (    final IWrapper value : values.keySet()) {
      Map<IWrapper,EquationInfo> map=baseMap.get(value);
      if (map != null) {
        EquationInfo info=map.remove(var);
        map.put(type,info);
      }
    }
  }
}
