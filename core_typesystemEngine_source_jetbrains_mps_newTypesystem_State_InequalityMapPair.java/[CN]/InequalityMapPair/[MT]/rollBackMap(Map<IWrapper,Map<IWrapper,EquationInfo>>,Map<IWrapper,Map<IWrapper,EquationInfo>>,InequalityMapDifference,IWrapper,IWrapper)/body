{
  Map<IWrapper,EquationInfo> values=baseMap.get(cur);
  if (diff != null) {
    for (    IWrapper wrapper : diff.getMap().keySet()) {
      values.remove(wrapper);
    }
    if (values.isEmpty()) {
      baseMap.remove(cur);
    }
    substituteInMap(baseMap,pairMap,cur,prev,diff.getMap());
  }
}
