{
  for (int i=0; i < SConceptPropertyOperations.getInteger(thisNode,"initialColumnCount"); i++) {
    SNode dataCell=SConceptOperations.createNewNode("jetbrains.mps.lang.editor.tableTests.structure.DataCell",null);
