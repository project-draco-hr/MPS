  }
  OutputStream os=null;
  try {
    os=cacheVFile.getOutputStream(BLDebugInfoCache.class);
    Element element=debugInfo.toXml();
    JDOMUtil.writeDocument(new Document(element),os);
  }
 catch (  IOException e) {
    LOG.error(e);
  }
 finally {
    if (os != null) {
      try {
        os.close();
      }
 catch (      IOException e) {
        LOG.error(e);
      }
    }
  }
  return cacheVFile;
}
