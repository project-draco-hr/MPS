{
  return new GraphAnalyzer<V>(){
    @Override public Iterable<V> forwardEdges(    V v){
      return MapSequence.<V,List<V>>fromMap(fwEdges).get(v);
    }
    @Override public Iterable<V> vertices(){
      return getVertices();
    }
    @Override public Iterable<V> backwardEdges(    V v){
      return MapSequence.<V,List<V>>fromMap(bkEdges).get(v);
    }
  }
;
}
