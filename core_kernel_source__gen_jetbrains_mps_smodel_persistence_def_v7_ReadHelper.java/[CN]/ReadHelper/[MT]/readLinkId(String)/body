{
  if (src == null) {
    return null;
  }
  int i0=src.indexOf(WriteHelper.MODEL_SEPARATOR_CHAR);
  int i1=src.lastIndexOf(WriteHelper.VERSION_SEPARATOR_CHAR);
  String text=WriteHelper.decode(src.substring(i0 + 1,(i1 < 0 ? src.length() : i1)));
  SModelReference modelRef=getSModelReference((i0 < 0 ? "" : src.substring(0,i0)));
  SNodeId nodeId=(text.equals("^") ? null : SNodeId.fromString(text));
  return new SNodePointer(modelRef,nodeId);
}
