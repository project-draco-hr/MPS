{
  if (x == myX && y == myY) {
    return;
  }
  int xOld=myX;
  int yOld=myY;
  super.moveTo(x,y);
  for (  EditorCell myEditorCell : getCells()) {
    myEditorCell.moveTo(myEditorCell.getX() + x - xOld,myEditorCell.getY() + y - yOld);
    if (((EditorCell_Basic)myEditorCell).isNeedsRelayout()) {
      markNeedsRelayout();
    }
  }
  adjustNeedsRelayout(xOld,yOld,x,y);
}
