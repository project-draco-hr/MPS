{
  SNode node=cell.getSNode();
  if (node == null) {
    return null;
  }
  String role=cell.getRole();
  if (role == null) {
    return null;
  }
  for (  SContainmentLink link : node.getConcept().getContainmentLinks()) {
    if (link.getName().equals(role)) {
      return link;
    }
  }
  return null;
}
