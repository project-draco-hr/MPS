{
  SAbstractConcept concept=MetaAdapterFactoryByName.getConcept(conceptFQName);
  if (concept.isValid()) {
    return concept.getDeclarationNode();
  }
  concept=MetaAdapterFactoryByName.getInterfaceConcept(conceptFQName);
  if (concept.isValid()) {
    return concept.getDeclarationNode();
  }
  return null;
}
