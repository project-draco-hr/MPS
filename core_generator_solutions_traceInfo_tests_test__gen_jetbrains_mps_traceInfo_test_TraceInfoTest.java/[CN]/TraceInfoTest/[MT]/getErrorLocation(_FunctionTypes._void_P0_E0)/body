{
  try {
    method.invoke();
  }
 catch (  Throwable t) {
    StackTraceElement stackTraceElement=t.getStackTrace()[0];
    SNode node=TraceInfoUtil.getJavaNode(stackTraceElement.getClassName(),stackTraceElement.getFileName(),stackTraceElement.getLineNumber());
    return node;
  }
  return null;
}
