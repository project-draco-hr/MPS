{
  if (myScope == null) {
    ModelAccess.instance().runReadAction(new Runnable(){
      public void run(){
        myScope=new ReachableClassifiersScope(myAuxModel.getSModel(),IClassifiersSearchScope.CLASSIFFIER,getModule().getScope());
      }
    }
);
  }
  ModelAccess.instance().runReadAction(new Runnable(){
    public void run(){
      fillVariables();
    }
  }
);
}
