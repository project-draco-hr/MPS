{
  AbstractCellListHandler handler=new BaseCommentAttribute_Component.commentedNodesListHandler_e15pfa_a0(node,"commentedNodes",editorContext);
  EditorCell_Collection editorCell=handler.createCells(editorContext,new CellLayout_Indent(),false);
  editorCell.setCellId("BCAC_refNodeList_commentedNodes");
  Style style=new StyleImpl();
  style.set(StyleAttributes.READ_ONLY,0,true);
  style.set(StyleAttributes.EDITABLE,0,false);
  editorCell.getStyle().putAll(style);
  editorCell.setRole(handler.getElementRole());
  return editorCell;
}
