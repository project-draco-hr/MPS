{
  if (myTest == null) {
    return;
  }
  try {
    SwingUtilities.invokeAndWait(new Runnable(){
      public void run(){
        ModelAccess.instance().runWriteAction(new Runnable(){
          public void run(){
            myTest.dispose();
            myTest=null;
          }
        }
);
      }
    }
);
  }
 catch (  InvocationTargetException e) {
    if (LOG.isEnabledFor(Level.ERROR)) {
      LOG.error("",e);
    }
  }
catch (  InterruptedException e) {
    if (LOG.isEnabledFor(Level.ERROR)) {
      LOG.error("",e);
    }
  }
}
