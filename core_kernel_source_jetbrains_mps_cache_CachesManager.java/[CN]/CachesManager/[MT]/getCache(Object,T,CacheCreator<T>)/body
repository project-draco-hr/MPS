{
synchronized (myLock) {
    AbstractCache result=myCaches.get(key);
    if (result != null || element == null || creator == null) {
      return result;
    }
    result=creator.create(key,element);
    putCache(key,result,new ArrayList<SModelDescriptor>(result.getDependsOnModels(element)));
    return result;
  }
}
