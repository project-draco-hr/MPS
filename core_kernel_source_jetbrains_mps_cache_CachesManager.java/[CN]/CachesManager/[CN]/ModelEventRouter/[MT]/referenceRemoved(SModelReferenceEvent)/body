{
  myCache.referenceRemoved(event);
  if (!myCache.isAttached())   return;
  for (  DataSet dataSet : myCache.getDataSets()) {
    if (dataSet.getDependsOnNodes().contains(event.getReference().getSourceNode())) {
      dataSet.referenceRemoved(event);
      if (!myCache.isAttached())       return;
    }
  }
}
