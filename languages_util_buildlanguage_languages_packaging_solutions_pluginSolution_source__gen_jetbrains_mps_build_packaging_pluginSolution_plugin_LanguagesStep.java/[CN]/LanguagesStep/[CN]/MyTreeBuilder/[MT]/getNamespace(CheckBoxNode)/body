{
  NodeData data=node.getData();
  String namespace="";
  if (data instanceof ModuleData) {
    ModuleData moduleData=(ModuleData)data;
    namespace=((StandaloneMPSProject)this.myMpsProject).getFolderFor(moduleData.getModule());
  }
  if (namespace == null) {
    return "";
  }
  return namespace;
}
