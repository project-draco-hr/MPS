{
  InputStream input=null;
  try {
    input=file.openInputStream();
    byte[] bytes=ReadUtil.read(input);
    return loadModel(bytes,file.getName().substring(file.getName().lastIndexOf(".")));
  }
 catch (  IOException e) {
    return null;
  }
 finally {
    if (input != null) {
      try {
        input.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
