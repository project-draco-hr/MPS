{
  AccountingEvent evt=new MonetaryEvent(Money.dollars(40),EventType.SERVICE_CALL,new MfDate(1999,1,5),new MfDate(1999,1,5),fixture.getWatson());
  fixture.getEventList().add(evt);
  try {
    fixture.getEventList().process();
  }
 catch (  RuntimeException expected) {
  }
  assertTrue(fixture.getEventList().hasProcessingErrors());
}
