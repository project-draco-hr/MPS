{
  myProject.getModelAccess().runWriteInEDT(new Runnable(){
    public void run(){
      if (nodeReference != null) {
        SNode node=nodeReference.resolve(myProject.getRepository());
        if (node != null) {
          NavigationSupport.getInstance().openNode(myProject,node,true,true);
        }
      }
    }
  }
);
}
