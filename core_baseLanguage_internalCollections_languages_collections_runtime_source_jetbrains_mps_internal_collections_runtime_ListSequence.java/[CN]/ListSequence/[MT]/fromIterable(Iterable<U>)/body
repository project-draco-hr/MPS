{
  if (Sequence.USE_NULL_SEQUENCE) {
    if (it == null) {
      return NullListSequence.instance();
    }
  }
  if (it instanceof IListSequence) {
    return (IListSequence<U>)it;
  }
  List<U> list=new ArrayList<U>();
  for (  U u : it) {
    list.add(u);
  }
  return new ListSequence<U>(list);
}
