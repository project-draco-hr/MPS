{
  List<SNode> result=Sequence.fromIterable(scope.getAvailableElements(null)).toListSequence();
  if (condition != null) {
    Iterator<SNode> it=ListSequence.fromList(result).iterator();
    while (it.hasNext()) {
      SNode node=it.next();
      if (!(condition.met(node))) {
        it.remove();
      }
    }
  }
  return result;
}
