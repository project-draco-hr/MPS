{
  if (role == null)   return null;
  List<T> result=null;
  for (PsiElement child=getFirstChild(); child != null; child=child.getNextSibling()) {
    if (child instanceof MPSPsiNode && role.equals(((MPSPsiNode)child).getContainingRole()) && aClass.isInstance(child)) {
      return (T)child;
    }
  }
  return null;
}
