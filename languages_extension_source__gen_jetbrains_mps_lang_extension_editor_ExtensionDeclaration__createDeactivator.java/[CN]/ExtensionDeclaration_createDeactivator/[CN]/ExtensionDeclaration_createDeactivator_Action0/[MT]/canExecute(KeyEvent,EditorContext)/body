{
  EditorCell contextCell=editorContext.getContextCell();
  if ((contextCell == null)) {
    return false;
  }
  SNode contextNode=contextCell.getSNode();
  if (contextNode == null) {
    return false;
  }
  if (contextNode.isInstanceOfConcept("jetbrains.mps.lang.extension.structure.ExtensionDeclaration")) {
    return true;
  }
  return false;
}
