        SNode downcast=SNodeFactoryOperations.replaceWithNewChild(_context.getSourceNode(),"jetbrains.mps.debug.evaluation.structure.DownCastToLowLevel");
        SLinkOperations.setTarget(downcast,"expression",_context.getSourceNode(),true);
        return downcast;
      }
      public String getMatchingText(      String pattern){
        return "/";
      }
      public String getVisibleMatchingText(      String pattern){
        return this.getMatchingText(pattern);
      }
      public String getDescriptionText(      String pattern){
        return "downcast to low-level type";
      }
    }
);
  }
  return result;
}
