{
  IntGraph g=new IntGraph(3);
  g.addEdges(1,0);
  g.addEdges(2,0);
  List<List<IVertex>> condensation=Graphs.findStronglyConnectedComponents(g.getGraph());
  Assert.assertEquals(Arrays.asList(Arrays.asList(new IntVertex(2)),Arrays.asList(new IntVertex(1)),Arrays.asList(new IntVertex(0))),condensation);
}
