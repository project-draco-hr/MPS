{
  for (  SNode call : ListSequence.fromList(SNodeOperations.getDescendants(getMethod(),null,false,new String[]{}))) {
    if (MethodCallAdapter.isMethodCall(call)) {
      if (new MethodCallAdapter(call).getMethodDeclaration() == getMethod()) {
        return true;
      }
    }
  }
  return false;
}
