{
  RuntimeEnvironment<String> re=new RuntimeEnvironment<String>();
  RBundle<String> a=bundle("a",A.class);
  RBundle<String> b=bundle("b",B.class).addDependency("a");
  re.add(a,b);
  re.init(b);
  b.clearDependencies();
  try {
    re.unload(a);
  }
 catch (  RuntimeEnvironmentException e) {
    fail(e.getMessage());
  }
}
