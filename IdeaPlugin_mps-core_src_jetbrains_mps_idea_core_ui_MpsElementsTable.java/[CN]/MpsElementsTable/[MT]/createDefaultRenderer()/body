{
  return new ColoredTableCellRenderer(){
    @Override protected void customizeCellRenderer(    JTable table,    Object value,    boolean selected,    boolean hasFocus,    int row,    int column){
      setPaintFocusBorder(false);
      setFocusBorderAroundIcon(true);
      setBorder(NO_FOCUS_BORDER);
      if (value != null) {
        T tableItem=(T)value;
        setIcon(MpsElementsTable.this.getIcon(tableItem));
        append(getText(tableItem),getTextAttributes(tableItem));
      }
    }
  }
;
}
