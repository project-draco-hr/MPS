{
  if (!myErrors.isEmpty() && myWhatToDo.getFailOnError()) {
    StringBuffer sb=new StringBuffer();
    sb.append(myErrors.size());
    sb.append(" errors during generation:\n");
    for (    String error : myErrors) {
      sb.append(error);
      sb.append("\n");
    }
    throw new BuildException(sb.toString());
  }
}
