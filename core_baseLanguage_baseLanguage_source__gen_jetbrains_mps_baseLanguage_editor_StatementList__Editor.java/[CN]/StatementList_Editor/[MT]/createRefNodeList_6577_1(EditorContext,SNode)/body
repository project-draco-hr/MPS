{
  AbstractCellListHandler handler=new StatementList_Editor.statementListHandler_6577_1(node,"statement",context);
  EditorCell_Collection editorCell=handler.createCells(context,new CellLayout_Indent(),false);
  editorCell.setCellId("refNodeList_statement_1");
{
    Style style=editorCell.getStyle();
    style.set(StyleAttributes.SELECTABLE,false);
  }
  editorCell.setCanBeFolded(true);
  editorCell.setRole(handler.getElementRole());
  return editorCell;
}
