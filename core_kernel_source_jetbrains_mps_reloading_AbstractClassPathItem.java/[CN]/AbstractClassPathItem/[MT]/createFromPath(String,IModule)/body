{
  IFile file=FileSystem.getFile(path);
  if (!file.exists())   throw new IOException("Can't load class path item " + path + " in "+ (module == null ? "" : module.toString())+ (file.isDirectory() ? ". Execute make in IDEA." : "."));
  IClassPathItem currentItem;
  if (file.isDirectory()) {
    currentItem=new FileClassPathItem(path);
  }
 else {
    currentItem=new JarFileClassPathItem(path);
  }
  return currentItem;
}
