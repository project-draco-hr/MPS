{
  SNode expression=SNodeOperations.replaceWithAnother(node,SLinkOperations.getTarget(node,"expr",true));
  if (SNodeOperations.isInstanceOf(SNodeOperations.getParent(expression),"jetbrains.mps.core.query.structure.MqlBinaryExpr")) {
