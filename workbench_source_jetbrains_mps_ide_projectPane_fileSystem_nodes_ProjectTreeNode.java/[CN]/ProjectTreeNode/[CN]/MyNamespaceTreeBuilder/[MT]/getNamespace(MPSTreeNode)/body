{
  String folder="";
  if (node instanceof ModuleTreeNode) {
    StandaloneMPSProject mpsProject=(StandaloneMPSProject)myProject.getComponent(MPSProject.class);
    folder=mpsProject.getFolderFor(((ModuleTreeNode)node).getModule());
  }
  if (folder == null) {
    return "";
  }
  return folder;
}
