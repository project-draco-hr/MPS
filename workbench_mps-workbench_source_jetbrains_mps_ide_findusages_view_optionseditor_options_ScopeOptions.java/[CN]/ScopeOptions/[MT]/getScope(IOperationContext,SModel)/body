{
switch (myScopeType) {
case GLOBAL:
    return new GlobalScope();
case PROJECT:
  return new ProjectScope(operationContext.getProject());
case MODULE:
return new ModulesScope(myModule);
case MODEL:
return new ModelsScope(myModel);
default :
LOG.error("Illegal scope type: " + myScopeType);
return new GlobalScope();
}
}
