{
  if (myModule == null || myModule.isEmpty()) {
    return null;
  }
  SModule module=getModuleByNamespace(myModule);
  if (module == null) {
    LOG.error("Module is not found for " + myModule + ". Using current module.");
    module=context.getModule();
    myModule=module.getModuleName();
  }
  return module;
}
