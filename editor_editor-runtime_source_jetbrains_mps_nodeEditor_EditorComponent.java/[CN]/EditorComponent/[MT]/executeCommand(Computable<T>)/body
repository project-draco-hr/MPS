{
  if (myInsideOfCommand) {
    return c.compute();
  }
  myInsideOfCommand=true;
  try {
    final Object[] result=new Object[1];
    getModelAccess().executeCommand(new Runnable(){
      @Override public void run(){
        result[0]=c.compute();
      }
    }
);
    return (T)result[0];
  }
  finally {
    myInsideOfCommand=false;
  }
}
