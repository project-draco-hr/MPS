{
  StringBuilder sb=new StringBuilder("Model was disposed through:");
  for (  StackTraceElement element : myModelDisposedStackTrace) {
    sb.append("\nat ");
    sb.append(element);
  }
  sb.append("\n");
  sb.append("EditorComponent.myDisposed == ");
  sb.append(isDisposed());
  sb.append("\n");
  return sb.toString();
}
