{
  getEditorContext().pushTracerTask("setting root cell",true);
  if (myRootCell != null) {
    ((EditorCell_Basic)myRootCell).onRemove();
  }
  myRootCell=rootCell;
  if (myRootCell != null) {
    ((EditorCell_Basic)myRootCell).onAdd();
  }
  revalidate();
  repaint();
  getEditorContext().popTracerTask();
}
