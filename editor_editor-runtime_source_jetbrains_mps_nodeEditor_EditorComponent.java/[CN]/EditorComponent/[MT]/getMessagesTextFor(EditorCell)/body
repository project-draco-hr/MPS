{
  List<HighlighterMessage> messages=getHighlighterMessagesFor(cell);
  if (messages.isEmpty()) {
    return null;
  }
  StringBuilder result=new StringBuilder();
  for (  HighlighterMessage message : messages) {
    if (result.length() != 0) {
      result.append("\n");
    }
    result.append(message.getMessage());
  }
  return result.toString();
}
