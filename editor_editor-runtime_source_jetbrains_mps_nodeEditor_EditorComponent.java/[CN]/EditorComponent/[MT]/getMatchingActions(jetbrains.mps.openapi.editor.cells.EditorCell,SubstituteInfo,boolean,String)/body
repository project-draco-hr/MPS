{
  return runRead(new Computable<List<SubstituteAction>>(){
    @Override public List<SubstituteAction> compute(){
      final ITypeContextOwner contextOwner=isSmart ? new NonReusableTypecheckingContextOwner() : getTypecheckingContextOwner();
      return TypeContextManager.getInstance().runTypeCheckingComputation(contextOwner,myNode,new Computation<List<SubstituteAction>>(){
        @Override public List<SubstituteAction> compute(        TypeCheckingContext context){
          return isSmart ? substituteInfo.getSmartMatchingActions(pattern,false,editorCell) : substituteInfo.getMatchingActions(pattern,false);
        }
      }
);
    }
  }
);
}
