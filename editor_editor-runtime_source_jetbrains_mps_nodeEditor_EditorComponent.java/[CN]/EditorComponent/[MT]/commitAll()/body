{
  if (getOperationContext() == null) {
    return;
  }
  jetbrains.mps.project.Project project=getOperationContext().getProject();
  if (project == null) {
    return;
  }
  ModelAccess.instance().runCommandInEDT(new Runnable(){
    @Override public void run(){
      doCommitAll(getRootCell());
    }
  }
,project);
}
