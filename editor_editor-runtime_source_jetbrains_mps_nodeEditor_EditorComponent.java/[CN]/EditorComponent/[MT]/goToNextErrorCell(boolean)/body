{
  if (getSelectedCell() == null) {
    return;
  }
  new CellNavigator(this){
    @Override boolean isSuitableCell(    jetbrains.mps.openapi.editor.cells.EditorCell cell){
      if (APICellAdapter.hasErrorMessages(cell)) {
        return true;
      }
      return false;
    }
  }
.goToNextCell(backwards);
}
