{
  try {
    EditorCell_Collection collectionCell=(Expand_Action.this.editorCell instanceof EditorCell_Collection ? (EditorCell_Collection)Expand_Action.this.editorCell : Expand_Action.this.editorCell.getParent());
    while (collectionCell != null) {
      if (collectionCell.isFolded()) {
        collectionCell.unfold();
        return;
      }
      collectionCell=collectionCell.getParent();
    }
  }
 catch (  Throwable t) {
    if (log.isErrorEnabled()) {
      log.error("User's action execute method failed. Action:" + "Expand",t);
    }
  }
}
