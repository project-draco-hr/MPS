{
  checkWriteAccess();
  if (myCommandLevel != 0) {
  }
 else {
    UndoContext context;
    if (command instanceof UndoContext) {
      context=(UndoContext)command;
    }
 else {
      context=new DefaultUndoContext();
    }
    UndoHelper.getInstance().startCommand(context);
    onCommandStarted();
  }
  myCommandLevel++;
}
