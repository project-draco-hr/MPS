{
  for (  IFile file : rootDir.getChildren()) {
    String path=file.getName();
    if (prefix != null) {
      path=prefix + "/" + file.getName();
    }
    if (file.isDirectory() && !file.getChildren().isEmpty()) {
      collectFilePathList(list,file,path);
    }
 else {
      list.add(path);
    }
  }
}
