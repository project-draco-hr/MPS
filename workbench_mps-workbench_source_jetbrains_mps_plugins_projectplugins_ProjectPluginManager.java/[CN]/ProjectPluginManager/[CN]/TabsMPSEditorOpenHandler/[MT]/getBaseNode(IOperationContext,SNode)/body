{
  for (  RelationDescriptor d : getTabDescriptors()) {
    SNode baseNode=d.getBaseNode(node);
    if (baseNode == node) {
      LOG.error("Editor tabs should not return node as a base node of itself: " + d.getClass().getName());
      continue;
    }
    if (baseNode != null)     return baseNode;
  }
  return null;
}
