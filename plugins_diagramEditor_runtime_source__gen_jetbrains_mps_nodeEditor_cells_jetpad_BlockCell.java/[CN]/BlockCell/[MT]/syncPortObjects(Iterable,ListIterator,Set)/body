{
  for (  Object port : ports) {
    if (existingPorts.contains(port)) {
      syncToNextObject(portsIterator,existingPorts,port);
    }
 else {
      portsIterator.add(port);
      existingPorts.add(port);
    }
  }
  while (portsIterator.hasNext()) {
    portsIterator.next();
    portsIterator.remove();
  }
}
