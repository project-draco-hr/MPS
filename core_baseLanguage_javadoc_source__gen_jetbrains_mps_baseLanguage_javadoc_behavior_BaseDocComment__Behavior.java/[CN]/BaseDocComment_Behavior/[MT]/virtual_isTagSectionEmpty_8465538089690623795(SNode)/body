{
  return ListSequence.fromList(SLinkOperations.getTargets(thisNode,"author",true)).count() == 0 && (SLinkOperations.getTarget(thisNode,"deprecated",true) == null) && ListSequence.fromList(SLinkOperations.getTargets(thisNode,"since",true)).count() == 0 && ListSequence.fromList(SLinkOperations.getTargets(thisNode,"version",true)).count() == 0;
}
