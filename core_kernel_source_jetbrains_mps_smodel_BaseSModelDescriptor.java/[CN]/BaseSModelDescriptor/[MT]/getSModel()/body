{
  SModel result;
  boolean fireInitialized=false;
synchronized (myLoadingLock) {
    if (!isInitialized()) {
      SModel model=loadModel();
      model.setModelDescritor(this);
      mySModel=model;
      fireInitialized=true;
    }
    result=mySModel;
  }
  if (fireInitialized) {
    fireModelInitialized();
  }
  return result;
}
