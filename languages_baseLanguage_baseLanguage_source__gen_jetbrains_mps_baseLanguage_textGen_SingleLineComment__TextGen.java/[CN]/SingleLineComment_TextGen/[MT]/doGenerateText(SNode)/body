{
  if (getBuffer().hasPositionsSupport()) {
    TraceInfoGenerationUtil.createPositionInfo(this,node);
  }
  this.appendNewLine();
  this.indentBuffer();
  this.append("// ");
  for (  SNode commentPart : SLinkOperations.getTargets(node,"commentPart",true)) {
    appendNode(commentPart);
    this.append(" ");
  }
  if (getBuffer().hasPositionsSupport()) {
{
