{
  IFile cacheFile=getCacheFile(sm);
  if (cacheFile == null || !cacheFile.exists())   return null;
  InputStream is=null;
  try {
    is=cacheFile.openInputStream();
    return load(is);
  }
 catch (  IOException e) {
    LOG.error(e);
  }
 finally {
    try {
      if (is != null) {
        is.close();
      }
    }
 catch (    IOException e) {
      LOG.error(e);
    }
  }
  return null;
}
