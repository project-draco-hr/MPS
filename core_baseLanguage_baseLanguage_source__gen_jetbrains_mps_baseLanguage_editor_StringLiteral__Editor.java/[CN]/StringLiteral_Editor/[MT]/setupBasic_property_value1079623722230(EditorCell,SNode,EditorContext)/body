{
  editorCell.putUserObject(EditorCell.CELL_ID,"property_value");
{
    Style inlineStyle=new Style(editorCell){
{
        this.set(StyleAttributes.SELECTABLE,new AttributeCalculator<Boolean>(){
          public Boolean calculate(          EditorCell cell){
            return StringLiteral_Editor.calculateBoolean3246_0(cell);
          }
        }
);
        this.set(StyleAttributes.FONT_STYLE,new AttributeCalculator<Integer>(){
          public Integer calculate(          EditorCell cell){
            return StringLiteral_Editor.calculateFontStyle3246_2(cell);
          }
        }
);
        this.set(StyleAttributes.EDITABLE,new AttributeCalculator<Boolean>(){
          public Boolean calculate(          EditorCell cell){
            return StringLiteral_Editor.calculateBoolean3246_2(cell);
          }
        }
);
        this.set(StyleAttributes.TEXT_COLOR,new AttributeCalculator<Color>(){
          public Color calculate(          EditorCell cell){
            return StringLiteral_Editor.calculateColor3246_1(cell);
          }
        }
);
        this.set(StyleAttributes.PADDING_LEFT,0.0);
        this.set(StyleAttributes.PADDING_RIGHT,0.0);
      }
    }
;
    inlineStyle.apply(editorCell);
  }
  if (true) {
    editorCell.setFocusPolicy(FocusPolicy.ATTRACTS_FOCUS);
  }
}
