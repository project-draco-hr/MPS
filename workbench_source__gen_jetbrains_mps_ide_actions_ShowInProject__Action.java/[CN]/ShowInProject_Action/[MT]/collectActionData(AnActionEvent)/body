{
  try {
    this.context=new ActionEventData(event).getOperationContext();
    if (this.context == null) {
      return false;
    }
    this.node=new ActionEventData(event).getNode();
    if (this.node == null) {
      return false;
    }
  }
 catch (  Throwable t) {
    return false;
  }
  return true;
}
