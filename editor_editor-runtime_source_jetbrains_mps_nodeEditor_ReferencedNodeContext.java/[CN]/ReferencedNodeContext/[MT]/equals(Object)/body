{
  if (obj == this)   return true;
  if (obj instanceof ReferencedNodeContext) {
    ReferencedNodeContext o=(ReferencedNodeContext)obj;
    return ObjectUtils.equals(myNode,o.myNode) && ObjectUtils.equals(myContextRoles,o.myContextRoles) && ObjectUtils.equals(myContextRefererNodes,o.myContextRefererNodes)&& myIsNodeAttribute == o.myIsNodeAttribute;
  }
 else {
    return false;
  }
}
