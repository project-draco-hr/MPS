{
  StringBuffer sb=new StringBuffer();
  sb.append("declarations ");
  sb.append(myDecls);
  sb.append("\n");
  sb.append("nesteds ");
  sb.append(myNesteds);
  sb.append("\n");
  sb.append("parents ");
  sb.append(myParents);
  sb.append("\n");
  sb.append("attributes ");
  sb.append(myAttributes);
  return sb.toString();
}
