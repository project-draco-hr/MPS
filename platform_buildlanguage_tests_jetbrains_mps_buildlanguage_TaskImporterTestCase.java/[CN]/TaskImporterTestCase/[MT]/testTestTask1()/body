{
  try {
    Map<String,Class<?>> attrs=SmartItemsImporter.getInstance().getAttributesFromTask(ANT_PATH,TEST_FOLDER,"jetbrains.mps.buildlanguage.resource.TestTask",Task.class);
    assertTrue(attrs.get("booleanattribute").equals(boolean.class));
    assertTrue(attrs.get("intattribute").equals(int.class));
    assertTrue(attrs.get("stringattribute").equals(String.class));
    assertEquals(3,attrs.size());
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
catch (  ClassNotFoundException e) {
    throw new RuntimeException(e);
  }
}
