{
  if (!myHandleWarnings) {
    return;
  }
  myWarningsCount++;
  report(MessageKind.WARNING,message,node);
  if (descriptions == null) {
    return;
  }
  for (  ProblemDescription d : descriptions) {
    if (d != null) {
      report(MessageKind.WARNING,"-- " + d.getMessage(),d.getNode());
    }
  }
}
