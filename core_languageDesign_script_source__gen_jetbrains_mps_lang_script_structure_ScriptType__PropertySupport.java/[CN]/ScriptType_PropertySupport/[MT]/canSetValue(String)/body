{
  if (value == null) {
    return true;
  }
  Iterator<ScriptType> constants=ListSequence.<ScriptType>fromList(ScriptType.getConstants()).iterator();
  while (constants.hasNext()) {
    ScriptType constant=constants.next();
    if (value.equals(constant.getName())) {
      return true;
    }
  }
  return false;
}
