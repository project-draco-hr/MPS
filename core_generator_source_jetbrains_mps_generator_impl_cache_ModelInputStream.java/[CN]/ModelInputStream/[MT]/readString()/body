{
  int c=readByte();
  if (c == 0x70) {
    return null;
  }
 else   if (c == 1) {
    int index=readInt();
    return myStrings.get(index);
  }
  StringBuilder sb=null;
  while (c == 42) {
    String prefix=readUTF();
    if (sb == null) {
      sb=new StringBuilder(prefix);
    }
 else {
      sb.append(prefix);
    }
    c=readByte();
  }
  String res=sb == null ? readUTF() : sb.toString() + readUTF();
  myStrings.add(res);
  return res;
}
