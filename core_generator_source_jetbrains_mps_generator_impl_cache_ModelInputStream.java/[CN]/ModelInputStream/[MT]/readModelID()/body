{
  int c=readByte();
  if (c == 0x70) {
    return null;
  }
 else   if (c == 0x28) {
    UUID uuid=new UUID(readLong(),readLong());
    return SModelId.regular(uuid);
  }
 else   if (c == 0x27) {
    return SModelId.foreign(readString());
  }
 else {
    throw new IOException("unknown id");
  }
}
