{
  if (tab == null || tab.getTabComponent() == null)   return;
  if (tab.getTip() == null)   tab.setTip(tab.getName());
  if (tab.getIcon() != null)   myTabbedPaneWrapper.addTab(tab.getName(),tab.getIcon(),tab.getTabComponent(),tab.getTip());
 else   myTabbedPaneWrapper.addTab(tab.getName(),tab.getTabComponent());
}
