{
  Program p=new SimpleProgramBuilder().emitTry().emitTry().emitRet().emitFinally().emitEndTry().emitNop().emitFinally().emitEndTry().emitNop().buildProgram();
  AnalysisResult<Boolean> result=p.analyze(new ReachabilityAnalyzer());
  Assert.assertEquals("0: try true\n" + "1: try true\n" + "2: ret true\n"+ "3: finally true\n"+ "4: endTry true\n"+ "5: nop false\n"+ "6: finally true\n"+ "7: endTry true\n"+ "8: nop false\n"+ "9: end true\n",result.toString());
}
