{
  if (value == null) {
    return true;
  }
  Iterator<MappingScriptKind> constants=MappingScriptKind.getConstants().iterator();
  while (constants.hasNext()) {
    MappingScriptKind constant=constants.next();
    if (value.equals(constant.getName())) {
      return true;
    }
  }
  return false;
}
