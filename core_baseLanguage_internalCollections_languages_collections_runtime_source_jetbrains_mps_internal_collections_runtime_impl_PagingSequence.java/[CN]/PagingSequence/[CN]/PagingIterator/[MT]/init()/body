{
switch (page) {
case TAKE:
case SKIP:
    this.inputIt=input.toIterable().iterator();
  this.countDown=length;
break;
case TAIL:
case CUT:
this.cache=new ArrayList<U>();
for (U o : input) {
cache.add(o);
}
this.inputIt=cache.iterator();
this.countDown=Math.max(0,cache.size() - length);
break;
default :
break;
}
}
