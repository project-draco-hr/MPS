{
  if (value == null) {
    return null;
  }
  Iterator<MyKind> constants=ListSequence.fromList(MyKind.getConstants()).iterator();
  while (constants.hasNext()) {
    MyKind constant=constants.next();
    if (value.equals(constant.getName())) {
      return constant.getValueAsString();
    }
  }
  return null;
}
