{
  boolean solutionCheck=false;
  if (myConfigurableItem instanceof Solution) {
    SolutionDescriptor descriptor=(SolutionDescriptor)myModuleDescriptor;
    solutionCheck=descriptor.getCompileInMPS() != myCheckBox.isSelected() || descriptor.getKind() != myComboBox.getSelectedItem();
  }
  return !(myGenOut.getText().equals(getGenOutPath())) || myPathsTableModel.isModified() || myLibraryTableModel.isModified()|| solutionCheck;
}
