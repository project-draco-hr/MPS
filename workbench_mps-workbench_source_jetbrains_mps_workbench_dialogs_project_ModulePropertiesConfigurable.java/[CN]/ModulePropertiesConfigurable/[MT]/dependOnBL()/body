{
  ModuleDependenciesManager dependenciesManager=new ModuleDependenciesManager(MPSModuleRepository.getInstance().getModuleById(myModuleDescriptor.getId()));
  IModule bl=MPSModuleRepository.getInstance().getModuleByFqName("jetbrains.mps.baseLanguage");
  return dependenciesManager.directlyUsedLanguages().contains(bl) || dependenciesManager.directlyUsedModules(true,true).contains(bl);
}
