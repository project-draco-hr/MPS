{
  int last0=Integer.MIN_VALUE;
  int last1=Integer.MIN_VALUE;
  for (  Tuples._2<Integer,Integer> p : ListSequence.fromList(pos)) {
    if (p._0() <= last0) {
      throw new RuntimeException("fail");
    }
    if (p._1() < last1) {
      throw new RuntimeException("fail");
    }
    last0=p._0();
    last1=p._1();
  }
}
