{
  if ("".equals(text)) {
    return;
  }
  if (workStack.isEmpty()) {
    workStack.push(null);
  }
 else   if (workStack.peek() != null && neq_8r1isy_a0a0b0e(text,workStack.peek())) {
    popState();
    workStack.push(null);
  }
  if (!(workStack.isEmpty()) && workStack.peek() == null) {
    workStack.pop();
    workStack.push(text);
    jm.currentProgress().beginWork(text,WORK_AMOUNT,0);
  }
}
