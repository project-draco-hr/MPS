{
  return (StringUtils.isEmpty(SPropertyOperations.getString(thisNode,"relativePath")) ? "${" + SPropertyOperations.getString(SLinkOperations.getTarget(thisNode,"macro",false),"name") + "}" : "${" + SPropertyOperations.getString(SLinkOperations.getTarget(thisNode,"macro",false),"name") + "}/"+ SPropertyOperations.getString(thisNode,"relativePath"));
}
