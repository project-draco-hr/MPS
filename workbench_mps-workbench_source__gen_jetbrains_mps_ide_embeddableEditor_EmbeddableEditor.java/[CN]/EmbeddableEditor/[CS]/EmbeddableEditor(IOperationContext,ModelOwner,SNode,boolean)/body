{
  myOwner=owner;
  myContext=context;
  myIsEditable=editable;
  myModel=((EditableSModelDescriptor)ProjectModels.createDescriptorFor(true));
  myModel.getSModel().addDevKit(GeneralPurpose_DevKit.MODULE_REFERENCE);
