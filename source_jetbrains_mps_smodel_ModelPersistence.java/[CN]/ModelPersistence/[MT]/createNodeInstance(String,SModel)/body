{
  try {
    if (!model.getUID().toString().startsWith(NameUtil.namespaceFromLongName(type)) && SModelUtil.findConceptDeclaration(NameUtil.conceptFQNameByClassName(type),GlobalScope.getInstance()) == null) {
      return createNodeWoStructure(model,type);
    }
    Method method=QueryMethod.getNewInstanceMethod(type);
    if (method == null) {
      return createNodeWoStructure(model,type);
    }
    return (SNode)method.invoke(null,model);
  }
 catch (  SecurityException e) {
    LOG.error(e);
  }
catch (  IllegalAccessException e) {
    LOG.error(e);
  }
catch (  IllegalArgumentException e) {
    LOG.error(e);
  }
catch (  InvocationTargetException e) {
    LOG.error(e);
  }
  return null;
}
