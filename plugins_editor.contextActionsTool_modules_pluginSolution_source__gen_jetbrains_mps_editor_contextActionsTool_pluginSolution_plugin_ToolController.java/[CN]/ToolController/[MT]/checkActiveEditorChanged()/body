{
  EditorComponent activeEditor=ContextActionsUtil.findActiveEditor(myToolComponent);
  if (activeEditor == null) {
    return;
  }
  if (check_ae45oa_a0c0r(myLastSelection) == null) {
    return;
  }
  if (activeEditor == check_ae45oa_a0d0r(myLastSelection)) {
    return;
  }
  if (activeEditor == check_ae45oa_a0e0r(myNextSelection)) {
    return;
  }
  Selection selection=activeEditor.getSelectionManager().getSelection();
  if (selection != null) {
    update(selection);
  }
}
