{
  super(new Pair<JavaThread,Integer>(threadReference,i));
  myIndex=i;
  myThread=threadReference;
  StackFrame stackFrame=getStackFrame();
  if (stackFrame != null) {
    myLocation=new JavaLocation(stackFrame.location());
    myClassFqName=myLocation.getUnitName();
  }
 else {
    myLocation=null;
    myClassFqName=null;
  }
}
