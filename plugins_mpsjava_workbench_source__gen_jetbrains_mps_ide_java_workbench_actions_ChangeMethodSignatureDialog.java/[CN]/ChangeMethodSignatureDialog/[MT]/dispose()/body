{
  if (myEditor != null) {
    myEditor.disposeEditor();
    myEditor=null;
  }
  if (myTempModel != null) {
    myProject.getModelAccess().runWriteAction(new Runnable(){
      public void run(){
        TemporaryModels.getInstance().dispose(myTempModel);
        myTempModel=null;
      }
    }
);
  }
  super.dispose();
}
