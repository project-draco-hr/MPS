{
  while (roots.hasNext()) {
    Root root=roots.next();
    File languageRoot=new File(root.getPath());
    if (languageRoot.exists()) {
      readLanguageDescriptors(languageRoot,owner);
    }
 else {
      String error="Couldn't load languages from " + languageRoot.getAbsolutePath() + "\nDirectory doesn't exist: ";
      LOG.error(error);
    }
  }
}
