{
  ModelFactory factory=new BinaryModelPersistence();
  try {
    InMemoryStreamDataSource source=new InMemoryStreamDataSource();
    factory.save(model,source);
    return source.myStream.toByteArray();
  }
 catch (  ModelSaveException e) {
    e.printStackTrace();
  }
catch (  IOException e) {
    e.printStackTrace();
  }
  return null;
}
