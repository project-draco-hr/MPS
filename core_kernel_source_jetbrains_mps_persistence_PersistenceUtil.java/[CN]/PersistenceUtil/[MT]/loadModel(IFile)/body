{
  ModelFactory factory=PersistenceFacade.getInstance().getModelFactory(FileUtil.getExtension(file.getName()));
  if (factory == null) {
    return null;
  }
  SModel model=factory.load(new FileDataSource(file),Collections.<String,String>singletonMap(ModelFactory.OPTION_CONTENT_ONLY,Boolean.TRUE.toString()));
  if (model != null) {
    model.load();
  }
  return model;
}
