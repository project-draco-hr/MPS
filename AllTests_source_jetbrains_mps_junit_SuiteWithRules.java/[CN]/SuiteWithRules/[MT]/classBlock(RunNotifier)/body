{
  Object suite;
  try {
    suite=new ReflectiveCallable(){
      @Override protected Object runReflectiveCall() throws Throwable {
        return createSuite();
      }
    }
.run();
  }
 catch (  Throwable e) {
    return new Fail(e);
  }
  Statement statement=super.classBlock(notifier);
  statement=withSuite(suite,statement);
  return statement;
}
