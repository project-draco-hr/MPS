{
  StringBuilder sb=new StringBuilder();
  for (  V vertex : myVertices) {
    sb.append(vertex);
    sb.append(" -> ");
    int j=0;
    Set<? extends IVertex> nexts=vertex.getNexts();
    for (    IVertex next : nexts) {
      sb.append(next);
      if (j < nexts.size() - 1) {
        sb.append(", ");
      }
      j++;
    }
    sb.append("\n");
  }
  return sb.toString();
}
