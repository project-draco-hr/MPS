{
  if (this.myParameterListHandler == null) {
    this.myParameterListHandler=new ConstructorDeclaration_Editor_ParameterListHandler(node,"parameter",context);
  }
  EditorCell_Collection editorCell=this.myParameterListHandler.createCells(context,new CellLayout_Horizontal());
  editorCell.setSelectable(true);
  editorCell.setDrawBorder(false);
  editorCell.setGridLayout(false);
  editorCell.setDrawBrackets(false);
  editorCell.setBracketsColor(Color.black);
  editorCell.putUserObject(EditorCell.ROLE,"parameter");
  editorCell.setLayoutConstraint("");
  return editorCell;
}
