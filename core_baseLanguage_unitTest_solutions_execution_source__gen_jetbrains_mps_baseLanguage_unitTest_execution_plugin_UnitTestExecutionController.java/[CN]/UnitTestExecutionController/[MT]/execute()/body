{
  if (ListSequence.fromList(myWhatToTest).isEmpty()) {
    throw new ExecutionException("Nothing to test.");
  }
  String workingDir=myConfigurationRunParameters.getWorkingDirectory();
  myCurrentProcess=OutputRedirector.redirect(new Junit_Command().setJrePath_String(myConfigurationRunParameters.getAlternativeJRE()).setVirtualMachineParameter_String(myConfigurationRunParameters.getVMParameters()).setWorkingDirectory_File((StringUtils.isNotEmpty(workingDir) ? new File(workingDir) : null)).createProcess(myWhatToTest),new UnitTestProcessListener(myDispatcher));
  if (myCurrentProcess == null) {
    myState.terminate();
    return null;
  }
  return myCurrentProcess;
}
