{
  if (argv.length > 0 && "-w".equals(argv[0])) {
    try {
      BufferedReader r=new BufferedReader(new InputStreamReader(System.in));
      r.readLine();
      r.close();
    }
 catch (    IOException ignore) {
    }
  }
  UnitTestRunner runner=new UnitTestRunner();
  try {
    runner.start(argv);
  }
 catch (  Throwable t) {
    t.printStackTrace(System.err);
    System.exit(1);
  }
}
