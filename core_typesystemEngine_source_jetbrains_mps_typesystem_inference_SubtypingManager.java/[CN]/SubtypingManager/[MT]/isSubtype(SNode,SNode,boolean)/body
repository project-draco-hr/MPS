{
  if (subtype == supertype)   return true;
  if (subtype == null || supertype == null)   return false;
  if (isSubtypeByReplacementRules(subtype,supertype))   return true;
  return isSubtype(NodeWrapper.createWrapperFromNode(subtype,null,true),NodeWrapper.createWrapperFromNode(supertype,null,true),null,null,isWeak);
}
