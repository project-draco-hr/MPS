{
  if (SNodeOperations.isInstanceOf(node,"jetbrains.mps.baseLanguage.structure.BaseVariableDeclaration")) {
    return ListSequence.fromList(getAvailableElements(SPropertyOperations.getString(SNodeOperations.cast(node,"jetbrains.mps.baseLanguage.structure.BaseVariableDeclaration"),"name"))).contains(node);
  }
 else {
    return parentScope.contains(node);
  }
}
