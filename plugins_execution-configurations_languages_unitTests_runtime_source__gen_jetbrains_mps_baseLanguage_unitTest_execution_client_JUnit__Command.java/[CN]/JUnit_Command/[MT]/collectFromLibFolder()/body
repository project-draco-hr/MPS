{
  List<URL> urls=ListSequence.fromList(new ArrayList<URL>());
  ClassloaderUtil.addIDEALibraries(urls);
  List<String> list=ListSequence.fromList(urls).select(new ISelector<URL,String>(){
    public String select(    URL it){
      return it.getPath();
    }
  }
).toListSequence();
  return list;
}
