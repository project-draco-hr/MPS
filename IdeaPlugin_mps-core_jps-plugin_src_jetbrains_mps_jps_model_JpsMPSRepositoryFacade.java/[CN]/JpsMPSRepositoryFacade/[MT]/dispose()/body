{
  if (!isInitialized) {
    return;
  }
  myRepository.getModelAccess().runWriteAction(new Runnable(){
    @Override public void run(){
      new ModuleRepositoryFacade(myRepository).unregisterModules(myProject);
      myProject.dispose();
      myJpsToMpsModules.clear();
      myRepo=null;
    }
  }
);
  disposeMPS();
  isInitialized=false;
}
