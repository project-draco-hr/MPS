{
  try {
    IValue value=VariablesTree.MPS_DEBUGGER_VALUE.getData(event.getDataContext());
    if (value != null) {
      CopyPasteUtil.copyTextToClipboard(value.getValuePresentation());
    }
  }
 catch (  Throwable t) {
    if (log.isErrorEnabled()) {
      log.error("User's action execute method failed. Action:" + "CopyValue",t);
    }
  }
}
