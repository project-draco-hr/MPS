{
  if (myClasses.containsKey(name)) {
    Class cl=myClasses.get(name);
    if (cl == null)     throw new ClassNotFoundException(name);
    return cl;
  }
  try {
    return loadClass(name,resolve,true,true);
  }
 catch (  ClassNotFoundException cnf) {
    myClasses.put(name,null);
    throw cnf;
  }
}
