{
  try {
    while (true) {
      WeakReference<JarFileData> ref=(WeakReference<JarFileData>)myQueue.poll();
      if (ref == null)       break;
      String path=myRefToPath.get(ref);
      myRefToPath.remove(ref);
      myPathToRef.remove(path);
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
