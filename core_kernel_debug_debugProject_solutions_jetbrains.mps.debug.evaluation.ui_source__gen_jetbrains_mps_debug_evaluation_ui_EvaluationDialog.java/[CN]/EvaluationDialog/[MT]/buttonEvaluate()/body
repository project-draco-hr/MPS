{
  try {
    IValueProxy evaluatedValue=myEvaluationData.evaluate();
    if (evaluatedValue != null) {
      setSuccess(evaluatedValue);
    }
 else {
      setFailure(null,"Evaluation returned null.");
    }
  }
 catch (  BaseEvaluationException e) {
    setFailure(e,null);
    LOG.error(e);
  }
}
