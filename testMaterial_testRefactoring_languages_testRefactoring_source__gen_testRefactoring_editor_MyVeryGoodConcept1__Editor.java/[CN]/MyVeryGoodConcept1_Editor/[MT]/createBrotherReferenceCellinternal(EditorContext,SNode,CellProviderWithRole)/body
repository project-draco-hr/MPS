{
  CellProviderWithRole provider=aProvider;
  provider.setAuxiliaryCellProvider(new MyVeryGoodConcept1_Editor._Inline());
  EditorCell editorCell=provider.createEditorCell(context);
  MyVeryGoodConcept1_Editor.setupBasic_BrotherReferenceCell(editorCell,node,context);
  if (editorCell instanceof EditorCell_Label) {
    MyVeryGoodConcept1_Editor.setupLabel_BrotherReferenceCell((EditorCell_Label)editorCell,node,context);
  }
  editorCell.setSubstituteInfo(provider.createDefaultSubstituteInfo());
  return editorCell;
}
