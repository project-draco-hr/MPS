          return val;
        }
        public String getMatchingText(        String pattern){
          if ((pattern == null || pattern.length() == 0) || pattern.startsWith("&")) {
            return "text";
          }
          if (XmlNameUtil.isAttValue(pattern)) {
            return pattern;
