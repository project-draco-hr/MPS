{
  if (checkMouseEvent(ev)) {
    for (    MouseListener listener : myMouseListeners) {
      listener.mousePressed(ev);
    }
    return true;
  }
  for (  LanguageHierarchiesComponent.ConceptContainer child : getChildren()) {
    if (child.mousePressed(ev)) {
      return true;
    }
  }
  return false;
}
