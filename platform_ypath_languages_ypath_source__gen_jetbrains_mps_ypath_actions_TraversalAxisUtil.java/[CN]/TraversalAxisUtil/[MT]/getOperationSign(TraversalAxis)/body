{
  if (axis == TraversalAxis.ANCESTORS) {
    return "^^";
  }
 else   if (axis == TraversalAxis.CHILDREN) {
    return "\\";
  }
 else   if (axis == TraversalAxis.DESCENDANTS) {
    return "\\\\";
  }
 else   if (axis == TraversalAxis.SELF_DESCENDANTS) {
    return "\\\\.";
  }
 else   if (axis == TraversalAxis.PRECEDING_SIBLINGS) {
    return "<<";
  }
 else   if (axis == TraversalAxis.PRECEDING_SIBLINGS_SELF) {
    return "<<.";
  }
 else   if (axis == TraversalAxis.FOLLOWING_SIBLINGS) {
    return ">>";
  }
 else   if (axis == TraversalAxis.SELF_FOLLOWING_SIBLINGS) {
    return ">>.";
  }
 else   if (axis == TraversalAxis.SELF_ANCESTORS) {
    return "^^.";
  }
 else {
    return "???";
  }
}
