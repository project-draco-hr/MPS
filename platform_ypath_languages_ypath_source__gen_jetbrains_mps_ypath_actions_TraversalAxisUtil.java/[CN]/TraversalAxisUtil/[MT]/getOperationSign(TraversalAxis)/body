{
switch (axis) {
case ANCESTORS:
    return "^^";
case CHILDREN:
  return "\\";
case DESCENDANTS:
return "\\\\";
case SELF_DESCENDANTS:
return "\\\\.";
case PRECEDING_SIBLINGS:
return "<<";
case PRECEDING_SIBLINGS_SELF:
return "<<.";
case FOLLOWING_SIBLINGS:
return ">>";
case SELF_FOLLOWING_SIBLINGS:
return ">>.";
case SELF_ANCESTORS:
return "^^.";
default :
return "???";
}
}
