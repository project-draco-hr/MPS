{
  ExtensionPoint<ConfigurationType> extensionPoint=Extensions.getArea(null).getExtensionPoint(ConfigurationType.CONFIGURATION_TYPE_EP);
{
    Remote_Kind runConfigurationKind=new Remote_Kind();
    ListSequence.fromList(RunConfigurationsInitializer_CustomApplicationPlugin.this.myRegisteredKinds).addElement(runConfigurationKind);
    extensionPoint.registerExtension(runConfigurationKind);
  }
}
