{
  flushAWT();
  doTearDown();
  flushAWT();
  final ApplicationEx application=ApplicationManagerEx.getApplicationEx();
  ThreadUtils.runInUIThreadAndWait(new Runnable(){
    public void run(){
      application.saveAll();
      Disposer.dispose(application);
    }
  }
);
  flushAWT();
  super.tearDown();
}
