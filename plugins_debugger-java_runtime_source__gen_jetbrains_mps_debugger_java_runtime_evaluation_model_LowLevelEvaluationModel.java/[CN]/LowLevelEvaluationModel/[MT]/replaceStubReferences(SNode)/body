      SModelDescriptor scopeModel=myAuxModule.getScope().getModelDescriptor(targetModel.getSModelReference());
      if (scopeModel != null && neq_qkk2f2_a0a1a1a0a7(scopeModel.getSModel(),targetModel)) {
