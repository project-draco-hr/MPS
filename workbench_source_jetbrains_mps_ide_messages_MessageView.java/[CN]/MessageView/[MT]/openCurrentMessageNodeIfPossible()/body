{
  final Message selectedMessage=(Message)myList.getSelectedValue();
  if (selectedMessage == null || selectedMessage.getHintObject() == null)   return;
  CommandProcessor.instance().executeLightweightCommand(new Runnable(){
    public void run(){
      ApplicationComponents.getInstance().getComponentSafe(NavigationManager.class).navigateTo(getProject(),selectedMessage.getHintObject());
    }
  }
);
}
