{
  if (obj == this)   return true;
  if (obj instanceof ReferencedNodeContext) {
    ReferencedNodeContext o=(ReferencedNodeContext)obj;
    return ObjectUtils.equals(myNodePointer,o.myNodePointer) && ObjectUtils.equals(myContextRoles,o.myContextRoles) && ObjectUtils.equals(myContextRefererNodes,o.myContextRefererNodes)&& ObjectUtils.equals(myAttributesStack,o.myAttributesStack);
  }
 else {
    return false;
  }
}
