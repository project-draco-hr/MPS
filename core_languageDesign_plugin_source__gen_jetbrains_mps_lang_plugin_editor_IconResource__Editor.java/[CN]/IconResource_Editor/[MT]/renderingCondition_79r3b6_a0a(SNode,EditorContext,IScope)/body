{
  IModule module=EditorUtil.findAnchorModule(node);
  if (module == null) {
    return false;
  }
  String path=MacrosUtil.expandPath(SPropertyOperations.getString(node,"path"),module.getModuleFqName());
  if (path == null) {
    return false;
  }
  return FileSystem.getInstance().getFileByPath(path).exists();
}
