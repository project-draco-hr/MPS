{
  for (  SNode row : ListSequence.fromList(getRows())) {
    SNode newColumn=SNodeFactoryOperations.createNewNode(SNodeFactoryOperations.asInstanceConcept(SLinkOperations.getTarget(myColumnsLinkDeclaration,MetaAdapterFactory.getReferenceLink(0xc72da2b97cce4447L,0x8389f407dc1158b7L,0xf979bd086aL,0xf98055fef0L,"target"))),null);
    Utils.insertElementAt(getColumns(row),newColumn,columnNumber);
  }
}
