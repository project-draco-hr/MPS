{
  ConceptDeclaration decl=(ConceptDeclaration)node;
  Language l=SModelUtil.getDeclaringLanguage(decl,scope);
  if (l == null) {
    return false;
  }
  String s=Macros.languageDescriptor().expandPath(SPropertyOperations.getString(node,"iconPath"),l.getDescriptorFile());
  if (s == null) {
    return false;
  }
  return new File(s).exists();
}
