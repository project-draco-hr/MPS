      public Color calculate(      EditorCell cell){
        return CellModel_ConceptProperty_Editor._StyleParameter_QueryFunction_6697_0((cell == null ? null : cell.getSNode()),(cell == null ? null : cell.getEditorContext()));
      }
    }
);
