{
  for (  AttributeConcept patternVar : (List<AttributeConcept>)(List)attributedNode.allChildren(new Condition<SNode>(){
    public boolean met(    SNode object){
      return BaseAdapter.isInstance(object,PatternVariableDeclaration.class) || BaseAdapter.isInstance(object,LinkPatternVariableDeclaration.class) || BaseAdapter.isInstance(object,PropertyPatternVariableDeclaration.class);
    }
  }
)) {
    loopVarSet.add(patternVar);
  }
}
