{
  if (value == null) {
    if (!isEmpty(pointer)) {
      return delete(styleMap,pointer);
    }
 else {
      return pointer;
    }
  }
 else {
    if (isEmpty(pointer)) {
      return insert(styleMap,index,pointer,value);
    }
 else {
      styleMap.values[pointer]=value;
      return pointer;
    }
  }
}
