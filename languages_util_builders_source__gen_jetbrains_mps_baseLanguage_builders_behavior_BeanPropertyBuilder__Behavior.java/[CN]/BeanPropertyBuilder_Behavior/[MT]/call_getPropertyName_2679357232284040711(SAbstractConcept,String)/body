{
  String prefix="set";
  if (methodName == null) {
    return null;
  }
  if (!(methodName.startsWith(prefix))) {
    return null;
  }
  if (methodName.length() <= prefix.length()) {
    return null;
  }
  String suffix=methodName.substring(prefix.length());
  return Character.toLowerCase(suffix.charAt(0)) + suffix.substring(1);
}
