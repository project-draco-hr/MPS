{
  if (!canFireEvent())   return;
  final SModelReferenceEvent event=new SModelReferenceEvent(getModelDescriptor(),reference,true);
  for (  SModelListener sModelListener : getModelListeners()) {
    try {
      sModelListener.referenceAdded(event);
    }
 catch (    Throwable t) {
      LOG.error(null,t);
    }
  }
}
