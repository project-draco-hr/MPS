{
  if (!canFireEvent()) {
    return;
  }
  final SModelPropertyEvent event=new SModelPropertyEvent(getModelDescriptor(),property,node,oldValue,newValue);
  for (  SModelListener sModelListener : getModelListeners()) {
    try {
      sModelListener.propertyChanged(event);
    }
 catch (    Throwable t) {
      LOG.error(null,t);
    }
  }
}
