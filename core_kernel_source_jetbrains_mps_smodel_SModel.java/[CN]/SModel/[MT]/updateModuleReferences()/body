{
  if (myModelDescriptor != null) {
    ModelChange.assertLegalChange(myModelDescriptor);
  }
  boolean changed=false;
  if (updateRefs(myDevKits)) {
    changed=true;
  }
  if (updateRefs(myLanguages)) {
    changed=true;
  }
  if (updateRefs(myLanguagesEngagedOnGeneration)) {
    changed=true;
  }
  return changed;
}
