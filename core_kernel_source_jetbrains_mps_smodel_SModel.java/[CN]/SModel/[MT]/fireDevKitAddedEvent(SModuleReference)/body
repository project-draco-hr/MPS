{
  if (!canFireEvent())   return;
  final SModelDevKitEvent event=new SModelDevKitEvent(getModelDescriptor(),ref,true);
  for (  SModelListener sModelListener : getModelListeners()) {
    try {
      sModelListener.devkitAdded(event);
    }
 catch (    Throwable t) {
      LOG.error(null,t);
    }
  }
}
