{
  if (!canFireEvent())   return;
  final SModelRootEvent event=new SModelRootEvent(getModelDescriptor(),root,true);
  for (  SModelListener sModelListener : getModelListeners()) {
    try {
      sModelListener.rootAdded(event);
    }
 catch (    Throwable t) {
      LOG.error(null,t);
    }
  }
}
