{
  ModelChange.assertLegalChange(this);
  if (myDisposed)   return;
  fireBeforeModelDisposed();
  myDisposed=true;
synchronized (myListenersLock) {
    myCommandListeners.clear();
    myListeners.clear();
  }
  for (  SNode sn : myIdToNodeMap.values()) {
    sn.dispose();
  }
  disposeFastNodeFinder();
  myIdToNodeMap=null;
  myRoots.clear();
}
