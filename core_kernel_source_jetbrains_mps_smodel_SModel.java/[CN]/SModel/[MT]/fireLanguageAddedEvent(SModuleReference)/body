{
  if (!canFireEvent())   return;
  final SModelLanguageEvent event=new SModelLanguageEvent(getModelDescriptor(),ref,true);
  for (  SModelListener sModelListener : getModelListeners()) {
    try {
      sModelListener.languageAdded(event);
    }
 catch (    Throwable t) {
      LOG.error(null,t);
    }
  }
}
