{
  List<DevKit> result=new ArrayList<DevKit>();
  for (  String dk : getDevKitNamespaces()) {
    DevKit devKit=scope.getDevKit(dk);
    if (devKit != null) {
      addDevkitModelsVersions(dk,devKit);
      result.add(devKit);
    }
 else {
      LOG.error("Can't find devkit " + dk + " in scope "+ scope);
    }
  }
  return result;
}
