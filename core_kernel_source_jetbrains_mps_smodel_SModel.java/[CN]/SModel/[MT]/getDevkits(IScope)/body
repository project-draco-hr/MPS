{
  ModelAccess.assertLegalRead();
  List<DevKit> result=new ArrayList<DevKit>();
  for (  ModuleReference dk : myDevKits) {
    DevKit devKit=scope.getDevKit(dk);
    if (devKit != null) {
      result.add(devKit);
    }
 else {
      LOG.error("Can't find devkit " + dk.getModuleFqName() + " in scope "+ scope);
    }
  }
  return result;
}
