{
  ModelRootEntry entry=null;
  try {
    entry=myModelRootEntries.get(modelRoot.getType()).getClass().newInstance();
  }
 catch (  InstantiationException e) {
    e.printStackTrace();
  }
catch (  IllegalAccessException e) {
    e.printStackTrace();
  }
  entry.setModelRoot(modelRoot);
  return entry;
}
