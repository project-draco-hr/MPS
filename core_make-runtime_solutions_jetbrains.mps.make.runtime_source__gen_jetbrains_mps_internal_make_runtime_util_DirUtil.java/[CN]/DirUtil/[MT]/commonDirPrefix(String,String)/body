{
  int lastMatchedSlash=-1;
  for (int i=0, x=Math.min(path1.length(),path2.length()); i < x; i++) {
    char c=path1.charAt(i);
    if (c != path2.charAt(i)) {
      break;
    }
    if (c == SLASH_CHAR) {
      lastMatchedSlash=i;
    }
  }
  return (lastMatchedSlash == -1 ? "" : path1.substring(0,lastMatchedSlash + 1));
}
