{
  if (!myFindUsagesSupported)   return false;
  if (myNeedSearchForStrings && !myModelRootManager.containsString(myModelDescriptor,modelDescriptor.toString()))   return false;
  SModel model=myModelDescriptor.getSModel();
  if (model == null)   return false;
  return SModelOperations.hasImportedModel(model,modelDescriptor.getSModelReference());
}
