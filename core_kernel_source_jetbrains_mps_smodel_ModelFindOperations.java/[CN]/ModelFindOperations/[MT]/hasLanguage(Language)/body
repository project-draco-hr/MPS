{
  if (!myFindUsagesSupported)   return false;
  if (myNeedSearchForStrings && !myModelRootManager.containsString(myModelDescriptor,language.getModuleFqName()))   return false;
  SModel model=myModelDescriptor.getSModel();
  if (model == null)   return false;
  return SModelOperations.hasLanguage(model,language.getModuleReference());
}
