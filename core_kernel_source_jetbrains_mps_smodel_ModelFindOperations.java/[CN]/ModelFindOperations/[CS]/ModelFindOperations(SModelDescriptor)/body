{
  myModelDescriptor=descriptor;
  DataSource source=descriptor instanceof BaseSModelDescriptorWithSource ? ((BaseSModelDescriptorWithSource)myModelDescriptor).getSource() : null;
  myDataSource=source instanceof RegularModelDataSource ? (RegularModelDataSource)source : null;
  myNeedSearchForStrings=(myModelDescriptor instanceof DefaultSModelDescriptor) && ((DefaultSModelDescriptor)myModelDescriptor).getUpdateableModel().getState() != ModelLoadingState.FULLY_LOADED && !(myModelDescriptor instanceof EditableSModelDescriptor && ((EditableSModelDescriptor)myModelDescriptor).isChanged());
}
