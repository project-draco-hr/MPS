{
  myModelDescriptor=descriptor;
  DataSource source=descriptor != null ? myModelDescriptor.getSource() : null;
  myDataSource=source instanceof FileDataSource ? (FileDataSource)source : null;
  myNeedSearchForStrings=(myModelDescriptor instanceof DefaultSModelDescriptor) && ((DefaultSModelDescriptor)myModelDescriptor).getLoadingState() != ModelLoadingState.FULLY_LOADED && !(myModelDescriptor instanceof EditableSModelDescriptor && ((EditableSModelDescriptor)myModelDescriptor).isChanged());
}
