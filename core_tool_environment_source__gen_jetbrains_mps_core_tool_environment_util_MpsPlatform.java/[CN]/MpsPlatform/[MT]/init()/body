{
  myCore=new MPSCore();
  myCore.init();
  myPersistence=new MPSPersistence(myCore.getPersistenceFacade());
  myTypesystem=new MPSTypesystem(myCore.getLanguageRegistry(),myCore.getClassLoaderManager());
  myGenerator=new MPSGenerator();
  myPersistence.init();
  myTypesystem.init();
  myGenerator.init();
}
