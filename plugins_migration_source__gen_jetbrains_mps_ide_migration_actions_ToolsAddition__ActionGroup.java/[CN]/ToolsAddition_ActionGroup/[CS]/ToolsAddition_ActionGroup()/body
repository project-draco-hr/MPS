{
  super("ToolsAddition",ID);
  this.setIsInternal(false);
  this.setPopup(false);
  try {
    ToolsAddition_ActionGroup.this.addAction("jetbrains.mps.ide.migration.actions.Migration20_Action");
{
      LabelledAnchor action=new LabelledAnchor(ToolsAddition_ActionGroup.LABEL_ID_migrationGroup);
      ActionManagerEx manager=ActionManagerEx.getInstanceEx();
      manager.registerAction(action.getId(),action,PluginId.getId("jetbrains.mps.ide.migration"));
      ToolsAddition_ActionGroup.this.addAction(action);
    }
  }
 catch (  Throwable t) {
    LOG.error("User group error",t);
  }
}
