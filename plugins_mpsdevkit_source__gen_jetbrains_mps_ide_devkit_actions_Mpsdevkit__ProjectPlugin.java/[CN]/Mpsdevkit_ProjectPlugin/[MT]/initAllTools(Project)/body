{
  List<BaseGeneratedTool> tools=ListSequence.<BaseGeneratedTool>fromList(new ArrayList<BaseGeneratedTool>());
  ListSequence.<BaseGeneratedTool>fromList(tools).addElement(new ModelRepository_Tool(project));
  ListSequence.<BaseGeneratedTool>fromList(tools).addElement(new ModuleRepository_Tool(project));
  ListSequence.<BaseGeneratedTool>fromList(tools).addElement(new NodeExplorer_Tool(project));
  ListSequence.<BaseGeneratedTool>fromList(tools).addElement(new TraceTool_Tool(project));
  return tools;
}
