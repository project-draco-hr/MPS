{
  return (List<MyEntry<K,V>>)(List)new ArrayWrapper<MyEntry>(MyEntry.class){
    protected MyEntry[] getArray(){
      if (myEntries == null) {
        return MyEntry.EMPTY_ARRAY;
      }
      return myEntries;
    }
    protected void setArray(    MyEntry[] newArray){
      myEntries=newArray;
    }
  }
;
}
