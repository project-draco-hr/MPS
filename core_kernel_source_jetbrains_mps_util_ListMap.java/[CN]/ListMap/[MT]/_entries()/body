{
  return (List<MyEntry<K,V>>)(List)new ArrayWrapper<MyEntry>(){
    protected MyEntry[] getArray(){
      return myEntries;
    }
    protected void setArray(    MyEntry[] newArray){
      myEntries=newArray;
    }
    protected MyEntry[] newArray(    int size){
      return new MyEntry[size];
    }
  }
;
}
