{
  int len=data.length;
  int wordStart=-1;
  for (int i=0; i < len; i++) {
    char c=data[i];
    if (c >= 'a' && c <= 'z' || c >= 'A' && c <= 'Z') {
      if (wordStart == -1) {
        wordStart=i;
      }
    }
 else     if (wordStart >= 0) {
      processWord(data,len,wordStart,i - wordStart,consumer);
      wordStart=-1;
    }
  }
}
