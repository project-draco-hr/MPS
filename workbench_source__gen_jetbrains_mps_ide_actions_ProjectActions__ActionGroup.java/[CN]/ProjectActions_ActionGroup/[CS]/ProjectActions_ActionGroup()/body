{
  super("",ID);
  this.setIsAlwaysVisible(true);
  this.setIsInternal(false);
  this.setPopup(false);
  try {
    this.add(new LabelledAnchor(ProjectActions_ActionGroup.LABEL_ID_projectNew));
    this.addSeparator();
    this.add(new MakeProject_Action());
    this.add(new RebuildProject_Action());
    this.add(new CleanProject_Action());
    this.addSeparator();
    this.add(new OptimizeProjectImports_Action());
  }
 catch (  Throwable t) {
    LOG.error("User group error",t);
  }
}
