{
  Method method=getCanBeChildMethod(fqName,context);
  if (method != null) {
    try {
      return (Boolean)method.invoke(null,context,new CanBeAChildContext(parentNode,link));
    }
 catch (    IllegalAccessException e) {
      LOG.error(e);
    }
catch (    InvocationTargetException e) {
      LOG.error(e);
    }
  }
  return true;
}
