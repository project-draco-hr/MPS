{
  String key=conceptFqName + "#" + referentRole;
synchronized (myLock) {
    INodeReferentSetEventHandler old=myNodeReferentSetEventHandlersMap.put(key,eventHandler);
    if (old != null) {
      LOG.error("'set referent' event handler is already registered for key '" + key + "' : "+ old);
    }
  }
}
