{
  String key=conceptFqName + "#" + propertyName;
  INodePropertySetter old=myNodePropertySettersMap.put(key,setter);
  if (old != null) {
    LOG.error("property setter is already registered for key '" + key + "' : "+ old);
  }
synchronized (myLock) {
    myNodePropertySettersCache.clear();
  }
}
