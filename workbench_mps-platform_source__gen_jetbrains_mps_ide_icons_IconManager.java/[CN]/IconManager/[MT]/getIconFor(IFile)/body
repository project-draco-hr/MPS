{
  if (!(file.exists())) {
    return null;
  }
  InputStream is=null;
  try {
    is=file.openInputStream();
    return loadIcon(is);
  }
 catch (  IOException e) {
    LOG.error(null,e);
    return null;
  }
 finally {
    try {
      if (is != null) {
        is.close();
      }
    }
 catch (    IOException e) {
      LOG.error(null,e);
    }
  }
}
