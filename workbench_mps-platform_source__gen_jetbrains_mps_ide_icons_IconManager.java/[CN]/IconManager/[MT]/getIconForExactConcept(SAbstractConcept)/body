{
  ConceptPresentation pres=ConceptRegistry.getInstance().getConceptProperties(concept);
  if (pres == null) {
    return null;
  }
  IconResource icn=pres.getIcon();
  if (icn == null) {
    return null;
  }
  InputStream res=icn.getResource();
  if (res == null) {
    return null;
  }
  return ConceptIconLoader.getIconFor(res);
}
