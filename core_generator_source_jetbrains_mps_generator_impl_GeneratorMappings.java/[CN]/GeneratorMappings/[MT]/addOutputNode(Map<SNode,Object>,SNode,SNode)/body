{
  Object o=currentMapping.get(inputNode);
  if (o == null) {
    currentMapping.put(inputNode,outputNode);
  }
 else   if (o instanceof List) {
    ((List<SNode>)o).add(outputNode);
  }
 else {
    List<SNode> list=new ArrayList<SNode>(4);
    list.add((SNode)o);
    list.add(outputNode);
    currentMapping.put(inputNode,list);
  }
}
