{
  for (  ASMMethod m : ac.getDeclaredMethods()) {
    if (m.isPrivate() && mySkipPrivate) {
      continue;
    }
    if (m.isStatic()) {
      continue;
    }
    if (m.isBridge()) {
      continue;
    }
    if (m.isCompilerGenerated()) {
      continue;
    }
    this.updateBaseMethod(m);
  }
}
