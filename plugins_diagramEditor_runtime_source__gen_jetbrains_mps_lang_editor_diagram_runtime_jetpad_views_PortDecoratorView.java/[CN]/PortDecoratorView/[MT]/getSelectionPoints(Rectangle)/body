{
  return ListSequence.fromList(ListSequence.fromListAndArray(new ArrayList<Vector>(),new Vector(-1,-1),new Vector(bounds.dimension.x,-1),new Vector(bounds.dimension.x,bounds.dimension.y),new Vector(-1,bounds.dimension.y))).select(new ISelector<Vector,Vector>(){
    public Vector select(    Vector it){
      return it.add(bounds.origin);
    }
  }
);
}
