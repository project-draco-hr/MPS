{
  InputPortCell inputPortCell=new InputPortCell(context,node){
    public Mapper<SNode,RectView> getMapper(){
      Mapper<SNode,RectView> mapper=new Mapper<SNode,RectView>(node,new RectView()){
        @Override protected void registerSynchronizers(        Mapper.SynchronizersConfiguration configuration){
          super.registerSynchronizers(configuration);
        }
      }
;
      RectView view=mapper.getTarget();
      view.background().set(Color.LIGHT_GRAY);
      view.dimension().set(new Vector(10,10));
      return mapper;
    }
    public void synchronize(){
    }
  }
;
  inputPortCell.setBig(true);
  return inputPortCell;
}
