{
  if (myDelegate == null) {
    runFailure(createTestDescription(ModuleSymbolicSuite.NOT_FOUND),new IllegalStateException("no runner for " + myClassName),notifier);
  }
 else   if (myTests == null) {
    runFailure(createTestDescription(ModuleSymbolicSuite.NO_TESTS),new IllegalStateException("no tests found in " + myClassName),notifier);
  }
 else {
    myDelegate.run(notifier);
  }
}
