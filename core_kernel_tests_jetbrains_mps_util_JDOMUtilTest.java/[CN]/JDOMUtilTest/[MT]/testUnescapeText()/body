{
  assertEquals("",JDOMUtil.unescapeText(""));
  assertEquals("a",JDOMUtil.unescapeText("a"));
  assertEquals("&",JDOMUtil.unescapeText("&amp;"));
  assertEquals("<",JDOMUtil.unescapeText("&lt;"));
  assertEquals(">",JDOMUtil.unescapeText("&gt;"));
  assertEquals("\"",JDOMUtil.unescapeText("&quot;"));
  assertEquals(" ",JDOMUtil.unescapeText("&#32;"));
  assertEquals(" ",JDOMUtil.unescapeText("&#x20;"));
  assertEquals(" ",JDOMUtil.unescapeText("&#X20;"));
  assertEquals("!&!",JDOMUtil.unescapeText("!&amp;!"));
  assertEquals("!<!",JDOMUtil.unescapeText("!&lt;!"));
  assertEquals("!>!",JDOMUtil.unescapeText("!&gt;!"));
  assertEquals("!\"!",JDOMUtil.unescapeText("!&quot;!"));
  assertEquals("! !",JDOMUtil.unescapeText("!&#32;!"));
  assertEquals("! !",JDOMUtil.unescapeText("!&#x20;!"));
  assertEquals("! !",JDOMUtil.unescapeText("!&#X20;!"));
  assertEquals("&asdad&aa",JDOMUtil.unescapeText("&asdad&aa"));
  assertEquals("&#100000;",JDOMUtil.unescapeText("&#100000;"));
  assertEquals("&",JDOMUtil.unescapeText("&"));
  assertEquals("&#",JDOMUtil.unescapeText("&#"));
  assertEquals("&#x",JDOMUtil.unescapeText("&#x"));
  assertEquals("&#X",JDOMUtil.unescapeText("&#X"));
}
