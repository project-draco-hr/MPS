{
  Map<String,PropertyConstraintsDescriptor> properties=new HashMap();
  properties.put("value",new BasePropertyConstraintsDescriptor("value",this){
    @Override public boolean hasOwnValidator(){
      return true;
    }
    @Override public boolean validateValue(    SNode node,    String propertyValue,    IScope scope){
      String propertyName="value";
      if (StringUtils.isEmpty((SPropertyOperations.getString(propertyValue)))) {
        return false;
      }
      return (SPropertyOperations.getString(propertyValue)).matches("(\\d|[a-fA-F])*");
    }
  }
);
  return properties;
}
