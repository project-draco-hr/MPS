        SLinkOperations.setTarget(node,"scriptsDirectory",new DirctoriesInLayout_MigrationScript.QuotationClass_6u5ptb_a0a0a0a0e0a0a0c0a().createNode(SPropertyOperations.getString(node,"scriptsFolder")),true);
