{
  int size=size();
  if (arr.length < size) {
    arr=(Object[])Array.newInstance(arr.getClass().getComponentType(),size);
  }
  Iterator it=iterator();
  Object[] result=arr;
  for (int i=0; i < size; i++) {
    result[i]=it.next();
  }
  if (arr.length > size) {
    arr[size]=null;
  }
  return arr;
}
