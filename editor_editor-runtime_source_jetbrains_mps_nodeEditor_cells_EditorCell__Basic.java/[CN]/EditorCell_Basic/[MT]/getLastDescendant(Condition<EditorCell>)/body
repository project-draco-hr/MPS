{
  EditorCell current=getLastChild();
  if (current == this)   return null;
  while (current != null) {
    if (condition.met(current))     return current;
    EditorCell result=current.getLastDescendant(condition);
    if (result != null) {
      return result;
    }
    current=current.getPrevSibling();
  }
  return null;
}
