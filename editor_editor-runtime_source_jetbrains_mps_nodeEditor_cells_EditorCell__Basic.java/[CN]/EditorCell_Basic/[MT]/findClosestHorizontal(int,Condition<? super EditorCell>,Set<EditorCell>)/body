{
  EditorCell best=null;
  int bestDistance=-1;
  for (  EditorCell cell : candidates) {
    if (!condition.met(cell)) {
      continue;
    }
    int distance=horizontalDistance(x,cell);
    if (bestDistance == -1 || distance < bestDistance) {
      best=cell;
      bestDistance=distance;
    }
  }
  return best;
}
