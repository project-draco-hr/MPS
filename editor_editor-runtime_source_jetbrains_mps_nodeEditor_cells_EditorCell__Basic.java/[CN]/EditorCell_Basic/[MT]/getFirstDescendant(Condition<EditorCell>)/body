{
  EditorCell current=getFirstChild();
  if (current == this)   return null;
  while (current != null) {
    if (condition.met(current))     return current;
    EditorCell result=current.getFirstDescendant(condition);
    if (result != null) {
      return result;
    }
    current=current.getNextSibling();
  }
  return null;
}
