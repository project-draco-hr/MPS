{
  NodeReadAccessCasterInEditor.runReadTransparentAction(new Runnable(){
    public void run(){
      myLinkDeclaration=link;
      if (myLinkDeclaration != null) {
        myIsReferenceCell=SNodeUtil.getLinkDeclaration_IsReference(link);
      }
 else {
        myIsReferenceCell=false;
      }
    }
  }
);
}
