{
  if (!myIsNeedRelayout) {
    return;
  }
  boolean drawBrackets=isDrawBrackets();
  if (drawBrackets) {
    myX+=BRACKET_WIDTH;
  }
  myX+=myGapLeft;
  relayoutImpl();
  if (drawBrackets) {
    myX-=BRACKET_WIDTH;
    myWidth+=2 * BRACKET_WIDTH;
  }
  myX-=myGapLeft;
  myWidth+=myGapLeft + myGapRight;
  myIsNeedRelayout=false;
}
