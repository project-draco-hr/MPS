{
switch (kind.charAt(0)) {
case 'i':
    myKind=ConceptKind.INTERFACE;
  break;
case 'l':
myKind=ConceptKind.IMPLEMENTATION;
break;
case 's':
myKind=ConceptKind.IMPLEMENTATION_WITH_STUB;
break;
default :
myKind=ConceptKind.NORMAL;
}
switch (kind.charAt(1)) {
case 'r':
myScope=StaticScope.ROOT;
break;
case 'n':
myScope=StaticScope.NONE;
break;
default :
myScope=StaticScope.GLOBAL;
}
}
