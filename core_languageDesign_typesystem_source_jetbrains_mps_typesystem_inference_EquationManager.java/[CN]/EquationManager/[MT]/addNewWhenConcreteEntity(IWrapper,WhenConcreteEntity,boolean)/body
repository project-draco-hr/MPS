{
  IWrapper representator=getRepresentatorWrapper(wrapper);
  if (!isShallow) {
    addWhenConcreteEntity(representator,entity);
    checkConcrete(representator);
  }
 else {
    if (!representator.isVariable()) {
      entity.run();
    }
 else {
      addShallowWhenConcreteEntity(representator,entity);
    }
  }
}
