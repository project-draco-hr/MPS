{
  Set<SReference> result=new HashSet<SReference>();
  SNode cellNode=editor.getRootCell().getSNode();
  List<SNode> list=cellNode.getSubnodes();
  list.add(0,cellNode);
  for (  SNode node : list) {
    for (    SReference ref : node.getReferences()) {
      if (ref.getTargetNode() == null) {
        result.add(ref);
      }
    }
  }
  return result;
}
