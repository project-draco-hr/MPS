{
  try {
    ApiMigrationHelper instance=new ApiMigrationHelper(((MPSProject)MapSequence.fromMap(_params).get("project")),((Project)MapSequence.fromMap(_params).get("iproject")),new ProjectScope(((MPSProject)MapSequence.fromMap(_params).get("project"))));
    instance.migrateMethods(new ApiTransformations().getTransformations());
  }
 catch (  Throwable t) {
    if (LOG.isEnabledFor(Priority.ERROR)) {
      LOG.error("User's action execute method failed. Action:" + "MigrateToNewSNodeMethods",t);
    }
  }
}
