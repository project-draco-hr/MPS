{
  if (myIcon == null)   return;
switch (myAlignment) {
case justify:
{
      if ((myWidth != -1 && myWidth != myIcon.getIconWidth()) || (myHeight != -1 && myHeight != myIcon.getIconHeight())) {
        paintIconScaled(g);
      }
 else {
        myIcon.paintIcon(getEditor(),g,myX,myY);
      }
      break;
    }
case center:
{
    int x=myX + (myWidth - myIcon.getIconWidth()) / 2;
    int y=myY + (myHeight - myIcon.getIconHeight()) / 2;
    myIcon.paintIcon(getEditor(),g,x,y);
    break;
  }
case title:
{
  break;
}
}
}
