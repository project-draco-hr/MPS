{
  if (withContent) {
    requiredWithContent.add(n);
  }
  if (artifactId != null) {
    mapArtifactId(n,artifactId);
  }
  if (!(requiredSet.add(n))) {
    return;
  }
  SNode parent=visible.parent(n);
  if (parent != null) {
    add(parent,true,null);
  }
  ListSequence.fromList(required).addElement(n);
}
