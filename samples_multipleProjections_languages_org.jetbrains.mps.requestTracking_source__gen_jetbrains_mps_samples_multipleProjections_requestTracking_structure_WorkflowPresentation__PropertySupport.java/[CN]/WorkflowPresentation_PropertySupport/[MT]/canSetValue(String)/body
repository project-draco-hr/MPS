{
  if (value == null) {
    return true;
  }
  Iterator<WorkflowPresentation> constants=ListSequence.fromList(WorkflowPresentation.getConstants()).iterator();
  while (constants.hasNext()) {
    WorkflowPresentation constant=constants.next();
    if (value.equals(constant.getName())) {
      return true;
    }
  }
  return false;
}
