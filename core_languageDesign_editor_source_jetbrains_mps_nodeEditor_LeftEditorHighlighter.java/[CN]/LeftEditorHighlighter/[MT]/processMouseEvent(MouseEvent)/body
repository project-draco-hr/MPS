{
switch (e.getID()) {
case MouseEvent.MOUSE_PRESSED:
case MouseEvent.MOUSE_RELEASED:
case MouseEvent.MOUSE_CLICKED:
    if (isInFoldingArea(e)) {
      processMousePressedInFoldingArea(e);
    }
 else {
      processMousePressedInIconsArea(e);
    }
}
if (!e.isConsumed()) {
  super.processMouseEvent(e);
}
}
