{
  myModel.add("a");
  myModel.add("b");
  myModel.addListDataListener(new MyListDataListener(){
    int removedOnce=1;
    @Override public void intervalRemoved(    ListDataEvent e){
      Assert.assertTrue(--removedOnce == 0);
    }
  }
);
  myModel.removeFirst(1);
  Assert.assertEquals(1,myModel.getSize());
}
