{
  if (path.contains("!")) {
    int index=path.indexOf("!");
    String jarFileName=path.substring(0,index);
    String entryPath=path.substring(index + 1);
    if (entryPath.startsWith("/")) {
      entryPath=entryPath.substring(1);
    }
    JarFileEntryFile root=new JarFileEntryFile(JarFileDataCache.instance().getDataFor(new File(jarFileName)),entryPath);
    return root;
  }
 else {
    return new FileSystemFile(new File(path));
  }
}
