{
  item.initIndex=-2;
  for (  IModule dependency : item.module.getAllDependOnModules()) {
    Item depItem=module2Item.get(dependency);
    if (depItem != null) {
      if (depItem.initIndex == -1) {
        initIndex=init(depItem,module2Item,initIndex);
      }
    }
  }
  item.initIndex=initIndex;
  initIndex++;
  return initIndex;
}
