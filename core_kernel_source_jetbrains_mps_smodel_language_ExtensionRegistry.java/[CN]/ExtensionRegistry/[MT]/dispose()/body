{
  INSTANCE=null;
  ModelAccess.instance().runWriteAction(new Runnable(){
    public void run(){
      myModuleToNamespace.clear();
      myExtensionDescriptors.clear();
      ExtensionRegistry.this.clear();
    }
  }
);
  if (myRepo != null) {
    myRepo.addModuleRepositoryListener(myListener);
  }
}
