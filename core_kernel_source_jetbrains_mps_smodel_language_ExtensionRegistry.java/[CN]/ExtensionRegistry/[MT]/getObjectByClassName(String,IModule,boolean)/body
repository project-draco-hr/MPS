{
  try {
    if (module == null) {
      return null;
    }
    if (!(module instanceof ClassLoadingModule))     return null;
    ClassLoadingModule clm=((ClassLoadingModule)module);
    Class clazz=clm.getClass(className);
    if (clazz == null) {
      return null;
    }
    return clazz.newInstance();
  }
 catch (  Throwable e) {
    LOG.debug("error loading class\"" + className + "\"",e);
  }
  return null;
}
