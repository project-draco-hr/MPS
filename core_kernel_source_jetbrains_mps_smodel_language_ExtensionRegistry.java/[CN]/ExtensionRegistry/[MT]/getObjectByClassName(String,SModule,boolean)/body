{
  try {
    if (module == null) {
      return null;
    }
    if (!(ClassLoaderManager.getInstance().canLoad(module))) {
      return null;
    }
    Class clazz=ClassLoaderManager.getInstance().getClass(module,className);
    if (clazz == null) {
      return null;
    }
    return clazz.newInstance();
  }
 catch (  Throwable e) {
    LOG.debug("error loading class\"" + className + "\"",e);
  }
  return null;
}
