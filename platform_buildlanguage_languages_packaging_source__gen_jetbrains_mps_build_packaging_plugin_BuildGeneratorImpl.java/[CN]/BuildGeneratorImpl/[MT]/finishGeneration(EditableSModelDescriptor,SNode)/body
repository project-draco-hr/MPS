{
  targetModelDescriptor.getModule().save();
  ModelAccess.instance().runWriteActionInCommand(new Runnable(){
    public void run(){
      targetModelDescriptor.save();
    }
  }
);
  MPSEditorOpener editorOpener=this.myProject.getComponent(MPSEditorOpener.class);
