{
  Thread thread=new Thread(){
    public void run(){
      ModelAccess.instance().runReadAction(new Runnable(){
        public void run(){
          myUsagesView.setRunOptions(FindUtils.makeProvider(new ConstantFinder(mySearchResults.getSearchResults())),null,new ButtonConfiguration(false,false,true),mySearchResults);
        }
      }
);
    }
  }
;
  thread.start();
}
