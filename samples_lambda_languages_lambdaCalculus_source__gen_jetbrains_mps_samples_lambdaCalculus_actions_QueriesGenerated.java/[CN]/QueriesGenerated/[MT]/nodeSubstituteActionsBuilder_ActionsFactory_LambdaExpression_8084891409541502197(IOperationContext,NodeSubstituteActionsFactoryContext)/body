    SNode outputConcept=MetaAdapterFactory.getConcept(0x7c9e280794ad4afcL,0xadf0aaee45eb2895L,0x448ba254edbfc79fL,"jetbrains.mps.samples.lambdaCalculus.structure.LetExpression").getDeclarationNode();
