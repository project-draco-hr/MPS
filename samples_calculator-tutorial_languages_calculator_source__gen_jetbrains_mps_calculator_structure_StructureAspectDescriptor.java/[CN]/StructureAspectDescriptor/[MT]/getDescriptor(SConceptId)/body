{
  Integer index=myIndexMap.get(id);
  if (index == null) {
    return null;
  }
switch (((int)index)) {
case 0:
    return myConceptCalculator;
case 1:
  return myConceptInputField;
case 2:
return myConceptInputFieldReference;
case 3:
return myConceptOutputField;
default :
throw new IllegalStateException();
}
}
