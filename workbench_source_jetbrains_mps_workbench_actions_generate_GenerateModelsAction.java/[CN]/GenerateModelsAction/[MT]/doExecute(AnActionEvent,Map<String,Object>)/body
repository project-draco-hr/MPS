{
  boolean checkSuccessful=myPluginManager.getTool(ModelCheckerTool_Tool.class).checkModelsBeforeGenerationIfNeeded(myContext,(List<SModelDescriptor>)((List)myModels),new Runnable(){
    public void run(){
      GeneratorUIFacade.getInstance().generateModels(myContext,myModels,getGenerationHandler(),myRebuildAll,false);
    }
  }
);
  if (!(checkSuccessful)) {
    return;
  }
  GeneratorUIFacade.getInstance().generateModels(myContext,myModels,getGenerationHandler(),myRebuildAll,false);
}
