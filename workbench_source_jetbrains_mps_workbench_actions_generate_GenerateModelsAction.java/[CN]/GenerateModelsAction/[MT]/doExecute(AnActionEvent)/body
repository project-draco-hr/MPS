{
  MPSProject project=myContext.getMPSProject();
  boolean checkSuccessful=project.getPluginManager().getTool(ModelCheckerTool_Tool.class).checkModelsBeforeGenerationIfNeeded(myContext,myModels,new Runnable(){
    public void run(){
      myGenManager.generateModelsFromDifferentModules(myContext,myModels,getGenerationType());
    }
  }
);
  if (!(checkSuccessful)) {
    return;
  }
  myGenManager.generateModelsFromDifferentModules(myContext,myModels,getGenerationType());
}
