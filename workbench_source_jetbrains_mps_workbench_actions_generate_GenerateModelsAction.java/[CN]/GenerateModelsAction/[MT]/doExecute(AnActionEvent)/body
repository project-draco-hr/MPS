{
  boolean checkSuccessful=myPluginManager.getTool(ModelCheckerTool_Tool.class).checkModelsBeforeGenerationIfNeeded(myContext,myModels,new Runnable(){
    public void run(){
      myGenManager.generateModelsFromDifferentModules(myContext,myModels,getGenerationHandler());
    }
  }
);
  if (!(checkSuccessful)) {
    return;
  }
  myGenManager.generateModelsFromDifferentModules(myContext,myModels,getGenerationHandler());
}
