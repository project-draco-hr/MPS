{
  MPSProject project=myContext.getMPSProject();
  if (!(project.getPluginManager().getTool(ModelCheckerTool_Tool.class).checkModelsBeforeGenerationIfNeeded(myContext.getProject(),myModels))) {
    return;
  }
  myGenManager.generateModelsFromDifferentModules(myContext,myModels,getGenerationType());
}
