{
  super(new JavaLexer());
  registerSelfStoppingLayer(new StringLiteralLexer('\"',JavaTokenType.STRING_LITERAL),new IElementType[]{JavaTokenType.STRING_LITERAL},IElementType.EMPTY_ARRAY);
  registerSelfStoppingLayer(new StringLiteralLexer('\'',JavaTokenType.STRING_LITERAL),new IElementType[]{JavaTokenType.CHARACTER_LITERAL},IElementType.EMPTY_ARRAY);
  LayeredLexer docLexer=new LayeredLexer(new JavaDocLexer());
  HtmlHighlightingLexer lexer=new HtmlHighlightingLexer(FileTypeManager.getInstance().getStdFileType("CSS"));
  lexer.setHasNoEmbeddments(true);
  docLexer.registerLayer(lexer,new IElementType[]{JavaDocTokenType.DOC_COMMENT_DATA});
  registerSelfStoppingLayer(docLexer,new IElementType[]{JavaTokenType.DOC_COMMENT},IElementType.EMPTY_ARRAY);
}
