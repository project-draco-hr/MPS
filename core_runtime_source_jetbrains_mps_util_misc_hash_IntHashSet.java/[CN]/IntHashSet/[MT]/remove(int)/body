{
  final Entry[] table=this.table;
  final int index=HashUtil.indexFor(key,table.length,shift,mask);
  Entry e=table[index];
  if (e == null)   return false;
  if (e.key == key) {
    table[index]=e.hashNext;
  }
 else {
    for (; ; ) {
      final Entry last=e;
      e=e.hashNext;
      if (e == null)       return false;
      if (e.key == key) {
        last.hashNext=e.hashNext;
        break;
      }
    }
  }
  size=size - 1;
  return true;
}
