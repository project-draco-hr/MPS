{
  InputStream stream=classLoader.getResourceAsStream(traceInfoResourceName(sm));
  if (stream == null)   return null;
  try {
    return load(stream);
  }
 catch (  IOException e) {
    return null;
  }
 finally {
    try {
      stream.close();
    }
 catch (    IOException e) {
      LOG.error(e);
    }
  }
}
