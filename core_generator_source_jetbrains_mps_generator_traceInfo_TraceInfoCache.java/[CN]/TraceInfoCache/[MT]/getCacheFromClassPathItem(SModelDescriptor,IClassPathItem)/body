{
  URL url=classPathItem.getResource(traceInfoResourceName(sm));
  if (url == null)   return null;
  InputStream stream=null;
  try {
    stream=url.openStream();
    if (stream == null)     return null;
    return load(stream);
  }
 catch (  IOException e) {
    return null;
  }
 finally {
    try {
      if (stream != null) {
        stream.close();
      }
    }
 catch (    IOException e) {
      LOG.error(e);
    }
  }
}
