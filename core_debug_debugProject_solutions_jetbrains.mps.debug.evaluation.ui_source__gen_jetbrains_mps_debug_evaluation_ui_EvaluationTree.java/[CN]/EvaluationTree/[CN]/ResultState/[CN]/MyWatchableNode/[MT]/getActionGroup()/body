{
  ActionGroup group=super.getActionGroup();
  ActionGroup treeActionGroup=getTreeActionGroup();
  if (group == null) {
    if (treeActionGroup != null) {
      return treeActionGroup;
    }
    return null;
  }
  if (treeActionGroup != null) {
    return new DefaultActionGroup(group,treeActionGroup);
  }
  return group;
}
