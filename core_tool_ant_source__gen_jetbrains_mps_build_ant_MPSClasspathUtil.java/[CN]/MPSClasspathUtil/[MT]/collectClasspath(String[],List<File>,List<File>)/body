{
  for (  String name : fileNames) {
    File file=null;
    for (    File home : homeFolders) {
      File f=new File(home,name);
      if (f.isFile()) {
        file=f;
        break;
      }
    }
    if (file == null) {
      throw new BuildException("cannot find `" + name + "' in "+ homeFolders.toString());
    }
 else {
      result.add(file);
    }
  }
}
