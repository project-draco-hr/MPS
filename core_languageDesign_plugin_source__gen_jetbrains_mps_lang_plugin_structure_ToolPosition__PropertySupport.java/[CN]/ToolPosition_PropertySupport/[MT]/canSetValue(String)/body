{
  if (value == null) {
    return true;
  }
  Iterator<ToolPosition> constants=ListSequence.<ToolPosition>fromList(ToolPosition.getConstants()).iterator();
  while (constants.hasNext()) {
    ToolPosition constant=constants.next();
    if (value.equals(constant.getName())) {
      return true;
    }
  }
  return false;
}
