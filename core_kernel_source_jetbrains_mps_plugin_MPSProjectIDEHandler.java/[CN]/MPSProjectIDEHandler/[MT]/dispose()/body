{
  IProjectHandler handler=myProject.getProjectHandler();
  if (handler != null) {
    try {
      handler.removeIdeHandler(this);
    }
 catch (    RemoteException e) {
      LOG.error(e);
    }
  }
  try {
    unexportObject(this,true);
  }
 catch (  NoSuchObjectException e) {
    throw new RuntimeException(e);
  }
}
