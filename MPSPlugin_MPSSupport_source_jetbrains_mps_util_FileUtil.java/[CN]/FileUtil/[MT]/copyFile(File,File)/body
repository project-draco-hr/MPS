{
  try {
    byte[] bytes=new byte[(int)f.length()];
    FileInputStream is=new FileInputStream(f);
    OutputStream os=new FileOutputStream(new File(toDir,f.getName()));
    ReadUtil.read(bytes,is);
    os.write(bytes);
    is.close();
    os.close();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
