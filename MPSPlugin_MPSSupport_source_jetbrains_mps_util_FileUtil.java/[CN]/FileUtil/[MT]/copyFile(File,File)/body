{
  try {
    byte[] bytes=new byte[(int)f.length()];
    FileInputStream is=new FileInputStream(f);
    File target;
    if (to.isDirectory()) {
      target=new File(to,f.getName());
    }
 else {
      target=to;
    }
    OutputStream os=new FileOutputStream(target);
    ReadUtil.read(bytes,is);
    os.write(bytes);
    is.close();
    os.close();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
