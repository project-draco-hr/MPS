{
  if (!to.exists()) {
    to.mkdir();
  }
  for (  File f : what.listFiles()) {
    if (f.isDirectory()) {
      if (".svn".equals(f.getName()))       continue;
      File fCopy=new File(to,f.getName());
      if (!fCopy.exists()) {
        fCopy.mkdir();
      }
      copyDir(f,fCopy);
    }
    if (f.isFile()) {
      copyFile(f,to);
    }
  }
}
