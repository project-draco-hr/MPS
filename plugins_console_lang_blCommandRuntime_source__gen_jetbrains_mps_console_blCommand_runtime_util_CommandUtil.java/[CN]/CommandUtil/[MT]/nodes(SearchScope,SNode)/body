{
  if (concept == null) {
    return Sequence.fromIterable(allModels(scope)).translate(new ITranslator2<SModel,SNode>(){
      public Iterable<SNode> translate(      SModel it){
        return SModelOperations.getNodes(it,null);
      }
    }
);
  }
 else {
    return instances(scope,concept);
  }
}
