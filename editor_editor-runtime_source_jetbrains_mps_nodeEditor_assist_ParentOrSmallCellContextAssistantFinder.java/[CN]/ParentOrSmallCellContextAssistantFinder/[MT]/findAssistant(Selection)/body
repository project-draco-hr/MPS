{
  EditorCell cell=ContextAssistantSelectionUtil.getSingleSelectedCell(selection);
  EditorCell last=null;
  while (cell != null) {
    ContextAssistant assistant=findAssistantInSubTree(cell,last);
    if (assistant != null)     return assistant;
    last=cell;
    cell=cell.getParent();
  }
  return null;
}
