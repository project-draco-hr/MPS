{
  if (myOwnNodes == null) {
    ISearchScope searchScope=SModelSearchUtil_new.createModelAndImportedModelsScope(myModel,true,myScope);
    Condition<SNode> condition=new Condition<SNode>(){
      public boolean met(      SNode object){
        if (!SModelStereotype.TEMPLATES.equals(object.getModel().getStereotype())) {
          return false;
        }
        Language language=object.getLanguage(myScope);
        return language != BootstrapLanguages.getInstance().getTLBase();
      }
    }
;
    myOwnNodes=searchScope.getNodes(condition);
  }
}
