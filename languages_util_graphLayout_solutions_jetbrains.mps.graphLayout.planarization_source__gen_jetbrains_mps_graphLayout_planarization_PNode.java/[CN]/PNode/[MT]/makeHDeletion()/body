{
  if (getState() == PQNode.State.FULL || getState() == PQNode.State.EMPTY) {
    return;
  }
 else {
    for (    PQNode child : ListSequence.<PQNode>fromList(getChildren())) {
      if (child.getState() == PQNode.State.PARTIAL) {
        if (child == myHChild) {
          child.makeHDeletion();
        }
 else {
          child.makeEmpty();
        }
      }
    }
  }
}
