{
  this.appendNewLine();
  this.appendWithIndent("/*");
  this.increaseDepth();
  for (  SNode commentPart : SLinkOperations.getTargets(node,"commentPart",true)) {
    TextGenManager.instance().appendNodeText(this.getContext(),this.getBuffer(),commentPart,this.getSNode());
    this.append(" ");
  }
  this.decreaseDepth();
  this.appendWithIndent("*/");
}
