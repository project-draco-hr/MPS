{
  int vertexCount=graph.length;
  int[] size=new int[vertexCount];
  for (int v=0; v < vertexCount; v++) {
    for (    int target : graph[v]) {
      size[target]++;
    }
  }
  int[][] result=new int[vertexCount][];
  for (int v=0; v < vertexCount; v++) {
    result[v]=new int[size[v]];
    size[v]=0;
  }
  for (int v=0; v < vertexCount; v++) {
    for (    int target : graph[v]) {
      result[target][size[target]++]=v;
    }
  }
  return result;
}
