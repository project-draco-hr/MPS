{
  return ((SLinkOperations.getTarget(_context.getNode(),"newCreateBlock",true) != null) ? SLinkOperations.getTarget(SLinkOperations.getTarget(_context.getNode(),"newCreateBlock",true),"body",true) : SLinkOperations.getTarget(SLinkOperations.getTarget(_context.getNode(),"createBlock",true),"body",true));
}
