{
switch (level) {
case Project.MSG_ERR:
    LOG.error(text);
  break;
case Project.MSG_WARN:
LOG.warn(text);
break;
case Project.MSG_INFO:
LOG.info(text);
break;
case Project.MSG_DEBUG:
case Project.MSG_VERBOSE:
LOG.debug(text);
break;
default :
LOG.fatal("[unknown level " + level + "] "+ text);
break;
}
}
