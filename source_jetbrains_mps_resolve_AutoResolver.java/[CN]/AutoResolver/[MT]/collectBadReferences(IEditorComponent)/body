{
  Set<SReference> result=new HashSet<SReference>();
  for (  SNode node : CollectionUtil.iteratorAsIterable(editor.getRootCell().getSNode().depthFirstChildren(true))) {
    for (    SReference ref : node.getReferences()) {
      if (!ref.isResolved()) {
        result.add(ref);
      }
    }
  }
  return result;
}
