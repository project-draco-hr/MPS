{
  try {
    this.context=new ActionEventData(event).getOperationContext();
    if (this.context == null) {
      return false;
    }
    this.frame=new ActionEventData(event).getFrame();
    if (this.frame == null) {
      return false;
    }
  }
 catch (  Throwable t) {
    return false;
  }
  return true;
}
