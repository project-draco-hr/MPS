{
  ModelAccess.instance().runWriteAction(new Runnable(){
    public void run(){
      for (      BootstrapModule bm : BootstrapModule.values()) {
        bm.get().onModuleLoad();
      }
      List<IModule> modules=new ArrayList<IModule>();
      updatePredefinedLibraries();
      update();
    }
  }
);
}
