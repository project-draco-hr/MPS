{
  if (myInitializing)   return;
  myInitializing=true;
  try {
    ModelAccess.instance().runWriteAction(new Runnable(){
      public void run(){
        updatePredefinedLibraries();
        updateCustomBuiltInLibraries();
        update();
      }
    }
);
  }
  finally {
    myInitializing=false;
  }
}
