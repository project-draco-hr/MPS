{
  final SideTransformHintSubstituteActionsHelper helper=new SideTransformHintSubstituteActionsHelper(cell.getSNode(),side,cell.getStyle().get(StyleAttributes.RT_ANCHOR_TAG),cell.getContext().getOperationContext());
  SubstituteInfo info=new AbstractNodeSubstituteInfo(cell.getContext()){
    @Override protected List<SubstituteAction> createActions(){
      return helper.createActions();
    }
  }
;
  return !info.hasExactlyNActions(prefix,false,0);
}
