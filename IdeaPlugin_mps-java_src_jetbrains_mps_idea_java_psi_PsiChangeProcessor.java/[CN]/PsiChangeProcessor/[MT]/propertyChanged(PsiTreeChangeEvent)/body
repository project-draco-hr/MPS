{
  if (!(event.getElement() instanceof PsiFileSystemItem && (PsiTreeChangeEvent.PROP_FILE_NAME.equals(event.getPropertyName()) || PsiTreeChangeEvent.PROP_DIRECTORY_NAME.equals(event.getPropertyName())))) {
    return;
  }
  PsiChangeData data=projectData(event.getElement());
  FSRename rename=new FSRename((PsiFileSystemItem)event.getElement(),(String)event.getOldValue());
  data.renamed.add(rename);
}
