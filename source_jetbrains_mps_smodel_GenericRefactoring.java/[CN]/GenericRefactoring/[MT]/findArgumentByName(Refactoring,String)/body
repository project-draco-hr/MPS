{
  for (  RequiredAdditionalArgument argument : refactoring.getArgumentses()) {
    if (name.equals(argument.getName())) {
      return argument;
    }
  }
  for (  RequiredAdditionalArgument argument : refactoring.getInternalArgumentses()) {
    if (name.equals(argument.getName())) {
      return argument;
    }
  }
  return null;
}
