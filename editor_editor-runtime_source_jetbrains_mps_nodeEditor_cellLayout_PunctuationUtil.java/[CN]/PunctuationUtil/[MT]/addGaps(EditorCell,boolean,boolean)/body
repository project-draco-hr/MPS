{
  if (cell instanceof EditorCell_Collection && (hasPunctuableLayout((EditorCell_Collection)cell) || APIStyleAdapter.getStyleAttribute(cell,StyleAttributes.DRAW_BORDER))) {
    return;
  }
  int gap=getHorizontalGap(cell.getParent());
  cell.setLeftGap(!skipLeft && hasLeftGap(cell) ? gap / 2 : 0);
  cell.setRightGap(!skipRight && hasRightGap(cell) ? gap / 2 : 0);
}
