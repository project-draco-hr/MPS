{
  IModule module=originalModel.getModelDescriptor().getModule();
  if (module == null) {
    return false;
  }
  if (!(module instanceof Solution)) {
    return false;
  }
  if (((SolutionDescriptor)module.getModuleDescriptor()).getKind() == SolutionKind.NONE) {
    return true;
  }
  return !(module.isCompileInMPS());
}
