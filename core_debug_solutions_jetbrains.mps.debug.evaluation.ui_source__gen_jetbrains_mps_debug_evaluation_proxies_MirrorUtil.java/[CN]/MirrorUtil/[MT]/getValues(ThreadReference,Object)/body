{
  List<Value> argValues=new ArrayList<Value>();
  for (  Object arg : args) {
    Value v;
    if (arg instanceof ValueProxy) {
      v=((IValueProxy)arg).getJDIValue();
    }
 else {
      v=MirrorUtil.getJDIValueFromRaw(arg,threadReference.virtualMachine());
    }
    argValues.add(v);
  }
  return argValues;
}
