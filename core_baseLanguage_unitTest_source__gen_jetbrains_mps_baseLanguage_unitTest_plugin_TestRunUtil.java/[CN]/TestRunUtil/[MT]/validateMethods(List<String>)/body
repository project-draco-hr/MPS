{
  if (methods == null) {
    return false;
  }
  for (  String methodName : ListSequence.fromList(methods)) {
    int separatorIndex=methodName.lastIndexOf(SEPARATOR);
    if ((getTestMethod(methodName.substring(0,separatorIndex),methodName.substring(separatorIndex + 1)) == null)) {
      return false;
    }
  }
  return true;
}
