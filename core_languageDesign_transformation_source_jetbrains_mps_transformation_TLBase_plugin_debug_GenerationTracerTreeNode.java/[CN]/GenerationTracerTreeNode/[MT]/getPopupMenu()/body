{
  if (myTracerNode.getKind() == Kind.INPUT || myTracerNode.getKind() == Kind.APPROXIMATE_INPUT) {
    return createPopupMenuForInputNode();
  }
  if (myTracerNode.getKind() == Kind.OUTPUT || myTracerNode.getKind() == Kind.APPROXIMATE_OUTPUT) {
    return createPopupMenuForOutputNode();
  }
  return null;
}
