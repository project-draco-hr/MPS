{
  SNodePointer nodePointer=myTracerNode.getNodePointer();
  if (nodePointer == null)   return;
  SNode node=nodePointer.getNode();
  if (node == null) {
    LOG.info("clicked node was deleted");
  }
  NavigationActionProcessor.executeNavigationAction(new EditorNavigationCommand(node,getProjectFrame().getEditorsPane().getCurrentEditor(),getProjectFrame().getEditorsPane()),getProjectFrame().getProject(),true);
}
