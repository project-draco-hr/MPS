{
  SNodePointer nodePointer=myTracerNode.getNodePointer();
  if (nodePointer == null)   return;
  SNode node=nodePointer.getNode();
  if (node == null) {
    LOG.info("clicked node was deleted");
  }
  NavigationActionProcessor.executeNavigationAction(new EditorNavigationCommand(node,myProjectFrame.getEditorsPane().getCurrentEditor(),myProjectFrame.getEditorsPane()),myProjectFrame.getProject(),true);
}
