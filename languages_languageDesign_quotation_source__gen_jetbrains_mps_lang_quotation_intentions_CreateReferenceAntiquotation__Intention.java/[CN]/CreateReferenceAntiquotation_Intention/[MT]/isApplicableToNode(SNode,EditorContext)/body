{
  EditorCell selectedCell=editorContext.getSelectedCell();
  if (!(selectedCell.isReferenceCell())) {
    return false;
  }
  SNode contextNode=SNodeOperations.cast(selectedCell.getSNode(),"jetbrains.mps.lang.core.structure.BaseConcept");
  if (contextNode == null) {
    return false;
  }
  return true;
}
