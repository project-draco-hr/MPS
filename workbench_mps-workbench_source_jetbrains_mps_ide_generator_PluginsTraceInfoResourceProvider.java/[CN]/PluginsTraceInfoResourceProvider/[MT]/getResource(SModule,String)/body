{
  if (InternalFlag.isInternalMode() && (SModuleOperations.isCompileInIdea(module))) {
    for (    IdeaPluginDescriptor plugin : PluginManager.getPlugins()) {
      URL url=plugin.getPluginClassLoader().getResource(resourceName);
      if (url != null) {
        return url;
      }
    }
  }
  return null;
}
