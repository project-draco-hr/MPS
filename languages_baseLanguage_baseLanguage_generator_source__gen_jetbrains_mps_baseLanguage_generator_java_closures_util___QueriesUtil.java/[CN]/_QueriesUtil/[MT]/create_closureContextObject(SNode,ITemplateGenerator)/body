        return true;
      }
      return ClosuresUtil.isClosureContextOwner(((SNode)object));
    }
  }
));
  SModel model=generator.getOutputModel();
  if (enclosingClosureOrContextOwner != null && ClosuresUtil.isClosureContextOwner(enclosingClosureOrContextOwner)) {
