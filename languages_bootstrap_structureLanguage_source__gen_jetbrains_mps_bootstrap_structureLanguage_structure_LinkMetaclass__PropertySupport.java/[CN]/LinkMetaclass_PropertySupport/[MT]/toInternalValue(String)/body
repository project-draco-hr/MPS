{
  if (value == null) {
    return null;
  }
  Iterator<LinkMetaclass> constants=LinkMetaclass.getConstants().iterator();
  while (constants.hasNext()) {
    LinkMetaclass constant=constants.next();
    if (value.equals(constant.getName())) {
      return constant.getValueAsString();
    }
  }
  return null;
}
