{
  assertCanChange();
  SModel oldModel=getCurrentModelInternal();
  if (oldModel == null) {
    return;
  }
  MapSequence.fromMap(myRootsPerFile).clear();
  MapSequence.fromMap(myRootsById).clear();
  unload();
  SModel newModel=getSModelInternal();
  replaceModelAndFireEvent(oldModel,newModel);
}
