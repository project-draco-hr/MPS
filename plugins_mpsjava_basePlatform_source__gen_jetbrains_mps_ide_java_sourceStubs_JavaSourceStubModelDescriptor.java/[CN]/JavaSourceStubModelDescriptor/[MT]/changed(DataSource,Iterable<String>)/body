{
  ModelAccess.assertLegalWrite();
  if (myModel == null) {
    return;
  }
  MapSequence.fromMap(myRootsPerFile).clear();
  MapSequence.fromMap(myRootsById).clear();
  final SModel cloned=createModel();
  super.replaceModel(new Runnable(){
    public void run(){
      myModel=cloned;
    }
  }
);
  cloned.setModelDescriptor(this);
}
