{
  EditorCell cell=message.getCell();
  if (myChanges.isEmpty()) {
    y1=cell.getY();
    y2=cell.getY() + cell.getHeight();
  }
 else {
    if (y1 > cell.getY()) {
      y1=cell.getY();
    }
    if (y2 < cell.getY() + cell.getHeight()) {
      y2=cell.getY() + cell.getHeight();
    }
  }
  myChanges.add(message);
}
