{
  if (curr == null)   return null;
  SNode firstChild=curr.getFirstChild();
  if (firstChild != null)   return firstChild;
  if (curr == original)   return null;
  do {
    if (curr.myNextSibling != null) {
      return curr.myNextSibling;
    }
    curr=curr.myParent;
  }
 while (curr != original);
  return null;
}
