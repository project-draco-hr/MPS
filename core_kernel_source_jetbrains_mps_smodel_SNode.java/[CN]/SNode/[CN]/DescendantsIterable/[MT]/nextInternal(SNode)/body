{
  if (curr == null) {
    return null;
  }
  if (curr.myFirstChild != null) {
    return curr.myFirstChild;
  }
  if (curr == original) {
    return null;
  }
  do {
    if (curr.myNextSibling != null) {
      return curr.myNextSibling;
    }
    curr=curr.myParent;
  }
 while (curr != original);
  return null;
}
