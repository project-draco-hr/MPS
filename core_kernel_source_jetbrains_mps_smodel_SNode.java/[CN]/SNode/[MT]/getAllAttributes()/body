{
  List<SNode> attributes=new ArrayList<SNode>(0);
  if (myChildren != null) {
    for (    SNode child : myChildren) {
      String role=child.getRole_();
      if (AttributesRolesUtil.isNodeAttributeRole(role) || AttributesRolesUtil.isLinkAttributeRole(role) || AttributesRolesUtil.isPropertyAttributeRole(role)) {
        attributes.add(child);
      }
    }
  }
  return attributes;
}
