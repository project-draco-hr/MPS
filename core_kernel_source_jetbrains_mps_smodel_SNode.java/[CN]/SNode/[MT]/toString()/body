{
  ModelAccess.assertLegalRead(this);
  fireNodeReadAccess();
  String s=null;
  try {
    s=getPersistentProperty(SNodeUtil.property_BaseConcept_alias);
    if (s == null) {
      s=getPresentation();
    }
  }
 catch (  RuntimeException t) {
    LOG.error(t,this);
  }
  if (s == null) {
    return "???";
  }
  return s;
}
