{
  if (lid == null) {
    return null;
  }
  String result=MPSModuleRepository.getInstance().getDebugRegistry().getLinkName(lid);
  if (result == null) {
    DebugRegistryUtil.fillDebugInfo(myModel);
    result=MPSModuleRepository.getInstance().getDebugRegistry().getLinkName(lid);
  }
  return result;
}
