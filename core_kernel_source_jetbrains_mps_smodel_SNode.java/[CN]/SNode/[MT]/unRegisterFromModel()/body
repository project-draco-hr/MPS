{
  if (myModel == null)   return;
  if (!myModel.isUpdateMode()) {
    for (    SReference ref : myReferences) {
      ref.makeDirect();
    }
  }
  myModel.unregisterNode(this);
  for (SNode child=firstChild(); child != null; child=child.nextSibling()) {
    child.unRegisterFromModel();
  }
  myModel=null;
}
