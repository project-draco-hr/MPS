{
  propertyRead(property);
  firePropertyReadAccessInEditor(property,false);
  String value=null;
  if (myProperties != null) {
    int index=getPropertyIndex(property);
    if (index != -1) {
      value=(String)myProperties[index + 1];
    }
  }
  fireNodePropertyReadAccess(property,value);
  return value;
}
