{
  ModelAccess.assertLegalRead(this);
  fireNodeReadAccess();
  fireNodeUnclassifiedReadAccess();
  List<SNode> result=new ArrayList<SNode>(_children());
  if (!includeAttributes) {
    Iterator<SNode> it=result.iterator();
    while (it.hasNext()) {
      SNode child=it.next();
      if (child.isAttribute())       it.remove();
    }
  }
  return result;
}
