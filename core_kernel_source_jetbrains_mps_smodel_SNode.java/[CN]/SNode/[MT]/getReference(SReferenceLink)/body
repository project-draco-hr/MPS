{
  assertCanRead();
  referenceRead(role);
  fireNodeReadAccess();
  SReference result=null;
  for (  SReference reference : myReferences) {
    if (reference.getReferenceLink().equals(role)) {
      result=reference;
      break;
    }
  }
  fireNodeReferentReadAccess(role,null);
  return result;
}
