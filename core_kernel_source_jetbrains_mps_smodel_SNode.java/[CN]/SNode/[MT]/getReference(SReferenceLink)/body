{
  assertCanRead();
  referenceRead(role);
  fireNodeReadAccess();
  SReference result=null;
  for (  SReference reference : myReferences) {
    if (((SReferenceLinkAdapter)reference.getReferenceLink()).isSameReference(((SReferenceLinkAdapter)role))) {
      result=reference;
      break;
    }
  }
  fireNodeReferentReadAccess(role,null);
  return result;
}
