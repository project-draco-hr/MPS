{
  ModelAccess.assertLegalRead(this);
  fireNodeReadAccess();
  SReference result=null;
  int count=0;
  for (  SReference reference : _references()) {
    if (reference.getRole().equals(role)) {
      result=reference;
      count++;
    }
  }
  if (count > 1) {
    LOG.errorWithTrace("ERROR: " + count + " referents for role '"+ role+ "' in "+ getDebugText());
  }
  return result;
}
