{
  SNode firstChild=firstChild();
  if (role != null) {
    while (firstChild != null && !role.equals(firstChild.getContainmentLink())) {
      firstChild=firstChild.treeNext();
    }
  }
  if (firstChild == null) {
    return Collections.emptyList();
  }
  return new ChildrenList(firstChild,role);
}
