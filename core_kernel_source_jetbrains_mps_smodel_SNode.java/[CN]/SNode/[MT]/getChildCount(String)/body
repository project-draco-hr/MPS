{
  if (ourMemberAccessModifier != null) {
    role=ourMemberAccessModifier.getNewChildRole(myModel,myConceptFqName,role);
  }
  int count=0;
  for (SNode child=getFirstChild(); child != null; child=child.myNextSibling) {
    if (role.equals(child.getRole_())) {
      count++;
    }
  }
  return count;
}
