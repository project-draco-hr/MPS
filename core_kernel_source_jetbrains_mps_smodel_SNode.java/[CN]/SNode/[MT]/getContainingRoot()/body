{
  ModelAccess.assertLegalRead(this,myModel != null ? myModel.getModelDescriptor() : null);
  SNode current=this;
  while (true) {
    current.fireNodeReadAccess();
    if (current.treeParent() == null) {
      if (getModel().isRoot(current)) {
        return current;
      }
 else {
        return null;
      }
    }
 else {
      current=(SNode)current.treeParent();
    }
  }
}
