{
  ModelAccess.assertLegalRead(this);
  SNode current=this;
  while (true) {
    current.fireNodeReadAccess();
    if (current.myParent == null) {
      if (getModel().getRoots().contains(current)) {
        return current;
      }
 else {
        return null;
      }
    }
 else {
      current=current.myParent;
    }
  }
}
