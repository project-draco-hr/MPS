{
  ModelAccess.assertLegalRead(this);
  SNode current=this;
  while (true) {
    current.fireNodeReadAccess();
    if (current.treeParent() == null) {
      if (myModel != null && myModel.isRoot(current)) {
        return current;
      }
 else {
        return null;
      }
    }
 else {
      current=(SNode)current.treeParent();
    }
  }
}
