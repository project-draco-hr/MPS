{
  try {
    Location location=myStackFrame.getStackFrame().location();
    SNode snode=StacktraceUtil.getVar(location.declaringType().name(),location.sourceName(),location.lineNumber(),myLocalVariable.name());
    return snode;
  }
 catch (  AbsentInformationException ex) {
    LOG.error(ex);
    return null;
  }
}
