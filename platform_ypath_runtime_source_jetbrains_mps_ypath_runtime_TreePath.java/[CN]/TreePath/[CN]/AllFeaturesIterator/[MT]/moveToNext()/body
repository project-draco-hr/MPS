{
  this.hasNext=false;
  this.next=null;
  while (!hasNext) {
    if (currentItr == null || !currentItr.hasNext()) {
      if (featuresItr.hasNext()) {
        this.currentItr=featuresItr.next().getContents(node).iterator();
        continue;
      }
 else {
        break;
      }
    }
 else {
      this.hasNext=true;
      this.next=currentItr.next();
    }
  }
}
