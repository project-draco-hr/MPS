{
  IModule owningModule=ModelAccess.instance().runReadAction(new Computable<IModule>(){
    @Override public IModule compute(){
      return model.getModule();
    }
  }
);
  if (owningModule == null) {
    LOG.error("couldn't create module context for node:" + "\ncouldn't find owner module for model '" + model.getSModelReference() + "'");
    return null;
  }
  return new ModuleContext(owningModule,project);
}
