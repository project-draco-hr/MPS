{
  SModule owningModule=ModelAccess.instance().runReadAction(new Computable<SModule>(){
    @Override public SModule compute(){
      return model.getModule();
    }
  }
);
  if (owningModule == null) {
    LOG.error("couldn't create module context for node:" + "\ncouldn't find owner module for model '" + model.getReference() + "'");
    return null;
  }
  return new ModuleContext(owningModule,project);
}
