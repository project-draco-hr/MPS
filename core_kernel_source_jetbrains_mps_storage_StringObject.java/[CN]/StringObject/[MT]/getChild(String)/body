{
  s=s.intern();
  if (myChildren == null) {
    myChildren=new HashMap<String,StringObject>(3);
  }
 else {
    StringObject o=myChildren.get(s);
    if (o != null)     return myChildren.get(s);
  }
  StringObject newObj=new StringObject(s);
  myChildren.put(s,newObj);
  newObj.myParent=this;
  return newObj;
}
