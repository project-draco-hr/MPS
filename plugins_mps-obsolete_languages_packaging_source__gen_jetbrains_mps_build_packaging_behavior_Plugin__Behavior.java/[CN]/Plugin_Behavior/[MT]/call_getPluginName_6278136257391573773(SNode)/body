{
  int lastIndexOf=BehaviorReflection.invokeVirtual(File.class,thisNode,"virtual_getPath_1213877333777",new Object[]{}).getPath().lastIndexOf('/');
  if (lastIndexOf < 0) {
    lastIndexOf=BehaviorReflection.invokeVirtual(File.class,thisNode,"virtual_getPath_1213877333777",new Object[]{}).getPath().lastIndexOf('\\');
  }
  if (lastIndexOf < 0) {
    return BehaviorReflection.invokeVirtual(File.class,thisNode,"virtual_getPath_1213877333777",new Object[]{}).getPath();
  }
  return BehaviorReflection.invokeVirtual(File.class,thisNode,"virtual_getPath_1213877333777",new Object[]{}).getPath().substring(lastIndexOf + 1);
}
