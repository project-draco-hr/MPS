{
  myPanel=new JPanel();
  myPanel.setLayout(new GridLayout(2,1));
  myPanel.setBorder(BorderFactory.createCompoundBorder(BorderFactory.createTitledBorder("View Options (not imp.)"),BorderFactory.createEmptyBorder(5,5,5,5)));
  JCheckBox viewOne=new JCheckBox("Show one result",myOptions.myShowOneResult);
  myPanel.add(viewOne);
  JCheckBox newTab=new JCheckBox("New tab",myOptions.myNewTab);
  myPanel.add(newTab);
}
