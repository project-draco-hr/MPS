{
  String className=this.getClass().getName();
  PluginId pluginId=PluginManager.getPluginByClassName(className);
  if (pluginId == null) {
    return null;
  }
  IdeaPluginDescriptor descriptor=PluginManager.getPlugin(pluginId);
  if (descriptor == null) {
    return null;
  }
  return descriptor.getPath();
}
