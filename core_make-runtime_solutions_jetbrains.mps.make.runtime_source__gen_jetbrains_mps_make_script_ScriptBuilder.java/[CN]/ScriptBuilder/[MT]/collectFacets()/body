{
  for (  IFacet.Name fn : SetSequence.<IFacet.Name>fromSet(facets)) {
    IFacet fct=FacetRegistry.getInstance().lookup(fn);
    if (fct != null) {
      MapSequence.<IFacet.Name,IFacet>fromMap(facetsView).put(fn,fct);
    }
 else {
      String msg="facet not found: " + fn;
      LOG.error(msg);
      error(fn,msg);
    }
  }
}
