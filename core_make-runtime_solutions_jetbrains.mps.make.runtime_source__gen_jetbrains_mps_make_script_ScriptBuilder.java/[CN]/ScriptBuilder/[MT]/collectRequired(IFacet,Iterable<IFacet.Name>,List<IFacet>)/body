{
  for (  IFacet.Name req : Sequence.fromIterable(facets)) {
    IFacet f=MapSequence.fromMap(facetsView).get(req);
    if (f == null) {
      String msg="not found required facet: " + req;
      LOG.error(msg);
      error(fct.getName(),msg);
    }
 else {
      ListSequence.fromList(required).addElement(f);
    }
  }
}
