{
  Program p=new SimpleProgramBuilder().emitClosure().emitRet().emitEndClosure().emitNop().buildProgram();
  AnalysisResult<Boolean> result=p.analyze(new ReachabilityAnalyzer());
  Assert.assertEquals("0: closure true\n" + "1: ret true\n" + "2: endClosure true\n"+ "3: nop true\n"+ "4: end true\n",result.toString());
}
