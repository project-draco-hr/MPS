{
  Program p=new SimpleProgramBuilder().emitClosure().emitTry().emitRet().emitFinally().emitEndTry().emitEndClosure().buildProgram();
  AnalysisResult<Boolean> result=p.analyze(new ReachabilityAnalyzer());
  Assert.assertEquals("0: closure true\n" + "1: try true\n" + "2: ret true\n"+ "3: finally true\n"+ "4: endTry true\n"+ "5: endClosure true\n"+ "6: end true\n",result.toString());
}
