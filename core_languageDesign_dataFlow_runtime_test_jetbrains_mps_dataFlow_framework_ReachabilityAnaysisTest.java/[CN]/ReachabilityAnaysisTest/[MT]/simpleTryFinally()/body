{
  Program p=new SimpleProgramBuilder().emitTry().emitFinally().emitEndTry().emitNop().buildProgram();
  AnalysisResult<Boolean> result=p.analyze(new ReachabilityAnalyzer());
  Assert.assertEquals("0: try true\n" + "1: finally true\n" + "2: endTry true\n"+ "3: nop true\n"+ "4: end true\n",result.toString());
}
