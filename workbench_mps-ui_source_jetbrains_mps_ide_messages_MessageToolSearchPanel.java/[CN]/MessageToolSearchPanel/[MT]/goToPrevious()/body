{
  if (myResults.isEmpty()) {
    return;
  }
  addToHistory();
  int selected=myList.getSelectedIndex();
  if (selected != -1) {
    for (int i=myResults.size() - 1; i >= 0; i--) {
      if (selected > myResults.get(i)) {
        selectAndReveal(myResults.get(i));
        return;
      }
    }
  }
  selectAndReveal(myResults.get(myResults.size() - 1));
}
