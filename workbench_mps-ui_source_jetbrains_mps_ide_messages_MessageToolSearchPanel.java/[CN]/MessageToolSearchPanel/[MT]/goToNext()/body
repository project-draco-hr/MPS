{
  if (myResults.isEmpty()) {
    return;
  }
  addToHistory();
  int selected=myList.getSelectedIndex();
  if (selected != -1) {
    for (    Integer index : myResults) {
      if (selected < index) {
        selectAndReveal(index);
        return;
      }
    }
  }
  selectAndReveal(myResults.get(0));
}
