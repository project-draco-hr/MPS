{
  boolean selectionKeystroke=keyEvent.isShiftDown() && !(keyEvent.isAltDown() || keyEvent.isControlDown()) && (keyEvent.getKeyCode() == KeyEvent.VK_UP || keyEvent.getKeyCode() == KeyEvent.VK_DOWN || keyEvent.getKeyCode() == KeyEvent.VK_LEFT || keyEvent.getKeyCode() == KeyEvent.VK_RIGHT);
  return selectionKeystroke;
}
