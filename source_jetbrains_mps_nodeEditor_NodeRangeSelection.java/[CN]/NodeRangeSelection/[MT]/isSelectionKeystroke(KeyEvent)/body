{
  if (keyEvent.isShiftDown() && keyEvent.isControlDown() && !(keyEvent.isAltDown())&& (keyEvent.getKeyCode() == KeyEvent.VK_UP || keyEvent.getKeyCode() == KeyEvent.VK_DOWN || keyEvent.getKeyCode() == KeyEvent.VK_LEFT || keyEvent.getKeyCode() == KeyEvent.VK_RIGHT)) {
    return true;
  }
  if (keyEvent.isShiftDown() && !(keyEvent.isAltDown()) && (keyEvent.getKeyCode() == KeyEvent.VK_UP || keyEvent.getKeyCode() == KeyEvent.VK_DOWN)) {
    return true;
  }
  return false;
}
