{
  AbstractCellListHandler handler=new ProjectDescription_Editor.macroListHandler_6967_0(node,"macro",context);
  EditorCell_Collection editorCell=handler.createCells(context,new CellLayout_Vertical(),false);
  editorCell.setCellId("refNodeList_macro");
{
