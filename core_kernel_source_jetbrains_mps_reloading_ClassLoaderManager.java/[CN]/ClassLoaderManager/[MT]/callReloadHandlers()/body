{
  for (  ReloadListener h : myReloadHandlers) {
    try {
      h.invalidateCaches();
    }
 catch (    Throwable t) {
      LOG.error(t);
    }
  }
  for (  ReloadListener h : myReloadHandlers) {
    try {
      h.unload();
    }
 catch (    Throwable t) {
      LOG.error(t);
    }
  }
  for (  ReloadListener h : myReloadHandlers) {
    try {
      h.load();
    }
 catch (    Throwable t) {
      LOG.error(t);
    }
  }
}
