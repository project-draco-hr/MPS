{
  LOG.assertCanWrite();
  monitor.start("Unloading classes...",1);
  try {
    monitor.step("Disposing old classes...");
    callListeners(new ListenerCaller(){
      @Override public void call(      ReloadListener l){
        l.unload();
      }
    }
);
  }
  finally {
    monitor.done();
  }
}
