{
synchronized (myLock) {
    RBundle<ModuleReference> bundle=myRuntimeEnvironment.get(module.getModuleReference());
    if (bundle == null) {
      if (reportError) {
        LOG.error("Can't find a bundle " + module.getModuleReference().getModuleFqName());
      }
      return null;
    }
    return bundle.getClassLoader();
  }
}
