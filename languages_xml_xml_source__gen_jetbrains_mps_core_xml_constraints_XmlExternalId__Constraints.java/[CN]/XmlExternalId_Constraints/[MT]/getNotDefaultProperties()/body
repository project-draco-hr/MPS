{
  Map<String,PropertyConstraintsDescriptor> properties=new HashMap();
  properties.put("publicId",new BasePropertyConstraintsDescriptor("publicId",this){
    @Override public boolean hasOwnValidator(){
      return true;
    }
    @Override public boolean validateValue(    SNode node,    String propertyValue,    IScope scope){
      String propertyName="publicId";
      return XmlNameUtil.isPublicId((SPropertyOperations.getString(propertyValue)));
    }
  }
);
  properties.put("systemId",new BasePropertyConstraintsDescriptor("systemId",this){
    @Override public boolean hasOwnValidator(){
      return true;
    }
    @Override public boolean validateValue(    SNode node,    String propertyValue,    IScope scope){
      String propertyName="systemId";
      return XmlNameUtil.isSystemId((SPropertyOperations.getString(propertyValue)));
    }
  }
);
  return properties;
}
