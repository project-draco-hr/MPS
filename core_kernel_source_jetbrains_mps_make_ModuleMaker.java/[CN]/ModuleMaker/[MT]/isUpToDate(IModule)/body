{
  if (!(m instanceof Solution) && !(m instanceof Language)) {
    return true;
  }
  if (m.isPackaged()) {
    return true;
  }
  if (!isClassesUpToDate(m)) {
    return false;
  }
  for (  IModule dep : m.getAllDependOnModules(IModule.class)) {
    if (!isClassesUpToDate(dep)) {
      return false;
    }
  }
  return true;
}
