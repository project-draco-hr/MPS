{
  try {
    monitor.start("Clean",2000);
    for (    IModule m : modules) {
      if (isExcluded(m))       continue;
      monitor.addText("Cleaning " + m.getModuleUID() + "...");
      FileUtil.delete(m.getClassesGen().toFile());
    }
    monitor.addText("Done");
  }
  finally {
    monitor.finish();
  }
}
