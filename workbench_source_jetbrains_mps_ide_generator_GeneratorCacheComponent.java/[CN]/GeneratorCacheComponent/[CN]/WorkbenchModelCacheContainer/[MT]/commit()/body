{
  if (myReadOnly) {
    return;
  }
  try {
    for (    File child : myFolder.listFiles()) {
      if (!myHashDir.getName().equals(child.getName())) {
        FileUtil.delete(child);
      }
    }
    myHashDir.renameTo(new File(myFolder,myHash));
  }
 catch (  SecurityException ex) {
    LOG.error(ex);
  }
}
