{
  if (path == null) {
    return "";
  }
  if (basedir == null || basedir.length() == 0 || !(path.startsWith(basedir))) {
    return path;
  }
  if (basedir.endsWith("/") || basedir.endsWith("\\")) {
    basedir=basedir.substring(0,basedir.length() - 1);
  }
  return path.substring(basedir.length() + 1);
}
