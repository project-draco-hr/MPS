{
  super("Internal",ID);
  this.setIsAlwaysVisible(true);
  this.setIsInternal(true);
  this.setPopup(false);
  try {
    this.add(new RemoveTransientModulesAction());
    this.addSeparator();
    this.add(new DumpKeyStrokes());
    this.addSeparator();
  }
 catch (  Throwable t) {
    LOG.error("User group error",t);
  }
}
