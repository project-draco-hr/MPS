{
  super("Internal",ID,project);
  this.setInternal(true);
  try {
    this.add(new RemoveTransientModulesAction(),this);
    this.addSeparator();
    this.add(new DumpKeyStrokes(),this);
    this.addSeparator();
    this.add(new InternalRefactoringAction(),this);
  }
 catch (  Throwable t) {
    LOG.error("User group error",t);
  }
}
