{
  super("Internal",ID,true,null,project);
  this.setPopup(false);
  try {
    this.add(new RemoveTransientModulesAction());
    this.addSeparator();
    this.add(new DumpKeyStrokes());
    this.addSeparator();
    this.add(new InternalRefactoringAction());
  }
 catch (  Throwable t) {
    LOG.error("User group error",t);
  }
}
