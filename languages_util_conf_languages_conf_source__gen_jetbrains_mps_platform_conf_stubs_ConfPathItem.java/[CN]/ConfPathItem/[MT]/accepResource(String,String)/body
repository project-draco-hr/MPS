{
  if (res == null || !(res.endsWith(SUFFIX))) {
    return false;
  }
  InputStream is=null;
  try {
    is=this.openResource(pkg,res);
    SAXBuilder saxBuilder=JDOMUtil.createBuilder();
    return ConfReader.canRead(saxBuilder.build(is));
  }
 catch (  IOException e) {
    return false;
  }
catch (  JDOMException e) {
    return false;
  }
 finally {
    if (is != null) {
      try {
        is.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
