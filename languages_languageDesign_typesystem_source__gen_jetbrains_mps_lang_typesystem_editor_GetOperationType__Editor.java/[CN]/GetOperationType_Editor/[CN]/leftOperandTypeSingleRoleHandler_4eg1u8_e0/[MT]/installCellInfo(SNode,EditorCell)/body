{
  if (editorCell.getSubstituteInfo() == null || editorCell.getSubstituteInfo() instanceof DefaultSubstituteInfo) {
    editorCell.setSubstituteInfo(new OldNewCompositeSubstituteInfo(myEditorContext,new SChildSubstituteInfo(editorCell,myOwnerNode,MetaAdapterFactory.getContainmentLink(0x7a5dda6291404668L,0xab76d5ed1746f2b2L,0x11fd11186b7L,0x11fd111e015L,"leftOperandType"),child),new DefaultChildSubstituteInfo(myOwnerNode,myContainmentLink.getDeclarationNode(),myEditorContext)));
  }
  if (editorCell.getRole() == null) {
    editorCell.setRole("leftOperandType");
  }
}
