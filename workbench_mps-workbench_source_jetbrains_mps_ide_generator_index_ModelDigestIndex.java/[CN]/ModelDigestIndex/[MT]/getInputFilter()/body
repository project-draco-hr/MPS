{
  return new InputFilter(){
    @Override public boolean acceptInput(    @NotNull VirtualFile file){
      FileType fileType=file.getFileType();
      return fileType.equals(MPSFileTypeFactory.MPS_FILE_TYPE) || fileType.equals(MPSFileTypeFactory.MPS_ROOT_FILE_TYPE) || fileType.equals(MPSFileTypeFactory.MPS_HEADER_FILE_TYPE);
    }
  }
;
}
