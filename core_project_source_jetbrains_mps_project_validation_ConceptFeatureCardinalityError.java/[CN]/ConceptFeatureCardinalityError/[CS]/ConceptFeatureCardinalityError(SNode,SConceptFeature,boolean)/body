{
  super(Severity.ERROR,node,isUpperBoundBroken ? "Only one  " + feature.getPresentableKind() + " allowed in role "+ feature.getPresentableName() : "No " + feature.getPresentableKind() + " in obligatory role "+ feature.getPresentableName());
  myFeature=feature;
  myUpperBoundBroken=isUpperBoundBroken;
}
