{
  new Thread(){
    public void run(){
      CommandProcessor.instance().executeLightweightCommand(new Runnable(){
        public void run(){
          myUsageView.setRunOptions(TreeBuilder.forFinder(new ConstantFinder(mySearchResults.getSearchResults())),null,false,mySearchResults);
        }
      }
);
    }
  }
.start();
}
