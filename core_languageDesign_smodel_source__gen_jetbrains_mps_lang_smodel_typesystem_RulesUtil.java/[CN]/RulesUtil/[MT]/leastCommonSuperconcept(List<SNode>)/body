      SetSequence.fromSet(allTypes).addSequence(ListSequence.fromList(supertypes));
      ListSequence.fromList(supertypes).addElement(concept);
    }
    frontier=newFrontier;
    newFrontier=SetSequence.<SNode>fromArray();
