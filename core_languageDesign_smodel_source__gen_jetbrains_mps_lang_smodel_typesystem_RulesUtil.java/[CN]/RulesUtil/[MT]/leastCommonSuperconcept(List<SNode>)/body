{
  Map<SNode,Set<SNode>> subTypesToSuperTypes=MapSequence.fromMap(new HashMap<SNode,Set<SNode>>());
  Set<SNode> keyset=SetSequence.<SNode>fromArray();
  Set<SNode> allTypes=SetSequence.<SNode>fromArray();
