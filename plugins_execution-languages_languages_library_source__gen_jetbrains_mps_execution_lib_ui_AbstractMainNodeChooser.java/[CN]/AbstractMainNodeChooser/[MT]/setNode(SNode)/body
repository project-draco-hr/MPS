{
  if (check_wlpn4v_a0a0i(this.myNodePointer,this) == node) {
    return;
  }
  if (node == null) {
    if (this.myNodePointer == null) {
      this.setText(null);
      return;
    }
 else {
      myNodePointer=null;
    }
  }
 else {
    this.myNodePointer=new jetbrains.mps.smodel.SNodePointer(node);
    this.setText(getFqName(node));
  }
  this.fireNodeChanged();
}
