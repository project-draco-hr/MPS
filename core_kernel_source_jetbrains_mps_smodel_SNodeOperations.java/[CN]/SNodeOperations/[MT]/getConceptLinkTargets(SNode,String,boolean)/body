{
  List<SNode> result=new ArrayList<SNode>();
  List<SNode> conceptLinks=getConceptLinks(node,linkName,lookupHierarchy);
  for (  SNode conceptLink : conceptLinks) {
    SNode target=SModelUtil.getConceptLinkTarget(conceptLink);
    if (target != null) {
      result.add(target);
    }
  }
  return result;
}
