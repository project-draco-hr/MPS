{
  SuspendContext newContext=previousState.myContext;
  LOG.assertLog(newContext != null);
  if (!newContext.suspends(myThread)) {
    newContext=null;
    for (    SuspendContext context : getAllPausedContexts()) {
      if (context.suspends(myThread)) {
        newContext=context;
        break;
      }
    }
  }
  return newContext;
}
