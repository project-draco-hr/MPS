{
  if (pattern == null || pattern.length() == 0) {
    return true;
  }
  String matchingText=null;
  try {
    matchingText=getMatchingText(pattern);
  }
 catch (  Exception e) {
    LOG.error(null,e);
  }
  if (matchingText == null || matchingText.length() == 0) {
    return false;
  }
  return matchingText.charAt(0) == pattern.charAt(0) && (matchingText.startsWith(pattern) || matchingText.matches(PatternUtil.getExactItemPatternBuilder(pattern,false,false).toString() + ".*"));
}
