{
  if (editorCell == null || action == null) {
    return false;
  }
  if (action.executeInCommand()) {
    getEditorContext().executeCommand(new Runnable(){
      @Override public void run(){
        action.execute(getEditorContext());
      }
    }
);
  }
 else {
    action.execute(getEditorContext());
  }
  return true;
}
