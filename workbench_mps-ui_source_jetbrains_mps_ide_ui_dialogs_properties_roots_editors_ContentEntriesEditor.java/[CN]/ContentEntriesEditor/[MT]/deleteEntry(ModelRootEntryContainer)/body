{
  if (!myModelRootEntries.contains(entry))   return;
  myEditorsListPanel.remove(entry.getComponent());
  int idx=myModelRootEntries.indexOf(entry);
  myModelRootEntries.remove(entry);
  if (myFocucedModelRootEntryContainer.equals(entry))   selectEntry(myModelRootEntries.size() > 0 ? myModelRootEntries.get(Math.max(idx - 1,0)) : null);
 else   myMainPanel.updateUI();
}
