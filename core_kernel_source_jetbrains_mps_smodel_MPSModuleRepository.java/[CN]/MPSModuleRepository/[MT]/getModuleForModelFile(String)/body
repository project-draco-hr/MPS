{
  assertCanRead();
  for (  IModule module : getAllModules()) {
    Set<SModelRoot> smodelRoots=module.getSModelRoots();
    for (    SModelRoot root : smodelRoots) {
      String rootPath=root.getPath();
      if (path.startsWith(rootPath)) {
        return module;
      }
    }
  }
  return null;
}
