{
  checkValidity();
  File dir=getModelDir(namespace);
  long result=dir.lastModified();
  if (dir.exists()) {
    for (    File file : dir.listFiles()) {
      if (file.getName().endsWith(MPSExtentions.DOT_CLASSFILE)) {
        result=Math.max(result,file.lastModified());
      }
    }
  }
  return result;
}
