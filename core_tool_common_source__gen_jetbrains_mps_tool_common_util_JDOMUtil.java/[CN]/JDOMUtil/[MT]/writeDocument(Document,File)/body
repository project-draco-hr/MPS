{
  if (!(file.getParentFile().exists())) {
    file.getParentFile().mkdirs();
  }
  if (!(file.exists())) {
    file.createNewFile();
  }
  OutputStream stream=new BufferedOutputStream(new FileOutputStream(file));
  try {
    writeDocument(document,stream);
  }
  finally {
    stream.close();
  }
}
