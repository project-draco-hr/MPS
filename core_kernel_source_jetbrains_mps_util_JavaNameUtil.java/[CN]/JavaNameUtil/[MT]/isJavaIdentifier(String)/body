{
  int len=text.length();
  if (len == 0)   return false;
  if (!Character.isJavaIdentifierStart(text.charAt(0))) {
    return false;
  }
  for (int i=1; i < len; i++) {
    if (!Character.isJavaIdentifierPart(text.charAt(i))) {
      return false;
    }
  }
  return !(JAVA_KEYWORDS.contains(text));
}
