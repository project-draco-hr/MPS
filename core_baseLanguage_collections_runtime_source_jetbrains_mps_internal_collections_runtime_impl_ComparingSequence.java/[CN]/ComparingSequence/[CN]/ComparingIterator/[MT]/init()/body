{
switch (kind) {
case SUBSTRACTION:
case INTERSECTION:
    for (    U o : right.toIterable()) {
      cardMap.postInc(o);
    }
  this.leftIt=left.toIterable().iterator();
break;
case UNION:
this.leftIt=left.toIterable().iterator();
this.rightIt=right.toIterable().iterator();
break;
case DISJUNCTION:
cache=new ArrayList<U>();
for (U o : right.toIterable()) {
cardMap.postInc(o);
cache.add(o);
}
this.leftIt=left.toIterable().iterator();
this.rightIt=cache.iterator();
break;
default :
break;
}
}
