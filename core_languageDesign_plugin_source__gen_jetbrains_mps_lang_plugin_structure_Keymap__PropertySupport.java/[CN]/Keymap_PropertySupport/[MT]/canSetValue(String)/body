{
  if (value == null) {
    return true;
  }
  Iterator<Keymap> constants=ListSequence.<Keymap>fromList(Keymap.getConstants()).iterator();
  while (constants.hasNext()) {
    Keymap constant=constants.next();
    if (value.equals(constant.getName())) {
      return true;
    }
  }
  return false;
}
