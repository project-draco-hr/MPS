{
  try {
    this.editorComponent=new ActionEventData(event).getAbstractEditorComponent();
    if (this.editorComponent == null) {
      return false;
    }
    this.node=new ActionEventData(event).getNode();
    if (this.node == null) {
      return false;
    }
  }
 catch (  Throwable t) {
    return false;
  }
  return true;
}
