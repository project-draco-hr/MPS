{
  try {
    this.editorComponent=context.get(AbstractEditorComponent.class);
    if (this.editorComponent == null) {
      return false;
    }
    this.node=context.getNode();
    if (this.node == null) {
      return false;
    }
  }
 catch (  Throwable t) {
    return false;
  }
  return true;
}
