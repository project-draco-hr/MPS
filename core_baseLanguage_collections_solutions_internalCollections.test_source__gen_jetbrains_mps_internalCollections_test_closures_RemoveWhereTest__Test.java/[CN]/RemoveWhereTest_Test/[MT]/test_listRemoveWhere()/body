{
  List<String> l1=Sequence.<String>fromIterable(Sequence.fromArray(new String[]{"a","bb","ccc","dddd"})).toListSequence();
  ListSequence.<String>fromList(l1).removeWhere(new IWhereFilter<String>(){
    public boolean accept(    String it){
      return it.length() > 2;
    }
  }
);
  Assert.assertEquals("a",ListSequence.<String>fromList(l1).first());
  Assert.assertEquals("bb",ListSequence.<String>fromList(l1).last());
}
