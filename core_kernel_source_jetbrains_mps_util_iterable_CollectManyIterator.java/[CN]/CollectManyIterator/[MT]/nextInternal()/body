{
  if (myCurrentInner != null && myCurrentInner.hasNext()) {
    return myCurrentInner.next();
  }
  while (myCurrentOuter.hasNext()) {
    IN element=myCurrentOuter.next();
    myCurrentInner=element != null ? translate(element) : null;
    if (myCurrentInner != null && myCurrentInner.hasNext()) {
      return myCurrentInner.next();
    }
  }
  return null;
}
