{
  byte b=readByte();
  if (b == NULL) {
    return null;
  }
  if (b == CONCEPT_INDEX) {
    return myConcepts.get(readShort());
  }
  if (b != CONCEPT) {
    throw new IOException(Integer.toHexString(b));
  }
  SConcept c=MetaAdapterFactory.getConcept(readLong(),readLong(),readLong(),readString());
  myConcepts.add(c);
  return c;
}
