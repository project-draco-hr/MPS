{
  SNode result=null;
  for (  SNode n : SNodeOperations.getChildren(myNode,myLink)) {
    if (this.concept != null && !(n.getConcept().isSubConceptOf(concept))) {
      continue;
    }
    if (getName(n).equals(refText)) {
      if (result != null) {
        return null;
      }
      result=n;
    }
  }
  return result;
}
