{
  SNode result=null;
  for (  SNode n : SNodeOperations.getChildren(myNode,myLink)) {
    if (this.conceptFqName != null && !(SModelUtil.isAssignableConcept(n.getConcept().getQualifiedName(),conceptFqName))) {
      continue;
    }
    if (getName(n).equals(refText)) {
      if (result != null) {
        return null;
      }
      result=n;
    }
  }
  return result;
}
