{
  Class aClass=forName(cl,classNode);
  Field field;
  try {
    field=aClass.getField(constantName);
  }
 catch (  NoSuchFieldException e) {
    throw new RuntimeException();
  }
  try {
    return field.get(null);
  }
 catch (  IllegalAccessException e) {
    throw new RuntimeException();
  }
}
