{
  super("Generate",Generate_ActionGroup.ID);
  this.setMnemonic("G".charAt(0));
  this.setInternal(false);
  this.add(new GenerateAllModelsInModuleAction_false(),this);
  this.addSeparator();
  this.add(new ToggleTransientModelsSavingAction(),this);
  this.addSeparator();
