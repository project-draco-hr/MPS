{
  super("Generate",ID,true,false,"G".charAt(0),project);
  this.setPopup(true);
  try {
    this.add(new GenerateAllModelsInModuleAction(false));
    this.addSeparator();
    this.add(new ToggleTransientModelsSavingAction());
    this.addSeparator();
    this.add(new GenerateFilesFromCurrentModelAction());
    this.add(new GenerateTextFromCurrentModelAction());
  }
 catch (  Throwable t) {
    LOG.error("User group error",t);
  }
}
