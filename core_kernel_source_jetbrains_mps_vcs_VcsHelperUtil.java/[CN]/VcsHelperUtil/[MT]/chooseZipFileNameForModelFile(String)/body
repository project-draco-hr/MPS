{
  Calendar cal=Calendar.getInstance();
  String timestamp=cal.get(Calendar.HOUR_OF_DAY) + "." + cal.get(Calendar.MINUTE)+ "."+ cal.get(Calendar.DAY_OF_MONTH)+ "."+ cal.get(Calendar.MONTH)+ "."+ cal.get(Calendar.YEAR);
  modelFilePath=modelFilePath + "." + timestamp;
  File zipfile=new File(modelFilePath + ".zip");
  int i=0;
  while (zipfile.exists()) {
    zipfile=new File(modelFilePath + "." + i+ ".zip");
    i++;
  }
  return zipfile;
}
