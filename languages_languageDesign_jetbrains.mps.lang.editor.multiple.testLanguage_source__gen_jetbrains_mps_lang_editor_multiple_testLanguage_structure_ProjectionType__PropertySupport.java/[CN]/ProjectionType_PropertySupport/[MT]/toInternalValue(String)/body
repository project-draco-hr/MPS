{
  if (value == null) {
    return null;
  }
  Iterator<ProjectionType> constants=ListSequence.fromList(ProjectionType.getConstants()).iterator();
  while (constants.hasNext()) {
    ProjectionType constant=constants.next();
    if (value.equals(constant.getName())) {
      return constant.getValueAsString();
    }
  }
  return null;
}
