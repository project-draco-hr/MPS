{
  myTabsPanel.removeAll();
  myRealTabs.clear();
  for (  EditorTabDescriptor d : myPossibleTabs) {
    if (!d.getNodes(myBaseNode.getNode()).isEmpty()) {
      EditorTab tab=new EditorTab(this,d);
      myRealTabs.add(tab);
      myTabsPanel.add(tab);
    }
  }
  myTabsPanel.add(new AddConceptButton());
}
