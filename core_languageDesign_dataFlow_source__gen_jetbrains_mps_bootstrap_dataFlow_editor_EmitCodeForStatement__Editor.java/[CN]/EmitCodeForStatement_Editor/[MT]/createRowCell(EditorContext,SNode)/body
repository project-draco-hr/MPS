{
  EditorCell_Collection editorCell=EditorCell_Collection.createHorizontal(context,node);
  EmitCodeForStatement_Editor.setupBasic_RowCell(editorCell,node,context);
  editorCell.setGridLayout(false);
  editorCell.setUsesBraces(false);
  editorCell.setCanBeFolded(false);
  editorCell.addEditorCell(this.createConstantCell(context,node,"code"));
  editorCell.addEditorCell(this.createConstantCell1(context,node,"for"));
  editorCell.addEditorCell(this.createCodeForCell(context,node));
  return editorCell;
}
