{
  if (oldNodeId != null) {
    String key=node.getModel().getUID() + "#" + oldNodeId;
    myMap.remove(key);
  }
  if (node.hasId()) {
    String key=node.getModel().getUID() + "#" + node.getId();
    LOG.assertLog(!myMap.containsKey(key),"attempt to put another node with same key: " + key);
    myMap.put(key,node);
  }
}
