{
  IFile cacheFile=getCacheFile(model);
  if (cacheFile == null)   return null;
  OutputStream os=null;
  try {
    os=cacheFile.openOutputStream();
    save(cache,os);
  }
 catch (  IOException e) {
    LOG.error(e);
  }
 finally {
    if (os != null) {
      try {
        os.close();
      }
 catch (      IOException e) {
        LOG.error(e);
      }
    }
  }
  return cacheFile.toFile();
}
