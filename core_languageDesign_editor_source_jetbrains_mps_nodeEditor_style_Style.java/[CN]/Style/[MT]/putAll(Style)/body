{
  List<StyleAttribute> added=new ArrayList<StyleAttribute>();
  for (int i=0; i < s.myAttributeValues.length; i++) {
    Object value=s.myAttributeValues[i];
    if (value != null) {
      myAttributeValues[i]=value;
      added.add(StyleAttributes.getAttribute(i));
    }
  }
  updateCache(added);
}
