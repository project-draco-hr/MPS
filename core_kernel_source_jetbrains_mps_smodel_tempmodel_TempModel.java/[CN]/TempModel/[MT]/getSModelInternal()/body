{
  if (mySModel != null) {
    return mySModel;
  }
synchronized (this) {
    if (mySModel == null) {
      mySModel=new jetbrains.mps.smodel.SModel(getReference()){
        @Override protected void performUndoableAction(        Computable<SNodeUndoableAction> action){
          if (!myTrackUndo)           return;
          super.performUndoableAction(action);
        }
      }
;
      mySModel.setModelDescriptor(this);
      fireModelStateChanged(ModelLoadingState.FULLY_LOADED);
    }
  }
  return mySModel;
}
