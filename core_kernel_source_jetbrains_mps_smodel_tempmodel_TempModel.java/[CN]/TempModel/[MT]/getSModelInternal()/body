{
  if (mySModel != null) {
    return mySModel;
  }
synchronized (this) {
    if (mySModel == null) {
      mySModel=new jetbrains.mps.smodel.SModel(getReference());
      mySModel.setModelDescriptor(this);
      fireModelStateChanged(ModelLoadingState.NOT_LOADED,ModelLoadingState.FULLY_LOADED);
    }
  }
  return mySModel;
}
