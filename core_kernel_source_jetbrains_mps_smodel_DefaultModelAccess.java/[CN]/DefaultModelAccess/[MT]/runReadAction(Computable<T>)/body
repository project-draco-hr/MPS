{
  if (canRead()) {
    return c.compute();
  }
  if (mySharedReadInWriteMode) {
    try {
      mySharedReadInWriteLock.readLock().lock();
      return c.compute();
    }
  finally {
      mySharedReadInWriteLock.readLock().unlock();
    }
  }
  getReadLock().lock();
  try {
    return c.compute();
  }
  finally {
    getReadLock().unlock();
  }
}
