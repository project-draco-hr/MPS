{
  if (InternalFlag.isInternalMode() && !(ourUpToDate)) {
    if (dryRun) {
      if (getFile().exists()) {
        return AbstractInstaller.State.OUTDATED;
      }
 else {
        return AbstractInstaller.State.NOT_INSTALLED;
      }
    }
 else {
      pack();
      ourUpToDate=true;
      return AbstractInstaller.State.INSTALLED;
    }
  }
 else {
    return AbstractInstaller.State.INSTALLED;
  }
}
