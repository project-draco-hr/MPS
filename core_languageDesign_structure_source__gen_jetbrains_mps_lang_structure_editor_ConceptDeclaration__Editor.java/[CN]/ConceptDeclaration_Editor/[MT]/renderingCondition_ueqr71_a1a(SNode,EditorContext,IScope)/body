{
  Language l=SModelUtil.getDeclaringLanguage(node,scope);
  if (l == null) {
    return false;
  }
  String s=Macros.languageDescriptor().expandPath(SPropertyOperations.getString(node,"iconPath"),l.getDescriptorFile());
  if (s == null) {
    return false;
  }
  return OldFileSystem.getFile(s).exists();
}
