{
  Language l=SModelUtil.getDeclaringLanguage(node,scope);
  if (l == null) {
    return false;
  }
  String s=MacrosFactory.languageDescriptor().expandPath(SPropertyOperations.getString(node,"iconPath"),l.getDescriptorFile());
  if (s == null) {
    return false;
  }
  return FileSystem.getInstance().getFileByPath(s).exists();
}
