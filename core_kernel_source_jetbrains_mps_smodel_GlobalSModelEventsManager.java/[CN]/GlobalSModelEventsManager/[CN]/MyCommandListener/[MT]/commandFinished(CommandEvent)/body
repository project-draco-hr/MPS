{
  if (myCommandEvents.isEmpty())   return;
  for (  SModelCommandListener l : myGlobalCommandListeners) {
    try {
      l.eventsHappenedInCommand(Collections.unmodifiableList(myCommandEvents));
    }
 catch (    Throwable t) {
      LOG.error(t);
    }
  }
  myCommandEvents=new ArrayList<SModelEvent>();
}
