{
  if (newParent instanceof IdeaFile && ((IdeaFile)newParent).findVirtualFile()) {
    try {
      myVirtualFile.move(ourRequestor(),((IdeaFile)newParent).myVirtualFile);
      return true;
    }
 catch (    IOException e) {
      IdeaFileSystemProvider.LOG.warn("Could not rename file: ",e);
      return false;
    }
  }
 else {
    return false;
  }
}
