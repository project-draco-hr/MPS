{
  if (findVirtualFile()) {
    try {
      myVirtualFile.delete(ourRequestor());
      myPath=myVirtualFile.getPath();
      myVirtualFile=null;
      return true;
    }
 catch (    IOException e) {
      IdeaFileSystemProvider.LOG.warn("Could not delete file: ",e);
      return false;
    }
  }
 else {
    return true;
  }
}
