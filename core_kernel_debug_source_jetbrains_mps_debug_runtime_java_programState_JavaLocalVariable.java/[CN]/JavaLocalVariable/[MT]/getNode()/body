{
  try {
    Location location=myStackFrame.location();
    SNode snode=StacktraceUtil.getNodeOrVar(location.declaringType().name(),location.sourceName(),location.lineNumber(),myLocalVariable.name());
    return snode;
  }
 catch (  AbsentInformationException ex) {
    LOG.error(ex);
    return null;
  }
}
