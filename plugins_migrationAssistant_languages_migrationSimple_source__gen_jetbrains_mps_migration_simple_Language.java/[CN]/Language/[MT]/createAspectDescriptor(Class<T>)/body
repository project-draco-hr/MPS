{
  if (descriptorClass == BehaviorAspectDescriptor.class) {
    return (T)new jetbrains.mps.migration.simple.behavior.BehaviorAspectDescriptor();
  }
  if (descriptorClass == ConstraintsAspectDescriptor.class) {
    return (T)new jetbrains.mps.migration.simple.constraints.ConstraintsAspectDescriptor();
  }
  if (descriptorClass == EditorAspectDescriptor.class) {
    return (T)new EditorAspectDescriptorImpl();
  }
  if (descriptorClass == StructureAspectDescriptor.class) {
    return (T)new jetbrains.mps.migration.simple.structure.StructureAspectDescriptor();
  }
  return super.createAspectDescriptor(descriptorClass);
}
