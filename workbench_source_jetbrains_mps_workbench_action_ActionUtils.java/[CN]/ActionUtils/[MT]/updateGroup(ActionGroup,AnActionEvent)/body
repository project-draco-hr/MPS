{
  try {
    group.update(e);
  }
 catch (  Throwable t) {
    LOG.error(t);
  }
  for (  AnAction child : group.getChildren(null)) {
    try {
      child.update(e);
    }
 catch (    Throwable t) {
      LOG.error(t);
    }
    if (child instanceof ActionGroup)     updateGroup((ActionGroup)child,e);
  }
}
