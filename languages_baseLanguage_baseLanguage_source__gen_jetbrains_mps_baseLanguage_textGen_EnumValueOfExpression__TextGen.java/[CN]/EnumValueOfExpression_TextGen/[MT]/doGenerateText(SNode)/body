{
  BaseLanguageTextGen.blClassifierRef(SNodeOperations.getReference(node,SLinkOperations.findLinkDeclaration("jetbrains.mps.baseLanguage.structure.EnumValueOfExpression","enumClass")),this);
  this.append(".");
  this.append("valueOf(");
  TextGenManager.instance().appendNodeText(this.getContext(),this.getBuffer(),SLinkOperations.getTarget(node,"value",true),this.getSNode());
  this.append(")");
}
