{
  myNodeId=nodeId;
  myRole=role;
  myModel=model;
  if (target != null) {
    if (target.getModel() != model) {
      myInternal=false;
      myNodePointer=new SNodePointer(target);
    }
 else {
      myInternal=true;
      myTargetId=target.getSNodeId();
    }
  }
 else {
    myIsDeleted=true;
  }
}
