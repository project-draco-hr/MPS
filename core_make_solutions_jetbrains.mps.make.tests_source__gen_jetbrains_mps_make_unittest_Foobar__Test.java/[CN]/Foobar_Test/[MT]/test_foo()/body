{
  Mockery m=new JUnit3Mockery();
  final ITarget t=m.mock(ITarget.class);
  m.checking(new Expectations(){
{
      oneOf(t).getName();
      will(returnValue(new ITarget.Name("foobar")));
    }
  }
);
  m.checking(new Expectations(){
{
      never(t).getName();
      will(returnValue(null));
    }
  }
);
  Assert.assertEquals(new ITarget.Name("foobar"),t.getName());
  m.assertIsSatisfied();
}
