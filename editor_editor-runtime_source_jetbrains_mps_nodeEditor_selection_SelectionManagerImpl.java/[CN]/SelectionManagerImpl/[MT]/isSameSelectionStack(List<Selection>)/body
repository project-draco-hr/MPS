{
  if (newSelectionStack.size() != mySelectionStack.size()) {
    return false;
  }
  int index=0;
  for (  Selection oldSelection : mySelectionStack) {
    Selection newSelection=newSelectionStack.get(index++);
    if (!oldSelection.isSame(newSelection)) {
      return false;
    }
  }
  return true;
}
