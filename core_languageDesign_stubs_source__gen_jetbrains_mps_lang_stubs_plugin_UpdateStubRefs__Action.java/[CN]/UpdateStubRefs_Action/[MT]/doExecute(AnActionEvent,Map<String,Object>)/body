{
  try {
    for (    SModelDescriptor d : ListSequence.fromList(SModelRepository.getInstance().getModelDescriptors())) {
      if (!(d instanceof EditableSModelDescriptor)) {
        continue;
      }
      if (!(SModelStereotype.isUserModel(d))) {
        continue;
      }
      SModelRepository.getInstance().markChanged(((EditableSModelDescriptor)d),true);
    }
    SModelRepository.getInstance().saveAll();
  }
 catch (  Throwable t) {
    LOG.error("User's action execute method failed. Action:" + "UpdateStubRefs",t);
  }
}
