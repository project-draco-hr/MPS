{
  for (  VirtualFile f : parentFiles) {
    if (!f.isDirectory())     continue;
    VirtualFile[] children=f.getChildren();
    for (    VirtualFile child : children) {
      if (!isIgnored(child)) {
        allChildren.add(child);
        getAllChildren(Collections.singleton(child),allChildren);
      }
    }
  }
  return allChildren;
}
