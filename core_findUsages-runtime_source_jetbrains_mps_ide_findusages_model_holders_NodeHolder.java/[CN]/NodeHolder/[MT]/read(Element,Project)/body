{
  Element nodeXML=element.getChild(NODE);
  if (nodeXML == null) {
    throw new CantLoadSomethingException("node is null");
  }
  SNode node=ComponentsUtil.nodeFromElement((Element)nodeXML.getChildren().get(0));
  if (node == null) {
    throw new CantLoadSomethingException("node is null");
  }
  myNodePointer=node.getReference();
}
