{
  if (languages.isEmpty()) {
    return;
  }
  for (  LanguageRegistryListener l : myLanguageListeners) {
    try {
      l.afterLanguagesLoaded(languages);
    }
 catch (    Exception ex) {
      LOG.error(format("Exception on language loading; languages: %s; listener: %s",languages,l),ex);
    }
  }
}
