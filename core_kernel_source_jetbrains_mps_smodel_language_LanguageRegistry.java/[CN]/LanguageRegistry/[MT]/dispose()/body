{
  ModelAccess.instance().runWriteAction(new Runnable(){
    @Override public void run(){
      notifyUnload(myLanguages.values(),true);
      myLanguageToNamespace.clear();
      myLanguages.clear();
      myLanguages=null;
    }
  }
);
  myClassLoaderManager.removeReloadHandler(myHandler);
  INSTANCE=null;
}
