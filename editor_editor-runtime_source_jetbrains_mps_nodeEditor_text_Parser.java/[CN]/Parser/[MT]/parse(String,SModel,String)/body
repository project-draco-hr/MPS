{
  Class cls;
  try {
    Language l=(Language)MPSModuleRepository.getInstance().getModuleByFqName(languageNamespace);
    String name=languageNamespace + ".parser.Parser";
    cls=l.getClass(name);
  }
 catch (  Exception e) {
    return null;
  }
  try {
    Method m=cls.getMethod("parse",String.class,SModel.class);
    return (List<SNode>)m.invoke(null,input,model);
  }
 catch (  Exception e) {
    return null;
  }
}
