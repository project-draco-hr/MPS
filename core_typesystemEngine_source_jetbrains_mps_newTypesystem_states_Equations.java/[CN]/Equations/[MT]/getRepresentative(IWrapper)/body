{
  IWrapper parent=myRepresentatives.get(wrapper);
  IWrapper current=wrapper;
  if (parent != null) {
    List<IWrapper> path=new LinkedList<IWrapper>();
    while (parent != null) {
      path.add(current);
      current=parent;
      parent=myRepresentatives.get(parent);
    }
  }
  return current;
}
