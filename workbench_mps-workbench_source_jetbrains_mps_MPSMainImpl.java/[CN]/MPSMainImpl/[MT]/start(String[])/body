{
  ensureCachesInvalidatedIfRunningNewerBuild();
  try {
    Class mainImplCls=MainImpl.class;
    Method method=mainImplCls.getDeclaredMethod("start",String[].class);
    method.setAccessible(true);
    method.invoke(null,new Object[]{args});
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
