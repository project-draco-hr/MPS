{
  String entity=NameUtil.singularize(getTitle());
  Frame frame=MPSDataKeys.FRAME.getData(DataManager.getInstance().getDataContext());
  int result=JOptionPane.showConfirmDialog(frame,"Do you want to create a new " + NameUtil.decapitalize(entity) + "?","Create New " + entity,JOptionPane.YES_NO_OPTION);
  return result == JOptionPane.YES_NO_OPTION;
}
