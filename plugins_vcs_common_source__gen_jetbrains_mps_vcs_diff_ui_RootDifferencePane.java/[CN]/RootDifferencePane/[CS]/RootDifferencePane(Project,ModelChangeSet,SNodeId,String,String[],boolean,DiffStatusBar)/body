{
  myChangeSet=changeSet;
  myRootId=rootId;
  myProject=project;
  myStatusBar=statusBar;
  myOldEditor=addEditor(0,as_guncoj_a0b0a6a22(myChangeSet.getOldModel(),SModelBase.class).getSModelInternal(),titles[0]);
  myNewEditor=addEditor(1,as_guncoj_a0b0a7a22(myChangeSet.getNewModel(),SModelBase.class).getSModelInternal(),titles[1]);
  linkEditors(true);
  linkEditors(false);
  myPanel.setSplitterProportionKey(PARAM_INSPECTOR_SPLITTER_POSITION);
  myPanel.setFirstComponent(myTopPanel);
  if (isInspectorShown) {
    myPanel.setSecondComponent(myBottomPanel);
  }
  myTraverser=new NextPreviousTraverser(myChangeGroupLayouts,myNewEditor.getMainEditor());
  createActionGroup(isEditable,rootName);
}
