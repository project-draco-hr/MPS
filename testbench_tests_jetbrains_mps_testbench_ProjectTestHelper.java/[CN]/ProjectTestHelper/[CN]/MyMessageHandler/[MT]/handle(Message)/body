{
switch (msg.getKind()) {
case ERROR:
    if (msg.getException() != null) {
      myGenerationErrors.add(msg.getException().toString());
    }
 else {
      myGenerationErrors.add(msg.getText());
    }
  break;
case WARNING:
myGenerationWarnings.add(msg.getText());
break;
case INFORMATION:
break;
}
}
