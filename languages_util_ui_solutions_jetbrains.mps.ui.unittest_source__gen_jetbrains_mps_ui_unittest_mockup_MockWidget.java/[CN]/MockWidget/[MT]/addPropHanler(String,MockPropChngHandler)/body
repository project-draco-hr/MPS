{
  MockWidget.PropHandlers hdlrs=(MapSequence.fromMap(this.propHandlers).containsKey(key) ? MapSequence.<String,MockWidget.PropHandlers>fromMap(this.propHandlers).get(key) : new MockWidget.PropHandlers(key));
  MapSequence.<String,MockWidget.PropHandlers>fromMap(this.propHandlers).put(key,hdlrs);
  hdlrs.add(hdlr);
}
