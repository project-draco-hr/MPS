{
  return findPrevCellWhichMeetsCondition(cell,new Condition<EditorCell>(){
    public boolean met(    EditorCell editorCell){
      if (editable) {
        return editorCell instanceof EditorCell_Label && ((EditorCell_Label)editorCell).isEditable();
      }
 else {
        return true;
      }
    }
  }
);
}
