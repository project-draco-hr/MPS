{
  new CellNavigator(this){
    boolean isSuitableCell(    EditorCell cell){
      for (      IEditorMessage m : getHighlightManager().getMessagesFor(cell.getSNode())) {
        if (m.getOwner() == getHighlightMessagesOwner()) {
          return true;
        }
      }
      return false;
    }
  }
.goToNextCell(backwards);
}
