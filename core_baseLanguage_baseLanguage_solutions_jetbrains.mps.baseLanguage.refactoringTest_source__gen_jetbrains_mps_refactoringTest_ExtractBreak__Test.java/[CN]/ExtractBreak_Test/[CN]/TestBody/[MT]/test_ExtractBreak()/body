{
  this.addNodeById("1230052480333");
  this.addNodeById("1230052480361");
  ExtractMethodRefactoringParameters params=new ExtractMethodRefactoringParameters(ListSequence.<SNode>fromArray(this.getNodeById("1230052480342")));
  params.setName("foo");
  ExtractMethodRefactoring ref=ExtractMethodFabric.createRefactoring(params);
  Assert.assertTrue(ref instanceof ExtractMethodWithExitPoints);
  ref.doRefactor();
  Assert.assertEquals(null,NodesMatcher.matchNodes(ListSequence.<SNode>fromArray(this.getNodeById("1230052480334")),ListSequence.<SNode>fromArray(this.getNodeById("1230052480362"))));
}
