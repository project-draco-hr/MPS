{
  if (obj == this)   return true;
  if (obj instanceof ReferenceContext) {
    ReferenceContext o=(ReferenceContext)obj;
    return EqualUtil.equals(myRole,o.myRole) && EqualUtil.equals(mySourceNode,o.mySourceNode) && EqualUtil.equals(myTargetNode,o.myTargetNode);
  }
 else {
    return false;
  }
}
