{
  if (myReplacementReference != null) {
    return myReplacementReference;
  }
synchronized (this) {
    if (myReferenceInfo == null) {
      return myReplacementReference;
    }
    myReplacementReference=myReferenceInfo.create(this);
    myReferenceInfo=null;
  }
  return myReplacementReference;
}
