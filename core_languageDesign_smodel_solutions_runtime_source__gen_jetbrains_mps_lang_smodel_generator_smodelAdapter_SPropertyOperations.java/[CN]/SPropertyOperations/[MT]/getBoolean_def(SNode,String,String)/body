{
  if (node != null) {
    String value=node.getProperty(propertyName);
    if (value != null) {
      if ("true".equals(value)) {
        return true;
      }
      if ("false".equals(value)) {
        return false;
      }
    }
  }
  if (defaultValue != null) {
    try {
      return Boolean.parseBoolean(defaultValue);
    }
 catch (    Exception e) {
    }
  }
  return false;
}
