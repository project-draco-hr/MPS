{
  try {
    Set<IModule> modules=SetSequence.<IModule>fromSet(new LinkedHashSet<IModule>());
    SetSequence.fromSet(modules).addSequence(ListSequence.<IModule>fromList(((MPSProject)MapSequence.<String,Object>fromMap(_params).get("project")).getProjectModules(IModule.class)));
    ProgressManager.getInstance().run(new DefaultMakeTask(((Project)MapSequence.<String,Object>fromMap(_params).get("ideaProject")),"Compiling",modules,false));
  }
 catch (  Throwable t) {
    if (log.isErrorEnabled()) {
      log.error("User's action execute method failed. Action:" + "CompileProject",t);
    }
  }
}
