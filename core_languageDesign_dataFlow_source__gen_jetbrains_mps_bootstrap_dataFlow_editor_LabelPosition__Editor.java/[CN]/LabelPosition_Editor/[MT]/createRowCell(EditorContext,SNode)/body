{
  EditorCell_Collection editorCell=EditorCell_Collection.createHorizontal(context,node);
  LabelPosition_Editor.setupBasic_RowCell(editorCell,node,context);
  editorCell.setGridLayout(false);
  editorCell.setUsesBraces(false);
  editorCell.setCanBeFolded(false);
  editorCell.addEditorCell(this.createConstantCell(context,node,"after"));
  editorCell.addEditorCell(this.createLabelReferenceCell(context,node));
  return editorCell;
}
