{
  CellProviderWithRole provider=aProvider;
  provider.setAuxiliaryCellProvider(new LabelPosition_Editor._Inline1());
  EditorCell editorCell=provider.createEditorCell(context);
  LabelPosition_Editor.setupBasic_LabelReferenceCell(editorCell,node,context);
  if (editorCell instanceof EditorCell_Label) {
    LabelPosition_Editor.setupLabel_LabelReferenceCell((EditorCell_Label)editorCell,node,context);
  }
  editorCell.setSubstituteInfo(provider.createDefaultSubstituteInfo());
  return editorCell;
}
