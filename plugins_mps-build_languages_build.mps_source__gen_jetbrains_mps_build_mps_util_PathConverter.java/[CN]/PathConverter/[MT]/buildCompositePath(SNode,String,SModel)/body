        SLinkOperations.setTarget(last,MetaAdapterFactory.getContainmentLink(new UUID(8755280088213897754l,-5075149991798053422l),8618885170173601777l,8618885170173601778l,"tail"),npath);
      }
      last=npath;
    }
  }
}
