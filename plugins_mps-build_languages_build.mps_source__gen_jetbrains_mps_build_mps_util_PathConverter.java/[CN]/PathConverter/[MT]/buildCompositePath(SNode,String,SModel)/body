      if (last == null) {
        SLinkOperations.setTarget(result,MetaAdapterFactory.getContainmentLink(new UUID(8755280088213897754l,-5075149991798053422l),7321017245476976379l,7321017245477039051l,"compositePart"),npath);
      }
