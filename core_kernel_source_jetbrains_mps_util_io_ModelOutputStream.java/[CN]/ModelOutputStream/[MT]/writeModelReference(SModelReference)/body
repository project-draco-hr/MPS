{
  if (ref == null) {
    writeByte(0x70);
  }
 else {
    Integer index=modelrefToIndex.get(ref);
    if (index == null) {
      modelrefToIndex.put(ref,myRefIndex++);
      if (ref.getModelId() != null) {
        writeByte(7);
        writeModelID(ref.getModelId());
      }
 else {
        writeByte(8);
      }
      writeString(ref.getModelName());
    }
 else {
      writeByte(9);
      writeInt(index);
    }
  }
}
