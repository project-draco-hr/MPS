{
  if (ref == null) {
    writeByte(0x70);
  }
 else {
    Integer index=moduleRefToIndex.get(ref);
    if (index == null) {
      moduleRefToIndex.put(ref,myModuleRefIndex++);
      if (ref.getModuleId() != null) {
        writeByte(0x17);
        writeModuleID(ref.getModuleId());
      }
 else {
        writeByte(0x18);
      }
      writeString(ref.getModuleName());
    }
 else {
      writeByte(0x19);
      writeInt(index);
    }
  }
}
