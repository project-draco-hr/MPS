      }
    }
  }
);
  Assert.assertTrue(c.myConcepts.contains(((SConceptAdapterById)MetaAdapterFactory.getConcept(0xf3061a5392264cc5L,0xa443f952ceaf5816L,0xf8c108ca66L,"jetbrains.mps.baseLanguage.structure.ClassConcept")).getId()));
