{
  HttpClient c=new HttpClient();
  String descr=myDescription.getText();
  try {
    login(c);
    postIssue(c,myMessage,(descr == null ? "" : descr + "\n\n\n") + ex2str(myEx));
  }
 catch (  Throwable e) {
    JOptionPane.showMessageDialog(myFrame,e.getMessage(),"Error",JOptionPane.ERROR_MESSAGE);
  }
  dispose();
}
