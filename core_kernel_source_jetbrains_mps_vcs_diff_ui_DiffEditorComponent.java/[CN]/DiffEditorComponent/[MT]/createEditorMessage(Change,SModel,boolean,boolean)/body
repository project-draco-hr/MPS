{
  Color color=null;
  if (show) {
    if (revertChanges) {
      color=change.getChangeType().getOpositeChange().getColor();
    }
 else {
      color=change.getChangeType().getColor();
    }
    if (change.isError()) {
      color=ERROR_COLOR;
    }
  }
  SNode id=model.getNodeById(change.getAffectedNodeId());
  return new ChangeEditorMessage(change,id,color,"",myOwner);
}
