{
  EditorCell cell=findNodeCell(node);
  if (cell == null) {
    return null;
  }
  if (cell.getY() > getViewport().getViewPosition().y) {
    return node;
  }
  for (  SNode child : node.getChildrenArray()) {
    SNode result=getFirtsVisibleNode(child);
    if (result != null) {
      return result;
    }
  }
  return null;
}
