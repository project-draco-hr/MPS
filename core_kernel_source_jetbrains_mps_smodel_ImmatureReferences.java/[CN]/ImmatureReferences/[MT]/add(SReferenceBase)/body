{
synchronized (myLock) {
    SModelReference modelRef=ref.getSourceNode().getModel().getSModelReference();
    List<SReferenceBase> items=myReferences.get(modelRef);
    if (items == null) {
      items=new ArrayList<SReferenceBase>();
      myReferences.put(modelRef,items);
    }
    items.add(ref);
  }
}
