{
  @SuppressWarnings("unchecked") T aspectDescriptor=(T)myAspectDescriptors.get(descriptorInterface);
  if (aspectDescriptor == null) {
    aspectDescriptor=createAspectDescriptor(descriptorInterface);
    if (aspectDescriptor == null) {
      return null;
    }
    @SuppressWarnings("unchecked") T alreadyThere=(T)myAspectDescriptors.putIfAbsent(descriptorInterface,aspectDescriptor);
    if (alreadyThere != null) {
      return alreadyThere;
    }
  }
  return aspectDescriptor;
}
