{
  ConceptDeclaration cd=(ConceptDeclaration)SModelUtil_new.findConceptDeclaration(conceptFQName,GlobalScope.getInstance());
  Icon icon=getIconFor(cd);
  if (icon == null) {
    if (cd.isRoot()) {
      return Icons.DEFAULT_ROOT_ICON;
    }
 else {
      return Icons.DEFAULT_NODE_ICON;
    }
  }
  return icon;
}
