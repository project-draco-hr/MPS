{
  try {
    if (module instanceof Language || module instanceof Solution) {
      InMemoryStreamDataSource ds=new InMemoryStreamDataSource();
      SModelBase m=(SModelBase)PersistenceFacade.getInstance().getDefaultModelFactory().create(ds,Collections.singletonMap(ModelFactory.OPTION_MODELNAME,"model-for-language-imports"));
      module.registerModel(m);
    }
  }
 catch (  IOException ex) {
    throw new RuntimeException(ex);
  }
}
