{
  if (rule instanceof TemplateRootMappingRuleInterpreted) {
    return wrapped.applyRule(rule,environment,context);
  }
  try {
    NodeReadEventsCaster.setNodesReadListener(listener);
    return wrapped.applyRule(rule,environment,context);
  }
  finally {
    NodeReadEventsCaster.removeNodesReadListener();
  }
}
