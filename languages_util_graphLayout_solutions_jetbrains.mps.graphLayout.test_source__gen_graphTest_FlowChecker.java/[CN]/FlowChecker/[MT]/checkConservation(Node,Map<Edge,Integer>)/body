{
  int inFlow=0;
  for (  Edge edge : ListSequence.fromList(node.getInEdges())) {
    inFlow+=MapSequence.fromMap(flow).get(edge);
  }
  int outFlow=0;
  for (  Edge edge : ListSequence.fromList(node.getInEdges())) {
    outFlow+=MapSequence.fromMap(flow).get(edge);
  }
  Assert.assertTrue(outFlow == inFlow);
}
