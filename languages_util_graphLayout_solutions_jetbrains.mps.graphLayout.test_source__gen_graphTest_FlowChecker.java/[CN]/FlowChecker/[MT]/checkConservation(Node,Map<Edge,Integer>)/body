{
  int inFlow=0;
  for (  Edge edge : ListSequence.<Edge>fromList(node.getInEdges())) {
    inFlow+=MapSequence.<Edge,Integer>fromMap(flow).get(edge);
  }
  int outFlow=0;
  for (  Edge edge : ListSequence.<Edge>fromList(node.getInEdges())) {
    outFlow+=MapSequence.<Edge,Integer>fromMap(flow).get(edge);
  }
  Assert.assertTrue(outFlow == inFlow);
}
