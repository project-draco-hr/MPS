{
  return ListSequence.fromList(SNodeOperations.getAncestors(_context.getEnclosingNode(),"jetbrains.mps.lang.smodel.structure.IfInstanceOf",false)).<SNode>select(new ISelector<SNode,SNode>(){
    public SNode select(    SNode it){
      return SLinkOperations.getTarget(it,"variable",true);
    }
  }
);
}
