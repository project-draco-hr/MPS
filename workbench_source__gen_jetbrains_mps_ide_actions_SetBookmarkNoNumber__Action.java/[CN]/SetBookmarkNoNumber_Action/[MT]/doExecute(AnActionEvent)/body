{
  try {
    BookmarkManager bookmarkManager=SetBookmarkNoNumber_Action.this.project.getComponent(BookmarkManager.class);
    bookmarkManager.setUnnumberedBookmark(SetBookmarkNoNumber_Action.this.node);
  }
 catch (  Throwable t) {
