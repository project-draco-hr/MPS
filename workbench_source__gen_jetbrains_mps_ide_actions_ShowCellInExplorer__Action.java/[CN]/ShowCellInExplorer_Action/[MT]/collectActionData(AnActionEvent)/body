{
  try {
    this.cell=new ActionEventData(event).getEditorCell();
    if (this.cell == null) {
      return false;
    }
    this.context=new ActionEventData(event).getOperationContext();
    if (this.context == null) {
      return false;
    }
  }
 catch (  Throwable t) {
    return false;
  }
  return true;
}
