{
  Entry newTail=new Entry(node);
  if (head == null) {
    head=newTail;
    head.next=head.prev=head;
  }
 else {
    Entry oldTail=head.prev;
    head.prev=newTail;
    head.prev.prev=oldTail;
    head.prev.next=oldTail.next;
    oldTail.next=newTail;
    if (oldTail.prev == oldTail) {
      oldTail.prev=newTail;
    }
  }
  size++;
}
