{
  Entry newHead=new Entry(node);
  if (head == null) {
    head=newHead;
    head.next=head.prev=head;
  }
 else {
    Entry oldHead=head;
    head=newHead;
    head.next=oldHead;
    head.prev=oldHead.prev;
    oldHead.prev=head;
    if (oldHead.next == oldHead) {
      oldHead.next=newHead;
    }
  }
  size++;
}
