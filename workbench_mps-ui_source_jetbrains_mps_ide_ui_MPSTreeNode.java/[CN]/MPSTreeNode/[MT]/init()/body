{
  if (isInitialized()) {
    return;
  }
  MPSTree tree=ModelAccess.instance().runReadAction(new Computable<MPSTree>(){
    @Override public MPSTree compute(){
      doInit();
      return getTree();
    }
  }
);
  if (tree != null) {
    ((DefaultTreeModel)getTree().getModel()).nodeStructureChanged(this);
  }
}
