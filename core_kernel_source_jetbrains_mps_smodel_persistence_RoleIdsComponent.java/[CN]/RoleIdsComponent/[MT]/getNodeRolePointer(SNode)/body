{
  if (ourHandler != null) {
    String linkRole=node.getRole_();
    if (linkRole == null) {
      return null;
    }
 else {
      String conceptFqName=node.getParent().getConceptFqName();
      return ourHandler.getNodeRolePointer(conceptFqName,linkRole);
    }
  }
 else {
    return null;
  }
}
