{
  return new IBreakpointPropertiesUi(){
    private JavaBreakpoint myBreakpoint;
    private JLabel myUi=new JLabel(kind.getPresentation());
    @Override public void setBreakpoint(    IBreakpoint breakpoint){
      myBreakpoint=(JavaBreakpoint)breakpoint;
    }
    @Override public JComponent getMainComponent(){
      return myUi;
    }
  }
;
}
