{
  executeWriteAction(new Runnable(){
    public void run(){
      PsiManager manager=PsiManager.getInstance(myProject);
      PsiClass cls=manager.findClass(fqName,GlobalSearchScope.projectProductionScope(myProject,false));
      if (cls == null)       return;
      cls.navigate(true);
      activateProjectWindow();
    }
  }
);
  return "OK";
}
