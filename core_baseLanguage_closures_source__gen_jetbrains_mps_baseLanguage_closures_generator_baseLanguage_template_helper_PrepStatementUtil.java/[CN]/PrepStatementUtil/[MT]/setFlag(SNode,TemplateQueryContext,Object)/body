{
  genContext.putStepObject("flag_" + ((SNode)sn).getId(),flag);
  List<SNode> allFlagged=getAllFlagged(genContext);
  if (allFlagged == null) {
    allFlagged=new ArrayList<SNode>();
    genContext.putStepObject("all_flagged",allFlagged);
  }
  if (!(allFlagged.contains(sn))) {
    allFlagged.add(sn);
  }
}
