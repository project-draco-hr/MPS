{
  editorCell.setSubstituteInfo(new OldNewCompositeSubstituteInfo(myEditorContext,new SChildSubstituteInfo(editorCell,myOwnerNode,MetaAdapterFactory.getContainmentLink(0xd7706f639be2479cL,0xa3daae92af1e64d5L,0x11b5282d0dbL,0x11b5282d0e0L,"inputNode"),child),new DefaultChildSubstituteInfo(myOwnerNode,myContainmentLink.getDeclarationNode(),myEditorContext)));
  if (editorCell.getRole() == null) {
    editorCell.setRole("inputNode");
  }
}
