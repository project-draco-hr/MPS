{
  String packageName="jetbrains.mps";
  String namespace=semanticModel.getNamespace();
  String name=semanticModel.getName();
  String tail=namespace;
  if (tail.length() > 0 && name.length() > 0) {
    tail+=".";
  }
  tail+=name;
  if (tail.length() > 0) {
    packageName=packageName + "." + tail;
  }
  return packageName;
}
