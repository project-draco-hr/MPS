{
  StringBuilder sb=new StringBuilder();
  String sep="";
{
    ICursor<Node> _zCursor1=CursorFactory.createCursor(nodes);
    try {
      while (_zCursor1.moveToNext()) {
        Node n=_zCursor1.getCurrent();
        sb.append(sep).append(n.getNodeName());
        sep=", ";
      }
    }
  finally {
      _zCursor1.release();
    }
  }
  return sb.toString();
}
