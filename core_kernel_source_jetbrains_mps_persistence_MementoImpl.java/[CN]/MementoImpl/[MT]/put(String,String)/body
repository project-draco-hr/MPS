{
  if (value == null) {
    if (values != null) {
      values.remove(key);
    }
    return;
  }
  if (key == null) {
    throw new IllegalArgumentException("Null key in memento doesn't make sense");
  }
  if (values == null) {
    values=new LinkedHashMap<String,String>();
  }
  values.put(key,value);
}
