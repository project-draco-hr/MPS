{
  super("Check method call scopes");
  this.addRefactoring(new AbstractMigrationRefactoring(operationContext){
    public String getName(){
      return "Testing";
    }
    public String getAdditionalInfo(){
      return "Testing";
    }
    public String getFqNameOfConceptToSearchInstances(){
      return "jetbrains.mps.baseLanguage.structure.InstanceMethodCallOperation";
    }
    public boolean isApplicableInstanceNode(    SNode node){
      if (SNodeOperations.isInstanceOf(node,"jetbrains.mps.debugger.java.privateMembers.structure.PrivateInstanceMethodCallOperation")) {
        return false;
      }
      String excludingPrefix="123collection";
      String startsFrom="v";
      SReference ref=SNodeOperations.getReference(node,SLinkOperations.findLinkDeclaration("jetbrains.mps.baseLanguage.structure.InstanceMethodCallOperation","instanceMethodDeclaration"));
      if (ref == null) {
        return false;
      }
      if (Utils.isInComment(node)) {
        return false;
      }
      String name=SPropertyOperations.getString(SLinkOperations.getTarget(node,"baseMethodDeclaration",false),"name");
      if (StringUtils.isEmpty(name)) {
        return false;
      }
      if (name.startsWith(excludingPrefix)) {
        return false;
      }
      if (!(name.startsWith(startsFrom))) {
        return false;
      }
      return !(Utils.checkScopes(node,Utils.getOldScopeFromRef(ref),Utils.getNewScopeForIOperation(ref,SConceptOperations.findConceptDeclaration("jetbrains.mps.baseLanguage.structure.InstanceMethodDeclaration")),true));
    }
    public void doUpdateInstanceNode(    SNode node){
      SReference ref=SNodeOperations.getReference(node,SLinkOperations.findLinkDeclaration("jetbrains.mps.baseLanguage.structure.InstanceMethodCallOperation","instanceMethodDeclaration"));
      Utils.checkScopes(node,Utils.getOldScopeFromRef(ref),Utils.getNewScopeForIOperation(ref,SConceptOperations.findConceptDeclaration("jetbrains.mps.baseLanguage.structure.InstanceMethodDeclaration")),true);
    }
    public boolean isShowAsIntention(){
      return false;
    }
  }
);
}
