{
  TestResult testResult=new TestResult();
  testResult.addListener(this);
  for (  Test test : tests) {
    if (test == null) {
      System.err.println("Can't run. Test is Null");
      continue;
    }
    if (test.countTestCases() == 0) {
      System.err.println("No tests found in " + test.getClass().getName());
      continue;
    }
    try {
      test.run(testResult);
    }
 catch (    Throwable t) {
      t.printStackTrace();
    }
  }
}
