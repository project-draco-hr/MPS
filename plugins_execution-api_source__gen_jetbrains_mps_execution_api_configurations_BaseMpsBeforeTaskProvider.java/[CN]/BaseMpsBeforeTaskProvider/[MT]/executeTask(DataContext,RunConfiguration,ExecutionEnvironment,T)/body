{
  if (!(configure(configuration,task))) {
    return false;
  }
  try {
    return task.execute(PlatformDataKeys.PROJECT.getData(context),env);
  }
 catch (  Throwable t) {
    if (LOG.isEnabledFor(Level.ERROR)) {
      LOG.error("Error during executing provider " + (myAlias + "(" + this.getClass().getName()+ ")"),t);
    }
  }
  return false;
}
