{
  editorCell.putUserObject(EditorCell.CELL_ID,node.getId() + "_ConstantCell14862_01");
{
    Style inlineStyle=new Style(editorCell){
{
        this.set(StyleAttributes.FONT_STYLE,MPSFonts.ITALIC);
        this.set(StyleAttributes.EDITABLE,true);
        this.set(StyleAttributes.TEXT_COLOR,new AttributeCalculator<Color>(){
          public Color calculate(          EditorCell cell){
            return ReplaceableAliasAndParms_Comp.calculateColor49(cell);
          }
        }
);
        this.set(StyleAttributes.DEFAULT_CARET_POSITON,CaretPosition.FIRST);
      }
    }
;
    inlineStyle.apply(editorCell);
  }
}
