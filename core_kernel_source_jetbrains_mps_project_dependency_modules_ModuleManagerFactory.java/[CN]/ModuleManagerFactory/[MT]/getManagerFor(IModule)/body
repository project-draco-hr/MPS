{
  if (module instanceof Solution) {
    return new ModuleDependenciesManager<Solution>(((Solution)module));
  }
 else   if (module instanceof Language) {
    return new LanguageDependenciesManager((Language)module);
  }
 else   if (module instanceof Generator) {
    return new GeneratorDependenciesManager((Generator)module);
  }
  throw new IllegalArgumentException("Unknown module type: " + module.getClass().getName());
}
