{
  Graph graph=SimpleDirectedGraphs.sandwatches();
  Map<Node,Integer> numbering=TopologicalNumbering.number(graph);
  Assert.assertTrue(MapSequence.<Node,Integer>fromMap(numbering).count() == graph.getNumNodes());
  Assert.assertTrue(MapSequence.<Node,Integer>fromMap(numbering).get(graph.getNode(0)) == 0);
  Assert.assertTrue(MapSequence.<Node,Integer>fromMap(numbering).get(graph.getNode(1)) == 0);
  Assert.assertTrue(MapSequence.<Node,Integer>fromMap(numbering).get(graph.getNode(2)) == 1);
  Assert.assertTrue(MapSequence.<Node,Integer>fromMap(numbering).get(graph.getNode(3)) == 2);
  Assert.assertTrue(MapSequence.<Node,Integer>fromMap(numbering).get(graph.getNode(4)) == 3);
  Assert.assertTrue(MapSequence.<Node,Integer>fromMap(numbering).get(graph.getNode(5)) == 3);
}
