{
  if (ourIdeaPath != null) {
    return ourIdeaPath;
  }
  String rootPath=getContainingJar(Document.class);
  if (rootPath == null) {
    ourIdeaPath=getHomePath();
    return ourIdeaPath;
  }
  File root=new File(rootPath);
  root=root.getAbsoluteFile();
  root=root.getParentFile();
  if (root != null) {
    root=root.getParentFile();
  }
  if (root == null) {
    ourIdeaPath=getHomePath();
  }
 else {
    ourIdeaPath=root.getAbsolutePath();
  }
  return ourIdeaPath;
}
