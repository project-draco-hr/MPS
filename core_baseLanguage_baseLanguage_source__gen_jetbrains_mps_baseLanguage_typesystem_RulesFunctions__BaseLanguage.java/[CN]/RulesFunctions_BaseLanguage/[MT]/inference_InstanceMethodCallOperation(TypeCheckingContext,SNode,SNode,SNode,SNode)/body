{
  Map<SNode,List<SNode>> mmap=MapSequence.<SNode,List<SNode>>fromMap(new HashMap<SNode,List<SNode>>());
  RulesFunctions_BaseLanguage.inference_equateParametersAndReturnType(typeCheckingContext,imco,returnType,mmap);
