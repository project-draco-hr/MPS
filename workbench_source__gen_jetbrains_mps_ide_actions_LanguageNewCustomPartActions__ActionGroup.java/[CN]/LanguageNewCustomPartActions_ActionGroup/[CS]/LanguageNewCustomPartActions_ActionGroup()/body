{
  super("",ID);
  this.setIsInternal(false);
  this.setPopup(false);
  try {
    for (    LanguageAspect aspect : LanguageAspect.values()) {
      this.addAction("jetbrains.mps.ide.actions.NewAspectModel_Action","jetbrains.mps.ide",aspect);
    }
  }
 catch (  Throwable t) {
    LOG.error("User group error",t);
  }
}
