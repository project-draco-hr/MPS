{
  super("",ID);
  this.setIsAlwaysVisible(true);
  this.setIsInternal(false);
  this.setPopup(false);
  try {
    for (    LanguageAspect aspect : LanguageAspect.values()) {
      this.add(new NewAspectModelAction(aspect));
    }
  }
 catch (  Throwable t) {
    LOG.error("User group error",t);
  }
}
