{
  EditorCell_Collection editorCell=EditorCell_Collection.createHorizontal(context,node);
  CustomPropertyImplementation_Editor.setupBasic_RowCell(editorCell,node,context);
  editorCell.setGridLayout(false);
  editorCell.setUsesBraces(false);
  editorCell.setCanBeFolded(false);
  editorCell.addEditorCell(this.createConstantCell(context,node,"set"));
  editorCell.addEditorCell(this.createSetAccessorCell(context,node));
  return editorCell;
}
