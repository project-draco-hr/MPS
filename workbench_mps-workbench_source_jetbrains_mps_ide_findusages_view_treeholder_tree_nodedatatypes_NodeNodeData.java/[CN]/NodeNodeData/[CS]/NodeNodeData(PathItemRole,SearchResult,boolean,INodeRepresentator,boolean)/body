{
  super(role,(isResultNode && nodeRepresentator != null) ? nodeRepresentator.getPresentation(result.getObject()) : snodeRepresentation((SNode)result.getPathObject()),nodeAdditionalInfo((SNode)result.getPathObject()),false,isResultNode,resultsSection);
  myNodePointer=new SNodePointer((SNode)result.getPathObject());
}
