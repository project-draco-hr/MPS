{
  if (value == null) {
    return true;
  }
  Iterator<DayOfWeek> constants=ListSequence.<DayOfWeek>fromList(DayOfWeek.getConstants()).iterator();
  while (constants.hasNext()) {
    DayOfWeek constant=constants.next();
    if (value.equals(constant.getName())) {
      return true;
    }
  }
  return false;
}
