{
  List<SNode> result=new ArrayList<SNode>();
  if (ss != null) {
    for (    Statement stmt : ss) {
      SNode statement=convertStatementRefl(pos,stmt);
      if (statement != null) {
        result.add(statement);
        pos.index++;
        pos.anchor=statement;
      }
    }
  }
  return result;
}
