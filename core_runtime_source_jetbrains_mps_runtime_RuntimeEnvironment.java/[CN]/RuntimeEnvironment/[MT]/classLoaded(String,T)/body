{
  if (myLoadedClasses.containsKey(name)) {
    T oldLoaderId=myLoadedClasses.get(name);
    if (!equals(oldLoaderId,id)) {
      throw new IllegalStateException("Class \"" + name + "\" was loaded by multiple module classloaders simultaneously.\n"+ "Classloaders: \n"+ "  "+ id.toString()+ "\n"+ "  "+ oldLoaderId.toString());
    }
  }
 else {
    myLoadedClasses.put(name,id);
  }
}
