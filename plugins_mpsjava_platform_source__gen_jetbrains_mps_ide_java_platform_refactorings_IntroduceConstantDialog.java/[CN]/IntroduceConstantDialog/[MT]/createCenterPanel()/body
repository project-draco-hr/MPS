{
  initPanel();
  int gridy=1;
  GridBagConstraints c;
  c=new GridBagConstraints();
  c.fill=GridBagConstraints.HORIZONTAL;
  c.gridx=0;
  c.gridy=gridy++;
  addVisibilityPanel(c);
  if (getRefactoring().hasDuplicates()) {
    addReplacingAll(gridy++);
  }
  c=new GridBagConstraints();
  c.gridy=gridy;
  c.weighty=1;
  myPanel.add(new JPanel(),c);
  return myPanel;
}
