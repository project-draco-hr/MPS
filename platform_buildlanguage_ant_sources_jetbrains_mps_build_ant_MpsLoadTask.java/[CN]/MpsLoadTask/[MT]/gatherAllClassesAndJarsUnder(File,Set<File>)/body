{
  File[] children=dir.listFiles();
  for (  File f : children) {
    if (!f.isDirectory()) {
      if (f.getName().endsWith(".jar") && !f.getName().contains("ant.jar")) {
        result.add(f);
      }
    }
  }
  for (  File f : children) {
    if (f.isDirectory()) {
      if (f.getName().equals("classes")) {
        result.add(f);
      }
 else {
        gatherAllClassesAndJarsUnder(f,result);
      }
    }
  }
}
