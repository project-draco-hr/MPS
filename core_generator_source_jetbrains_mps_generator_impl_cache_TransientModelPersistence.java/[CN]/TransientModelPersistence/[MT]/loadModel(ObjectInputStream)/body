{
  int version=is.readInt();
  if (version != VERSION) {
    return null;
  }
  String modelReference=is.readUTF();
  SModel m=new TransientSModel(SModelReference.fromString(modelReference));
  m.setLoading(true);
  List<SNode> roots=loadNodes(m,is);
  for (  SNode root : roots) {
    m.addRoot(root);
  }
  return m;
}
