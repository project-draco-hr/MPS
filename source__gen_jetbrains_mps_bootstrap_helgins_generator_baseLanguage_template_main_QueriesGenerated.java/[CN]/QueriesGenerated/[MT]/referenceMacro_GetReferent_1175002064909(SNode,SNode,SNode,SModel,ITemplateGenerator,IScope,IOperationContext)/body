{
  SNode clt;
  if (SNodeOperations.isInstanceOf(node,"jetbrains.mps.bootstrap.helgins.structure.InferenceRule")) {
    return SLinkOperations.getTarget(new QuotationClass_().createNode(generator.getTargetModel()),"classifier",false);
