{
  SNode rule=SNodeOperations.getAncestor(SLinkOperations.getTarget(_parameterObject.getNode(),"patternVarDecl",false),"jetbrains.mps.bootstrap.helgins.structure.AbstractRule",false,false);
  if (rule != null) {
    return _parameterObject.getGenerator().findOutputNodeByInputNodeAndMappingName(rule,"applicableNodePatternField");
