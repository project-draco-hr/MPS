{
  SNode rule=SNodeOperations.getAncestor(node,"jetbrains.mps.bootstrap.helgins.structure.AbstractRule",false,false);
  SNode method=generator.findOutputNodeByInputNodeAndMappingName(rule,"mainMethodForRule");
