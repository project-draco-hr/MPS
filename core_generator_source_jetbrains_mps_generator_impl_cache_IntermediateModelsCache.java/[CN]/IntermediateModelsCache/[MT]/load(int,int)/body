{
  try {
    InputStream stream=myCacheContainer.openStream(getStorageName(majorStep,minorStep));
    ModelInputStream is=new ModelInputStream(stream);
    try {
      return TransientModelWithMetainfo.load(is);
    }
  finally {
      is.close();
    }
  }
 catch (  IOException e) {
    isOk=false;
  }
  return null;
}
