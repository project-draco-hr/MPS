{
  try {
    InputStream stream=myCacheContainer.openStream(STEP + step);
    ObjectInputStream is=new ObjectInputStream(new BufferedInputStream(stream));
    try {
      return TransientModelWithMetainfo.load(is);
    }
  finally {
      is.close();
    }
  }
 catch (  IOException e) {
    isOk=false;
  }
  return null;
}
