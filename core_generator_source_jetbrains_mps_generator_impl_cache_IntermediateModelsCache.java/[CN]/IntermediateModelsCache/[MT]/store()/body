{
  try {
    OutputStreamWriter writer=new OutputStreamWriter(myCacheContainer.createStream(SIGNATURE));
    try {
      writer.write(mySignature);
    }
  finally {
      writer.close();
    }
    if (isOk) {
      myCacheContainer.commit();
    }
 else {
      myCacheContainer.revert();
    }
  }
 catch (  IOException e) {
    myCacheContainer.revert();
  }
}
