{
  int y=event.getY();
  List<IGutterMessage> messages=getMessagesAt(y);
  if (messages.size() > 0) {
    StringBuffer text=new StringBuffer();
    for (    IGutterMessage msg : messages) {
      if (text.length() > 0) {
        text.append("\n");
      }
      text.append(msg.getMessage());
    }
    return text.toString();
  }
  return null;
}
