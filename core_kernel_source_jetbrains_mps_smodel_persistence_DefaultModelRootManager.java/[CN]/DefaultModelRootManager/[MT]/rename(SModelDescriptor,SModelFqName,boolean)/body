{
  if (!changeFile) {
    sm.save();
    return;
  }
  IFile oldFile=sm.getModelFile();
  SModelRoot root=sm.getSModelRoot();
  IFile newFile=createFileForModelUID(root,modelFqName);
  ((BaseSModelDescriptor)sm).changeModelFile(newFile);
  sm.save();
  oldFile.delete();
  renameFile(oldFile,newFile);
}
