{
  ourModelAccess.runWriteAction(new Runnable(){
    public void run(){
      new ModuleRepositoryFacade(ourRepository).unregisterModules(myModuleOwner);
    }
  }
);
  ourModelAccess.runWriteAction(new Runnable(){
    @Override public void run(){
      myTmpDir.delete();
      myTmpDir=null;
    }
  }
);
}
