{
  try {
    Language language=getLanguageForConceptFqName(fqName);
    if (language == null) {
      return null;
    }
    Class cls=language.getClass(fqName + DESCRIPTOR_SUFFIX);
    if (cls != null) {
      return (ConceptDescriptor)cls.getField("INSTANCE").get(null);
    }
  }
 catch (  Exception e) {
  }
  return null;
}
