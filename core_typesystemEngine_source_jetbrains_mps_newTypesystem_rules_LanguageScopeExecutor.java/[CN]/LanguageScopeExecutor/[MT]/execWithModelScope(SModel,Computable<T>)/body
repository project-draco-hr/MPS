{
  LanguageScope languageScope=sModel == null ? LanguageScope.getGlobal() : LanguageScopeFactory.getInstance().getLanguageScope(sModel.getModelDepsManager().getAllImportedLanguages());
  try {
    LanguageScope.pushCurrent(languageScope,computable);
    return computable.compute();
  }
  finally {
    LanguageScope.popCurrent(languageScope,computable);
  }
}
