{
  SNode rtype=TypeChecker.getInstance().getTypeOf(SLinkOperations.getTarget(expr,"rightExpression",true));
  SNode ltype=TypeChecker.getInstance().getTypeOf(SLinkOperations.getTarget(expr,"leftExpression",true));
  if (TypeChecker.getInstance().getSubtypingManager().isSubtype(ltype,new QuotationClass_1().createNode()) && TypeChecker.getInstance().getSubtypingManager().isSubtype(rtype,new QuotationClass_().createNode())) {
    return true;
  }
  return false;
}
