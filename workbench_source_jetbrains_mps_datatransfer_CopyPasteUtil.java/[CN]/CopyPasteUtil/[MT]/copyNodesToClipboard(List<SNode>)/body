{
  StringBuilder stringBuilder=new StringBuilder();
  int i=1;
  int size=nodes.size();
  for (  SNode node : nodes) {
    stringBuilder.append(node.getDebugText());
    if (i < size) {
      stringBuilder.append("\n");
    }
    i++;
  }
  copyNodesAndTextToClipboard(nodes,stringBuilder.toString());
}
