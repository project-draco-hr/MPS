{
{
    this.append("{");
    Iterable<SNode> collection=SLinkOperations.getTargets(node,"item",true);
    final SNode lastItem=Sequence.fromIterable(collection).last();
    for (    SNode item : collection) {
      appendNode(item);
      if (item != lastItem) {
        append(", ");
      }
    }
    this.append("}");
  }
}
