{
  if (IMakeService.INSTANCE.get().isSessionActive()) {
    return false;
  }
  IModule cmd=((IModule)MapSequence.fromMap(_params).get("cmodule"));
  if (cmd instanceof Generator) {
    cmd=((Generator)cmd).getSourceLanguage();
  }
  String text=new MakeActionParameters(((IOperationContext)MapSequence.fromMap(_params).get("context")),null,null,((List<IModule>)MapSequence.fromMap(_params).get("modules")),cmd).actionText(false);
  if (text != null) {
    event.getPresentation().setText(text);
    return true;
  }
  return false;
}
