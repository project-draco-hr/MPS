{
  try {
{
      List<SNode> nodes=new ActionEventData(event).getNodes();
      boolean error=false;
      if (nodes != null) {
        for (        SNode node : nodes) {
          if (!(SNodeOperations.isInstanceOf(node,"jetbrains.mps.baseLanguage.structure.Statement"))) {
            error=true;
            break;
          }
        }
      }
      if (error) {
        this.nodes=null;
      }
 else {
        this.nodes=new ArrayList<SNode>(nodes);
      }
    }
    if (this.nodes == null) {
      return false;
    }
  }
 catch (  Throwable t) {
    return false;
  }
  return true;
}
