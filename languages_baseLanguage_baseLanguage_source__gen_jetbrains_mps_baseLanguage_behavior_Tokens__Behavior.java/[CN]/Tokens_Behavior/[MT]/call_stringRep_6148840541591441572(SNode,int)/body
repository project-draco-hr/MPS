{
  StringBuilder sb=new StringBuilder();
  for (int k=0; k < ListSequence.fromList(SLinkOperations.getTargets(thisNode,"token",true)).count() - skipFromEnd; k++) {
    if (k > 0) {
      sb.append('.');
    }
    sb.append(SPropertyOperations.getString(ListSequence.fromList(SLinkOperations.getTargets(thisNode,"token",true)).getElement(k),"value"));
  }
  return sb.toString();
}
