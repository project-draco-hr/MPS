{
  String rawName=SPropertyOperations.getString(fieldDeclaration,MetaAdapterFactory.getProperty(new UUID(-3554657779850784990l,-7236703803128771572l),1169194658468l,1169194664001l,"name"));
  if (rawName == null || rawName.length() == 0) {
    return "unnamedField";
  }
  CodeStyleSettings codeStyleSettings=CodeStyleSettingsRegistry.getSettings(project);
  if (codeStyleSettings == null) {
    return rawName;
  }
  return getPreparedName(rawName,codeStyleSettings.getFieldPrefix(),codeStyleSettings.getFieldSuffix());
}
