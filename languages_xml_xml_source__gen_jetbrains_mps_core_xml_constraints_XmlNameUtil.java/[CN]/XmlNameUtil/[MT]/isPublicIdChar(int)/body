{
  if (c <= 0x20) {
    return c == 0x20 || c == 0xD || c == 0xA;
  }
  if (c >= 0x30 && c <= 0x39) {
    return true;
  }
  if (c >= 0x41 && c <= 0x5A) {
    return true;
  }
  if (c >= 0x61 && c <= 0x7A) {
    return true;
  }
  return c == '-' || c == '\'' || c == '(' || c == ')' || c == '+' || c == ',' || c == '.' || c == '/' || c == ':' || c == '=' || c == '?' || c == ';' || c == '*' || c == '#' || c == '@' || c == '$' || c == '_' || c == '%';
}
