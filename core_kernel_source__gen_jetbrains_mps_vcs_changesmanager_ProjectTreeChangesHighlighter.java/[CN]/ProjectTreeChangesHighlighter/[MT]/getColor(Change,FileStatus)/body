{
switch (change.getChangeType()) {
case ADD:
    if (modelFileStatus == FileStatus.ADDED || modelFileStatus == FileStatus.UNKNOWN) {
      return modelFileStatus.getColor();
    }
  return FileStatus.COLOR_ADDED;
case CHANGE:
return FileStatus.COLOR_MODIFIED;
case DELETE:
return null;
default :
return null;
}
}
