{
  if (url == null) {
    return null;
  }
  InputStream stream=null;
  try {
    stream=url.openStream();
    if (stream == null) {
      return null;
    }
    Document doc=JDOMUtil.loadDocument(stream);
    return DebugInfo.fromXml(doc.getRootElement(),sm);
  }
 catch (  IOException e) {
    return null;
  }
catch (  JDOMException e) {
    return null;
  }
 finally {
    try {
      if (stream != null) {
        stream.close();
      }
    }
 catch (    IOException e) {
      if (LOG.isEnabledFor(Level.ERROR)) {
        LOG.error("",e);
      }
    }
  }
}
