{
switch (kind) {
case SUBSTRACTION:
case INTERSECTION:
    for (    U o : right.toIterable()) {
      cardMap.postInc(o);
    }
  leftIt=left.toIterable().iterator();
break;
case UNION:
leftIt=left.toIterable().iterator();
rightIt=right.toIterable().iterator();
break;
case DISJUNCTION:
cache=new ArrayList<U>();
for (U o : right.toIterable()) {
cardMap.postInc(o);
cache.add(o);
}
leftIt=left.toIterable().iterator();
rightIt=cache.iterator();
break;
default :
break;
}
}
