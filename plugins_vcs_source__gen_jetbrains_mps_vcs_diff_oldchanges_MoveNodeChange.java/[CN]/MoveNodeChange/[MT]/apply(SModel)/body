{
  SNode node=m.getNodeById(myNodeId);
  if (node == null) {
    return false;
  }
  SNode parent=m.getNodeById(myNewParent);
  if (parent == null) {
    return false;
  }
  node.getParent().removeChild(node);
  SNode prevSibling=m.getNodeById(myPrevSibling);
  parent.insertChild(prevSibling,myNewRole,node);
  return true;
}
