{
  int len=data.length;
  Map<IndexEntry,Integer> result=new HashMap<IndexEntry,Integer>();
  int wordStart=-1;
  for (int i=0; i < len; i++) {
    char c=data[i];
    if (c >= 'a' && c <= 'z' || c >= 'A' && c <= 'Z') {
      if (wordStart == -1) {
        wordStart=i;
      }
    }
 else     if (wordStart >= 0) {
      processWord(result,data,len,wordStart,i - wordStart);
      wordStart=-1;
    }
  }
  return result;
}
