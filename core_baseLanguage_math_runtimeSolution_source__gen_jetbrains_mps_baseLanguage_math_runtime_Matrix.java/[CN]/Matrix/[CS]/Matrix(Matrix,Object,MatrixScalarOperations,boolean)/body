{
  myRows=m1.myRows;
  myColumns=m1.myColumns;
  this.myOperations=oper;
  this.myCarrier=new Object[myRows][myColumns];
  Object m2c;
  if (invert) {
    m2c=oper.inv(m2);
  }
 else   m2c=oper.cast(m2);
  for (int i=0; i < myRows; i++) {
    for (int j=0; j < myColumns; j++) {
      myCarrier[i][j]=oper.mul(m1.get(i,j),m2c);
    }
  }
}
