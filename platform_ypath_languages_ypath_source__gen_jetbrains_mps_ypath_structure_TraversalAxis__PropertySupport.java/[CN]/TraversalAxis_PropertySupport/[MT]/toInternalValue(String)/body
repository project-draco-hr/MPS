{
  if (value == null) {
    return null;
  }
  Iterator<TraversalAxis> constants=TraversalAxis.getConstants().iterator();
  while (constants.hasNext()) {
    TraversalAxis constant=constants.next();
    if (value.equals(constant.getName())) {
      return constant.getValueAsString();
    }
  }
  return null;
}
