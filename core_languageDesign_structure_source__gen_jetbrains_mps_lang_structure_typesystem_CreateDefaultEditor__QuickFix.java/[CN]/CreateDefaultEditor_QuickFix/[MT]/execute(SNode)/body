{
  SNode editorDeclaration=SNodeOperations.cast(ConceptEditorHelper.createNewConceptAspectInstance(LanguageAspect.EDITOR,((SNode)CreateDefaultEditor_QuickFix.this.getField("node")[0]),SConceptOperations.findConceptDeclaration("jetbrains.mps.lang.editor.structure.ConceptEditorDeclaration")),"jetbrains.mps.lang.editor.structure.ConceptEditorDeclaration");
  new DefaultEditorBuilder(editorDeclaration).buildExpressionLike();
}
