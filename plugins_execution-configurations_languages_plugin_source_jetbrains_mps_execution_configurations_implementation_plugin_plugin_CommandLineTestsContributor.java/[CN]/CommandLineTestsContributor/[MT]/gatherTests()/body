{
  List<Request> requests=new LinkedList<Request>();
  int i=0;
  while (i < myArgs.length) {
    if ("-c".equals(myArgs[i])) {
      i++;
      requests.add(parseRequestFromClass(myArgs[i]));
    }
 else     if ("-m".equals(myArgs[i])) {
      i++;
      parseRequestFromMethod(requests,myArgs[i]);
    }
    i++;
  }
  return requests;
}
