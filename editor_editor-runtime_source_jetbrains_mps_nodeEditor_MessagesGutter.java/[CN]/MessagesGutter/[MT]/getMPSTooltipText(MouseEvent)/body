{
  int y=event.getY();
  List<SimpleEditorMessage> messages=getMessagesAt(y);
  if (messages.size() > 0) {
    StringBuilder text=new StringBuilder();
    for (    SimpleEditorMessage msg : messages) {
      if (text.length() > 0) {
        text.append("\n");
      }
      text.append(msg.getMessage());
    }
    return text.toString();
  }
  return null;
}
