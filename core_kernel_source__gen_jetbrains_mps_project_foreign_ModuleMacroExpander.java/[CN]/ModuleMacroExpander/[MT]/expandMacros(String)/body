{
  if (path == null) {
    return null;
  }
  if (myProjectMacroExpander != null) {
    path=myProjectMacroExpander.expandMacros(path);
  }
  if (myModuleFile != null) {
    path=path.replaceAll("\\$MODULE_DIR\\$/",myModuleFile.getParent().getPath());
    path=path.replaceAll("\\$MODULE_DIR\\$",myModuleFile.getParent().getPath());
  }
  return path;
}
