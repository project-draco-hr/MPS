{
  SNode operand=SLinkOperations.getTarget(_context.getParentNode(),"operand",true);
  if (!(SNodeOperations.isInstanceOf(operand,"jetbrains.mps.baseLanguage.structure.DotExpression"))) {
    return false;
  }
  if (!(SNodeOperations.isInstanceOf(SLinkOperations.getTarget(operand,"operation",true),"jetbrains.mps.uiLanguage.structure.EventAccessOperation"))) {
    return false;
  }
  return true;
}
