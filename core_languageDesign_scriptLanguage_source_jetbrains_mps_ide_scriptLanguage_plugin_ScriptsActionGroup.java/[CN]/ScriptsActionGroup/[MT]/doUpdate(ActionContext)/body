{
  clear();
  setVisible(true);
  if (context.get(MPSProject.class) == null) {
    setEnabled(false);
    return;
  }
  setEnabled(true);
  try {
    List<Language> languages=GlobalScope.getInstance().getVisibleLanguages();
    for (    Language language : languages) {
      addLanguageScriptsGroup(language);
    }
  }
 catch (  ClassNotFoundException e) {
    LOG.error(e);
    setEnabled(false);
  }
}
