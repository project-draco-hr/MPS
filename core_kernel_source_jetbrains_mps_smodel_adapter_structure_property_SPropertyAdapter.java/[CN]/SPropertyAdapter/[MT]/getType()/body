{
  SNode propertyNode=getDeclarationNode();
  if (propertyNode == null) {
    return null;
  }
  SNode dataType=propertyNode.getReferenceTarget("dataType");
  if (dataType == null) {
    return null;
  }
  if (dataType.isInstanceOfConcept(SNodeUtil.concept_PrimitiveDataTypeDeclaration)) {
    return new SPrimitiveDataTypeAdapter((BehaviorReflection.invokeNonVirtual(Boolean.TYPE,dataType,"jetbrains.mps.lang.structure.structure.PrimitiveDataTypeDeclaration","call_isBoolean_1220268791641",new Object[]{}) ? SPrimitiveDataType.BOOL : ((BehaviorReflection.invokeNonVirtual(Boolean.TYPE,dataType,"jetbrains.mps.lang.structure.structure.PrimitiveDataTypeDeclaration","call_isInteger_1220268780075",new Object[]{}) ? SPrimitiveDataType.INT : SPrimitiveDataType.STRING))));
  }
  return new SDataTypeAdapter();
}
