{
  int low=0;
  int high=children.size() - 1;
  while (low <= high) {
    int mid=(low + high) >>> 1;
    FileSystemListenersContainer.Node c=children.get(mid);
    int cmp=pathPart.compareTo(c.pathPart);
    if (cmp < 0) {
      high=mid - 1;
    }
 else     if (cmp > 0) {
      low=mid + 1;
    }
 else {
      return mid;
    }
  }
  return -(low + 1);
}
