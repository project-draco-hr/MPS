{
  Set<String> usedNames=(Set<String>)getUserObject(USED_NAMES);
  if (usedNames == null) {
    usedNames=new HashSet<String>();
    putUserObject(USED_NAMES,usedNames);
  }
  int count=1;
  String name=roughName;
  while (usedNames.contains(name)) {
    name=roughName + (count++);
  }
  usedNames.add(name);
  return name;
}
