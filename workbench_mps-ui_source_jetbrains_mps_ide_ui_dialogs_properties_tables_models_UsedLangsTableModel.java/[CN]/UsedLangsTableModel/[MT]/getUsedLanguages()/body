{
  final List<SModuleReference> list=new ArrayList<SModuleReference>();
  ModelAccess.instance().runReadAction(new Runnable(){
    @Override public void run(){
      for (      SModuleReference tableItem : myTableItems)       if (MPSModuleRepository.getInstance().getModule(tableItem.getModuleId()) instanceof Language)       list.add(tableItem);
    }
  }
);
  return list;
}
