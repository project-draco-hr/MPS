{
  List<Word> words=CollectionUtil.iteratorAsList(sentence.words());
  for (int i=0; i < words.size(); i++) {
    Word word=words.get(i);
    toElement(word,element);
    if (i != words.size() - 1) {
      element.addContent(" ");
    }
  }
  if (sentence.getWordsCount() != 0) {
    element.addContent(".");
  }
 else {
    System.out.println("Skiping");
  }
}
