{
  if (myStepNode == null) {
    return;
  }
switch (myChangeGrouping) {
case Change:
    addIndividualChanges();
  break;
case Input:
addChangesByInput();
break;
case Output:
addChangesByOutput();
break;
}
if (myGroupByStep) {
myResult.add(myStepNode);
}
 else {
for (TraceNodeUI n : myStepNode.getChildren()) {
myResult.add(n);
}
}
myStepNode=null;
}
