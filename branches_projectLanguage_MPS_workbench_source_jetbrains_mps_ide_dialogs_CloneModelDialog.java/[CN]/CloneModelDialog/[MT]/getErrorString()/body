{
  if (myModelProperties.getRoot() == null)   return "Please specify root";
  if (myModelProperties.getLongName() == null || myModelProperties.getLongName().length() == 0)   return "Please specify name";
  if (!myModelProperties.getLongName().startsWith(myModelProperties.getRoot().getPrefix()))   return "Incorrect namespace for specified root";
  if (myModelProperties.getLongName().equals(myModelProperties.getRoot().getPrefix()))   return "Model fqName is the same as prefix. Can't import";
  if (myModelProperties.getImportedLanguages().size() < 1)   return "Model must have at least one language";
  return null;
}
