{
  Set<SNode> nodes=new HashSet<SNode>();
  for (  IWrapper wrapper : wrappers) {
    SNode wrapperNode=wrapper.getNode();
    if (wrapperNode != null) {
      nodes.add(wrapperNode);
    }
  }
  SNode join=joinNodes(nodes);
  if (join == null) {
    return null;
  }
  return NodeWrapper.createWrapperFromNode(join,null);
}
