{
  Set<SNode> nodes=new HashSet<SNode>();
  for (  IWrapper wrapper : wrappers) {
    SNode wrapperNode=wrapper.getNode();
    if (wrapperNode != null) {
      nodes.add(wrapperNode);
    }
  }
  SNode meet=meetNodes(nodes);
  if (meet == null) {
    return null;
  }
  return NodeWrapper.createWrapperFromNode(meet,null);
}
