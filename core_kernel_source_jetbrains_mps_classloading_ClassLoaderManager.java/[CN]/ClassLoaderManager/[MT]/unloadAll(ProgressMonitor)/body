{
  LOG.assertCanWrite();
  monitor.start("Unloading classes...",1);
  try {
    monitor.step("Disposing old classes...");
    for (    MPSClassesListener listener : myClassesHandlers) {
      try {
        listener.onClassesUnload(null);
      }
 catch (      Throwable t) {
        LOG.error(t);
      }
    }
  }
  finally {
    monitor.done();
  }
}
