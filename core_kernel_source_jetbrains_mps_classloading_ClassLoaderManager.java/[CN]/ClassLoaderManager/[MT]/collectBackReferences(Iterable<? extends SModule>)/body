{
  Set<SModule> modules=new HashSet<SModule>();
  Set<SModule> queue=new HashSet<SModule>();
  queue.addAll(modules);
  while (!queue.isEmpty()) {
    SModule module=queue.iterator().next();
    if (!modules.contains(module)) {
      modules.add(module);
      queue.addAll(myBackRefs.get(module));
    }
  }
  return modules;
}
