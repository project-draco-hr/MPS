{
  GenerationTracerView tracerView=new GenerationTracerView(this,tracerNode,getProject());
  myTracerViews.add(tracerView);
  Content content=addContent(tracerView.getComponent(),tracerNode.getNodePointer().toString(),jetbrains.mps.ide.devkit.generator.icons.Icons.getIcon(tracerNode),true);
  getContentManager().setSelectedContent(content);
  Content noTabsContent=getContentManager().getContent(myNoTabsComponent);
  if (noTabsContent != null) {
    getContentManager().removeContent(noTabsContent,true);
  }
  openToolLater(true);
}
