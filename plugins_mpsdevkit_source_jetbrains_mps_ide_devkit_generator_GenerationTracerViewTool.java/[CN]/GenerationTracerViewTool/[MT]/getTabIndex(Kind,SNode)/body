{
  int index=0;
  for (  GenerationTracerView tracerView : myTracerViews) {
    TracerNode tracerNode=tracerView.getRootTracerNode();
    if (tracerNode.getKind() == kind && tracerNode.getNodePointer().resolve(MPSModuleRepository.getInstance()) == node) {
      return index;
    }
    index++;
  }
  return -1;
}
