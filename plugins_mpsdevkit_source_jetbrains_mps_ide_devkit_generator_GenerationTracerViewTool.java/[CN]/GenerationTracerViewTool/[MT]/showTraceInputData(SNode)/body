{
  int index=getTabIndex(GenerationTracerView.Kind.TraceForward,node.getReference());
  if (index > -1) {
    selectIndex(index);
    openToolLater(true);
    return true;
  }
  TraceNodeUI tracerNode=buildForwardTrace(node);
  if (tracerNode == null) {
    return false;
  }
  showTraceView(GenerationTracerView.Kind.TraceForward,tracerNode,node);
  return true;
}
