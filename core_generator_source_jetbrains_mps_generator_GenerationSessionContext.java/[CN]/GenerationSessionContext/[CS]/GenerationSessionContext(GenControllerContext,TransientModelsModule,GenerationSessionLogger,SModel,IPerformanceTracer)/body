{
  myEnvironment=environment;
  myTransientModule=transientModule;
  myOriginalInputModel=inputModel;
  myPerfTrace=performanceTracer;
  myLogger=logger;
  myValidation=new RoleValidation(environment.getOptions().isShowBadChildWarning());
