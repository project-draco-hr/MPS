{
  if (value == null) {
    return true;
  }
  Iterator<LinkMetaclass> constants=ListSequence.<LinkMetaclass>fromList(LinkMetaclass.getConstants()).iterator();
  while (constants.hasNext()) {
    LinkMetaclass constant=constants.next();
    if (value.equals(constant.getName())) {
      return true;
    }
  }
  return false;
}
