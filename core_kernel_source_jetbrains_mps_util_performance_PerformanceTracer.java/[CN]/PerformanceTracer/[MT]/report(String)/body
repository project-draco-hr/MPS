{
  if (top == 0) {
    myStack[0].task.tasks.addAll(myStack[0].children.values());
    myStack[0].task.merge(new HashSet<String>(Arrays.asList(separate)));
    StringBuilder sb=new StringBuilder();
    sb.append("[");
    sb.append(traceName);
    sb.append("]\n");
    myStack[0].task.toString(sb,0);
    return sb.toString();
  }
 else {
    return null;
  }
}
