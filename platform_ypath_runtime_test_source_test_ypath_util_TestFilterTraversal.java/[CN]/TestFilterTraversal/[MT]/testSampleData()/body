{
  IFilter<YNode> nameStartsWithD=new IFilter<YNode>(){
    public boolean accept(    YNode t){
      return t.getName().startsWith("D");
    }
  }
;
  Assert.assertEquals(TREE1_PRECEDING_SIBLINGS_D,YNode.convertToString(TREE_PATH.from(TREE1).traverse(TraversalAxis.DESCENDANTS).filter(nameStartsWithD).traverse(TraversalAxis.PRECEDING_SIBLINGS)));
  Assert.assertEquals(TREE1_FOLLOWING_SIBLINGS_D,YNode.convertToString(TREE_PATH.from(TREE1).traverse(TraversalAxis.DESCENDANTS).filter(nameStartsWithD).traverse(TraversalAxis.FOLLOWING_SIBLINGS)));
}
