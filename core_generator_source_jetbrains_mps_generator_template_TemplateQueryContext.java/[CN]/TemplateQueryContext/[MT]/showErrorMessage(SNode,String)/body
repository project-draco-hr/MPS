{
  SNode inputNode=(node != null) ? node : getInputNode();
  SNodeReference tn=getTemplateNodeRef();
  SNodeReference rnr=getRuleNode();
  myGenerator.getLogger().error(rnr == null ? tn : rnr,message,GeneratorUtil.describeIfExists(inputNode,"input node"),GeneratorUtil.describeIfExists(tn,"template node"));
}
