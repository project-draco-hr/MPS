{
  if (myModel.isUpdateMode()) {
    return;
  }
  if (oldRef != null) {
    myModel.fireReferenceRemovedEvent(oldRef);
  }
  if (newRef != null) {
    myModel.fireReferenceAddedEvent(newRef);
  }
  final ModelEventDispatch md=myEventDispatch;
  if (md != null) {
    md.fireReferenceChange(node,l,oldRef,newRef);
  }
}
