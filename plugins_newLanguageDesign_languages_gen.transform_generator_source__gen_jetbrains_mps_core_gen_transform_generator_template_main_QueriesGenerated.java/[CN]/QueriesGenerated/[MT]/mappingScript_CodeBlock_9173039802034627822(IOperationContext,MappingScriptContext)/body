{
  for (  SNode reverseOp : ListSequence.fromList(SModelOperations.getNodes(_context.getModel(),"jetbrains.mps.baseLanguage.collections.structure.ReverseOperation"))) {
    SNode dotExp=SNodeOperations.as(SNodeOperations.getParent(reverseOp),"jetbrains.mps.baseLanguage.structure.DotExpression");
    if ((dotExp == null)) {
      continue;
    }
    SNode operand=SLinkOperations.getTarget(dotExp,"operand",true);
    if (SNodeOperations.isInstanceOf(TypeChecker.getInstance().getTypeOf(operand),"jetbrains.mps.baseLanguage.collections.structure.ListType")) {
      continue;
    }
    SNode toList=SConceptOperations.createNewNode("jetbrains.mps.baseLanguage.structure.DotExpression",null);
    SLinkOperations.setTarget(toList,"operand",SNodeOperations.copyNode(operand),true);
    SLinkOperations.setTarget(toList,"operation",SConceptOperations.createNewNode("jetbrains.mps.baseLanguage.collections.structure.ToListOperation",null),true);
    SNodeOperations.replaceWithAnother(operand,toList);
  }
}
