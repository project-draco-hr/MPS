{
  if (!s.isReturnMode()) {
    return super.succ(s);
  }
 else {
    Set<ProgramState> result=new HashSet<ProgramState>();
    Program.TryFinallyInfo info=getEnclosingTryFinally();
    if (info != null) {
      result.add(new ProgramState(info.getFinally(),true));
    }
 else {
      result.add(new ProgramState(getProgram().end(),true));
    }
    return result;
  }
}
