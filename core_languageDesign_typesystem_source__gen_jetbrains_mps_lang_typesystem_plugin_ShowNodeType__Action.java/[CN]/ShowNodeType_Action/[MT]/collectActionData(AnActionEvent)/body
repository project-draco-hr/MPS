{
  if (!(super.collectActionData(event))) {
    return false;
  }
  this.context=event.getData(MPSDataKeys.OPERATION_CONTEXT);
  if (this.context == null) {
    return false;
  }
  this.frame=event.getData(MPSDataKeys.FRAME);
  if (this.frame == null) {
    return false;
  }
  this.node=event.getData(MPSDataKeys.SNODE);
  if (this.node == null) {
    return false;
  }
  return true;
}
