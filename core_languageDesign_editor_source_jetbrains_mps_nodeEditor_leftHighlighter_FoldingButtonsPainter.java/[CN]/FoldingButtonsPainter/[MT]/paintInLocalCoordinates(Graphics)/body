{
  Rectangle clipBounds=g.getClipBounds();
  for (  FoldingButton button : myFoldingButtons.values()) {
    if (isVisible(button,clipBounds)) {
      button.paintFeedback(g);
    }
  }
  for (  FoldingButton button : myFoldingButtons.values()) {
    if (isVisible(button,clipBounds)) {
      button.paint(g);
    }
  }
}
