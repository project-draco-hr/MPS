{
  File solutionDescriptorFile=new File(path);
  try {
    File dir=solutionDescriptorFile.getParentFile();
    if (!(dir.exists())) {
      dir.mkdirs();
    }
    solutionDescriptorFile.createNewFile();
    return solutionDescriptorFile;
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return null;
}
