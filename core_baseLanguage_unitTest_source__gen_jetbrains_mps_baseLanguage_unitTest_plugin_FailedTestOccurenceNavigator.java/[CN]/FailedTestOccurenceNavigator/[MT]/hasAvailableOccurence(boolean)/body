{
  DefaultMutableTreeNode node=this.testTree.getCurrentNode();
  if (node == null) {
    return false;
  }
  while (true) {
    TestMethodTreeNode leaf=(TestMethodTreeNode)(next ? node.getNextLeaf() : node.getPreviousLeaf());
    if (leaf == null) {
      return false;
    }
    if (TestTree.isFailed(leaf)) {
      return true;
    }
    node=leaf;
  }
}
