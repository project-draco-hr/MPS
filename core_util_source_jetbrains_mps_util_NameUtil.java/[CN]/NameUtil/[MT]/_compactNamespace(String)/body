{
  if (namespace.length() > 10) {
    String[] parts=namespace.split("\\.");
    StringBuilder result=new StringBuilder();
    for (int i=0; i < parts.length; i++) {
      if (i != 0) {
        result.append(".");
      }
      if (i < parts.length - 2) {
        result.append(parts[i].charAt(0));
      }
 else {
        result.append(parts[i]);
      }
    }
    return result.toString();
  }
  return namespace;
}
