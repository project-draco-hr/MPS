{
  for (  Node node : ListSequence.fromList(graph.getNodes())) {
    Assert.assertTrue(MapSequence.fromMap(representation).get(node).width >= MapSequence.fromMap(horConstraints).get(node));
    Assert.assertTrue(MapSequence.fromMap(representation).get(node).height >= MapSequence.fromMap(verConstraints).get(node));
  }
}
