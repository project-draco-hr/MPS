{
  if (!(super.collectActionData(event))) {
    return false;
  }
  this.editorContext=event.getData(MPSDataKeys.EDITOR_CONTEXT);
  if (this.editorContext == null) {
    return false;
  }
  this.project=event.getData(MPSDataKeys.PROJECT);
  if (this.project == null) {
    return false;
  }
  return true;
}
