{
  return ListSequence.fromList(SLinkOperations.getTargets(getOldEditorComponent(),"method",true)).findFirst(new IWhereFilter<SNode>(){
    public boolean accept(    SNode it){
      return "changeSelection".equals(SPropertyOperations.getString(it,"name")) && (int)ListSequence.fromList(SLinkOperations.getTargets(it,"parameter",true)).count() == 1;
    }
  }
);
}
