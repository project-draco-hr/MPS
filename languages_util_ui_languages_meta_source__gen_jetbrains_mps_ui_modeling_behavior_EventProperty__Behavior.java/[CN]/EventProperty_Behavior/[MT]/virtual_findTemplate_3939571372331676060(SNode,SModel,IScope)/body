{
  return ListSequence.fromList(SModelOperations.getNodesIncludingImported(model,scope,"jetbrains.mps.ui.modeling.structure.EventPropertyGetter")).findFirst(new IWhereFilter<SNode>(){
    public boolean accept(    SNode epg){
      return SLinkOperations.getTarget(epg,"property",false) == thisNode;
    }
  }
);
}
