{
  StringBuilder res=new StringBuilder();
  for (  SNode dep : SLinkOperations.getTargets(_context.getNode(),"dependencies",true)) {
    if (res.length() > 0) {
      res.append(", ");
    }
    res.append(SPropertyOperations.getString(SLinkOperations.getTarget(dep,"target",false),"name"));
  }
  return res.toString();
}
