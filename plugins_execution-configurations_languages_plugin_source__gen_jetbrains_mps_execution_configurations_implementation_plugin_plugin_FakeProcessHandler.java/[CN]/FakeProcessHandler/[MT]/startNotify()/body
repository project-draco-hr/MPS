{
  super.startNotify();
  try {
    myFakeProcess.init();
  }
 catch (  IOException e) {
    if (LOG.isEnabledFor(Level.ERROR)) {
      LOG.error("Process could not be constructed",e);
    }
    throw new IOError(e);
  }
  myOutputReader.startReading();
  myErrorReader.startReading();
  myExecutor.setReady();
}
