{
  try {
    ProjectPropertiesDialog dialog=new ProjectPropertiesDialog(((Project)MapSequence.fromMap(_params).get("project")));
    dialog.show();
  }
 catch (  Throwable t) {
    if (log.isErrorEnabled()) {
      log.error("User's action execute method failed. Action:" + "MPSProjectPaths",t);
    }
  }
}
