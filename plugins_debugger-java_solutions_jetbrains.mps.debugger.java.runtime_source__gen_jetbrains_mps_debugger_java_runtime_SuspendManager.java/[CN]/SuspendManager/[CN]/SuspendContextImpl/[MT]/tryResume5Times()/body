{
  int resumeAttempts=5;
  while (--resumeAttempts > 0) {
    try {
      tryResume();
      break;
    }
 catch (    ObjectCollectedException e) {
    }
catch (    InternalException e) {
      if (e.errorCode() == 13 && getSuspendPolicy() == EventRequest.SUSPEND_ALL) {
      }
 else {
        SuspendManager.LOG.error(e);
        break;
      }
    }
  }
}
