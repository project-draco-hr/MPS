{
synchronized (this) {
    if (!(myDisposed)) {
      myDisposed=true;
      myModelDescriptor.removeModelListener(myModelListener);
      myEventsCollector.remove(myModelDescriptor);
      myEventsCollector.dispose();
      myQueue.runTask(new Runnable(){
        public void run(){
          myDifference.removeChangeSet();
        }
      }
);
    }
  }
}
