{
  StringBuilder sb=new StringBuilder();
  sb.append("Function(");
  int j=0;
  for (  Object arg : this.myArgs) {
    sb.append(arg);
    if (j != this.getParamsCount() - 1) {
      sb.append(", ");
    }
    j++;
  }
  for (int i=this.myArgs.size(); i < this.getParamsCount(); i++) {
    sb.append("__");
    if (i != this.getParamsCount() - 1) {
      sb.append(", ");
    }
  }
  sb.append(") : ");
  sb.append(this.myType);
  return sb.toString();
}
