{
  IFilter filter;
  if (matchValue != null) {
    filter=new IFilter(){
      public boolean accept(      Object n){
        File node=(File)n;
        String actualValue=FileTreePath.FILE_relativeName_Property.Getter.getValue(node);
        return matchValue.equals(actualValue);
      }
    }
;
  }
 else {
    filter=new IFilter(){
      public boolean accept(      Object n){
        File node=(File)n;
        String actualValue=FileTreePath.FILE_relativeName_Property.Getter.getValue(node);
        return actualValue == null;
      }
    }
;
  }
  return (IFilter<File>)filter;
}
