{
  if (value == null) {
    return null;
  }
  Iterator<DayDataType> constants=ListSequence.fromList(DayDataType.getConstants()).iterator();
  while (constants.hasNext()) {
    DayDataType constant=constants.next();
    if (value.equals(constant.getName())) {
      return constant.getValueAsString();
    }
  }
  return null;
}
