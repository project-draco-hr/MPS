{
  List<EditorCell> result=new ArrayList<EditorCell>();
  for (  EditorCell cell : getVisibleChildCells()) {
    if (cell instanceof EditorCell_Collection) {
      result.add(cell);
      result.addAll(((EditorCell_Collection)cell).dfsCells());
    }
 else {
      result.add(cell);
    }
  }
  return result;
}
