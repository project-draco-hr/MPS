{
  try {
    Language language=getLanguageForConceptFqName(fqName);
    if (language == null) {
      return null;
    }
    Class cls=language.getClass(getDescriptorClassName(fqName));
    if (cls != null) {
      return (T)cls.newInstance();
    }
  }
 catch (  Exception e) {
  }
  return null;
}
