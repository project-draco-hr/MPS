{
  if (clipBounds.x > myTextColumnWidth) {
    return;
  }
  for (  AbstractLeftColumn column : myLeftColumns) {
    if (clipBounds.x > column.getX() + column.getWidth()) {
      continue;
    }
    column.paint(g);
    UIUtil.drawVDottedLine((Graphics2D)g,column.getX() + column.getWidth() - 1,(int)clipBounds.getMinY(),(int)clipBounds.getMaxY(),getBackground(),Color.GRAY);
  }
}
