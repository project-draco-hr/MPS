{
  if (HelpUtil.isHelpAction(item)) {
    item.execute();
  }
 else {
    jumpToEditor(false);
    myEditorContext.getRepository().getModelAccess().executeCommand(new Runnable(){
      @Override public void run(){
        item.execute();
      }
    }
);
    myEditorContext.getContextAssistantManager().scheduleUpdate();
  }
}
