{
  final TemporalCollection rules=getRulesTemporalCollectionFor(event.getEventType());
  if (rules == null) {
    throw new MissingPostingRuleException();
  }
  try {
    return (PostingRule)rules.get(event.getWhenOccurred());
  }
 catch (  IllegalArgumentException e) {
    throw new MissingPostingRuleException();
  }
}
