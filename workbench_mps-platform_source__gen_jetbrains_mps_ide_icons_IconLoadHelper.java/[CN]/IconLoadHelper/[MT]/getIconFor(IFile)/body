{
  if (!(file.exists())) {
    return null;
  }
  InputStream is=null;
  try {
    is=file.openInputStream();
    return loadIcon(is);
  }
 catch (  IOException e) {
    if (LOG.isEnabledFor(Level.ERROR)) {
      LOG.error("",e);
    }
    return null;
  }
 finally {
    try {
      if (is != null) {
        is.close();
      }
    }
 catch (    IOException e) {
      if (LOG.isEnabledFor(Level.ERROR)) {
        LOG.error("",e);
      }
    }
  }
}
