{
  Throwable exception=null;
  try {
    clazz.getMethod(methodName).invoke(obj);
  }
 catch (  NoSuchMethodException e) {
    e.printStackTrace();
  }
catch (  IllegalAccessException e) {
    e.printStackTrace();
  }
catch (  InvocationTargetException e) {
    exception=e.getCause();
  }
  return exception;
}
