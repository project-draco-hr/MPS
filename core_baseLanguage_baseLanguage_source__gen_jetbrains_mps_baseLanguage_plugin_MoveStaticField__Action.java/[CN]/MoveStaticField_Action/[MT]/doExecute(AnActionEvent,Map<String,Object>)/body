{
  try {
    Object newNode;
    newNode=MoveNodeDialog.getSelectedObject(((MPSProject)MapSequence.fromMap(_params).get("project")).getProject(),((SNode)MapSequence.fromMap(_params).get("target")),new MoveNodeDialog.NodeFilter("Select classifier to move"){
      public boolean check(      Object selectedObject,      SNode node,      SModelDescriptor model){
        return selectedObject instanceof SNode && SNodeOperations.isInstanceOf(((SNode)selectedObject),"jetbrains.mps.baseLanguage.structure.Classifier") && ListSequence.fromList(SNodeOperations.getAncestors(node,null,false)).contains((SNode)selectedObject);
      }
    }
);
    if (newNode == null) {
      return;
    }
    new RefactoringFacade().execute(RefactoringContext.createRefactoringContextByName("jetbrains.mps.baseLanguage.refactorings.MoveStaticField",Arrays.asList("destination"),Arrays.asList(((SNode)newNode)),((SNode)MapSequence.fromMap(_params).get("target")),((MPSProject)MapSequence.fromMap(_params).get("project"))));
  }
 catch (  Throwable t) {
    LOG.error("User's action execute method failed. Action:" + "MoveStaticField",t);
  }
}
