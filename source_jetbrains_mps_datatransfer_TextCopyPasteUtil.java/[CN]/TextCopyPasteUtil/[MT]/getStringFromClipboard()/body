{
  Clipboard cb=Toolkit.getDefaultToolkit().getSystemClipboard();
  Transferable content=cb.getContents(null);
  if (content == null)   return null;
  String result;
  try {
    result=(String)content.getTransferData(DataFlavor.stringFlavor);
  }
 catch (  Exception e) {
    return null;
  }
  return result;
}
