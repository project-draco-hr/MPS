{
  HashMap<String,INodePropertyValidator> result=new HashMap<String,INodePropertyValidator>();
  result.put("base",new BaseNodePropertyValidator(){
    public boolean checkPropertyValue(    final SNode node,    final String propertyName,    final String propertyValue,    final IScope scope){
      return (SPropertyOperations.getInteger(propertyValue)) >= 2 && (SPropertyOperations.getInteger(propertyValue)) <= 64;
    }
  }
);
  return result;
}
