{
{
    ICursor<SNode> _zCursor1=CursorFactory.createCursor(SNodeOperations.getDescendants(node,null,false));
    try {
      while (_zCursor1.moveToNext()) {
        SNode child=_zCursor1.getCurrent();
        ListOperations.addElement(_results,child);
      }
    }
  finally {
      _zCursor1.release();
    }
  }
}
