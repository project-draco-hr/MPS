{
  ArrayList<SNode> result=new ArrayList<SNode>();
  for (  SNode node : myNodes) {
    for (    SNode child : SNodeOperations.getChildren(node)) {
      if (condition == null || condition == Condition.TRUE_CONDITION || condition.met(child)) {
        result.add(child);
      }
    }
  }
  return result;
}
