{
  List<Runner> runners=new ArrayList<Runner>();
  List<Object[]> parametersList=getParametersList(testClass);
  for (int i=0; i < parametersList.size(); i++) {
    FilepathParameterized.TestClassRunnerForParameters runner=new FilepathParameterized.TestClassRunnerForParameters(testClass.getJavaClass(),parametersList,i);
    runners.add(runner);
  }
  return runners;
}
