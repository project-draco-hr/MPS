{
  try {
    final Set<IModule> modules=SetSequence.fromSet(new LinkedHashSet<IModule>());
    SetSequence.fromSet(modules).addSequence(ListSequence.fromList(RecompileProject_Action.this.project.getProjectSolutions()));
    SetSequence.fromSet(modules).addSequence(ListSequence.fromList(RecompileProject_Action.this.project.getProjectLanguages()));
    SetSequence.fromSet(modules).addSequence(ListSequence.fromList(RecompileProject_Action.this.project.getProjectDevKits()));
    ProgressManager.getInstance().run(new DefaultMakeTask(RecompileProject_Action.this.ideaProject,"Rebuilding",modules,true));
  }
 catch (  Throwable t) {
    if (log.isErrorEnabled()) {
      log.error("User's action execute method failed. Action:" + "RecompileProject",t);
    }
  }
}
