{
  if ((SLinkOperations.getTarget(classCreator,"baseMethodDeclaration",false) == null)) {
    return;
  }
  SNode visibility=SLinkOperations.getTarget(SLinkOperations.getTarget(classCreator,"baseMethodDeclaration",false),"visibility",true);
