{
  boolean founded=false;
  DefaultMutableTreeNode node=testTree.getCurrentNode();
  do {
    MPSTreeNode temp=(MPSTreeNode)(next ? node.getNextNode() : node.getPreviousNode());
    selected=selected + (next ? 1 : -1);
    if (accept(temp)) {
      founded=true;
    }
    node=temp;
  }
 while (!(founded));
}
