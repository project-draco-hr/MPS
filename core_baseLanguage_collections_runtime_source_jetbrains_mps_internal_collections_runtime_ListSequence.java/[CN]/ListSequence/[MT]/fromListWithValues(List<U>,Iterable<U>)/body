{
  List<U> tmp=list;
  if (Sequence.USE_NULL_SEQUENCE) {
    if (list == null && it == null) {
      return NullListSequence.instance();
    }
 else     if (list == null) {
      tmp=new ArrayList<U>();
    }
 else     if (it == null) {
      return fromList(list);
    }
  }
  for (  U u : it) {
    tmp.add(u);
  }
  if (tmp instanceof IListSequence) {
    return (IListSequence<U>)tmp;
  }
  return new ListSequence<U>(tmp);
}
