{
  Map<String,Method> methodMap=new HashMap<String,Method>();
  Class<?> cls=BaseBehaviorDescriptor.getGeneratedClass(fqName,BaseBehaviorDescriptor.behaviorClassByConceptFqName(fqName));
  if (cls != null) {
    Method[] methods=cls.getMethods();
    for (    Method method : methods) {
      String methodName=method.getName();
      if (methodName.startsWith(BehaviorDescriptor.NON_VIRTUAL_METHOD_PREFIX) || methodName.startsWith(BehaviorDescriptor.VIRTUAL_METHOD_PREFIX) || methodName.startsWith(BehaviorDescriptor.CONSTRUCTOR_METHOD_NAME)) {
        if (!methodMap.containsKey(methodName)) {
          methodMap.put(methodName,method);
        }
      }
    }
  }
  return Collections.unmodifiableMap(methodMap);
}
