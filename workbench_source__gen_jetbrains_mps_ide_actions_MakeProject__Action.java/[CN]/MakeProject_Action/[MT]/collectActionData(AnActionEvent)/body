{
  try {
    if (!(super.collectActionData(event))) {
      return false;
    }
    this.ideaProject=event.getData(MPSDataKeys.PROJECT);
    if (this.ideaProject == null) {
      return false;
    }
    this.project=event.getData(MPSDataKeys.MPS_PROJECT);
    if (this.project == null) {
      return false;
    }
  }
 catch (  Throwable t) {
    return false;
  }
  return true;
}
