{
  final List<String> errors=new ArrayList<String>();
  ModelAccess.instance().runReadAction(new Runnable(){
    public void run(){
      for (      SModel sm : models) {
        if (!SModelStereotype.isUserModel(sm))         continue;
        checkModelNodes(sm.getSModel(),errors);
      }
    }
  }
);
  return errors;
}
