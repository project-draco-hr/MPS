{
  List<String> proposals=getProposals(getText());
  if (proposals.isEmpty()) {
    myHint.hide();
    return;
  }
  if (proposals.size() == 1) {
    if (proposals.get(0).equals(getText())) {
      myHint.hide();
      return;
    }
  }
  if (!myHint.isVisible()) {
    myHint.show();
  }
  myHint.setProposals(proposals);
}
