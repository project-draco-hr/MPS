{
  if (obj == this)   return true;
  if (obj instanceof ReferencedNodeContext) {
    ReferencedNodeContext o=(ReferencedNodeContext)obj;
    return EqualUtil.equals(myContextRole,o.myContextRole) && EqualUtil.equals(myContextRefererNode,o.myContextRefererNode) && EqualUtil.equals(myNode,o.myNode);
  }
 else {
    return false;
  }
}
