{
  Iterable<String> test=ListSequence.fromListAndArray(new ArrayList<String>(),"vodka","tequila","whisky");
  Assert.assertEquals("vodka tequila whisky",IterableUtils.join(Sequence.fromIterable(test)," "));
  Assert.assertEquals("vodka, tequila, whisky",IterableUtils.join(Sequence.fromIterable(test),", "));
}
