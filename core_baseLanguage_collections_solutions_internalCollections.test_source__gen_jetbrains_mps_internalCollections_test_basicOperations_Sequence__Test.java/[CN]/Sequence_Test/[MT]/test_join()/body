{
  Iterable<String> test=ListSequence.<String>fromListAndArray(new ArrayList<String>(),"vodka","tequila","whisky");
  Assert.assertEquals("vodka tequila whisky",IterableUtils.join(Sequence.<String>fromIterable(test)," "));
  Assert.assertEquals("vodka, tequila, whisky",IterableUtils.join(Sequence.<String>fromIterable(test),", "));
}
